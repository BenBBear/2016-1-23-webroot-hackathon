		$("#s-navi-trigger").click(function(){
			if($snavi.find(".acordion_tree").css("display") == "none"){
				 $snavi.addClass("active");
				 $snavi.find(".acordion_tree").slideDown("normal");
			} else {
				$snavi.removeClass("active");
				$snavi.find(".acordion_tree").slideUp("normal");
			}
		});
	});
</script>

</body></html>
