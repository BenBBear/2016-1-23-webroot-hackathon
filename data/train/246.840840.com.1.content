<script>
    (function(window, $) {
        var cookie = (function() {
            var res = {};
            var cookies = document.cookie.split('; ');
            var matched = null;
            for(var i = 0, len = cookies.length; i < len; i++) {
                matched = cookies[i].match(/^(.+?)=(.+)$/);
                if (matched && matched.length === 3) {
                    res[matched[1].toString()] = matched[2];
                }
            }
            return res;
        })();
        var revisitData = (function() {
            var res = {
                hasData: false
            };
            var revisitDataString = cookie && cookie.AHARevisitData ? decodeURIComponent(cookie && cookie.AHARevisitData) : null;
            var splited = null;
            var prop = null;
            if (revisitDataString) {
                splited = revisitDataString.split('&');
                for(var i = 0, len = splited.length; i < len; i++) {
                    prop = splited[i].split('=');
                    res[prop[0]] = prop[1];
                }
                res.hasData = true;
            }
            return res;
        })();

        var addComma = function(value) {
            var val = value.toString().replace(/^(-?\d+)(\d{3})/, '$1,$2');
            if (val !== value) {
                return addComma(val);
            }
            return val;
        };
        var toLocaleDateString = function(value) {
          return value.replace(/^(\d{4})(\d{2})(\d{2})$/, '$1年$2月$3日');
        };

        var hasData = revisitData.hasData;
        var $body = $('body');
        var deferred = $.Deferred();

        // ローディングを隠す
        deferred.then(function() {
            return $('#loading-area').fadeOut().promise();
        }).done(function() {
            $body.removeClass('is-loading');
        });


        // データの有無を判定
        if (hasData) {
            // データがある場合のHTMLを生成・表示
            $('.js-revisit-area').html(
                $('#tmpl-revisit').html()
                    .replace(/\{\{premium\}\}/g, addComma(revisitData.PREMIUM))
                    .replace(/\{\{sttdate\}\}/g, toLocaleDateString(revisitData.STT_DATE))
                    .replace(/\{\{estdate\}\}/g, toLocaleDateString(revisitData.EST_DATE))
                    .replace(/\{\{=if\(epolicyFlag\)\{\}\}(.+)\{\{\}\}\}/, (revisitData.EPOLICY_FLAG === '1' ? '$1' : ''))
            );
            $body.toggleClass('is-revisit', hasData);
            window.setTimeout(function() {
                $('#mbx1401_v03').hide(); // T&Tが勝手に書き換えたHTMLを非表示にする
                deferred.resolve();
            }, 400);
        } else {
            // データがない場合のHTMLを生成・表示
            window.setTimeout(function() {
                deferred.resolve();
            }, 400);
        }
    })(this, jQuery);
</script>
