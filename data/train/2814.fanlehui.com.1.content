<script type="text/javascript">
//施工案例图片滚动 效果
(function(){
	
	var caseBox  = $('#caseBox');
	var oUl = caseBox.find('ul');
	var oLi = caseBox.find('ul li');
	
	var oLiW = oLi.width();
	var moveW = (oLiW+8)*oLi.size();
	var n = 0;
	var speed = 15;
	
	oUl.width( moveW );		//设置UL宽度
	
	caseBox.append( oUl.clone() );
	
	var timer = setInterval(move,speed)
	
	caseBox.parent().hover(function(){			//鼠标放上去停止运动
		clearInterval(timer);
	},function(){
		timer = setInterval(move,speed)
	});
	
	function move(){							//运动的方法
		n++;
		if(n == moveW){
			n = 0
		}
		caseBox.css('left',-n);	
	}
	
	
})();
</script>
