$(function(){
	$("#btnFindEmail").click(function(){
		var fname = $("#ep_first_name").val();
		var lname = $("#ep_last_name").val();
		var year = $("#ep_year").val();
		var mon = $("#ep_month").val();
		var day = $("#ep_day").val();
		var nick = $("#ep_nickname").val();


      	var url = "/usr/ajax/findID.do";
      	var data = {};
      	
      	data.firstName = fname;
      	data.lastName = lname;
      	data.birthYear = year;
      	data.birthMonth = mon;
      	data.birthDate = day;
      	data.nickName = nick;
      	
		$.post(url, data, function(result){
			// alert(result.msg);
			if(result.result == '100'){
				$("#ep_user_email").val(result.emailID);
				$("#findEmailErrorBox").addClass("hidden");
			}else{
				$("#findEmailErrorBox").removeClass("hidden");
			}
		});	     

		
	});
	
	var findPwdCk = '0';
	$("#btnFindPwd").click(function(){
		var email = $("#ep_user_email2").val();
		var url = "/usr/ajax/findPWD.do";
		if(findPwdCk == '1'){
			return false;
		}
		$("#spanFindPwd").html("<img src='/common/img/lodingBar.gif'>Eメール発信中");
		findPwdCk = '1';
		
		
      	var data = {};
      	
      	data.emailID = email;
      	
		$.post(url, data, function(result){
			// alert(result.msg);
			if(result.result == '100'){
				$("#ep_user_email").val(result.emailID);
				$("#findEmailErrorBox").addClass("hidden");
				alert("仮パスワードを発送しました。");
				$("#spanFindPwd").html("仮パスワードを送信");
				findPwdCk = '0';
			}else{
				$("#findEmailErrorBox").removeClass("hidden");
				alert("仮パスワードが’発送できませんでした。");
			}
		});	     

		
	});
	
	$("#btnJoin").click(function(){
		// email form validation		
	 	var data = {};
		var emailID = $("#emailID").val();
		data.emailID = emailID;
      	var url = "/usr/ajax/checkID.do";
      	
      	var chackIdCode = "";
      	$.ajaxSetup({ async:false });
		$.post(url, data, function(result){
			chackIdCode = result.result;
		});
		$.ajaxSetup({ async:true });
		if(chackIdCode == '101'){
			alert("既に使用中のIDです。");
			$("#emailID").val('');
			$("#emailID").focus();
			return false;
		} 
		
		var passwd = $("#passwd").val();
		var firstName = $("#firstName").val();
		var lastName = $("#lastName").val();
		var nickName = $("#nickName").val();
		var birthYear = $("#birthYear").val();
		var birthMonth = $("#birthMonth").val();
		var birthDate = $("#birthDate").val();
		
		if(emailID === ''){ alert("Eメールを入力してください。");	return false; }
		if(passwd === ''){ alert("パスワードを入力してください。");	return false; }
		if(firstName === ''){ alert("姓を入力してください。");	return false; }
		if(lastName === ''){ alert("名を入力してください。");	return false; }
		if(nickName === ''){ alert("性別を入力してください。");	return false; }
		if(birthYear === ''){ alert("生年月日をすべて入力してください。");	return false; }
		if(birthMonth === ''){ alert("生年月日をすべて入力してください。");	return false; }
		if(birthDate === ''){ alert("生年月日をすべて入力してください。");	return false; }
		
		// 비밀번호 일치 여부 체크		
		var passwd = $("#passwd").val();
		var passwdCheck = $("#passwd_check").val();
		if(passwd !== passwdCheck){
			alert("パスワードが一致しません。");
			return false;
		}
		
		// 이용약관 및 개인정보보호정책 체크 확인
		var size = $("input[name=registration_check]:checked").size();
		if(size != 2){
			alert("利用規約およびプライバシーポリシーのチェックを行ってください。");
			return false;
			
		}
		
		layer_close();
		layer_open('Member_registration4');
		
	});
	
});
</script>


<script>
$(function(){
	$("#btnAlarmCheckAll").click(function(){
		$("input[name=alarmCode]:checkbox").each(function() {	
			$(this).attr("checked", true);
			$(this).next().addClass("checked");
		});
		$('.customInput input').customInput();
	});
	
	$("#btnAlarmUnCheckAll").click(function(){
		$("input[name=alarmCode]:checkbox").each(function() {
			$(this).attr("checked", false);
			$(this).next().removeClass("checked");
		});
		$('.customInput input').customInput();
	});
	
	$("#btnAlarmDone").click(function(){
		// 여기서 form submit
		var interest = [];
		$("input[name=alarmCode]:checked").each(function(){
			interest.push($(this).val());			
		});
		
		$("#interest").val(interest.join(","));
		
		var dataString = $("#joinMemberForm").serialize();
		
		$.ajax({
            type: "POST",
            url: "/usr/ajax/join.do",
            data: dataString,
            dataType: "json",
            success: function(data) {
            
                alert(data.msg);
                
                if(data.result == '100'){
                	location.href="/";
                }
            },
            error: function(){
                alert('error');
            }
        });
		
	});
});


function fn_test(){
	alert("준비중입니다.");
}
</script>
	
