<script>
$(function(){
	var loggedin = 'false';
	
	if(loggedin === 'true'){
		
		$("#idLogout").removeClass("hidden").show();
		$("#idLogin").hide(); 
	}
	
	$("#btnEmailLogin").click(function(){
		// 이메일 로그인
      	var url = "/usr/ajax/login.do";
      	var data = {};
      	data.joinType="10002";
      	
      	var id = $("#login_id").val();
      	var pwd = $("#login_pw").val();
      	
      	if(id == ''){
      		alert('Eメールを入力してください。');
      		return false;
      	}
      	
      	if(pwd== ''){
      		alert('パスワードを入力してください。');
      		return false;
      	}
    	
    	data.emailID = id;
    	data.passwd = pwd;
      	
		$.post(url, data, function(result){

			if(result.result == '120'){
				$("#idLogout").removeClass("hidden").show();
				$("#idLogin").hide(); 
				layer_close();		
				$("#joinType").val("10002");
		
			}
			
			alert(result.msg);
			
		});
	});
	
	$("#btnLogout").click(function(){
		var joinType = $("#joinType").val();
		
		if(joinType == '10000'){
			fb_logout();
		}else if(joinType == '10001'){
			helper.disconnectServer();
		}else if(joinType == '10002'){
			var url = "/usr/logout.do";
		  	document.location.href=url;
		}
	});
	
});
  // This is called with the results from from FB.getLoginStatus().
  function statusChangeCallback(response) {
      
    console.log('statusChangeCallback');
    console.log(response);

	if (response.status === 'connected') {
      
      	var access_token = response.authResponse.accessToken;
      	var url = "/usr/ajax/login.do";
      	var data = {};
      	data.joinType="10000";
      	data.accessToken = access_token;
		$.post(url, data, function(result){
			if(result.result == '120'){
			 //alert(result.result);
				$("#idLogout").removeClass("hidden").show();
				$("#idLogin").hide(); 
				layer_close();		
				$("#joinType").val("10000");
		
			}
		});	     
  
    }
  }

  window.fbAsyncInit = function() {
  FB.init({
    appId      : '465267540278710',
    //appId      : '624408357660518',
    cookie     : true,  // enable cookies to allow the server to access 
                        // the session
    xfbml      : true,  // parse social plugins on this page
    version    : 'v2.1' // use version 2.1
  });


  FB.getLoginStatus(function(response) {
	statusChangeCallback(response);
  });

  };
  
  // Load the SDK asynchronously
  (function(d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/en_US/sdk.js";
    fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'facebook-jssdk'));

  function fb_logout(){
	  FB.logout(function(response) {
		  var url = "/usr/logout.do";
		  document.location.href=url;
      });
  }
  
  function fb_login(){
	    FB.login(function(response) {
	        if (response.authResponse) {
	        	statusChangeCallback(response);
	        } else {
	            alert('フェイスブックログイン失敗');
	        }
	    }, {
	        scope: 'publish_stream,email'
	    });
	}
</script>
