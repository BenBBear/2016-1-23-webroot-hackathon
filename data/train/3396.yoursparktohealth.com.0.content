<script type="text/javascript">
        // check if it's mobile device
        if (isMobileBrowser()){
            if (location.search.substring(1).toLowerCase().indexOf('version=desktop') > -1){
                DeleteCookie("advocare_version_microsite_mobile");
                // set cookie expiration time at one hour later
                var expDate = new Date();
                var oneHour = 60 * 1000 * 60;
                var expDateInMs = expDate.getTime() + oneHour;
                expDate.setTime(expDateInMs);
                SetCookie("advocare_version_microsite_mobile", "desktop", expDate);
            }
            
            if ((GetCookie("advocare_version_microsite_mobile") == null) || (GetCookie("advocare_version_microsite_mobile")== "")){
                location.href='/111114478/Mobile/Default.aspx';
            }
        }

        // get cookie value
        function getCookieVal(offset){
	        var endstr = document.cookie.indexOf(';', offset);
	        if (endstr == -1) endstr = document.cookie.length;
	        return unescape(document.cookie.substring(offset,endstr));
        }
        
        // get cookie
        function GetCookie(name){
              var arg = name + '=';
              var alen = arg.length;
              var clen = document.cookie.length;
              var i = 0 ;
              while (i < clen){
                    var j = i + alen;
                    if (document.cookie.substring(i,j) == arg)
                          return getCookieVal(j);
                    i = document.cookie.indexOf(' ',i) + 1;
                    if (i == 0) break;
              }
              return null;
        }

        // delet cookie
        function DeleteCookie(name,path,domain){
              if(GetCookie(name)){
                    document.cookie = name + '=' + 
                    ((path)?'; path=' + path:'') + 
                    ((domain)?'; domain=' + domain:'') + 
                    '; expire= Thu, 01-Jan-70 00:00:01 GMT';
              }
        }

        // set cookie
        function SetCookie(name, value, expires, path, domain, secure){
              document.cookie = name + '=' + value + 
              ((expires)?'; expires=' + expires.toGMTString():'') + 
              ((path)?'; path=' + path:'') + 
              ((domain)?'; domain=' + domain:'') + 
              ((secure)?'; secure=' + secure:'');
        }

        // check if it's mobile device
        function isMobileBrowser() {
            var agent=navigator.userAgent.toLowerCase();
            
            //CHECK THE HTTP_USER_AGENT  
            //HEADER VARIABLE FOR ANY ONE OF THE FOLLOWING 
            if (agent != null && agent != "")
            {
                //Create a list of all mobile types 
                var mobiles = new Array("blackberry", "iphone", "android");

                //Loop through each item in the list created above  
                //and check if the header contains that text 
                for (var i = 0; i < mobiles.length; i++) {
                    if (agent.indexOf(mobiles[i].toLowerCase()) > -1)
                    {
                        return true;
                    }
                }
            }
            return false;
        }
    </script>



