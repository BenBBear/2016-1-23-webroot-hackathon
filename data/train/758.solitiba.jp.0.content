    ;(function (window, $) {
	var document = window.document;
	$(document).ready(function () {

		// required jquery.bxslider.js
		slider_e = $('#main-panel').bxSlider({
			mode:'fade',
			auto: true,
			pagerCustom: '#main-panel-thumbnails',
			easing: 'swing',
			speed: 715,
			slideWidth: 2000,
			controls: false,
			preloadImages: 'all',
			onSlideAfter: function() {
				slide_index = $('#main-panel').attr('class');
				if(slide_index) {
					slide_index = slide_index.replace('cur', '');
					slider_e.goToSlide(slide_index);
					$('#main-panel').attr('class', '');
				}
			}
	    });
		$('#main-panel-thumbnails a').hover(function() {
			slide_index = $(this).attr('data-slide-index');
			$('#main-panel').attr('class', 'cur'+slide_index);
			slider_e.stopAuto();
			slider_e.goToSlide(slide_index);
		}, function() {
			slider_e.startAuto();
		});
		$('#main-panel-thumbnails a').click(function() {
			if ($(this).attr('href') != "") {
				window.open($(this).attr('href'));
			}
		});
    //lightbox
    $("#contents").on("click", ".open-lightbox", function(){
      var t = $(document).scrollTop();
      $(".lightbox").css({top: t + 20 + "px"}).fadeIn(300);
      $(".lightbox-bg").fadeTo(300,0.7);
      $("#contents").on("click", ".close-lightbox", function(){
        $(".lightbox,.lightbox-bg").fadeOut(300);
      });
    });
	});
}(this, jQuery));

var html_test = "";

$(function() {
	var agent = navigator.userAgent;
	if(agent.search(/iPhone/) != -1 || agent.search(/iPad/) != -1 || agent.search(/iPod/) != -1 || agent.search(/Android/) != -1){
		$('#splink' ).css('display', 'block');
	}

	var popupFlag = false;
	$(".popup_window").on("click",function(){
		var documentHeight = $(document).height();
		var offsetHeight = $("body").scrollTop() || $(window).scrollTop();
		var windowHeight = $(window).height();
		var popupTop = offsetHeight + (windowHeight / 2 - 200);
		$("#bg-layer").height(documentHeight).fadeTo(300, 0.5);
		$("#popup-frame").css("top",popupTop).fadeIn(300);
		var userAgent = window.navigator.userAgent.toLowerCase();
		var appVersion = window.navigator.appVersion.toLowerCase();
		if($("#video").length > 0){
			if($("#video")[0].currentTime){
				$("#video")[0].currentTime = 0;
			}
			if (userAgent.indexOf('msie') != -1) {
			  	if (appVersion.indexOf('msie 6.') != -1 || appVersion.indexOf('msie 7.') != -1 || appVersion.indexOf('msie 8.') != -1) {
			  		if ($("#popup-frame2").html()) {
			  			$("#popup-frame").html($("#popup-frame2").html());
			  			$("#popup-frame2").html('');
			  		}
				} else {
					$("#video")[0].play();
				}
			} else {
			  	if (appVersion.indexOf('msie 6.') != -1 || appVersion.indexOf('msie 7.') != -1 || appVersion.indexOf('msie 8.') != -1) {
				} else {
					$("#video")[0].play();
				}
			}
		}else {
			$("#flcontent").replaceWith('<div id="flcontent" style="display: none;"></div>');
			$("#flspace").css("visibility","hidden");
		}
		popupFlag = true;

		if(popupFlag == true) {
			$("#bg-layer").on("click",function(){
				closeAction();
			});
			$("#popup-frame .close-button").live("click",function(){
				closeAction();
			});
		}
		var closeAction = function(){
			$("#bg-layer,#popup-frame").fadeOut(300);
			if($("#video").length > 0){
				var userAgent = window.navigator.userAgent.toLowerCase();
				var appVersion = window.navigator.appVersion.toLowerCase();
				if (userAgent.indexOf('msie') != -1) {
				  	if (appVersion.indexOf('msie 6.') != -1 || appVersion.indexOf('msie 7.') != -1 || appVersion.indexOf('msie 8.') != -1) {
				  		if ($("#popup-frame").html()) {
					  		$("#popup-frame2").html($("#popup-frame").html());
					  		$("#popup-frame").html('');
				  		}
					} else {
						$("#video")[0].pause();
					}
				} else {
				  	if (appVersion.indexOf('msie 6.') != -1 || appVersion.indexOf('msie 7.') != -1 || appVersion.indexOf('msie 8.') != -1) {
					} else {
						$("#video")[0].pause();
					}
				}

			}else {
				var flashvars = {};
				var params = {wmode:"transparent"};
				var attributes = {};
				swfobject.embedSWF("swf/index.swf", "flcontent", "100%", "616", "8.0.0", false, flashvars, params, attributes);
				swffit.fit("flcontent", 900, 616, null, 616, true, false );
				$("#flcontent").height(616)
				var userAgent = window.navigator.userAgent.toLowerCase();
				var appVersion = window.navigator.appVersion.toLowerCase();
				if (appVersion.indexOf("msie 9.") != -1) {
				} else {
					document.getElementById('flcontent').style.position = 'absolute';
					document.getElementById('flcontent').style.top = '0px';
				}
				$("#video-content").replaceWith('<div id="video-content"></div>');
				$("#flspace").css("visibility","visible");
			}
			popupFlag = false;
		};
	});


	$(".popup_window_csr").click(function(){
		var newPopup = new $pop( this.href, { type: 'iframe', title: '', effect: 'normal',  width: 618, height: 417 , modal: true } ); return false;
	});
});



