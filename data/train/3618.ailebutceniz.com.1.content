$(document).ready(function() {
	$("#ggf").validationEngine({promptPosition : "bottomLeft", scroll: false});
	$("#ggf").validationEngine('attach');
	$(".yuvar:not(:#yS,#kayan) a")
	.mouseover(
		function(){
			$(this).parent(".yuvar").animate({"background-color": "#3FA5DD"});
		}
		,100
	)
	.mouseout(
		function() {
			$(this).parent(".yuvar").animate({"background-color": "#ADD56A"});
		}
		,100
	);
	
	$("#kayan a")
	.mouseover(
		function(){
			$(this).parent().animate({"background-color": "#3FA5DD"});
			$(this).parent().children(".aYazi").slideDown();
		}
		,100
	)
	.mouseout(
		function() {
			$(this).parent().children(".aYazi").slideUp();
			$(this).parent().animate({"background-color": "#ADD56A"});
		}
		,100
	);

} );
