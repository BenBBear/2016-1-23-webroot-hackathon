    // 日付セット
    function SetArrYmd(warrymd) {
    
      var warrArray = new Array;
      var warrY;
      var warrM;
      var warrD;
      var cnt;
      
      // 送られてきた日付をセット
      if (document.frmKensaku.hidPULLDOWN.value == '0') {
        document.ffrmKensaku.txtARRYMD.value = warrymd;
        document.frmKensaku.txtARRYMD.focus();
        document.frmKensaku.txtARRYMD.select();
        
      } else {
        warrArray = warrymd.split('/');
        
        warrY = warrArray[0];
        warrM = Number(warrArray[1]);
        warrD = Number(warrArray[2]);
        
        for (cnt = 0; cnt < document.frmKensaku.cmbARRY.options.length; cnt++) {
          if (document.frmKensaku.cmbARRY.options[cnt].value == warrY) {
            document.frmKensaku.cmbARRY.selectedIndex = cnt;
            break;
          }
        }
        
        document.frmKensaku.cmbARRM.selectedIndex = warrM - 1;
        document.frmKensaku.cmbARRD.selectedIndex = warrD - 1;
        
      }
      
      // サブウインドウを閉じる
      CloseSubWindow();
      
    }
    
    // ルーム＆日付選択
    function PostASPY0110(wpath, wtype, wdate) {
    
      
      // 日付設定
      SetArrYmd(wdate);
      
