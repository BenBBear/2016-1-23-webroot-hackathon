        document.frmKensaku.submit();
        
      }
      
    }
    
    // 空室検索
    function PostASPY(wpath, waspnumber, sessionid) {
    
      var tmpText;
      var sIndex;
      var cnt;
      
      // テキストボックス
      if (document.frmKensaku.hidPULLDOWN.value == '0') {
        // 宿泊日チェック
        if (CheckYMD(document.frmKensaku.txtARRYMD) == false) {
          return false;
        }
        
        // 泊数チェック
        if (document.frmKensaku.txtHAKSU.value == '') {
          alert('泊数を入力してください');
          document.frmKensaku.txtHAKSU.focus();
          document.frmKensaku.txtHAKSU.select();
          return false;
        }
        if (CheckNum(document.frmKensaku.txtHAKSU) == false) {
          return false;
        }
        
        // 人数チェック
        if (document.frmKensaku.txtADULT.value == '' || document.frmKensaku.txtADULT.value == '0') {
          alert('大人人数を入力してください');
          document.frmKensaku.txtADULT.focus();
          document.frmKensaku.txtADULT.select();
          return false;
        }
        if (CheckNum(document.frmKensaku.txtADULT) == false) {
          return false;
        }
        
        if (document.frmKensaku.hidCHILDoption.value != '0') {
        
          for (cnt = 0; cnt < Number(document.frmKensaku.hidCHILDoption.value); cnt++) {
            if (CheckNum(document.frmKensaku.txtCHILD[cnt]) == false) {
              return false;
            }
          }
          
        }
        
        // 室数チェック
        if (CheckNum(document.frmKensaku.txtSUMROOM) == false) {
          return false;
        }
        
        // 1室人数チェック
        if (document.frmKensaku.hidPERSONselect.value == '1') {
          if (CheckNum(document.frmKensaku.txtPERSON) == false) {
            return false;
          }
        }
        
        // データ格納
        document.frmKensaku.hidARRYMD.value = document.frmKensaku.txtARRYMD.value;
        document.frmKensaku.hidHAKSU.value = document.frmKensaku.txtHAKSU.value;
        document.frmKensaku.hidADULT.value = document.frmKensaku.txtADULT.value;
        document.frmKensaku.hidSUMROOM.value = document.frmKensaku.txtSUMROOM.value;
        
        if (document.frmKensaku.hidPERSONselect.value == '1') {
          document.frmKensaku.hidPERSON.value = document.frmKensaku.txtPERSON.value;
        }
        
        if (document.frmKensaku.hidCHILDoption.value != '0') {
        
          for (cnt = 0; cnt < Number(document.frmKensaku.hidCHILDoption.value); cnt++) {
            document.frmKensaku.hidCHILD[cnt].value = document.frmKensaku.txtCHILD[cnt].value;
          }
          
        }
        
      // コンボボックス
      } else {
        // 宿泊日チェック
        if (CheckComboYMD(document.frmKensaku.cmbARRY, document.frmKensaku.cmbARRM, document.frmKensaku.cmbARRD) == false) {
          return false;
        }
        
        // データ格納
        sIndex = document.frmKensaku.cmbARRY.selectedIndex;
        tmpText = document.frmKensaku.cmbARRY.options[sIndex].value;
        sIndex = document.frmKensaku.cmbARRM.selectedIndex;
        tmpText = tmpText + '/' + document.frmKensaku.cmbARRM.options[sIndex].value;
        sIndex = document.frmKensaku.cmbARRD.selectedIndex;
        tmpText = tmpText + '/' + document.frmKensaku.cmbARRD.options[sIndex].value;
        document.frmKensaku.hidARRYMD.value = tmpText
        sIndex = document.frmKensaku.cmbHAKS.selectedIndex;
        document.frmKensaku.hidHAKSU.value = document.frmKensaku.cmbHAKS.options[sIndex].value;

        sIndex = document.frmKensaku.cmbADULT.selectedIndex;
        document.frmKensaku.hidADULT.value = document.frmKensaku.cmbADULT.options[sIndex].value;

        sIndex = document.frmKensaku.cmbSUMROOM.selectedIndex;
        document.frmKensaku.hidSUMROOM.value = document.frmKensaku.cmbSUMROOM.options[sIndex].value;
        
        if (document.frmKensaku.hidPERSONselect.value == '1') {
          sIndex = document.frmKensaku.cmbPERSON.selectedIndex;
          document.frmKensaku.hidPERSON.value = document.frmKensaku.cmbPERSON.options[sIndex].value;
        }
        
        if (document.frmKensaku.hidCHILDoption.value != '0') {
        
          for (cnt = 0; cnt < Number(document.frmKensaku.hidCHILDoption.value); cnt++) {
            sIndex = document.frmKensaku.cmbCHILD[cnt].selectedIndex;
            document.frmKensaku.hidCHILD[cnt].value = document.frmKensaku.cmbCHILD[cnt].options[sIndex].value;
          }
          
        }
        
	    // 常にコンボボックス
	    switch (document.frmKensaku.hidROOMselect.value) {
	    
	    case '1':
	      sIndex = document.frmKensaku.cmbrOOMTYPE.selectedIndex;
	      document.frmKensaku.hidROOMTYPE.value = document.frmKensaku.cmbrOOMTYPE.options[sIndex].value;
	      break;
	      
	    case '2':
	      sIndex = document.frmKensaku.cmbSEARCHTYPE.selectedIndex;
	      document.frmKensaku.hidSEARCHTYPE.value = document.frmKensaku.cmbSEARCHTYPE.options[sIndex].value;
	      break;

		default:
			document.frmKensaku.hidROOMTYPE.value = '';
			document.frmKensaku.hidSEARCHTYPE.value = '';
			break;

	    }

      }
      
      // サブウインドウを閉じる
      CloseSubWindow();
      
      // ポスト
      document.frmKensaku.action = wpath + 'ASPY' + waspnumber + '.asp?a=a&LB01=server1';
      document.frmKensaku.method = 'POST';
      
      return true;
      
    }
    
