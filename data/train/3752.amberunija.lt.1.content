						$('#btn-skaiciuoti-indeli').click(function(){														
							var indelioSuma = $("#indeliai-field-1").val();							
							var valiuta = $("#indeliai-field-2").val();							
							var laikotarpis = $('#indeliai-field-3').val();	
							var laikotarpisMen = $('#indeliai-field-3 option:selected').text();
							var laikotarpisMet = laikotarpisMen/12;													
							if(!isNonEmptyNumber(indelioSuma))							
							{								
								alert('Prašome įvesti visus reikiamus duomenis!');								
								return false;							
							}														
							var palukanos = [[],[1,1],[3,1],[6,1],[9,1],[12,1.3],[15,1.4],[18,1.5],[24,1.7],[36,2.0],[48,2.3],[60,3]];	

/* S = P*(1+i)^n */
var a = 1 + palukanos[laikotarpis][1]/100;
var b = laikotarpisMet;
var c = Math.pow(a, b);
var d = indelioSuma;
prieaugis = d * c;
					
							 var prieaugis = Math.round(prieaugis*100)/100; 
													
