    <script type="text/javascript" language="javascript">
        $(function () { 
            SetNavigator();
            
        });

        function SetNavigator() {
            var navigatorWidth = 0;
            $(".navigator ul li").each(function (i, obj) {
                navigatorWidth += parseInt($(obj).css('width').replace('px', '')) + parseInt($(obj).css('margin-right').replace('px', ''));
            });
            $(".navigator ul").css('width', navigatorWidth + 5 + 'px');
        }
    </script>

    <style type="text/css">
    
        .navigator {
            height: 35px;
        }

        .navigator ul {
            list-style-type: none;
            margin:0 0 0 auto;
            padding:0;
            height:20px;
        }

        .navigator ul li {
            float: left;
            margin-right: 15px;
        }

        .navigator ul li a {
            text-decoration: none;
            font-size:16px;
            color:#666;
        }

        .navigator ul li a:hover {
            color: #333;
            border-top: 2px solid #777;
            padding-top: 1px;
        }
    
    </style>

</head>

