	for (i = 0; i < msgIdx; i ++) {
		msg += (XJS_MT0_MsgAry[i] + "<BR>");
	}
	msg += XJS_MT0_MsgAry[msgIdx].substring(0,charIdx);
	return msg;
}

// 文字を一文字づつ表示
function XJS_MT0_Typing() {
	if (XJS_MT0_CharIdx == -1) {
		XJS_MT0_MsgIdx = 0;
		XJS_MT0_MsgLength = XJS_MT0_MsgAry[XJS_MT0_MsgIdx].length;
		XJS_MT0_CharIdx = 0;
	} else if (XJS_MT0_CharIdx <= XJS_MT0_MsgLength) {
		var a;
		if (XJS_MT0_CharIdx >= XJS_MT0_MsgLength && XJS_MT0_MsgNum == XJS_MT0_MsgIdx)
			a = "";
		else
			a = "_";
		var msg = '<FONT COLOR="#ff0000"><B>' + XJS_MT0_GetMsg(XJS_MT0_MsgIdx,XJS_MT0_CharIdx) + a + '</B></FONT>';
		if (document.all || document.getElementById) {
			XJS_MT0_MsgObj.innerHTML = msg;
		} else if (document.layers) {
			XJS_MT0_MsgObj.document.open();
			XJS_MT0_MsgObj.document.write(msg);
			XJS_MT0_MsgObj.document.close();
		}
		if (("あ".length) == 1)
			XJS_MT0_CharIdx += 1;
		else
			XJS_MT0_CharIdx += 2;
	} else {
		XJS_MT0_MsgIdx ++;
		if (XJS_MT0_MsgIdx <= XJS_MT0_MsgNum) {
			XJS_MT0_MsgLength = XJS_MT0_MsgAry[XJS_MT0_MsgIdx].length;
			XJS_MT0_CharIdx = 0;
		} else {
			return;
		}
	}
	setTimeout("XJS_MT0_Typing()",XJS_MT0_Speed);
}

// スタイルシートの初期設定
function XJS_MT0_Init() {
