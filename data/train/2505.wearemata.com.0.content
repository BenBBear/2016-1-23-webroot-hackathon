<script type="text/javascript">
$(function(){
	$('.a80').animate({opacity: 0.75},1).css('cursor', 'pointer');
	$('.a80').hover(function(){
		$(this).stop().animate({opacity: 1}, 250);
	}, function() {
		$(this).stop().animate({opacity: 0.75}, 250);
	});
	
});
nvAct = 10;
nvTot = 15;
imgP = 'g1';
imgPact = 1;
imgPtot = 34;

function imgPz(ia, ib ,ic ,id) {
	imgP = ia

		$('#prensaImg').animate({opacity: 0, marginTop:'-=12', marginLeft:'-=10', height:'+=20', width:'+=20'},200).animate({marginLeft:'-=2', marginTop:'+=12', height:'-=20', width:'-=20'},1, function(){
				$('body').append('<img src="img/mata_prensa_'+ia+'_img1.jpg" id="imgOp0" height="0">');	
	$("#imgOp0").bind("load",function(){
			$('#prensaImg')
			.attr('src', 'img/mata_prensa_'+ia+'_img1.jpg')
			.animate({opacity: 1, marginLeft:'+=12'},250);
			imgPact = 1;
			imgPtot = ib;
			//alert (imgPtot)
			$('#clnNum').text(imgPact+' / '+imgPtot);
			$('#clnNom').text(ic);
			$('#clnDet').text(" "+id);
			$(this).remove();
		})
		
	});
	
	
	
		for(b=1; b<10; b++){
			
			cdf = 'img/mata_prensa_'+imgP+'_img'+b+'.jpg';
			
			
				$("#novedades .ddd").append('<img src="'+cdf+'" id="imgOp'+b+'" >');
				$("#imgOp"+b).remove()
			
		
		//imgP = a
	}
	
			
}
function nmc(a){
	if(a === 0){
		if(nvAct > 10){
			$('.nvcnt').animate({opacity: 0.3},50).animate({marginLeft: "+=88px", opacity: 1},  350);
			nvAct -= 1;
		}
	} else {
		if(nvAct <  nvTot){
				$('.nvcnt').animate({opacity: 0.3},50).animate({marginLeft: "-=88px", opacity: 1},  350);
			nvAct += 1;
		}
	}
};

function nmd(a){
				
	if(a === 0){
		if(imgPact > 1){
			imgPact -= 1;
			$('#clnNum').text(imgPact+' / '+imgPtot);
			cdf = 'img/mata_prensa_'+imgP+'_img'+imgPact+'.jpg';
			/*alert(cdf)
			$("#page").append('<img src="'+cdf+'" id="imgOp" >');*/
			$('#prensaImg').animate({opacity: 0},150, function(){
				$(this).attr('src', cdf).animate({opacity: 1},150);
				//$("#imgOp").remove()
				});
			
		}
	} else {
		if(imgPact <  imgPtot){
			imgPact += 1;
			$('#clnNum').text(imgPact+' / '+imgPtot);
			cdf = 'img/mata_prensa_'+imgP+'_img'+imgPact+'.jpg';
			/*$("#page").append('<img src="'+cdf+'" id="imgOp" >');*/
				$('#prensaImg').animate({opacity: 0},150, function(){
					$(this).attr('src', cdf).animate({opacity: 1},150);
					//$("#imgOp").remove()
					})
			
		}
	}
};
</script>
