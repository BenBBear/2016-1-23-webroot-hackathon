<script type="text/javascript">

(function($) {
  //-------------------------------------------------------
  var now = new Date();
  var diff = location.hash? parseInt(location.hash.substr(1), 10) : 0;
  if (!isNaN(diff)) {
    now = new Date( now.getTime() + diff);
  }

  var today     = getKey( now.getTime() );
  var yesterday = getKey( now.getTime() - 86400000 );

  var y = now.getFullYear();
  var m = now.getMonth() + 1;
  var d = now.getDate();

  var rooms = [];
  $.ajax({
  url: "/ajax/oekakichat/banner",
  data: {"app_path": "chokaigi2015", "namespace": "calendar", "year": y, "month": m, "date": d}
  }).success(function( data ) {
    $.each(data, function(i, calendar){
     rooms[calendar.id] = {
      'snapshot': calendar.snapshot,
      'nickname': calendar.owner.nickname
     };
    });

    //-------------------------------------------------------
    var keys = [];
    if (Object.keys) {
      keys = Object.keys(rooms);
    } else {
      for(var prop in rooms) {
        if (data.hasOwnProperty(prop)) keys.push(prop);
      }
    }
    if (keys.length == 0) return;

    var idx = Math.floor(Math.random() * keys.length) ;
    var roomId = keys[ idx ];
    var room = rooms[roomId];

    //-------------------------------------------------------
    var $calendarDay = $(".calendar_day ");
    $calendarDay.find(".year" ).text(y);
    $calendarDay.find(".month").text(m);
    $calendarDay.find(".date" ).text(d);

    var calenderUrl   = 'http://oekaki.seiga.nicovideo.jp/chokaigi2015/calendar';
    var wallUrl       = calenderUrl + '/' + roomId;
    var todayUrl      = calenderUrl + '$' + today;
    var yesterdayUrl  = calenderUrl + '$' + yesterday;
    var thumbUrl      = room.snapshot;

    var $top_calendar_box = $("#top_calendar_box");

    $(".calendar_thumnail img")
      .error(function() {
        $top_calendar_box.hide();
      })
      .attr('src', thumbUrl);
    $(".calendar_thumnail a, .calendar_usr a").attr('href', wallUrl);
    $(".calendar_usr .nickname").text(room.nickname);

    $(".btn_link_yesterday").attr('href', yesterdayUrl);
    $(".btn_link_today").attr('href', todayUrl);

    $("#top_ads_box").after( $top_calendar_box );
    $("#top_pop_box").remove();
    $("#top_blomaga_box").insertAfter(".ads_300x60");
    
    $top_calendar_box.show();

  }).error(function(){
    return;
  });
  
  //-------------------------------------------------------
  function getKey(time) {
    var now = new Date( time );
    var y = now.getFullYear();
    var m = now.getMonth() + 1;
    var d = now.getDate();

    return (y - 2000)
          + (((m < 10)? '0' : '') + m)
          + (((d < 10)? '0' : '') + d);
  }

})(jQuery);
</script>
