    <script type="text/javascript">
      
      $j(function(){
        var $thumbnailBox = $j('#top_info_box').find(".top_info_thumnail");
        var $thumbnailBoxSpring = $j('#top_info_box').find(".top_info_thumnail_spring");
        var $thumbnail = $thumbnailBox.children("a").find("img");
        
        var initialImage = $thumbnail.attr("src");
        
        var $article = $j('#top_info_box').find("dd");
        
        var thumbnailTopPadding = ($j('#top_info_box').find(".content_area_inner").height() - $thumbnailBox.innerHeight()) / ($article.length - 1);
        var imageCount = 0;
        $article.each(function(idx){
          var thumbnailUrl = $j(this).attr("data-thumbnail");
          var thumbnailTitle = $j(this).attr("data-title");
          var thumbnailLink = $j(this).attr("data-link");
          if(thumbnailUrl && thumbnailUrl.length > 0){
            $thumbnailBox.css("display", "table-cell");
            imageCount++;
            if($thumbnail.attr("src") == initialImage){
              $thumbnailBoxSpring.css("padding-top", thumbnailTopPadding * idx);
              $thumbnail.attr("src", thumbnailUrl);
              $j(this).attr("class", "selected");
              $thumbnailBox.children("a").attr({"title": thumbnailTitle, "href" : thumbnailLink});
            }
            $j(this).mouseover(function(){
              $thumbnailBoxSpring.css("padding-top", thumbnailTopPadding * idx);
              $thumbnailBox.children("a").attr({"title": thumbnailTitle, "href" : thumbnailLink});
              $j('#top_info_box').find("dd.selected").attr("class", "");
              $j(this).attr("class", "selected");
              if($thumbnail.attr("src") != thumbnailUrl){
                $thumbnail.attr("src", initialImage);
                $thumbnail.attr("src", thumbnailUrl);
              }
            });
          }
        });
        if(imageCount == 0){
          var thumbnailBoxWidth = $thumbnailBox.width()
          $article.each(function(){
            $j(this).width($j(this).width() + thumbnailBoxWidth);
          });
          var $articleBox = $j('#top_info_box').find("dl");
          $articleBox.width($articleBox.width() + thumbnailBoxWidth);
          $thumbnailBox.remove();
        }
      });
      
    </script>
