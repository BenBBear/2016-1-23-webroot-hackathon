	<script language="javascript">
		var navlistHidden=true;
		var runSlideshow=true;
		var photocount = 0;


		var lightboxOn;
	lightboxOn = false;

	var allCaptions = new Array();
	allCaptions[0] = "";

	var allImages = new Array();
	allImages[0] = "";
	var onimg = 0;


		function e(which) {
			return document.getElementById(which);
		}
		function mobileMenuClick() {
			if (navlistHidden==true)
			{
				e("navlist").style.display='block';
				navlistHidden = false;
			} else {
				e("navlist").style.display='none';
				navlistHidden = true;
			}
		}



		function toggleLightbox(onimgis,movingLightbox)
	{
		
		// alert(lightboxOn);

		onimg = onimgis;
		
		win_width = $(window).width();

		if (win_width > 768 || document.getElementById('top').scrollWidth > 768)
		{
		
			win_height = $(window).height();
			body_height = document.getElementById('top').scrollHeight;
			if (win_height > body_height)
			{
				the_height = win_height;
			} else {
				the_height = body_height;
			}

			// alert(win_height);

			if (lightboxOn==false || lightboxOn==undefined || movingLightbox==true)
			{
				document.getElementById('mylightbox-image').src=allImages[onimg];
				document.getElementById('mylightbox').style.height = the_height + 'px';
				document.getElementById('mylightbox2').style.height = the_height + 'px';
				document.getElementById('mylightbox').style.display='block';
				document.getElementById('mylightbox-image-container').style.display='block'; 

				document.getElementById('mylightbox-caption').innerHTML = allCaptions[onimg];
				
				document.getElementById('arrow-left').style.marginTop= ((($('#mylightbox-image').height()*1)/2)-60) + 'px';
				document.getElementById('arrow-right').style.marginTop=((($('#mylightbox-image').height()*1)/2)-60) + 'px';
				lightboxOn = true;
			} else {
				document.getElementById('mylightbox').style.display='none'; 
				document.getElementById('mylightbox-image-container').style.display='none'; 
				lightboxOn = false;
			}

		}

		return false;
		

	}

	function toggleNextImage() {

		var onimgis;
		onimgis = onimg;
		onimgis = onimgis + 1;
		if (onimgis >= allImages.length)
		{
			onimgis = 0;
		}

		// alert(onimgis);
		// alert(allImages[onimgis]);

		toggleLightbox(onimgis,true);

	}

	function togglePrevImage() {

		var onimgis;
		onimgis = onimg;
		onimgis = onimgis - 1;
		if (onimgis < 0)
		{
			onimgis = allImages.length - 1;
		}

		toggleLightbox(onimgis,true);

	}




		window.addEventListener('resize', function(event){
		  // do stuff here
		  if (window.innerWidth > 768)
		  {
			e("navlist").style.display='block';
				navlistHidden = false;
		  }

			$(".fadein").css("height", ((330*$(".fadein").outerWidth())/500));
			// e("fadein").css("height",
			// alert($(".fadein").outerWidth());

		});

		function init()
		{
			
			if (runSlideshow==true)
			{
			
				$('.fadein img:gt(0)').hide();
				setInterval(function(){
							$('.fadein :first-child').fadeOut().next('img').fadeIn().end().appendTo('.fadein').end();
							}, 4000);

				$(".fadein").css("height", ((330*$(".fadein").outerWidth())/500));

			}

		}

	</script>
