//                $("#TB_title").remove();
//                $("#TB_ajaxContent").addClass("thickbox");
                  alert('クッキー登録に失敗しました。');
            }

	},'json')
    .done(function() { // alert("成功しました。");



   })
 .fail(function(xhr, status, error) {
//	 alert(xhr.status);
//	 alert(xhr.statusText);
