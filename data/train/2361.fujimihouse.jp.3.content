<script type="text/javascript">


$.favoriteAdd = function(code) {
	var param = '';
	var count = 0;
	if(code == 'multiple'){
		$('input[name="detail"]:checked').each(function() {
			if ( param != '' ) param += ',';
			param += $(this).val().substring(7);
			count += 1;
		});
		if ( count == 0 ) {
			alert('物件を指定してください。');
		} else {
			$.callFavoriteAdd(param,code);
		}
	}else{
		$.callFavoriteAdd(code,code);
	}
};
$.callFavoriteAdd = function(bids,code,callback)
{
	var url = 'http://fujimihouse.jp/cookie_favorite_regist/?in_favorite_codes=';
	var param = bids;
	$.post(url + param,{},function(msg){

            if (msg.success == "true" ) {
                for (var i = 0 ; i < msg.room_id.length; i++ ) {

                    $(".room" + msg.room_id[i]).removeClass("favBtn");
                    $(".room" + msg.room_id[i]).addClass("favBtnOff");
                    $(".room_msg" + msg.room_id[i]).text('お気に入りに追加済み');
                    if($("." + msg.room_id[i] + "_fav_link").length) {
                        $("." + msg.room_id[i] + "_fav_link").children().unwrap();
                    }
                }

	            if(code == 'multiple'){
//                        tb_show('', '#TB_inline?height=115&width=400&inlineId=addSuccessBox', '');
//                        $("#TB_title").remove();
//                        $("#TB_ajaxContent").addClass("thickbox");
//                        setTimeout(function(){
