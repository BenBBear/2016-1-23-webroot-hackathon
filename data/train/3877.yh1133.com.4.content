	}
	} 
	catch(e){}
}

	


function setFrameSize(id){
    var iframe = document.getElementById(id);
	try{
	var bHeight = iframe.contentWindow.document.body.scrollHeight;
    var dHeight = iframe.contentWindow.document.documentElement.scrollHeight;
    var height = Math.max(bHeight, dHeight);
	
	if (id=='mainFrame') {
		if(document.getElementById("mainFrame").contentWindow.cLeft_height==1){
			iframe.height = 593;
		}else{
		if (bHeight < 510){
			iframe.height = 510;
		}else{
			iframe.height = bHeight;
		}
		}
	}	else	{
	//alert(height);
	//document.getElementById("Left_scroll").style.paddingTop="0px";
		
		if (bHeight < 510){
			iframe.height = 510;
		}else{
			iframe.height = bHeight;
		}
	}
	}catch (ex){}
}


	
