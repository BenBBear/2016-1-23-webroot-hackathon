<script language="javascript">
function chkValues()
{
	var v;
	v=0;
	if(document.frm.optPollRes[0].checked) v=1;
	if(document.frm.optPollRes[1].checked) v=2;
	if(document.frm.optPollRes[2].checked) v=3;
	if(!v)
	{
		alert("Please select an option.");
		return;
	}
	document.frm.optPoll.value=v;
	//document.frm.submit();
	openWindow(v);
}
</script>
<script language=Javascript>
function chkValues1()
{
	var v=document.frmUpload.txtName.value;
	if(v.length==0)
	{
		alert("Please enter your name.");
		document.frmUpload.txtName.focus();
		return false;
	}

	var v=document.frmUpload.txtComments.value;
	if(v.length==0)
	{
		alert("Please enter the comments.");
		document.frmUpload.txtComments.focus();
		return false;
	}
	document.frmUpload.submit();
}
