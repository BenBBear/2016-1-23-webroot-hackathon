function adjustTwitterHeight(){
	var leftHeight = $(".home_l").height();
	var rightHeight = $(".home_r").height();

	if (leftHeight == null) {
		leftHeight = $(".left_inner").height();
	} 
	if (rightHeight == null) {
		rightHeight = $(".right_inner").height();
	}

	var diffHeight = leftHeight - rightHeight;
	var twitterHeight = diffHeight + $('.twitter-timeline').height();
	$(".twitter-timeline").css("height", twitterHeight+"px"); 
}

