<script type="text/template" id='sponsor_widget_data'>
    <div class="widget-content sponsor">
        <% if (content.commonData.type_id == 9) { %>
            <% if (content.commonData.sponsor_image == '') { %>
                <%- content.commonData.no_content %>
            <% } else { %>
                <div class="sponsor">
                    <%
                    // loop sponsor items
                    _.each(content.sponsorDisplay,function(media_item,media_label,list1){
                    %>
                        <div class="box_content_item">
                            <% if ((content.commonData.media_label_visibility == 1) && (media_label != '')) { %>
                                <h3><%- media_label %></h3>
                            <% } %>
                            <div class="sponsorlist">
                                <%
                                // loop media items
                                _.each(media_item,function(image_data,media_id,list2){
                                %>
                                    <% if (image_data.path != '') { %>
                                        <a href="<%- image_data.path %>" target="_blank" class="sponsor_button">
                                            <img src="<%- image_data.image %>" width="<%- image_data.width %>" alt="<% if (image_data.is_company) { %><%- image_data.sponser %><% } %>" title="<% if (image_data.is_company) { %><%- image_data.sponser %><% } %>" />
                                        </a>
                                    <% } else { %>
                                        <span class="sponsor_button">
                                            <img src="<%- image_data.image %>" width="<%- image_data.width %>" alt="<% if (image_data.is_company) { %><%- image_data.sponser %><% } %>" title="<% if (image_data.is_company) { %><%- image_data.sponser %><% } %>" />
                                        </span>
                                    <% } %>
                                <%
                                //media items loop ends
                                    });
                                %>
                            </div>
                        </div>
                    <%
                    //sponsor items loop ends
                        });
                    %>
                </div>
            <% } %>
        <% } else if (content.commonData.type_id == 10) { %>
            <%
            style_width_height = 'style=width:150px;height:' + content.commonData.image_height + 'px;overflow:hidden;margin:0;text-align:center;';
            count_image = 0;
            _.each(content.sponsorDisplay,function(media_item,media_label,list1){
                if (count_image > 0) {
                    return false;
                }
                _.each(media_item,function(image_data,media_id,list2){
                    count_image++;
                    return false;
                });
            });
            %>
            <% if (count_image == 0) { %>
                <%- content.commonData.no_content %>
