<script type="text/template" id='news_widget_data'>
    <% 
    if (content.message) { %>
            <p><%- content.message %></p>
    <% } else { %>                      
            <% // repeat news items
           if (content.news_data)  
            newsCount = content.news_data.length;
            _.each(content.news_data,function(news,key,list) {   
                imgName = news.media_image;
                widget_news_type = news.news_display_type;      
                flagclas = (key < (newsCount - 1)) ? false: true; // used for display underline in news widget
                %>
                <!-- NEWS TEMPLATE CONTENT STARTS HERE -->
                <div class="team_news">
                    <div class="box_content_item image_withText_wrapper <%- (flagclas == true)?' nobrdr':''%>">
                        <% if (widget_news_type == 'IMAGE' || widget_news_type == 'TEXT') {                              
                                if (widget_news_type == 'IMAGE' && imgName != '' && news.img_exist) { %>
                                    <h3><a href="<%- news.detail_url %>"><%- news.title %></a></h3>
                                    <div class="image_wrapper">
                                        <a href="<%- news.detail_url %>"><img  width="99" height="66" src="<%- news.img_full_path %>" alt="" /></a>
                                    </div>
                                    <div class="text_wrapper">
                                <% }
                                else if (widget_news_type == 'IMAGE') { %>
                                    <div class="text_wrapper w100p">
                                        <h3><a href="<%- news.detail_url %>"><%- news.title %></a></h3>
                                        </div>
                                    <div class="clear"></div>
                                    <div class="text_wrapper w100p">
                                <% }
                                if (widget_news_type == 'TEXT') { %>
                                    <div class="text_wrapper w100p">
                                            <h3><a href="<%- news.detail_url %>"><%- news.title %></a></h3>
                                <% } %>
                                <p class="pre_content">
                                    <% if (widget_type_id == 5) { %>                   
                                            <span class ="team followed_by_date"><%- news.team_name %></span>
                                    <% }  %> 
                                    <span class="date_styler"><%- news.start_date %></span>
                                </p>
                                <p>
                                    <%= news.teaser_text %>
                                    <a href="<%- news.detail_url %>" class="more"><%- news.more %></a>
                                </p>
                            </div>
                        <% } else if (widget_news_type == 'CALENDER') {%> 
                                <h3><a href="<%- news.detail_url %>"><%- news.title %></a></h3>
                                <div class="date-wrapper">
                                    <span class="day"><%- news.startdate_day %></span>
                                    <span class="month"><%- news.startdate_month %></span>
                                </div>
                                <div class="text_wrapper">
                                    <p>
                                        <% if (widget_type_id == 5) { %>                    
                                        <span class ="team"><%- news.team_name %></span><br />
                                        <% } %>       
                                        <%= news.teaser_text %>
                                        <a href="<%- news.detail_url %>" class="more"><%- news.more %></a>  
                                    </p>
                                </div>
                        <% } %>
                    </div>
                </div>
                <!-- NEWS TEMPLATE CONTENT ENDS HERE -->
            <% }); %> 
    <% } %>
</script>
<!-- END OF TEMPLATE AREA -->

<script type="text/template" id='calendar_widget_data'>
    <div class="widget-content calendar">
        <% if (content.calendarDisplay == 'no_data') { %>
            <%- content.commonData.no_data %>
        <% } else { %>
            <%
            item_count = 0;
            // loop items
            _.each(content.calendarDisplay,function(calendar,key,list){
                item_count++;
                link_details = "";
                if (calendar.description_type != 'INTERNAL LINK') {
                    link_details += ' target=_blank ';
                }
                if (calendar.description_type == 'EDITOR') {
                    link_details += ' class=view_detail ';
                }
                calendar_color = ' style=color:' + calendar.event_color;
            %>
                <% if (item_count == content.calendarDisplay.length) { %>
                    <div class="image_withText_wrapper nobrdr">
                <% } else { %>
                    <div class="image_withText_wrapper">
                <% } %>

                    <p class="cal_date"> <%- calendar.event_period %> </p>

                    <% if (calendar.calendar_display != '') { %>
                        <div class="widget_team_class"> <%- calendar.calendar_display %> </div>
                    <% } %>

                    <h3>
                        <% if (calendar.description_type == 'EDITOR') { %>
                            <a openin="modelbox" url="<%- calendar.editor_link %>" href="#" onclick ='return false;'><font <%- calendar_color %>> <%- calendar.calendar_title %> </font></a>
                        <% } else { %>
                            <a href="<%- calendar.link %>" <%- link_details %> event_id="<%- calendar.id %>" widget_type="<%- content.commonData.widget_type %>"><font <%- calendar_color %>> <%- calendar.calendar_title %> </font></a>
                        <% } %>
                    </h3>

                </div>
            <%
            //loop ends
                });
            %>
        <% } %>
    </div>
</script>                    </div>
