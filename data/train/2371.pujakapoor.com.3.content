  };
</script>

<script>
  BN.using(['dropdowns.ResponsiveNavMenu', 'banners.Fader', 'sliders.GalleryCarousel', 'autocomplete.Autocomplete','search.ClientLoginForm'], function($){
	
	// Agent Roster Listings
	
	var agentInfo = (function() {
		var listings = pageInfo.agentListings;
		var active = $('.activeListings');
		var soldTab = $('.soldTab');
		var sold = $('.soldListings');
		var statusNav = $('.statusNav');

		function numberWithCommas(x) {
      return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
    };

		function createListingTemplate(listing) {
			var template = $('.agentTemplate').html();

      var agentTemplate = BN.template(template, {
        address : listing.Address,
        city : listing.City,
        state : listing.State,
        zip : listing.Zipcode,
        pic : listing.Picture,
        tag : listing.TagLine,
        url : listing.VirtualTourURL == '' ?  listing.ListingDetailHref : listing.VirtualTourURL,
        price : '$' + numberWithCommas(listing.Price) ? '$' + numberWithCommas(listing.Price) : '',
        beds : listing.ListingBedroomCount ? '<i class="flaticon-bed2 listingIcons"></i>' + listing.ListingBedroomCount + ' Beds' : '',
        baths : listing.ListingBathroomCount ? '<i class="flaticon-shower8 listingIcons"></i>' + listing.ListingBathroomCount + ' Baths' : '',
        garage : listing.GarageCount ? '<i class="fa fa-car listingIcons"></i>' + listing.GarageCount + ' Garages' : '',
        sqft : listing.ListingInteriorSize ? '<i class="fa fa-home listingIcons"></i>' + numberWithCommas(listing.ListingInteriorSize) : ''
      }, '');

      return agentTemplate;
		}

		function init() {
			var hasSolds = false;
			$.each(listings, function(_, listing) {
				var agentListing = createListingTemplate(listing);

				if (listing.Status === 'Sold') {
					sold.append(agentListing);
					hasSolds = true;
				} else {
					active.append(agentListing);
				}
			});
			
			if (hasSolds) {
				soldTab.show();
			}

			statusNav.on('click', 'li', function() {
				statusNav.children().removeClass('activated');
				$(this).addClass('activated');

				$(this).hasClass('activeTab') ? toggleList(sold) : toggleList(active);
			});
		}

		function toggleList(that) {
			that.fadeOut(300);
    	$('.customListings').not(that).fadeIn(300);
		}

		if(statusNav.get(0)) init();
	})();
	
	// End of Agent Roster Listings
	
	
    var win = $(window);

    // Main Nav
    var responsiveNav = new BN.dropdowns.ResponsiveNavMenu(pageSettings.navigation.container, pageSettings.navigation.opts);
    responsiveNav.init();

    // Main Banner
    if($(pageSettings.banner.container).length > 0) {
      var banner = new BN.banners.Fader(pageSettings.banner.container, pageSettings.banner.opts);
      banner.init()
    }

    // Client Login Form
    var loginForm = new BN.search.ClientLoginForm("#global_contact_form");
    if($("#global_contact_form").length > 0)
    {
      loginForm.init();
    }

    // Featured Carousel
    var featuredListings = (function() {
      var opts, featuredListingSlider;

      function clearSlider() {
        featuredListingSlider.destroy();
        $(pageSettings.featured.container).empty();
      }

      function setSlideNumber() {
        return win.width() > pageSettings.featured.winSize[0] ? pageSettings.featured.slideNums[0] : win.width() > pageSettings.featured.winSize[1] ? pageSettings.featured.slideNums[1] : win.width() > pageSettings.featured.winSize[2] ? pageSettings.featured.slideNums[2] : pageSettings.featured.slideNums[3];
      }

      function startSlider() {
        opts = pageSettings.featured.opts;
        if(pageSettings.featured.responsive) opts.visibileSlidesNum = setSlideNumber();

        featuredListingSlider = new BN.sliders.GalleryCarousel(pageSettings.featured.container, 'Featured', opts);
        featuredListingSlider.init();
      }

      function init() {
        startSlider();

        win.on('resize', function() {
          if(pageSettings.featured.responsive) {
            if(setSlideNumber() !== opts.visibileSlidesNum) {
              clearSlider();
              startSlider();
            }
          }
        });
      }

      if($(pageSettings.featured.container).get(0)) init();
    })();


    // Quick Search
    var quickSearch = (function() {
      var quickSearch = $(pageSettings.quickSearch.quicksearch),
        refineLocation = $(pageSettings.quickSearch.refinelocationinput),
        refineSearch_autocomplete = new BN.autocomplete.Autocomplete(pageSettings.quickSearch.refinelocationinput,{excludeCat:'["school","streetname","mls#"]'}),
        propertySearchField = quickSearch.find(pageSettings.quickSearch.searchinput),
        propertySearchButton = quickSearch.find(pageSettings.quickSearch.propertySearchButton),
        advancedSearch = quickSearch.find(pageSettings.quickSearch.advancedSearch),
        // quickSearchTop = parseInt(quickSearch.css("top").replace(/[^0-9]/g, ""), 10),
        autocomplete  = new BN.autocomplete.Autocomplete(pageSettings.quickSearch.searchinput);

      function submitSearch(e) {
        var searchval,
          min_price = Number($.trim(quickSearch.find(pageSettings.quickSearch.propertySearchMinPrice).val()).replace(",","")),
          max_price = Number($.trim(quickSearch.find(pageSettings.quickSearch.propertySearchMaxPrice).val()).replace(",","")),
          search_options = {},
          search_options_string,
          propertySearchField = quickSearch.find(pageSettings.quickSearch.searchinput),
          location = [{Location:{}}];

        //If autocomplete then use rel value
        if(propertySearchField.attr('rel') == null || propertySearchField.attr('rel') == '') {
          searchval = propertySearchField.val();
        } else {
          searchval = '';

          var item = JSON.parse(propertySearchField.attr('rel'));

          location[0].Location.LocationValue = item.locationValue;
          location[0].Location.LocationDisplay = item.value;
          location[0].Location.MlsId = item.mlsId;
          location[0].Location.LocationType = item.locationType;
          location[0].Location.GeoResource = item.geoResource;
          location[0].Location.GeoType = item.geoType;
          location[0].Location.GeoSearchType = item.geoSearchType;
          location[0].Location.KmlURL = item.KmlURL;
          location[0].Location.Point = item.Point;

          if(location[0].Location.LocationType == "5" || location[0].Location.LocationType == "9") searchval = location[0].Location.LocationValue;

          search_options.location = location;
        }

        e.preventDefault();

        search_options.beds = quickSearch.find(pageSettings.quickSearch.propertySearchBeds).val();
        search_options.baths = quickSearch.find(pageSettings.quickSearch.propertySearchBaths).val();
        search_options.proptype = quickSearch.find(pageSettings.quickSearch.propertySearchPropertyType).val();
        search_options.openhouse = (quickSearch.find(pageSettings.quickSearch.propertySearchOpenHouse).attr("checked") == "checked") ? "true" : "false";
        search_options.agentListings = (quickSearch.find(pageSettings.quickSearch.propertySearchAgentListings).attr("checked") == "checked") ? "true" : "false";
        search_options.minPrice = (isNaN(min_price)) ? 0 : min_price;
        search_options.maxPrice = (isNaN(max_price)) ? 0 : max_price;
        search_options.maxPrice = (isNaN(max_price)) ? 0 : max_price;

        search_options_string = JSON.stringify(search_options);

        $("<form action='" + pageInfo.baseUrl + "search-results' method='POST' style='display:none;'>" +
          "<input type='hidden' name='search_init_location' value='" + searchval + "'>" +
          "<input type='hidden' name='search_init_options' value='" + search_options_string + "'>" +
          "<input type='submit' value='submit'>" +
        "</form>").appendTo('body').submit();
      };

      function openAdvancedSearch(e){
        //quickSearch.css({"top" : quickSearchTop - quickSearch.find(pageSettings.quickSearch.advancedContainer).outerHeight(true)});
        advancedSearch.fadeIn(250);
      };

      function hideAdvancedSearch(e){
        //quickSearch.css({"top" : quickSearchTop});
        advancedSearch.hide();
      };

      function init() {
        autocomplete.init();

        propertySearchField.keydown(function(e){
          if(e.keyCode == 13)submitSearch(e);
        });

        propertySearchButton.click(submitSearch);

        if(pageSettings.quickSearch.useAdvanced) {
          propertySearchField.on("focus", openAdvancedSearch);
          quickSearch.find(pageSettings.quickSearch.closeOptionsButton).on("click", hideAdvancedSearch);
        }
      }

      if(quickSearch.get(0)) init();
    })();

  });
</script>
		

		
