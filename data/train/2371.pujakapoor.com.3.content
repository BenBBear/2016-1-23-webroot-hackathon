		<script type="text/javascript">
var autocompleteSettings={
	searchinput             		:'.search-input',
	refinelocationinput     		:'#r_location_input',
	quicksearch             		:'.quick-search',
	advancedSearch          		:'.quick-search-advanced-options',
	advancedSearchToggle    		:'.quick-search-advanced-options-toggle',
	propertySearchButton    		:'.search-submit',
	propertySearchBeds      		:'.quick-search-bedrooms',
	propertySearchBaths     		:'.quick-search-bathrooms',
	propertySearchPropertyType     	        :'.quick-search-propertyType',
	propertySearchAgentListings     	:'.quick-search-agent-listings',
	propertySearchOpenHouse 		:'.quick-search-open-house',
	propertySearchMinPrice  		:'.quick-search-price-minimum',
	propertySearchMaxPrice  		:'.quick-search-price-maximum',
	searchContainer         		:'.quick-search-bar',
	advancedContainer       		:'.quick-search-advanced-options',
	closeOptionsButton      		:'.close-additional-options'
};



</script>



<script type="text/javascript">
BN.using(["ui.UiComponents", "autocomplete.Autocomplete"], function($){
	var quickSearch   = $(autocompleteSettings.quicksearch),
		refineLocation   = $(autocompleteSettings.refinelocationinput),
		refineSearch_autocomplete  = new BN.autocomplete.Autocomplete(autocompleteSettings.refinelocationinput,{excludeCat:'["school","streetname","mls#"]'}),
		autocomplete  = new BN.autocomplete.Autocomplete(autocompleteSettings.searchinput);
		
		
	var initializeQuickSearch = function(){
		var advancedSearch          		= quickSearch.find(autocompleteSettings.advancedSearch),
			advancedSearchToggle    		= quickSearch.find(autocompleteSettings.advancedSearchToggle),
			propertySearchField     		= quickSearch.find(autocompleteSettings.searchinput),
			propertySearchButton    		= quickSearch.find(autocompleteSettings.propertySearchButton),
			propertySearchBeds      		= quickSearch.find(autocompleteSettings.propertySearchBeds),
			propertySearchBaths     		= quickSearch.find(autocompleteSettings.propertySearchBaths),
			propertySearchPropertyType    		= quickSearch.find(autocompleteSettings.propertySearchPropertyType),
			propertySearchAgentListings    		= quickSearch.find(autocompleteSettings.propertySearchAgentListings),
			propertySearchOpenHouse			= quickSearch.find(autocompleteSettings.propertySearchOpenHouse),
			propertySearchMinPrice  		= quickSearch.find(autocompleteSettings.propertySearchMinPrice),
			propertySearchMaxPrice  		= quickSearch.find(autocompleteSettings.propertySearchMaxPrice),
			searchContainer         		= quickSearch.find(autocompleteSettings.searchContainer),
			advancedContainer       		= quickSearch.find(autocompleteSettings.advancedContainer),
			closeOptionsButton      		= quickSearch.find(autocompleteSettings.closeOptionsButton),
			advancedSearchHeight    		= advancedContainer.outerHeight(true),
			quickSearchTop          		= parseInt(quickSearch.css("top").replace(/[^0-9]/g, ""), 10);
			
		var submitSearch = function(e){

			var searchval,
				min_price = Number($.trim(propertySearchMinPrice.val()).replace(",","")),
				max_price = Number($.trim(propertySearchMaxPrice.val()).replace(",","")),
				search_options = {},
				search_options_string;
			var location = [{Location:{}}];
			//If autocomplete then use rel value
			if(propertySearchField.attr('rel') == null || propertySearchField.attr('rel') == '')
			{
				searchval = propertySearchField.val();
			}	
			else{
				searchval='';
				var item = JSON.parse(propertySearchField.attr('rel'));
				location[0].Location.LocationValue = item.locationValue;
				location[0].Location.LocationDisplay = item.value;
		        	location[0].Location.MlsId = item.mlsId;
		        	location[0].Location.LocationType = item.locationType;
	                	location[0].Location.GeoResource = item.geoResource;
            	        	location[0].Location.GeoType = item.geoType;
	                	location[0].Location.GeoSearchType = item.geoSearchType;
				location[0].Location.KmlURL = item.KmlURL;
				location[0].Location.Point = item.Point;
		        	if (location[0].Location.LocationType == "5" || location[0].Location.LocationType == "9")
                		searchval = location[0].Location.LocationValue;
				search_options.location = location;
			}
			e.preventDefault();
			
			search_options.beds      = propertySearchBeds.val();
			search_options.baths     = propertySearchBaths.val();
			search_options.proptype     = propertySearchPropertyType.val();
			search_options.openhouse = (propertySearchOpenHouse.attr("checked") == "checked") ? "true" : "false";
     		        search_options.agentListings = (propertySearchAgentListings.attr("checked") == "checked") ? "true" : "false";
			search_options.minPrice  = (isNaN(min_price)) ? 0 : min_price;
			search_options.maxPrice  = (isNaN(max_price)) ? 0 : max_price;
			search_options.maxPrice  = (isNaN(max_price)) ? 0 : max_price;
			search_options_string    = JSON.stringify(search_options);
			
			$("<form action='" + pageInfo.baseUrl + "search-results' method='POST' style='display:none;'>" + 
				"<input type='hidden' name='search_init_location' value='" + searchval + "'>" +
				"<input type='hidden' name='search_init_options' value='" + search_options_string + "'>" +
				"<input type='submit' value='submit'>" +
			"</form>").appendTo('body').submit();
		};
		
		var openAdvancedSearch = function(e){
			//quickSearch.css({"top" : quickSearchTop - advancedSearchHeight});
			advancedSearch.fadeIn(250);
		};
		
		var hideAdvancedSearch = function(e){
			//quickSearch.css({"top" : quickSearchTop});
			advancedSearch.hide();
		};
		
		propertySearchField.keydown(function(e){
			if(e.keyCode == 13)submitSearch(e);
		});
		propertySearchButton.click(submitSearch);
		propertySearchField.on("focus", openAdvancedSearch);
		closeOptionsButton.on("click", hideAdvancedSearch);           
	};
		
	// on document ready
	$(function(){

		if(quickSearch.length > 0) {
			initializeQuickSearch();
			autocomplete.init();
		}

		if(refineLocation.length > 0) {
			refineSearch_autocomplete.init();
		}
		
		$(".keep_inputs").baynet('inputs_keep_init_val');
		$(".keep_inputs_use_data").baynet('inputs_keep_init_val', {use_data : true});


	});
	
});
</script>


		<script>
  var pageSettings = {};


  /**** Navigation ****/

  pageSettings.navigation = {
