    opts: {}
  };


  /**** Main Banner ****/

  pageSettings.banner = {
    container: '#hero',
    opts: {
      fadeSpeed: 1000,
      autoPlayFadeSpeed: 3000,
      autoPlay: true,
      autoPlayDelay: 3000
    }
  };


  /**** Featured Listings ****/

  pageSettings.featured = {
    container: '#featuredListings',
    opts: {
      visibileSlidesNum : 1,
      listingLimit : 99,
      useCustomControls : true,
      customControlPreviousSelector : "#featuredListingsControlPrevious",
      customControlNextSelector : "#featuredListingsControlNext",
      listingItemTemplateSelector : "#featuredListingTemplate",
      autoPlay : true,
      direction : 'left',
      delay : 6000,
	  speed : 1500,
      initialDelay : 2000
    },
    winSize: [768, 481],
    slideNums: [4, 3, 1, 1],
    responsive: true
  };


  /**** Quick Search ****/

  pageSettings.quickSearch = {
    searchinput: '.search-input',
    refinelocationinput: '#r_location_input',
