<script type="text/javascript">

$(function(){
	resizing();
	$(window).resize(function(){
		resizing();
	});
});

function resizing() {
		$("#sidebar").css("height", $(window).height());
		
		var height = $(window).height() / 3;
		var width = ($(window).width() - $("#sidebar").width() ) / 3;
		
		if(height < 220) {
			height = 220;
		}
		if(width < 330) {
			width = 330;
		}
		
		$("#container li").css({
			"height": height,
			"width": width
		});
		$("#container li div").css({
			"height": height,
			"width": width
		});
		
		
		$("#container li").each(function(){
			var index = $("#container li").index(this);
			var p =  "-" + width * (index % 3) + "px -" + height * Math.floor(index / 3) + "px";
            $(this).css({
				"top": height * Math.floor(index / 3),
				"left": width * (index % 3),
				backgroundPosition: p,
				backgroundSize:width * 3
			});
        });
		$("#container li div").each(function(){
			var index = $("#container li div").index(this);
			var p =  "-" + width * (index % 3) + "px -" + height * Math.floor(index / 3) + "px";
			
            $(this).css({
				backgroundPosition: p,
				backgroundSize:width * 3
			});
		});
}

</script>

