    <script type="text/javascript">
        $(function()
            {
            Dwellant = { };

            $("#mainForm input[name=save]").val("635891014518302000_GJN7MopX9YkAkBzxcRqAZaIuGOE=");

            $("#returnUrl").val('');

            document.getElementById("mainForm").action = 'https://ivernacourt-secure.dwellant.com/Account/LogOn';
            
            $("#customSignIn").click(function(event)
                {
                codwellers.util.Window.busy();
            });


            $("#customRegister,.customRegister").click(function(event)
                {
                event.preventDefault();

                document.location = 'https://ivernacourt-secure.dwellant.com/Welcome';
            });

            window.showMiscellaneousDialog = function(event, href)
            {
                event.preventDefault();

                codwellers.util.Dialog2.quickShow(href);
            };

            $("#contactUs").click(function(event)
                {
                event.preventDefault();

                codwellers.util.Dialog2.quickShow("/Account/AddFeedback/$Account$LogOn");
            });
            
            $("#help").click(function(event)
            {
                event.preventDefault();

                codwellers.util.Dialog2.quickShow("/Help/Display/$Account$LogOn");
            });
            
            $("#terms").click(function(event)
            {
                event.preventDefault();

                codwellers.util.Dialog2.quickShow("/Help/Display/_TermsAndConditions?Title=Terms+and+conditions");
            });
            
            $("#privacyPolicy").click(function(event)
            {
                event.preventDefault();

                codwellers.util.Dialog2.quickShow("/Help/Display/_PrivacyPolicy?Title=Privacy+policy");
            });
            
            $("#cookiePolicy").click(function(event)
            {
                event.preventDefault();

                codwellers.util.Dialog2.quickShow("/Help/Display/_PrivacyPolicy?Title=Privacy+policy");
            });
            
            $("a.masterDialog").click(function(event)
                {
                window.showMiscellaneousDialog(event, this.href);
            });

            $("#aboutUs").click(function(event)
                {
                window.showMiscellaneousDialog(event, "/Marketing/PopupPanel");
            });

            $("#forgottenPassword").click(function(event)
                {
                document.location = 'https://ivernacourt-secure.dwellant.com/Account/ForgottenPasswordStandard';
            });
           
            $("body").on("click", "a.slideShow", function (event)
            {
                event.preventDefault();

                var trigger = $(this);

                var pauseTime2 = trigger.attr("cod-pause");

                if (pauseTime2 == null)
                    pauseTime2 = "9000";

                var dialog = new codwellers.util.Dialog2("/Account/SlideShowDialog?pauseTime=" + pauseTime2);

                dialog.loadAndShow();

                return false;
            });
            

            //cookies stuff
            $('#dwe-btnCookiesOk').click(function () {
                $.cookie("cookieNotification", "ok", { expires: 365 });
                $('div.dwe-cookie-container').fadeOut();
            });
            if ($.cookie("cookieNotification") != "ok") 
                $('div.dwe-cookie-container').fadeIn();

            var errorText = "" ;

            if (errorText != "")
            {
                $("#errorText").text(errorText).css("color", "red");
                $("#errorContainer").show();
            }

            document.getElementById("displayNameOrEmailAddress").focus();

            $("#searchForFreeTextButton").click(function()
                {
                codwellers.util.Window.busy();

                this.form.submit();
            });


             $('#hiddenLogin').hide();
             $('#hiddenLoginLink').show();

                // initialise superfish menu
                // call supersubs first, then superfish, so that subs are 
                // not display:none when measuring. Call before initialising 
                // containing tabs for same reason.             
                $("ul.sf-menu").supersubs({
                    minWidth: 6,   // minimum width of sub-menus in em units 
                    maxWidth: 20,   // maximum width of sub-menus in em units 
                    extraWidth: 0     // extra width can ensure lines don't sometimes turn over 
                    // due to slight rounding differences and font-family 
                }).superfish({
                    dropShadows: false
                });

            var slideShow = $("#slideShow");

            if (slideShow.size() > 0) {
                
                var pauseTime = slideShow.attr("cod-pause");

                if (pauseTime == null)
                    pauseTime = "9000";
                
                $.post("/Account/SlideShow?pauseTime=" + pauseTime, { },
                    function(data)
                    {
                        slideShow.html(data);
                    });


            }

        });
    </script>

