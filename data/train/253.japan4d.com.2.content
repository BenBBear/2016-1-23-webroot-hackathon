    //image swapping function:
    function hiLite(imgDocID, imgObjName, comment) {
        if (browserVer == 1) {
            document.images[imgDocID].src = eval(imgObjName + ".src");
            window.status = comment; return true;
        }
    }
    //end hiding -->
    ShowLuckyNumber();

