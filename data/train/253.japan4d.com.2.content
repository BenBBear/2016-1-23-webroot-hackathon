    var strOutput;
    function ShowLuckyNumber() {

        var luckyNumber = Math.floor((Math.random() * 9999) + 1);
        var imgStr = "";

        var luckyNums = document.getElementById("luckyNums");

        var frontTag = "<img src='pics/";
        var backTag = ".png' width='80px' />";

        if (luckyNumber < 10)
            strOutput = "000" + luckyNumber;
        else if (luckyNumber < 100)
            strOutput = "00" + luckyNumber;
        else if (luckyNumber < 1000)
            strOutput = "0" + luckyNumber;
        else
            strOutput = "" + luckyNumber;


        for (var i = 0; i < 4; i++) {
            imgStr += frontTag + strOutput.substr(i, 1) + backTag;
        }
        luckyNums.innerHTML = imgStr;
        return false;
    }

