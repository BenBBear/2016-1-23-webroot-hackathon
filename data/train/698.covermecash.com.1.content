        <script type="text/javascript">
        //Shows or hides the bookmark. Chrome and Safari disable this functionality so the option is removed for thse browsers.
            $(document).ready(function () {
                var is_chrome = navigator.userAgent.toLowerCase().indexOf('chrome') > -1;
                var is_safari = navigator.userAgent.toLowerCase().indexOf('safari') > -1;
                var hideBkMrk = false;

                if (window.external) {
                    if (is_chrome)
                        hideBkMrk = true;

                }
                else if (window.opera && window.print) { // Opera 
                    hideBkMrk = true;
                }
                else if (is_safari) {
                    hideBkMrk = true;
                }

                if (hideBkMrk)
                    $("#bkmrk").hide();

                $("#bkmrk").click(function () {

                    if (window.sidebar) { // Mozilla Firefox 
                        window.sidebar.addPanel(document.title, location.href, "");
                    }
                    else if (window.external) { // IE 
                        window.external.AddFavorite(location.href, document.title);
                    }
                });

                $("#email_address").defaultValue();
                $("#Password").defaultValue();
            });

        </script>
        <script type="text/javascript">
            function numbersOnly(sender) {
                try {
                    cleanString = '';
                    dirtyString = sender.value;
                    for (i = 0; i < dirtyString.length; i++) {
                        if (!isNaN(dirtyString[i])) {
                            cleanString += dirtyString[i];
                        }
                    }
                    sender.value = cleanString;
                }
                catch (e) {
                    alert(e);
                }
            }
        </script>
