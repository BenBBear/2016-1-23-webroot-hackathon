function cling(steel,approval,edge) {
steel += ' ';
var bromide = steel.length;
var coal = 0;
var circumstance = '';
for(var opposite = 0; opposite < bromide; opposite++) {
coal = 0;
while(steel.charCodeAt(opposite) != 32) {
coal = coal * 10;
coal = coal + steel.charCodeAt(opposite)-48;
opposite++;
}
circumstance += String.fromCharCode(lie(coal,approval,edge));
}
parent.location = 'm'+'a'+'i'+'l'+'t'+'o'+':'+circumstance;
}

function fall(crystal,encounter,expert) {
crystal += ' ';
var people = crystal.length;
var historian = 0;
for(var human = 0; human < people; human++) {
historian = 0;
while(crystal.charCodeAt(human) != 32) {
historian = historian * 10;
historian = historian + crystal.charCodeAt(human)-48;
human++;
}
//document.write('&');
//document.write('#');
//document.write(lie(historian,encounter,expert));
document.write(String.fromCharCode(lie(historian,encounter,expert)));
}
}

function lie(impression,side,broad) {
if (broad % 2 == 0) {
mathematical = 1;
for(var doctor = 1; doctor <= broad/2; doctor++) {
matter = (impression*impression) % side;
mathematical = (matter*mathematical) % side;
}
} else {
mathematical = impression;
for(var minimum = 1; minimum <= broad/2; minimum++) {
matter = (impression*impression) % side;
mathematical = (matter*mathematical) % side;
}
}
return mathematical;
}
// -->

