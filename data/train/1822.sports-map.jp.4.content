<script type="text/javascript">
var prev_address = '';

$(document).ready(function(){
  init();
});

function init() {
  resizeInfoArea();
  setAddressInfoText('#address_text');

  $.timer(1000, function(timer) {
    var address = $('#address_text').val();
    if(address.length == 0) {
      $('#address_list').hide();
    }else if(prev_address != address) {
      prev_address = address;
      showAddressList(address);
    }
  });

  randomtip();

}

function randomtip() {
  var length = $("#tips li").length;
  var ran = Math.floor(Math.random()*length) + 1;
  $("#tips li:nth-child(" + ran + ")").show();
};

function resizeInfoArea() {
  var reportAreaHeight = $('#top_report').innerHeight();
  $('#top_info').height(reportAreaHeight);
}

function showAddressList(keyword) {
  var geocoder = new google.maps.Geocoder();
  opts = {
    address: keyword,
    language: 'ja',
    country: 'JP'
  };
  geocoder.geocode(opts, makeAddressList);
}

function makeAddressList(results, status) {
  var locations = getLocations(results, status);
  if(locations.length == 0) {
    $('#address_list').hide();
    return;
  }

  var list = new Array();
  for(var i=0; i<locations.length; i++) {
    var location = locations[i];
    var tag = '';
    tag += '<div style="min-height:20px;font-size:14px;text-align: left">';
    tag += '<img src="/assets/icons/icon_map.png" style="vertical-align:middle;"> ';
    tag += '<a href="/map?ll=' + location.lat + '%2C' + location.lng + '">' + location.address + '</a>';
    tag += '</div>';
    list.push(tag);
  }
  $('#address_list').html(list.join(''));
  $('#address_list').show();
}
</script>
