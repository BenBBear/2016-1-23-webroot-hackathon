<?php
$host = $_SERVER["SERVER_NAME"];
$agent = $_SERVER["HTTP_USER_AGENT"];

if (preg_match("/.*(DoCoMo).*/i", $agent) == true) {
	//DoCoMo(グーグルボット以外)
	if (preg_match("/.*(Googlebot).*/i", $agent) == false) {
		header("Location: http://".$host."/m/i/");
		exit();
	}
} elseif (preg_match("/^(J\-PHONE|Vodafone|MOT\-[CV]|Softbank)/i", $agent) == true) {
	//SoftBank
	header("Location: http://".$host."/m/s/");
	exit();
} elseif (preg_match("/^KDDI\-/i", $agent) == true || preg_match("/UP\.Browser/i", $agent) == true) {
	//AU
	if (preg_match("/.*(Googlebot).*/i", $agent) == false) {
		// au(グーグルボット以外)
		header("Location: http://".$host."/m/e/");
		exit();
	}
} elseif(stristr($agent, 'Android') || stristr($agent, 'iPhone') || stristr($agent, 'iPad') || stristr($agent, 'iPod') ){
	//スマートフォン(Android,iPhone,iPad,iPod)
	header("Location: http://".$host."/m/smart/");
	exit();
}
