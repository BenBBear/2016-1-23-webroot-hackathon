    <script type='text/javascript'>

        $(document).ready(function() {

          $(document).on('submit', "#jobSearch", function() {

            if($("#jobType").val() == '') {
              alert("You must select a job type [full time/hourly]");
              return false;
            }
          });

          $(document).on('change', "#jobType", function() {

            if($(this).val()=='full') {
              $("#jobSearch").attr("action", "https://www3.apply2jobs.com/tractorsupply/ProfExt/index.cfm?fuseaction=mExternal.searchJobs");
              $("#keyword").attr("name", "txtKeyword");
            } else if($(this).val() == 'part') {
              $("#jobSearch").attr("action", "https://www3.apply2jobs.com/tractorsupply/HVExt/index.cfm?fuseaction=mhvExternal.showSearchResults");
              $("#keyword").attr("name", "TXTWHAT");
            }

          });

        });
    </script>
