/* Simple grid system ------------------------------- */


.container:after {
     clear: both; 
     content: "."; 
     display: block; 
     height: 0; 
     visibility: hidden;
}


.grid1, .grid2, .grid3, .grid4, .grid5, .grid6, .grid7, .grid8, .grid9, .grid10, .grid11 {
     float: left;
     margin-left: 3%;
     // sizing;
          box-sizing: border-box;
          -moz-box-sizing: border-box;
          -ms-box-sizing: border-box;
          -webkit-box-sizing: border-box;
}


.grid1 {
    width: 5.58%;
}


.grid2 {
    width: 14.16%;
}


.grid3 {
    width: 22.75%;
}


.grid4 {
