<script>
$(document).ready(function(){
  var mySwiper = $('.swiper-container').swiper({
    //Your options here:
        loop:true,
        autoplay:5000,
        speed:1000,
        calculateHeight:true,
        touchRatio:0.6,
        DOMAnimation: true
  });
  
  
	$(window).bind("orientationchange",function(){
		if(Math.abs(window.orientation) === 90){
			mySwiper.resizeFix();
			mySwiper.reInit();
		}else{
			mySwiper.resizeFix();
			mySwiper.reInit();
		}
	})
  
  $('.thums-inner ul').simplyScroll({
		autoMode: 'loop',
		speed: 1,
		frameRate: 24,
		horizontal: true,
		pauseOnHover:	true,
		pauseOnTouch: true
  });
  
  $('.arrow-left').on('click', function(e){
    e.preventDefault()
    mySwiper.swipePrev()
  });
  $('.arrow-right').on('click', function(e){
    e.preventDefault()
    mySwiper.swipeNext()
  });
  
  $('.thums-inner a').on('click', function(e){
    e.preventDefault();
    var slideNum = $(this).data("slide");
    mySwiper.stopAutoplay();
    mySwiper.swipeTo(slideNum);
    mySwiper.startAutoplay();
  })
  
  $("#sns .twitter").socialbutton("twitter", {
    button : "horizontal",
    text : "2016年コンチネンタルタイヤカレンダープレゼント https://www.continental-campaign.jp/",
    url : "https://www.continental-campaign.jp/",
  }).width(95);
  
  $("#sns .facebook").socialbutton("facebook_like", {
    button : "button_count",
    url : "https://www.continental-campaign.jp/",
  }).width(110);
});
</script>
