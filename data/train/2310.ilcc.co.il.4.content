    <script type="text/javascript">
        



var reload_after_login = false;
var login_post_url = "/accounts/login/";
function submit_login(){
    $(" #fields").hide(100);
    var username = $("#login-name").val();
    var password = $("#login-pass").val();
    var csrftoken = $("[name=csrfmiddlewaretoken]", '#fields').val();
    $.post(login_post_url, {"username": username, "password" : password, csrfmiddlewaretoken : csrftoken},process_login);
}

function process_login(data){
    if (data == "invalid") {//login info was invalid
        $("#login_result").html('Login invalid')
        $('#show_login_btn').show();
        $('#login a.login_text').show();
        bind_btns();
        $('#search_section').show();
    }else if (data == "disabled") {//account disabled
        $("#login_result").html('Account disabled')
        $('#show_login_btn').show();
        $('#login a.login_text').show();
        bind_btns();
        $('#search_section').show();
    } else { //successful
        //if (reload_after_login) //if user arrived to membership form via link reload after login so the form will appear
        location.reload(true); // talig: I think we should always reload after login
        $("#login_result").html(',' + data);
        $("#login_result").append('(<a href="#" class="logout_btn">התנתק</a>)');
        $("#login_welcome").show();
        bind_btns();
        $('#search_section').show();
    }
    $("#login_result").show();
}
function bind_btns(){
    $(".logout_btn").click(function(){
       $.post("/accounts/logout/", { csrfmiddlewaretoken : $("[name=csrfmiddlewaretoken]", '#fields').val() }, function(data){
           location.reload(true) ;
       })
    });
    $('#login_submit_btn').one('click', submit_login);
}

function expose_login() {
    $('#search_section').hide();
    $("#login_result").text("");
    $(" #fields").show(100);
    $('#show_login_btn').hide();
    $('#login a.login_text').hide();
}

$('document').ready(function(){
    $('#show_login_btn').click(function(){
		expose_login();
    });
    $('#already_registered_btn').click(function(){
		expose_login();
		window.scroll(0,0);
    });
    bind_btns();
});

        $(".subscribe fieldset input.submit").click(function(){
            var email_addr =   $(".subscribe fieldset #subscribe-input").val() || "";
            if (email_addr == "")
                return;
            $.post("/frontend/add_to_mailing_list/", {'email':email_addr},function(){
                 $(".subscribe fieldset #subscribe-input").val("כתובתך נשמרה")
            });
        });

    </script>

  </body>
</html>
