		<script type="text/javascript">

			var thumb_flg  = 1; // サムネイル画像フラグ
			var title_flg  = 1; // 説明文フラグ
			var repeat_flg = 0;    // 再生設定
			var page_cnt = 4	//ページ数

			var navi   = "#HeaderSlide_thumb"; // サムネイル画像

			var slide_id  = "#HeaderSlide_image"; // スライド画像
			var txt_id    = "#HeaderSlide_text";  // 説明文
			
			var page	= "a.SlidePage"		//ページ送り
			var next	= "a.page_right";	//ページ送り(右)
			var prev	= "a.page_left";	//ページ送り(左)

			
			var slide_pattern = "fade"; // スライドパターン
			var slide_speed   = "10000";   // スライドスピード(画像切り替え)
			// #1339 2014/06/17 t-ichikawa エフェクトスピード追加
			var effect_speed   = "650";   // スライドスピード(エフェクト)

		
			$(document).ready(function(){


	if($("#PCHeaderSlide").length){

				/*
				$('.bxslider').bxSlider({
					auto: true,
					//adaptiveHeight: true,
					 mode: 'fade',
					pager: false,
					controls:false
					//autoControls: true
