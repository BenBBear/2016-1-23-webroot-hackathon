    this.url += this.furnished;

    this.go();
  },

  rental : function() {
    //short term specific
    this.url += 'rental/';
    this.url += this.neighborhood + '/';
    this.url += this.unit_type + '/';
    this.url += this.min_price + '/';
    this.url += this.max_price;

    this.go();
  },

  go : function() {
    this.url += '/1/10';
    window.location.href = this.url;
  }
}

