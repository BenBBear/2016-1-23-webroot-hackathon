function setCookie(cookieName, cookieValue, nDays) {
  var today = new Date();
  var expire = new Date();
  if (nDays == null || nDays == 0) nDays = 0;

  expire.setTime(today.getTime() + (3600000 * 24 * nDays));

  document.cookie = cookieName + "=" + encodeURIComponent(cookieValue)
          + ";expires=" + expire.toGMTString()
          + ";path=/"; //;HttpOnly;secure;";
}

//When the form is submitted, pass the info to ajax
function loginGo(){

    var user = $("#UserName").val();
    if (user == 'User Name / Email' || user == '') {
        document.location.href = "/CPanel/Password.asp?user=" + encodeURIComponent(user) + "&org=creekrednation.org"
    } else {

        var password = $("#TheKey").val();
        if (password == 'Password') 
            password = "";

        login(user, password);
    }

}

    //Login script moved to included scripts
function login(user, key) {
    
    auth();

    var service = "https://api.leagueathletics.com/api/login?user=" + encodeURIComponent(user) + "&key=" + encodeURIComponent(key) + "&n=true&org=creekrednation.org"
    
    $.ajax({
        url: service,
        global: true,
        dataType: "jsonp",
        cache: false,
        jsonpCallback: "callback",
        success: function(x) {

            $("#login-button").text("Transferring...");
            $("#UserName, #TheKey").val("");

            var sRole = "";
            if (x.rights) sRole = x.rights.ARole13318;

            if (key.length == 0 && x.error.length == 0 && x.msg.length > 0 ) {

                document.location.href = "/CPanel/Password.asp?org=creekrednation.org&msg=" + x.msg;

            } else if (x.sessionID && (sRole || x.rights.TargetPage)) { 
                
                var loc = location.href.toLowerCase();
                var domain = "https://LeagueAthletics.com";
                if (loc.indexOf("dev.leagueathletics.com") > -1) 
                    domain = "https://dev.LeagueAthletics.com";

                if (x.rights.TargetPage && !sRole) {
                    return location.href = domain + x.rights.TargetPage;
                };//end if

                if (sRole == "Member" || sRole == "BoardForum" || !sRole) {
                    
                    if (loc.indexOf("/roster.asp") == -1) {
                        location.href = domain + "/Registration/Family.asp?edit=1&AdultID=" + x.rights.MemberID13318 + "&snid=" + x.sessionID + "&org=creekrednation.org"
                        var test = '';
                    } else {
                        gotoSecureURL(x.sessionID);
                    }

                } else if (sRole == "Official") {
                    location.href = domain + "/OfficialsHome.asp?snid=" + x.sessionID + "&org=creekrednation.org";

                } else {
                    gotoSecureURL(x.sessionID);
                }                
            } else {

                document.location.href = "/CPanel/Password.asp?org=creekrednation.org&msg=" + x.error;
                
            }
        },
        error: function(x, error, message) {
          
            $("#UserName, #TheKey").val("");
            document.location.href = "/CPanel/Password.asp?org=creekrednation.org&msg=" + x.error;

        }
    });
}


// Redirects to a secure LA address version of the current page.
function gotoSecureURL(sessionID) {

    // Get their currrent URL
    var loc = location.href;
    
    //If they login from the Password page, take them to the homepage.
    if (loc.toLowerCase().indexOf("/cpanel/password.asp") != -1){
    	loc = loc.toLowerCase().replace("/cpanel/password.asp", "/default.asp");
    }

    //If they login from SecretWord.asp, take them to the selected page
    if (loc.toLowerCase().indexOf("/secretword.asp?file=teamdocs.asp") != -1){
        loc = loc.toLowerCase().replace("/secretword.asp?file=teamdocs.asp&", "/teamdocs.asp?");
    }else if (loc.toLowerCase().indexOf("/secretword.asp?file=schedule.asp") != -1){
        loc = loc.toLowerCase().replace("/secretword.asp?file=schedule.asp&", "/schedule.asp?");
    }else if (loc.toLowerCase().indexOf("/secretword.asp?file=pictures.asp") != -1){
        loc = loc.toLowerCase().replace("/secretword.asp?file=pictures.asp", "/pictures.asp");
    }else if (loc.toLowerCase().indexOf("/secretword.asp") != -1){
        loc = loc.toLowerCase().replace("/secretword.asp", "/roster.asp");
    }
    // Replace the host name portion with LeagueAthletics.com
    var host = location.hostname.toLowerCase();
    if (host.indexOf("dev.leagueathletics.com") == -1)
        loc = loc.replace(host, "LeagueAthletics.com");

    // Change protocal to https or add it if not present.
    loc = loc.replace("http://", "https://");                
    if (loc.indexOf("https://") == -1 )
        loc = "https://" + loc;

    // Strip off any existing snid= values.
    var iSnid = loc.indexOf("&snid=");
    if (iSnid >= 0) {
        var iNd = loc.indexOf("&", iSnid + 1)
        if (iNd == -1) iNd = loc.length - 1;
        loc = loc.substr(0, iSnid) + loc.substr(iNd);
    }
                
    // Add org= parameter if not already there.
    if (loc.indexOf("org=") == -1) {
        if (loc.indexOf("?") == -1) {
            loc += "?";
        } else {
            loc += "&";
        }               

        loc += "org=creekrednation.org";
    }

    // Redirect to secure version of the page.
    var aLoc = loc.split("#");
    var hashLocation = aLoc[1] || '';
    if (hashLocation) hashLocation = '#' + hashLocation;
    var tempLoc = aLoc[0] + "&snid=" + sessionID + hashLocation;
    location.href = tempLoc;
    //location.href = loc + "&snid=" + sessionID;
}


//Growl at them
function growl(msg, status) {
    var msg = msg;
    $.bootstrapGrowl(msg, {
        ele: 'body', // which element to append to
        type: status, // (null, 'info', 'error', 'success')
        offset: { from: 'top', amount: 40 }, // 'top', or 'bottom'
        align: 'right', // ('left', 'right', or 'center')
        width: 320, // (integer, or 'auto')
        delay: 10000,
        allow_dismiss: true,
        stackup_spacing: 10 // spacing between consecutively stacked growls.
    });
}
