    	 	slider.goToSlide(slide);
    	}

    	$(".slider_prev").click(function(){
    		slider.goToPrevSlide();
    	});
    	
    	$(".slider_next").click(function(){
    		slider.goToNextSlide();
    	});

    }

    //Getting a time window of two months, today being in the middle
    var today = new Date();
    today.setDate(today.getDate() - 30);
    var Month = today.getMonth() + 1;
    var Day = today.getDate();
    var Year = today.getFullYear();
    today = Month + "-" + Day + "-" + Year;

    var oneMonth = new Date();
    oneMonth.setDate(oneMonth.getDate() + 30);
    var oMonth = oneMonth.getMonth() + 1;
    var oDay = oneMonth.getDate();
    var oYear = oneMonth.getFullYear();
    oneMonth = oMonth + "-" + oDay + "-" + oYear;

    var getScheduleCntr = 0;
    function getSchedule(){
        $.ajax({
            url: "https://api.leagueathletics.com/API/schedules?org=creekrednation.org&noCache=true",    
            data: {
            	dtStart: today,
            	dtEnd: oneMonth
            },
            global: true,
            dataType: "jsonp",
            cache: false,
            jsonpCallback: "callback",
            timeout: 10000
        })
 
            .done(function(data) {

            var events = data.Events;
            
            var results = [];
            var schedule = [];
            $.each(events, function(i){
            	//var today = new Date();
            	var today = moment();
            	var StartDate = moment(events[i].StartDate);
            	if (events[i].isGame && !events[i].Cancelled){
            		if (StartDate < today){
            			results.push(events[i]);
	                }else{
	                	schedule.push(events[i]);
	                }
            	}
            });


            //This removes events with no scores entered
            for (i = results.length - 1; i >= 0; i--){
            	if (results[i].Home.Score == null){
            		results.splice(i, 1);
            	}
            }


            //Keeping the last 30 results and the next 20 upcoming events
            if (results.length > 30){
            	results.splice(0, (results.length - 30));
            }
            if (schedule.length > 20){
            	schedule = schedule.slice(0, 20);
            }

            events = results.concat(schedule);

            //Tells us which slide to start on. We pass this in the init slider function
            var resultSlides = Math.floor(results.length / 6); //6 events per slide
            if (events.length % 6 == 0 && resultSlides > 0){
            	resultSlides = resultSlides - 1;
            }

            if (events){
            	$(".slider_prev, .slider_next").show();
            }

            $.each(events, function(i) {

                var today = moment();

                var StartDate = moment(events[i].StartDate);
                var FinishDate = moment(events[i].FinishDate);

                // if (today.isDST()){
                // 	StartDate.subtract(1, "hour");
                // 	FinishDate.subtract(1, "hour");
                // }

                var month = StartDate.month();
                switch(month){
                    case 0:
                        month = "Jan";
                        break;
                    case 1:
                        month = "Feb";
                        break;
                    case 2:
                        month = "Mar";
                        break;
                    case 3:
                        month = "Apr";
                        break;
                    case 4:
                        month = "May";
                        break;
                    case 5:
                        month = "Jun";
                        break;
                    case 6:
                        month = "Jul";
                        break;
                    case 7:
                        month = "Aug";
                        break;
                    case 8:
                        month = "Sep";
                        break;
                    case 9:
                        month = "Oct";
                        break;
                    case 10:
                        month = "Nov";
                        break;
                    case 11:
                        month = "Dec";
                        break;
                    default:
                        month = "TBD";
                }

                var day = StartDate.date();

                var weekDay = StartDate.day();
                switch(weekDay){
                    case 0:
                        weekDay = "Sun";
                        break;
                    case 1:
                        weekDay = "Mon";
                        break;
                    case 2:
                        weekDay = "Tue";
                        break;
                    case 3:
                        weekDay = "Wed";
                        break;
                    case 4:
                        weekDay = "Thu";
                        break;
                    case 5:
                        weekDay = "Fri";
                        break;
                    case 6:
                        weekDay = "Sat";
                        break;
                    default:
                        weekDay = "";
                }

                //Home Team
                var homeTeam = events[i].Home.Name;
                if (!homeTeam){
                	homeTeam = "TBD";
                }
                var homeScore = events[i].Home.Score;
                if (homeScore == null){
                	homeScore = "--";
                }
                var homeID = events[i].Home.ID;
                if (!homeID){
                	homeID = events[i].Away.ID;
                }
                

                //Away Team
                var awayTeam = events[i].Away.Name;
                if (!awayTeam){
                	awayTeam = "TBD";
                }
                var awayScore = events[i].Away.Score;
                if (awayScore == null){
                	awayScore = "--";
                }

                var time = StartDate.format("h:mm a");

                var gameFinal = false;
                if (today > StartDate){
                	time = "Final";
                	gameFinal = true; //This determines which page the user should go to when they click the slide, result or details
                }
                if (today > StartDate && today < FinishDate){
                	time = "<span class='slider_in_progress'>In Progress</span>";
                	gameFinal = true;	
                }

                var facility = events[i].Facility.Name;
                var gameid = events[i].ID;
                var associd = events[i].AssocID;


                $(".bxslider").append(
                    "<div class='score_slide' onclick='goToDetails(" + gameFinal + ", " + gameid + ", " + associd + ", " + homeID + ")'>" +
                        "<div class='slide_inner'>" +
                        "<div class='slide-header'>" +
                            "<div class='slide-header-date'>" +
                                weekDay + ", " + month + " " + day + 
                            "</div>" +
                            "<div class='slide-header-time'>" +
                                time +
                            "</div>" +
                        "</div>" +
                        "<div class='slide-away-team'>" +
                            "<div class='slide-team-name'>" +
                                "<span class='away-team'>" + awayTeam + "</span>" +
                            "</div>" +
                            "<div class='slide-team-score'>" +
                                awayScore +
                            "</div>" +
                        "</div>" +
                        "<div class='slide-home-team'>" +
                            "<div class='slide-team-name'>" +
                                "<span class='home-team'>" + homeTeam + "</span>" +
                            "</div>" + 
                            "<div class='slide-team-score'>" +
                                homeScore +
                            "</div>" +
                        "</div>" +
                        "<div class='slide-footer'>" +
                            "<div class='slide-footer-text'>" +
                                facility + 
                            "</div>" +
                        "</div>" +
                        "</div>" +
                    "</div>"
                );
            });
			initSlider(resultSlides);
        })
        .fail(function(data) {
        	if (data.error && getScheduleCntr < 3){
        		getScheduleCntr++;
        		getSchedule();
        	}
        });
    }
    getSchedule();

    function goToDetails(final, id, associd, homeid){
    	if (final){
    		window.location = "/GameDetails.asp?game=" + id + "&AssocID=" + associd + "&org=creekrednation.org";
    	}else{
    		window.location = "/ScheduleDetail.asp?ID=" + id + "&AssocID=" + associd + "&menu=Teams&item=Schedule&org=creekrednation.org&MyTeam=" + homeid;
    	}
    }
</script>

