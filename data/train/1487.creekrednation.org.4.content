<script type="text/javascript">
    $(function(){
        $(".colpick-input").focus(function(event) {
            var input = $(this);
            var initValue = input.val();
            if (initValue){
                initValue = initValue.replace("#", "");
            }
            input.colpick({
                color: initValue,
                submit: false,
                colorScheme:'dark',
                layout:'hex',
                onChange: function(hsb, hex){
                    input.val("#"+hex);
                    colpickCallback();
                }
            });
        });
    })
</script>

