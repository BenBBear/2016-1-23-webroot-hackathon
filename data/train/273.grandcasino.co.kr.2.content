<script type="text/javascript">
$(function(){
	var url    = document.URL;

	if(url.indexOf('http://paradisecasino.co.kr') > -1){
		location.replace('http://www.paradisecasino.co.kr');
	}

	var winHeight = window.innerHeight;

	window.setToken = function(token){
