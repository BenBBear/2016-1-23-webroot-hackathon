	});

	$("[name=lang]").change(function() {
		$("#changeLocale").attr('action', '/mobile/').attr('method', 'get').submit();
	});

	$('#isPushAccepted').on('change', function(e) {
		var token = window.localStorage.getItem('deviceToken');

		if(token != ''){
			$.ajax({
				type: "POST",
				url: '/mobile/proxy/device/push.json;jsessionid=1F646AEA87EC1A35ACF4AD6050B8AE8E.node1',
				data : {
					'token' : window.localStorage.getItem('deviceToken'),
					'isPushAccepted' : $("#isPushAccepted").is(":checked")
				}
			}).done(function(result) {
				if(result == true) {
