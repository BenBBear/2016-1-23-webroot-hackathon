	};

	$("#menu-aside").on("click",function(){
		var token = window.localStorage.getItem('deviceToken');

		if(token != '' && token != null){
			$.ajax({
				type: "POST",
				url: '/mobile/proxy/device/ispushaccepted.json;jsessionid=1F646AEA87EC1A35ACF4AD6050B8AE8E.node1',
				data : {
					'token' : window.localStorage.getItem('deviceToken')
				}
			}).done(function(result) {
				if(result == true) {
					$("#isPushAccepted").attr("checked", true);
				} else {
					$("#isPushAccepted").attr("checked", false);
				}
			});
		}else{
			$('#pushArea').hide();
			//$("#isPushAccepted").attr("checked", true);
		}
		$('html, body, .dim-black, .aside-menu').css('height', winHeight);
		$(".aside-menu").addClass("active");
		$('html').addClass('scroll-none');

	});
	$("#btnClose, .dim-black").on("click",function(){
		$(".aside-menu").removeClass("active");
		setTimeout(function(){
			$(".aside-menu").scrollTop(0);
		}, 250);
		$('html, body, .dim-black, .aside-menu').css('height', 'auto');
		$('html').removeClass('scroll-none');
	});

	//디자인변경으로제거
	var asidemenu=".aside-menu>nav"
	$(">a",asidemenu).on("click",function(){
		$(this).toggleClass('active');
