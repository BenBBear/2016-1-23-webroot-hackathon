<script language="JavaScript">
	function showLeagueChoser() {
		var obj = document.getElementById('leagueChoser');
		if (obj.style.display == 'none') {
			obj.style.display = 'block';
		} else {
			obj.style.display = 'none';
		}
	}

	function setLeagueStatsContent(id_event, event_name, layerName) {
		document.getElementById('liMatches').style.className = '';
		document.getElementById('liMatchesNext').style.className = '';
		document.getElementById('liMain').style.className = '';
		document.getElementById('liGoals').style.className = '';
		document.getElementById('liPositions').style.className = '';
		document.getElementById('statsLeague').innerHTML = document.getElementById('statsLeague'+layerName+id_event).innerHTML;	
		document.getElementById('li'+layerName).style.className = 'current';		

		id_event_curr = id_event;
		event_name_curr = event_name;
		document.getElementById('leagueLabel').innerHTML = event_name; 
	}

	function getRandomStatsBoxName() {
		var randomnumber = Math.floor(Math.random()*2);
		var itemName = '';
		switch (randomnumber) {
			case 0:
				itemName = 'Team';
				break;
			case 1:
				itemName = 'Coach';
				break;
			case 2:
				itemName = 'Referee';
				break;
		}

		return itemName;
	}

	function showRanking(itemName) {
		document.getElementById('liRankingPlayerGoals').style.className = '';
		document.getElementById('liRankingPlayerPenalties').style.className = '';
		document.getElementById('liRankingPlayerAdmonishes').style.className = '';
		//document.getElementById('liRankingTeamConsecutiveWins').style.className = '';
		document.getElementById('liRankingCoachEfficacy').style.className = '';
		document.getElementById('liRankingTopPerformances').style.className = '';

		document.getElementById('liRanking'+itemName).style.className = 'current';
		document.getElementById('rankingsBox').innerHTML = document.getElementById('rankingsBox'+itemName).innerHTML;
	}
	
	function initPage() {
		setLeagueStatsContent(397, 'Liga de Fútbol Profesional 2015/16', 'Matches');

		var itemName = 'Player'; // we always want player on the left
		document.getElementById('statsBox1').innerHTML = document.getElementById('statsBox'+itemName).innerHTML;

		var itemName2 = '';
		while ((itemName2 = getRandomStatsBoxName()) == itemName) {} 

		document.getElementById('statsBox2').innerHTML = document.getElementById('statsBox'+itemName2).innerHTML;

		document.getElementById('rankingsBox').innerHTML = document.getElementById('rankingsBoxPlayerGoals').innerHTML;
	}
	
	var id_event_curr = 0;
	var event_name_curr = '';	
</script>
