<script type="text/javascript">

(function() {

	function footerLayout() {
		var windowWidth = $(window).width();

		if ( windowWidth >= 1100 ) {
			$( '.site-footer .contact' ).insertAfter( $('.site-footer .contact').siblings('.footer-menu-wrapper') );
		} else if ( windowWidth < 1100 )  {
			$( '.site-footer .contact' ).insertBefore( $('.site-footer .contact').siblings('.connect') );
		}
	}

	function specsAdjust() {
		var windowWidth = $(window).width();

		if ( windowWidth >= 1000 ) {
			$( '.specs-wrapper .spec' ).each(function() {
				$(this).removeAttr('style');
				$(this).children('.arrow').removeAttr('style');
			});

			$( '.specs-wrapper .spec' ).each(function() {
				var width = $(this).width();
				var halfwidth = width / 2;
				var adjust = halfwidth - 15;
				var left = adjust * -1;

				var margin = halfwidth - 8;

				$(this).css('left', left );
				$(this).children('.arrow').css('margin-left', margin );
			});
		} else if ( windowWidth < 1000 && windowWidth > 600 ) {
			$( '.specs-wrapper .spec' ).each(function() {
				$(this).removeAttr('style');
				$(this).children('.arrow').removeAttr('style');
			});

			$( '.specs-wrapper .left .spec' ).each(function() {

				$(this).css('left', '-20px' );
				$(this).children('.arrow').css('left', '47px' );
			});
			$( '.specs-wrapper .right .spec' ).each(function() {
				var width = $(this).width();

				$(this).css('right', '-20px' );
				$(this).children('.arrow').css('right', '47px' );
			});

		} else if ( windowWidth <= 600) {

			$( '.specs-wrapper .spec' ).each(function() {
				$(this).removeAttr('style');
				$(this).children('.arrow').removeAttr('style');
			});
		}
	}


    function homepageSlides(){

    	var windowWidth = $(window).width();

	
	
    } // end of homepageSlides()

    function landingPageFeature() {
    	var windowWidth = $(window).width();

    	if ( windowWidth >= 768 ) {
			$( '.feature' ).each(function( index ) {
				var featureHeight = $(this).height();
				var textHeight = $(this).find('.text').height();

				$(this).find('.text').css('top', (featureHeight - textHeight) / 2 )
			});
		} else {
			$('.feature .text').removeAttr('style');
		}
    }


	function loadContentSlider() {

		

	} // end of justReady()


	function largeVideo() {

			}

	function appsList() {

		
	} // end of appsList()




	function homeBannerAdjust() {
		var windowWidth = $(window).width();

	
	} // end of homeBannerAdjust()

	function wistiaVideo() {

		// grabs all the embeds on the page and pauses them
		function pauseAll() {
			for (i = 0; i < wistiaEmbeds.length; i++) {
				wistiaEmbeds[i].pause();
			}
			$('.video-wrapper .video-screenshot').fadeIn();
			$('.zombie, .video-overlay').fadeIn();
		}

		$('.video-play-button-round').click(function() {
			var video = $(this).data('video');
			var videoElement = $('#' + video + ' iframe' ).get(0);
			var videoURL = $(this).data('video-link');
			var wistiaEmbed = jQuery(videoElement)[0].wistiaApi;

			wistiaEmbed.play();

			if ( $(this).hasClass('playing') ) { // pause curently playing video
				wistiaEmbed.pause();
				$(this).removeClass('playing').addClass('paused');
				$(this).parent().parent().find('.zombie').fadeIn();
				$(this).parent().find('.video-screenshot').fadeIn();
				$(this).parent().find('.video-overlay').fadeIn();

			} else if ( $(this).hasClass('paused') ) { // play video and pause all the rest

				wistiaEmbeds.onFind(function(video) {
				    pauseAll();
				});

				$('.video-play-button-round.playing').each(function(){
					$(this).parent().find('.video-screenshot').fadeIn();
					$(this).addClass('paused').removeClass('playing');
				});

				$(this).parent().parent().find('.zombie').fadeOut();
				$(this).parent().find('.video-screenshot').fadeOut();
				$(this).parent().find('.video-overlay').fadeOut();
				$(this).removeClass('paused').addClass('playing');

				// play video
				wistiaEmbed.play();

			} else { // play video for the first time

				// binds all the embed on the page for 'pause' event
				wistiaEmbeds.onFind(function(video) {
				    pauseAll();
				});

				$(this).parent().find('.video-screenshot').fadeOut();
				$(this).parent().find('.video-overlay').fadeOut();
				$('.video-play-button-round.playing').addClass('paused').removeClass('playing');
				wistiaEmbed.play();

				$(this).addClass('playing');
				$(this).parent().find('.poster').hide();
			}
		});
	}

	function pressKitList() {
		$('.asset-list h2').click(function() {
			$(this).parent().addClass('current');

			$('.asset-list:not(.current)').addClass('closed').removeClass('open').find('.assets').slideUp();
			$(this).parent().addClass('open').removeClass('closed').find('.assets').slideToggle();

			$(this).parent().removeClass('current');
		});

		$('.asset-list:last-child .assets').slideToggle().addClass('open').removeClass('closed');
	}

	function countryList() {
		$(".country_list").change(function () {

			$result = false;

			$(".country_list option:selected").each(function () {
				var val = $(this).val();

				$('.country_results .country').each(function( index ) {
					if ( $(this).hasClass(val) ) {
						$(this).slideDown();
						$result = true;
					} else {
						$(this).slideUp();
					}
				});
			});

			if ( $result == false ) {
				$('.country.default').slideDown();
			}
		})
	}

	function specBall() {
			}

	function sliderBall() {
			}

	function languageSelect() {
		$('.footer_language_list').click( function(e){
			e.preventDefault();
	    	$(this).addClass('select-language');

		});
	}

	function browserFixes() {
		var is_chrome = navigator.userAgent.toLowerCase().indexOf('chrome');
		if (is_chrome > 1) {
			$('html').addClass('chrome');
		}
	}

	function discoverPage(){
		if($('.categories').length){
			$('.categories').hover(function(){
				$(this).addClass('hover');
			},function(){
				$(this).removeClass('hover');
			});
		}
	}

	// A very basic select switcher for retail partners.
	$('#tabs .tab-content').hide();
	$('#tabs #usa-stores').show();
	$('#country-retailer-select').val("#usa-stores");
	$('#country-retailer-select').change(function() {
		var currentTab = $(this).val();
		$('#tabs .tab-content').hide();
		$(currentTab).show();
	});


	$('.nofollow').click(function(e){
		e.preventDefault();
	});

	$(window).on({
		resize: function() {

			// homepage
			homepageSlides();
			homeBannerAdjust();
			specsAdjust();

			// all pages
			footerLayout();

			// landing pages
			landingPageFeature();
		}
	});
	$(document).on({
		ready: function() {

			// homepage
			wistiaVideo();
			loadContentSlider();
			homepageSlides();
			sliderBall();
			specsAdjust();
			specBall();

			// homepage and Apps
			largeVideo();

			// apps page
			appsList();

			// landing pages
			landingPageFeature();

			// discover page
			discoverPage();

			pressKitList();
			countryList();

			// all pages
			footerLayout();
			languageSelect();
			browserFixes();
		}
	});

})();

</script>

