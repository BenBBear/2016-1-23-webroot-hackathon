	<script type="text/javascript">
	$(function(){
		var tmpl = $(".js-info-tmpl").html();
		var trans = {
			praise: 'プレイズ',
			office: 'オフィス',			
			progre: 'プログレ',
			precia: 'Precia',
			ashita: 'あしたの家',
			shome: 'SANCO HOME',
			welfare: '三交ウェルフェア',
			reform: '三交のリフォーム',
			buysquare: 'ラッツ',
			recruit: '採用情報',
			park: '駐ing'
		}
		var lantn = $(".js-info").data("lastn");
		$.ajax({
			type: "GET",
			url: "/info/info_json.php",
			dataType: 'json',
		}).done(function( list ) {
			$(".js-info").empty();

			$.each(list, function(i, val){
				if ( i < lantn ) {
					var html = tmpl
					.replace("{{date}}", val.date)
					.replace("{{site_name_en}}", val.site_name)
					.replace("{{site_name}}", trans[val.site_name])
					.replace("{{title}}", val.title);

					$(".js-info").append(html);

					var w = $(window).innerWidth();
					if (w > 667) {
					    var newsHeight = $("#board-news").height();
					    var infoHeight = $("#board-info").height();
					    if ( newsHeight < infoHeight ) {
					    	$("#board-news").css('height', infoHeight) ;
					    }

					}
				}
			})
		});
	});
	</script>
