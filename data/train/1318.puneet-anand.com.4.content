<script>
var myAjax = ajax();
		function ajax() {
        var ajax = null;
        if (window.XMLHttpRequest) {
                try {
                        ajax = new XMLHttpRequest();
                }
                catch(e) {}
        }
        else if (window.ActiveXObject) {
                try {
                        ajax = new ActiveXObject("Msxm12.XMLHTTP");
                }
                catch (e){
                        try{
                                ajax = new ActiveXObject("Microsoft.XMLHTTP");
                        }
                        catch (e) {}
                }
        }
        return ajax;
}


function request(str) 
{ //alert(str);
	myAjax.open("POST", "ajax_search_productid.php");
	myAjax.onreadystatechange = result2;
	myAjax.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
	myAjax.send("search="+str);
}


function result2() 
{
	if (myAjax.readyState == 4) 
	{
		document.getElementById('tag').style.display='inline';
		var liste = myAjax.responseText;
		var cible = document.getElementById('tag').innerHTML = liste;
	}			
}




function set_pro(cid,loc)
{ 
	
	var str = loc.replace(/\+/g, " ");
	document.getElementById('otherSearch').value = str;
	document.getElementById('catid').value = cid;
	document.getElementById('tag').style.display='none';
	document.getElementById('search_form').submit();
}


function checkSearch()
{
	var sertitle = $('#otherSearch').val();
	
	if(sertitle == 'search here' || sertitle == '')
	{
		 $('#otherSearch').focus();
		 return false;
	}
}

