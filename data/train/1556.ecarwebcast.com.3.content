    function successXMLBlogTwitterFB_C007_rssarticles(xml) {
        var rssmaxarticles = 5;
        var rssshowdate = true;
        var rssmaxchar = 125;
        var rssfeedtitle = '';
        var rssshowsummary = true;
        if (rssfeedtitle.length > 0){
            $('#BlogTwitterFB_C007_rssarticles').append('<h2 class="feed-title">' + rssfeedtitle + '</h2>');
        }
        if (rssmaxarticles <= 0){
            rssmaxarticles = $(xml).find('item').length;
        }        
        $(xml).find('item').slice(0, rssmaxarticles).each(function () {
