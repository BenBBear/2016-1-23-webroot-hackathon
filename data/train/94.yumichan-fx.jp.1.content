<script type="text/javascript">
var FxCompanySorter = function(companies){
	this.companies = companies;
	this.euroYenComparator = this.createSpreadComparator("euroYen");
	this.euroDollarComparater = this.createSpreadComparator("euroDollar");
	this.dollarYenComparater = this.createSpreadComparator("dollarYen");
	this.currentSortKey = "priority";
};
FxCompanySorter.prototype.createSpreadComparator = function(property){
	var filter = this.filter;
	return function(one, another){
		oneValue = filter(one[property]);
		anotherValue = filter(another[property]);
		
		if (anotherValue === oneValue) {
			return another.priority - one.priority;
		}

		if (oneValue === null && anotherValue !== null) {
			return 1;
		}

		if (oneValue !== null && anotherValue === null) {
			return -1;
		}
		return oneValue - anotherValue;
	};
};
FxCompanySorter.prototype.filter = function(value)
{
	var result = /\d+[.\d]*/.exec(value);
	if (result === null) {
		return result;
	}
	
	return Number(result[0]);
};
FxCompanySorter.prototype.getCurrentSortKey = function()
{
	return this.currentSortKey;
};
FxCompanySorter.prototype.getCompanies = function(){
	return this.companies;
};
FxCompanySorter.prototype.sortByCashBack = function(){
	this.companies.sort(this.compareCashBack);
	this.currentSortKey = "cashBack";
};
FxCompanySorter.prototype.compareCashBack = function(one, another){
	var oneValue = one.cashBack.replace("円", "");
	var anotherValue = another.cashBack.replace("円", "");
	if (oneValue == anotherValue) {
		return another.priority - one.priority;
	}
	return anotherValue - oneValue;
};

FxCompanySorter.prototype.sortByEuroYen = function(){
	this.companies.sort(this.euroYenComparator);
	this.currentSortKey = "euroYen";
};

FxCompanySorter.prototype.sortByEuroDollar = function(){
	this.companies.sort(this.euroDollarComparater);
	this.currentSortKey = "euroDollar";
};
FxCompanySorter.prototype.sortByDollarYen = function(){
	this.companies.sort(this.dollarYenComparater);
	this.currentSortKey = "dollarYen";
};
FxCompanySorter.prototype.sortByTradeUnit = function(){
	this.companies.sort(this.compareTradeUnit);
	this.currentSortKey = "tradeUnit";
};
FxCompanySorter.prototype.compareTradeUnit = function(one, another){
	var oneValue = one.tradeUnit.replace("通貨", "");
	var anotherValue = another.tradeUnit.replace("通貨", "");
	if (oneValue == anotherValue) {
		return another.priority - one.priority;
	}
	return oneValue - anotherValue;
};
FxCompanySorter.prototype.sortByPriority = function(){
	this.companies.sort(this.comparePriority);
	this.currentSortKey = "priority";
};
FxCompanySorter.prototype.comparePriority = function(one, another){
	return another.priority - one.priority;
};
jQuery(document).ready(function($) {
		

		$table = $("table#compare");
		var companies = [];
		var size = $("tbody tr:nth-child(3n+1)", $table).size();
		$("tbody tr:nth-child(3n+1)", $table).each(function() {
			$tr = $(this);
			var company = {};
			company.name = $("td", $tr).eq(0).html();
			company.cashBack = $("td", $tr).eq(1).text();
			company.euroYen = $("td", $tr).eq(2).text();
			company.euroDollar = $("td", $tr).eq(3).text();
			company.dollarYen = $("td", $tr).eq(4).text();

		    var tradeUnit = $("td", $tr).eq(5).text();
		    switch (tradeUnit) {
			case '1通貨':
				company.tradeUnit = '1通貨';
				break;
			case '1千通貨':
				company.tradeUnit = '1000通貨';
				break;
			case '1万通貨':
				company.tradeUnit = '10000通貨';
				break;

			}
			company.priority = size - $(this).index();

			company.lowerRow = $tr.next().next().detach();
			company.middleRow = $tr.next().detach();
			company.upperRow = $tr.detach();
			companies.push(company);
		});

		var sorter = new FxCompanySorter(companies);
		$("thead tr:first th:nth-child(2)", $table).click(function(){
			sorter.sortByCashBack();
		     renderCompareTable($("tbody", $table), sorter);
		     renderSortButton(this);
		});
		$("thead tr:first th:nth-child(4)", $table).click(function(){
			sorter.sortByTradeUnit();
		     renderCompareTable($("tbody", $table), sorter);
		     renderSortButton(this);
		});

		$("thead tr:last th:nth-child(1)", $table).click(function(){
			sorter.sortByEuroYen();
		     renderCompareTable($("tbody", $table), sorter);
		     renderSortButton(this);
		});

		$("thead tr:last th:nth-child(2)", $table).click(function(){
			sorter.sortByEuroDollar();
		     renderCompareTable($("tbody", $table), sorter);
		     renderSortButton(this);
		});

		$("thead tr:last th:nth-child(3)", $table).click(function(){
			sorter.sortByDollarYen();
		     renderCompareTable($("tbody", $table), sorter);
		     renderSortButton(this);
		});
		sorter.sortByPriority();
        renderCompareTable($("tbody", $table), sorter);

        function renderSortButton(value)
        {
		     $("img.compare_sort").attr("src", "http://www.yf-i.sakura.ne.jp/wordpress/wp-content/themes/yfi/images/compare_sort.png");
		     $("img.compare_sort", value).attr("src", "http://www.yf-i.sakura.ne.jp/wordpress/wp-content/themes/yfi/images/compare_sort_active.png");
        }
		function renderCompareTable($parent, sorter) {
			$parent.empty();
			var companies = sorter.getCompanies();
			for ( var i = 0; i < companies.length; i++) {
				var company = companies[i];
				$parent.append(company.upperRow.clone());
				$parent.append(company.middleRow.clone());
				$parent.append(company.lowerRow.clone());

			}
		}
});

</script>
</head>

