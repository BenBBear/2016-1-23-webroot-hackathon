function cookieCheck() {
    document.cookie = "xxx";
    if (document.cookie == "") {
        return false;
    } else {
        return true;
    }
}
$(function(){
    var res = cookieCheck();
    if (res){
        $("#enbCookie").val("1");
    } else {
        $("#enbCookie").val('0');
    }
    
    $(this).submit(function(e){
        if (0 == $("#enbCookie").val() && $("#CustomerAutoLogin").attr("checked")){
            alert("クッキーを有効にしないと自動ログインはできません。");
            e.preventDefault();
            return false;
        }
    });
});
//]]>
</script>

