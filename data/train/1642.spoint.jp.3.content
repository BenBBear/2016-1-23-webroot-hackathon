$(function(){
	$(".boxButton01,.boxButton02").click(function(){
		if($(this).find("a").attr("target") == "_blank"){
			window.open($(this).find("a").attr("href"), '_blank');
		}else{
			window.location=$(this).find("a").attr("href");
		}
		return false;
	});

	$(".boxButton01,.boxButton02").hover(
		// mouseenterイベント・リスナ
		// （マウスを乗せたときに背景色を黄色に）
		function() {
		  $(this).find("a").css('color', 'white');
		  $(this).find("span").css('color', 'white');
		},

		// mouseleaveイベント・リスナ
		// （マウスを外したときに背景色を白色に）
		function() {
		  $(this).find("a").css('color', '#231815');
		  $(this).find("span").css('color', '#231815');
		}

	);
});

