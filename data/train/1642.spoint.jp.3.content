<script type="text/javascript">
$(function(){
	$(".header_countup").css({"opacity":0});
	cu = $(".header_countup");
	for(var i = 0 ; cu.length > i; i++) {
		cu.eq(i).attr("data-num",cu.eq(i).html());
		cu.eq(i).attr("data-count",0);
		cu.eq(i).html(0);
	}
	$(".header_countup").animate({"opacity":1},1000);
	var set = function(){
		cu = $(".header_countup");
		var endflg = true;
		for(var i = 0 ; cu.length > i; i++) {
			num = parseFloat(cu.eq(i).attr("data-num"));
			count = parseFloat(cu.eq(i).attr("data-count"));
			count += num / 100;
			cu.eq(i).attr("data-count",count);
			if(count > num){
				cu.eq(i).html(separate(Math.round(num)));
			}else{
				cu.eq(i).html(separate(Math.round(count)));
				endflg = false;
			}
		}
		if(!endflg){
			setTimeout(set,10);
		}
	}
	set();

	/*
	$(".header_countup").each(function(){
		num = $(this).html();
		$(this).html(0);
		$(this).animate({"opacity":1},1000);
		c = 100;
		setInterval(function(){
			count += num / c;
			if(count > num){
				$("#main #main_h .kensu .num").html(Math.round(num));
			}else{
				$("#main #main_h .kensu .num").html(Math.round(count));
				setTimeout(set,10);
			}
		},10)
	});
*/
});
function separate(num){
    // 文字列にする
    num = String(num);

    var len = num.length;

    // 再帰的に呼び出すよ
    if(len > 3){
        // 前半を引数に再帰呼び出し + 後半3桁
        return separate(num.substring(0,len-3))+','+num.substring(len-3);
    } else {
        return num;
    }
}
