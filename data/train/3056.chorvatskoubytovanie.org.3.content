var map;

var myLatlng = new google.maps.LatLng(44.719082,14.891796);
var imageIcon="images/lodging_b.png";
var marker;

function init(coods,tip,mapzm)
	{
	if (!mapzm)
		{
			mapzm=10;
		}
		
	if (tip=="")
		{
			tip="ROADMAP";
		}

		
	lat=0;
	lng=0;	
	


	lat=44.719082;
	lng=14.891796;

	geocoder = new google.maps.Geocoder();
	myLatlng = new google.maps.LatLng(lat,lng);
	if (coods!="")
		{
	    var myOptions = {
		      zoom: 7,
		      center: myLatlng,
		      mapTypeId: google.maps.MapTypeId.ROADMAP,
		      scrollwheel: false
	   		 }		
		}	
	else
		{
	    var myOptions = {
		      zoom: 7,
		      center: myLatlng,
		      mapTypeId: google.maps.MapTypeId.ROADMAP,
		      scrollwheel: false
		     }		
		}

	map = new google.maps.Map(document.getElementById("map"), myOptions);
	var infowindow = new google.maps.InfoWindow();

		 

			allcods=coods.split(";");

			if (allcods.length>1)
			{
			
				for ( var i = 0; i <= allcods.length; i++) 
				{
					if(allcods[i]!==undefined)
					{
					
						lnlt=allcods[i].split(",");
						lat=lnlt[0];
						if (lnlt[1]!==undefined)
							{
							var tlng=lnlt[1].split(":");
							}
						
						var lng=tlng[0];
						var tite=tlng[1];
						var idapp=tlng[4];
						var imeslike=tlng[3];
						var acclink=tlng[5];
						var nazivgrada=tlng[6];
						var vise_info=tlng[7];
						var imageIcon="images/lodging_type_"+tlng[2]+".png";
						
						var myLatlng22 = new google.maps.LatLng(lat,lng);
						
	
						   var marker = new google.maps.Marker({
						        position: myLatlng22, 
						        map: map,
						        draggable:false,
						      	icon: imageIcon,
						        title:tite+"\nClick to view picture and link to offer",
						        sadrzaj:"<div class='infoimg'><img src='http://www.croatia-accommodation.info/accommodation-croatia/croatia-accommodation-"+idapp+"-small-"+imeslike+"' title='"+tite+"' height='80' width='100'></div><div class='infotext'><b>"+tite+"</b><br>"+nazivgrada+"<br><a href='"+acclink+"'><b>"+vise_info+"</b></a></div>"
							    }); 
					     if (allcods.length>3)
					     {
							google.maps.event.addListener(marker, 'click', function() {
								infowindow.setContent(this.sadrzaj);	  
	 						    infowindow.open(map,this);
						  });
						 }
						  
	
					}
					 				
				}
				if (allcods.length<3)
				{
					 map.setZoom(mapzm);
					 lnlt=allcods[0].split(",");
					 lat=lnlt[0];
					 if (lnlt[1]!==undefined)
					  {
						var tlng=lnlt[1].split(":");
					  }
				     map.setCenter(new google.maps.LatLng(lat,lng));
				}
			}
			else
			{
				
				codeAddress(coods,mapzm);
			}


     $(window).bind('resize', function() {
			      map.setCenter(new google.maps.LatLng(44.719082,14.891796));
			      map.panTo(new google.maps.LatLng(44.719082,14.891796));
		      			      
		  });

      
	if (tip=="ROADMAP")
		{
			 map.setMapTypeId(google.maps.MapTypeId.ROADMAP);
		}
	if (tip=="SATELLITE")
		{
			map.setMapTypeId(google.maps.MapTypeId.SATELLITE);
		} 	
			

	}

	

function setTip(tip)
	{
		//tip=map.getMapTypeId();
	if (tip=="ROADMAP")
		{
			 map.setMapTypeId(google.maps.MapTypeId.ROADMAP);
		}
	if (tip=="SATELLITE")
		{
			map.setMapTypeId(google.maps.MapTypeId.SATELLITE);
		} 
	
	}	
	
function codeAddress(address,mapzm) {

    geocoder.geocode( { 'address': address}, function(results, status) {
      if (status == google.maps.GeocoderStatus.OK) {
        map.setCenter(results[0].geometry.location);
        map.setZoom(mapzm);
        var marker = new google.maps.Marker({
            map: map,
            position: results[0].geometry.location
        });
      } else {
        
        infowindow = new google.maps.InfoWindow();
        myLatlng22 = new google.maps.LatLng(44.419082,14.991796);
        map.setCenter(myLatlng22);
        map.setZoom(7);
        var marker = new google.maps.Marker({
            map: map,
            position: myLatlng22
         });
        infowindow.setContent("<div class='infotext' style='height:40px;width:150px;'>Owner did not enter correct location.</div>");	  
	    infowindow.open(map,marker); 
         
      }
    });
  }

 
	
