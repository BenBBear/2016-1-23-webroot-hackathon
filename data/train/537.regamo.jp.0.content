<script type="text/javascript">
$(document).ready(function() {
	$('#maga_btn').click(function(){
		var opt = {};
		var err = {};
		var flg = false;
		$('#mail_magazine_form').find(':input').each(function(){
			var n = $(this).attr('name');
			var v = $(this).val();
			err[n] = '';
			opt[n] = v;
			if(v=='') {
				err[n] = '必須項目です。';
				flg = true;
			}
		});
		
		if(flg){
			for(var k in err){
				$('#maga_'+k).html(err[k]+'<br />');
			}
			return false;
		}
		$.ajax({
			type: 'post',
			url: 'http://regamo.jp/mail/mail_magazine.php',
			data: opt,
			success: function(res){
				if(res=='ok'){
					$('#magazine_window').html('<div class="pda5">ご登録ありがとうございました。確認メールをお送りしました。</div>');
				}else{
					alert(res);
				}
			},
		});
	});
	
	$('#mail').keypress(function (e) {
		if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
			var opt = {};
			var err = {};
			var flg = false;
			$('#mail_magazine_form').find(':input').each(function(){
				var n = $(this).attr('name');
				var v = $(this).val();
				err[n] = '';
				opt[n] = v;
				if(v=='') {
					err[n] = '必須項目です。';
					flg = true;
				}
			});
			
			if(flg){
				for(var k in err){
					$('#maga_'+k).html(err[k]+'<br />');
				}
				return false;
			}
			$.ajax({
				type: 'post',
				url: 'http://regamo.jp/mail/mail_magazine.php',
				data: opt,
				success: function(res){
					if(res=='ok'){
						$('#magazine_window').html('<div class="pda5">ご登録ありがとうございました。確認メールをお送りしました。</div>');
					}else{
						alert(res);
					}
				},
			});
			return false;
		}
	});
	
	$('input[title]').each(function() {
		if($(this).val() === '') {$(this).val($(this).attr('title'));}
		$(this).focus(function() {
			if($(this).val() === $(this).attr('title')) {$(this).val('').addClass('focused');}
		});
		$(this).blur(function() {
			if($(this).val() === '') {$(this).val($(this).attr('title')).removeClass('focused');}
		});
	});
});
</script>	
