    	countryValue = dojo.byId("change_country_link_clickable").innerHTML;
      try{
			
			dojo.byId("change_country_link_unClickable").onmouseover = countryEventMouseOver;
			dojo.byId("change_country_link_clickable").onmouseover = countryEventMouseOver;
			dojo.byId("change_country_link_unClickable").onmouseout = countryEventMouseOut;
			dojo.byId("change_country_link_clickable").onmouseout = countryEventMouseOut;
      }catch(e){}
			
	});
    function countryEventMouseOut(e){
        try{		    	
				if(!e)
					e=window.event;
				if(!e.target)		
					e.srcElement.innerHTML = dojo.trim(countryValue);
				else 			
				   e.target.innerHTML = dojo.trim(countryValue);
        }catch(ee){}
	}
	function countryEventMouseOver(e){
		try{            
			if(!e)
				e=window.event;				
			if(!e.target)			
				e.srcElement.innerHTML = MessageHelper.messages["SPLASH_CHANGE_REGION"];
			else
				e.target.innerHTML = MessageHelper.messages["SPLASH_CHANGE_REGION"];
		}catch(ee){}		
	}
</script>
