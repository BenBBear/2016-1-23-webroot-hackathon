    <script type="text/javascript">
        dojo.addOnLoad(function()
        {
            GELS.Util.fixHeight("#catalog-list .catalog-item");
            GELS.Util.fixCataloglistheight("catalog-list");
        
           // TODO Refactoring
            dojo.addOnLoad(function() {
                // Add hover effect on product listing entries incl. grabbing the link and adding click event
                // Note: The onmouseenter/onmouseleave will trigger on child elements as well. This means
                //       that e.target will not always be the .category-item element, so we need to find
                //       the parent div in these cases before applying style etc.
                dojo.query(".catalog-item").onmouseenter(function(e){
                    var parentDiv = GELS.Util.getParentDiv(e.target);
                    // dojo.addClass(parentDiv, 'hovered');
                }).onmouseleave(function(e){
                    var parentDiv = GELS.Util.getParentDiv(e.target);
                    //dojo.removeClass(parentDiv, 'hovered');
                }).onclick(function(e){
                    var parentDiv = GELS.Util.getParentDiv(e.target);
                    document.location.href = dojo.query(".category-link", parentDiv)[0];
                });
            });
        });
    </script>
