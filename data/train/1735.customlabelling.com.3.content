	function clearSearchInput(){
		if(dijit.byId("mySuggessionList").getValue()== "Search GE Healthcare Life Sciences")			
		dijit.byId("mySuggessionList").setValue("");	
	}
	function resetSearchInput(){
		if(dijit.byId("mySuggessionList").getValue()== "")			
		dijit.byId("mySuggessionList").setValue("Search GE Healthcare Life Sciences");	
	}
	function trySearch(e){
		if(e.keyCode==13)
		{
		search();
		}
	}
	function search(){
		document.CatalogSearchForm.searchTerm.value = dijit.byId('mySuggessionList').getValue();
		console.debug("---- searchTerm: "+ document.CatalogSearchForm.searchTerm.value);
		document.CatalogSearchForm.searchTerm.value=trim(document.CatalogSearchForm.searchTerm.value);
		if (document.CatalogSearchForm.searchTerm.value == null || document.CatalogSearchForm.searchTerm.value == ''||document.CatalogSearchForm.searchTerm.value == 'Search GE Healthcare Life Sciences') {
			MessageHelper.formErrorHandleClient("mySuggessionList","Please enter a search term");
		}
		else {
			document.CatalogSearchForm.action=getAbsoluteURL()+'GSASearchCatalog';		  
			submitSpecifiedForm(document.CatalogSearchForm);			
		}
	}
	
	
	function getItemNumber(itemComplex){
		// HPQC #2615 Changes
		//itemComplex = itemComplex.replace(/[()]/g,' ');
		// as per security vulnerability we are going to remove <>'%;+, chars in serach term.
		itemComplex = itemComplex.replace(/[<>'%;+,]/g,'');
		return itemComplex;
			// Code Commented For HPQC #2615
			//return	itemComplex.substring(0,itemComplex.indexOf('('));
	}
	
	function viewItemNumber(newValue){	
		if(newValue.indexOf('(')>0)
			dijit.byId("mySuggessionList").setValue(getItemNumber(newValue));
	}
	
    dojo.addOnLoad(function() {
    	    	
        var filteringSelect = new dijit.form.ComboBox({
