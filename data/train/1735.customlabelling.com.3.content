     // as per security vulnerability we are going to remove <>"'%;)(&+, chars in serach term.
    	dojo.connect(dojo.byId("mySuggessionList"), "onkeyup", function(event) {
      		var entryterm = dojo.byId("mySuggessionList").value;
      		 dojo.byId("mySuggessionList").value = entryterm.replace(/[<>%;+,']/g,'');
      	  });
        
        // Calling the above JS functions
        dojo.connect(filteringSelect,"onFocus",clearSearchInput);
        dojo.connect(filteringSelect,"onBlur",resetSearchInput);
        dojo.connect(filteringSelect,"onKeyPress",trySearch);
        dojo.connect(filteringSelect,"onChange",viewItemNumber);  
        dojo.connect(filteringSelect,"_selectOption",search);
        }); 
        
		dojo.addOnLoad(function() {parseWidget("change_country"); });
</script>

