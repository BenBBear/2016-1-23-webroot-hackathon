    function validaMesFacil(idForm){
            if(CheckMesFacil()){
        $('#enviarMesFacil').fadeOut(100);
        $('#enviarMesFacil').hide();
        var nom=escape($('#i_nom').val());
        var cognoms=escape($('#i_cognoms').val());
        var mail=escape($('#i_email').val());
        if($('#i_mobil').val()==$('#i_mobil').attr('vDefecte')){
          var mobil='';
        }else{                      
          var mobil=escape($('#i_mobil').val());
        }
        if($('#i_direccio').val()==$('#i_direccio').attr('vDefecte')){
          var direccio='';
        }else{                      
          var direccio=escape($('#i_direccio').val());
        }
        if($('#i_cp').val()==$('#i_cp').attr('vDefecte')){
          var cp='';
        }else{                      
          var cp=escape($('#i_cp').val());
        }
        if($('#i_poblacio').val()==$('#i_poblacio').attr('vDefecte')){
          var poblacio='';
        }else{                      
          var poblacio=escape($('#i_poblacio').val());
        }
        if($('#i_mesInfo').is(':checked')){ var mesInfo='SI'; }else{ var mesInfo='NO'; }
        var diaVenciment=escape($('#diaVenciment :selected').val());
        var mesVenciment=escape($('#mesVenciment :selected').val());
        var anyVenciment=escape($('#anyVenciment :selected').val());
        //var captcha_code=escape($('#i_captcha_code').val());
        var dataString = 'nom='+nom+'&cognoms='+cognoms+'&Mail='+mail+'&Mobil='+mobil+'&Direccio='+direccio+'&CP='+cp+'&Poblacio='+poblacio+'&Dia_venciment='+diaVenciment+'&Mes_venciment='+mesVenciment+'&Any_venciment='+anyVenciment+'&Mes_info='+mesInfo;
        $.ajax({
          type: "POST",
          url: "http://activita.cat/lib/guardaMesFacil.php",
          data: dataString,
          success: function EsperemResposta(resposta) {
            if(resposta=="OK"){
              $('#enviarMesFacil').fadeIn(300);
              Sexy.info('Dades enviades correctament, moltes gràcies');
            }else{
              $('#enviarMesFacil').fadeIn(300);
              Sexy.error('ERROR: Ja ha estat donat d\'alta amb aquest e-mail. Si te cap dubte, contacti amb nosaltres.');
            }
          }
        });
        return false;
      }else{
        Sexy.error('Hi ha dades incorrectes, revisi els camps en vermell i torni a enviar el formulari. Moltes gràcies.');
        return false;
      }
      return false;
    }
    function validaNews(idForm){
      if(CheckNews()){
        $('#btNews').fadeOut(100);
        $('#btNews').hide();       
        var mail=escape($('#i_n_email').val());
        var dataString = 'Mail=' + mail;
        $.ajax({
          type: "POST",
          url: "lib/guardaNews.php",
          data: dataString,
          success: function EsperemResposta(resposta) {
            if(resposta=="OK"){
              $('#btNews').fadeIn(300);
              Sexy.info('Dades enviades correctament, moltes gràcies');
            }else{
              $('#btNews').fadeIn(300);
              Sexy.error('ERROR: Ja ha estat donat d\'alta amb aquest e-mail. Si te cap dubte, contacti amb nosaltres.');
            }
          }
        });
        return false;
      }else{
        return false;
      }
      return false;
    }
    /* ]]> */
  </script>
