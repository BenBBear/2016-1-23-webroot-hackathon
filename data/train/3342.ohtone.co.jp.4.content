<script>
$(function() {
$(".menutrigger").waypoint({
handler: function(direction) {//ここまでさっきと一緒
          
    if (direction == 'down') { //スクロールが下方の場合実行
    
        $("#header").addClass('header_defo');
        $("#header.header_defo").hide().slideDown(300);
    
    }else if(direction == 'up'){　//スクロールが上方の場合実行
    
        $("#header.header_defo").slideUp(200,function(){
            $("#header").removeClass('header_defo').slideDown(300);
        });

    }// if end

}
});
});
</script>


<script>
