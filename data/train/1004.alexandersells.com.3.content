		function numberWithCommas(x) {
      return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
    };

		function createListingTemplate(listing) {
			var template = $('.agentTemplate').html();

      var agentTemplate = BN.template(template, {
        address : listing.Address,
        city : listing.City,
        state : listing.State,
        zip : listing.Zipcode,
        pic : listing.Picture,
        tag : listing.TagLine,
        url : listing.VirtualTourURL == '' ?  listing.ListingDetailHref : listing.VirtualTourURL,
        price : '$' + numberWithCommas(listing.Price) ? '$' + numberWithCommas(listing.Price) : '',
        beds : listing.ListingBedroomCount ? '<i class="flaticon-bed2 listingIcons"></i>' + listing.ListingBedroomCount + ' Beds' : '',
        baths : listing.ListingBathroomCount ? '<i class="flaticon-shower8 listingIcons"></i>' + listing.ListingBathroomCount + ' Baths' : '',
        garage : listing.GarageCount ? '<i class="fa fa-car listingIcons"></i>' + listing.GarageCount + ' Garages' : '',
        sqft : listing.ListingInteriorSize ? '<i class="fa fa-home listingIcons"></i>' + numberWithCommas(listing.ListingInteriorSize) : ''
      }, '');

      return agentTemplate;
		}

		function init() {
			var hasSolds = false;
			$.each(listings, function(_, listing) {
				var agentListing = createListingTemplate(listing);

				if (listing.Status === 'Sold') {
					sold.append(agentListing);
					hasSolds = true;
				} else {
					active.append(agentListing);
				}
			});
			
			if (hasSolds) {
				soldTab.show();
			}

			statusNav.on('click', 'li', function() {
				statusNav.children().removeClass('activated');
				$(this).addClass('activated');

				$(this).hasClass('activeTab') ? toggleList(sold) : toggleList(active);
			});
		}

		function toggleList(that) {
			that.fadeOut(300);
    	$('.customListings').not(that).fadeIn(300);
		}

		if(statusNav.get(0)) init();
	})();
	
