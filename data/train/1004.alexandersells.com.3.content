          location = [{Location:{}}];

        //If autocomplete then use rel value
        if(propertySearchField.attr('rel') == null || propertySearchField.attr('rel') == '') {
          searchval = propertySearchField.val();
        } else {
          searchval = '';

          var item = JSON.parse(propertySearchField.attr('rel'));

          location[0].Location.LocationValue = item.locationValue;
          location[0].Location.LocationDisplay = item.value;
          location[0].Location.MlsId = item.mlsId;
          location[0].Location.LocationType = item.locationType;
          location[0].Location.GeoResource = item.geoResource;
          location[0].Location.GeoType = item.geoType;
          location[0].Location.GeoSearchType = item.geoSearchType;
          location[0].Location.KmlURL = item.KmlURL;
          location[0].Location.Point = item.Point;

          if(location[0].Location.LocationType == "5" || location[0].Location.LocationType == "9") searchval = location[0].Location.LocationValue;

          search_options.location = location;
        }

        e.preventDefault();

        search_options.beds = quickSearch.find(pageSettings.quickSearch.propertySearchBeds).val();
        search_options.baths = quickSearch.find(pageSettings.quickSearch.propertySearchBaths).val();
        search_options.proptype = quickSearch.find(pageSettings.quickSearch.propertySearchPropertyType).val();
        search_options.openhouse = (quickSearch.find(pageSettings.quickSearch.propertySearchOpenHouse).attr("checked") == "checked") ? "true" : "false";
        search_options.agentListings = (quickSearch.find(pageSettings.quickSearch.propertySearchAgentListings).attr("checked") == "checked") ? "true" : "false";
        search_options.minPrice = (isNaN(min_price)) ? 0 : min_price;
        search_options.maxPrice = (isNaN(max_price)) ? 0 : max_price;
        search_options.maxPrice = (isNaN(max_price)) ? 0 : max_price;

        search_options_string = JSON.stringify(search_options);

        $("<form action='" + pageInfo.baseUrl + "search-results' method='POST' style='display:none;'>" +
          "<input type='hidden' name='search_init_location' value='" + searchval + "'>" +
          "<input type='hidden' name='search_init_options' value='" + search_options_string + "'>" +
          "<input type='submit' value='submit'>" +
        "</form>").appendTo('body').submit();
      };

      function openAdvancedSearch(e){
        //quickSearch.css({"top" : quickSearchTop - quickSearch.find(pageSettings.quickSearch.advancedContainer).outerHeight(true)});
        advancedSearch.fadeIn(250);
      };

      function hideAdvancedSearch(e){
        //quickSearch.css({"top" : quickSearchTop});
        advancedSearch.hide();
      };

      function init() {
        autocomplete.init();

        propertySearchField.keydown(function(e){
          if(e.keyCode == 13)submitSearch(e);
        });

        propertySearchButton.click(submitSearch);

        if(pageSettings.quickSearch.useAdvanced) {
          propertySearchField.on("focus", openAdvancedSearch);
          quickSearch.find(pageSettings.quickSearch.closeOptionsButton).on("click", hideAdvancedSearch);
        }
      }

      if(quickSearch.get(0)) init();
    })();

  });
</script>
		
		
		
			

		
