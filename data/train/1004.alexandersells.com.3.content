<script type="text/javascript">
	BN.using(["sliders.GalleryCarousel"], function($){
		var $window = $(window);
	
		jQuery(document).ready(function(){			
			var soldListingSlider;		
			var soldListingContainer = $("#soldListings");
			var mobileSet = false;
			var tabletSet = false;
			var winSizeTimeout;
			
			var isWindowTabletSize = function(e){
				return $window.width() < 768;
			};
						
			var isWindowMobileSize = function(e){
				return $window.width() < 481;
			};
			
			var initializeSlider = function(viewableSlides){
				if(soldListingSlider)soldListingSlider.destroy();
				soldListingSlider = new BN.sliders.GalleryCarousel("#soldListings", "Sold", {
					visibileSlidesNum              : viewableSlides,
					listingLimit                   : 99,
					useCustomControls              : true,
					autoPlay                       : false,
					customControlPreviousSelector  : "#soldListingsControlPrevious",
					customControlNextSelector      : "#soldListingsControlNext",
					<!--galleryNamePlaceholderSelector : "#soldGalleryNamePlaceholder",-->
					listingItemTemplateSelector    : "#soldListingTemplate"
				});
				soldListingSlider.init();
			};
			
			var onSoldWindowResize = function(){
				if(isWindowMobileSize() && !mobileSet) {
					soldListingContainer.empty();
					initializeSlider(1);
					mobileSet = true;
					tabletSet = false;
				} else if(isWindowTabletSize() && !isWindowMobileSize() && !tabletSet) {
					soldListingContainer.empty();
					initializeSlider(4);
					tabletSet = true;
					mobileSet = false;
				}else if((mobileSet || tabletSet) && !isWindowMobileSize() && !isWindowTabletSize()) {
					soldListingContainer.empty();
					initializeSlider(6);
					mobileSet = false;
					tabletSet = false;
				} 
			};
			
			mobileSet = isWindowMobileSize();
			tabletSet = isWindowTabletSize();
			initializeSlider(mobileSet ? 1 : tabletSet ? 4 : 6);
			
			$window.on("resize", function(e){
				clearTimeout(winSizeTimeout);
				winSizeTimeout = setTimeout(onSoldWindowResize, 200);
			});
		});
	});
</script>

				
				
