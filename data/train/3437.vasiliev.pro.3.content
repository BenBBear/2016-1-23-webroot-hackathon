	<meta name="viewport" content="initial-scale=1, width=device-width, maximum-scale=1, minimum-scale=1, user-scalable=no">

	<script>
		$(document).ready(function(){
			$(".main").onepage_scroll({
				sectionContainer: "section",
				responsiveFallback: 600
			});
			
			var win_height = $(window).height();
			$('.left_aside_menu').css('height', win_height);
			$(window).resize(function() {
				var win_new_height = $(this).height();
				$('.left_aside_menu').css('height', win_new_height);
			});
			
			var l_a_menu_width = $('.left_aside_menu').width();
			
			$('.left_aside_menu').css('left', '-' + l_a_menu_width + 'px');
			
			$('.mymenu').click(function() {
				if ( $('.mymenu').hasClass('mymenu_is_open') ) {
					$('.mymenu').removeClass('mymenu_is_open');
					$('.left_aside_menu').animate({left: '-' + l_a_menu_width + 'px'}, 500);
					$('.mymenu').animate({left: '15px'}, 500);
				} else {
					$('.left_aside_menu').animate({left: '0',}, 500);
					$('.mymenu').animate({left: l_a_menu_width+15 + 'px',}, 500);
					$('.mymenu').addClass('mymenu_is_open');
				}
			});
		});
		
	</script>
