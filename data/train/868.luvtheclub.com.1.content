<script>

if(true){

var seconds = 60;

function timer(){
  $('div#jqibox').css('z-index', '999999');
  $('p.countdown').html("Your session will expire in 60 seconds. <span class='bolder';>Please click the OK button to continue using the site.</span>");
  count = seconds;
  countdown = setInterval(function(){
    $('p.countdown').html("Your session will expire in "+count+" seconds. <span class='bolder';>Please click the OK button to continue using the site.</span>");
    if (count == 0){
      $.prompt.goToState('state1');
    }
    count--;
  }, 1000);
};

function keepalive(){
  $.ajax({
    url: '/'
  });
};

function killsession(){
  $.ajax({
    url: '/landing/clear'
  });
};

var promptstates = {
  state0: {
    //countdown
    html: '<p class="countdown">Loading...</p>',
    position: {},
    submit: function(){ keepalive(); clearInterval(countdown); timeout(); }
  },
  state1: {
    html: '<p>Your session has expired. <span class="bolder">Please click the OK button below to return to the home page.</span></p>',
    position: {},
    submit: function(){ window.location = '/'; }
  }
};

function timeout(){
  setTimeout(function(){
    $.prompt(promptstates, { opacity: 0.9, loaded: function(){ timer(); }, statechanged: function(){ killsession(); } });
}, (60*60*1000) );
};

$(function(){ timeout(); });

}

</script>
