            <script type="text/javascript">


                $(document).ready(function() {

                    if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
                        $('.CashbackTooltip').click(function () {
                            var tooltip = $(this).find('.CashbacktooltipBubble');
                            if ($(tooltip).is(':hidden')){
                                $(tooltip).show();
                            }else if ($(tooltip).is(':visible')){
                                $(tooltip).hide();
                            };

                        });
                    } else {
                        $('.CashbackTooltip').hover(function () {
                            var tooltip = $(this).find('.CashbacktooltipBubble');
                            if ($(tooltip).is(':hidden')){
                                $(tooltip).show();
                            }else if ($(tooltip).is(':visible')){
                                $(tooltip).hide();
                            };

                        });
                    };




                    // allow digits only
                    digitsOnly(); // see main.js

                    $('#calculate').click(function(e) {                
                        e.preventDefault();
                        doCashbackCalc();
                        $(".cbctotal").fadeIn("slow");
                    });

                    $('.cbcformContainer').on('click focus', 'input', function(){
                        if($(this).val() == $(this).data('default')) {
                            $(this).val('');
                        }
                    })

                    $("#focused-element").focus();

                });

                function digitsOnly() {
                    $("input[data-type='spend']").keydown(function (e) {

                        if ($.inArray(e.keyCode, [46, 8, 9, 27, 13/*, 110, 190*/]) !== -1 ||
                            (e.keyCode == 65 && e.ctrlKey === true) || 
                            (e.keyCode >= 35 && e.keyCode <= 39)) {
                                return;
                        }
                        if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                            e.preventDefault();
                        }
                    });
                }

                function doCashbackCalc() {
                    var form = new Object();
                    $('.cbcformContainer :input').not('button, input[type="submit"]').each(function(i, el) {
                        if(typeof form[$(el).data('category')] == 'undefined') {
                            form[$(el).data('category')] = new Object();
                        }

                        form[$(el).data('category')][$(el).data('type')] = $(el).val();
                    });

                    //petrol catch
                    if(form['groceries']['spend'] < 50){
                        form['travel']['pc'] = 0;
                    }

                    subTotal = 0;
                    //groceries[spend] = 3.53, groceries[pc] = 3
                    $.each(form, function(i, o) {
                        subTotal += (o.spend * o.pc / 100);
                    });

                    total = parseFloat(subTotal).toFixed(2);
                    total = parseFloat(total * 12).toFixed(2);

                    if (total >= 1000) {
                        total = Math.round(total);
                    }

                    if(!$.isNumeric(total)) {
                        total = '0.00 (invalid input)';
                    }

                    $('.cashbackSavings').text(total);

                    $('.monthlyCashbackSavings').text(parseFloat(total / 12).toFixed(2));
                }

            </script>
