    <script>
    $(document).ready(function() {
        $('#videoLightbox').dialog({
            autoOpen: false,
            modal: true,
            title: "RFMD Video",
            width: 'auto',
            resizable: false,
            draggable: false,
            position: {my: "center", at: "center", of: window},
            open: function() {
                if ($(this).parent().width() > $(window).width()) {
                    $(this).width($(window).width() * 0.8);
                    $(this).find('iframe').attr('width', $(window).width() * 0.8);
                    $(this).find('iframe').attr('height', $(window).width() * 0.8 * 0.5625);
                }
                modalOpenPositioning();
                $(this).parent('.ui-dialog').find('.ui-dialog-titlebar').hide();
                $(this).parent('.ui-dialog').find('.ui-dialog .ui-dialog-content').css('padding', '0');
                $('#videoLightbox').css('padding', '2px 0 1px');
                $('#videoLightbox').dialog("option", "position", {my: "center", at: "center", of: window});
                //sales adjust
                if($('#content').hasClass('salesContent')){
                    $(this).parent().css('left', '+='+$('#salesSection').width()/2 );
                }
                $(this).addClass('repaint');
            },
            beforeClose: function() {
                modalClosePositioning();
            }
        });
        $("#videoLightbox").bind("dialogclose", function(event, ui) {
            $('#theVideo').attr('src', '');
        });

    });

    function SetVideo(source, title) {
        $('#theVideo').attr('src', source);
        $('#videoLightbox').dialog("option", "title", title);
        $('#videoLightbox').dialog('open');
        $('.ui-widget-overlay').click(function() {
            $('#videoLightbox').dialog('close');
        });
    }
    ;

</script>

