    $(document).ready(function() {
        $('.videoSetterLink').click(function(e) {
            if ($('html').hasClass('videoautoplay')) {
                e.preventDefault();
                SetVideo('//player.vimeo.com/video/122643963?title=0&amp;byline=0&amp;portrait=0&amp;color=440259&amp;autoplay=1');
                return false;
            } else {
                e.preventDefault();
                SetVideo('//player.vimeo.com/video/122643963?title=0&amp;byline=0&amp;portrait=0&amp;color=440259&amp;autoplay=0');
                return false;
            }
        });

        formLogic();

        $('form').on('click keyup', function() {
            formLogic();
        })

    });

    function formLogic() {

        if ($('[name="customer[occupancy]"]:checked').val() == 'owner' && $('.js-ownerdetails').is(':visible')) {
            $('.js-threemonths').slideDown({duration: 550, easing: 'easeInOutExpo'});
        } else if ($('[name="customer[occupancy]"]:checked').val() !== 'owner') {
            $('.js-threemonths').slideUp({duration: 550, easing: 'easeInOutExpo'});
        }

        if ( $('[name="customer[ownedFor3Months]"]').is(':checked') && $('.js-threemonths').is(':visible')) {
            $('#Mainaddress').hide().promise().done(function() {
                $('.js-prevAddress').slideDown({duration: 550, easing: 'easeInOutExpo'});
                $('.yourAddressClosed').removeClass('hidden');
            });

        } else if ( !$('[name="customer[ownedFor3Months]"]').is(':checked')) {
            $('[name="customer[prevAddressOwned]"]').attr('checked', false);
            $('.js-prevAddress').hide().promise().done(function() {
                $('#Mainaddress').slideDown({duration: 550, easing: 'easeInOutExpo'});
                $('.yourAddressClosed').addClass('hidden');
            });

        }

        if ($('[name="customer[prevAddressOwned]"]:checked').val() == 'true' && $('[name="customer[ownedFor3Months]"]:checked').is(':checked')) {
            $('.js-prevAddressDetails').slideDown({duration: 550, easing: 'easeInOutExpo'});
        } else {
            $('.js-prevAddressDetails').slideUp({duration: 550, easing: 'easeInOutExpo'});
        }

        if (
                (
                $('[name="customer[occupancy]"]:checked').val() == 'tenant'
                || ($('[name="customer[occupancy]"]:checked').val() == 'owner' && !$('[name="customer[ownedFor3Months]"]:checked').is(':checked'))
                || ($('[name="customer[occupancy]"]:checked').val() == 'owner' && $('[name="customer[ownedFor3Months]"]:checked').is(':checked') && $('[name="customer[prevAddressOwned]"]:checked').val() == 'false')
                || ($('[name="customer[occupancy]"]:checked').val() == 'owner' && $('[name="customer[ownedFor3Months]"]:checked').is(':checked')
                        && $('[name="customer[prevAddressOwned]"]:checked').val() == 'true' && $('[name="customer[prevAddress]"]').val() !== '' && $('[name="customer[prevPostcode]"]').val() !== '')
                )
                /*&& $('.addressDetailes').is(':visible') */
                && ($('[name="comms[CLI]"]').val() || $('#check1').is(':checked') || document.activeElement.name == 'comms[CLI]' )
                && $('.js-addressPostcode').hasClass('addressSelected')
                ) {
            $('.GetStarted').show();
        } else {
            $('.GetStarted').hide();
        }
    }
</script>


