<script language="javascript">

function changeModleBox(make)
{
	xmlHttp1 = getXMLHttp();
	//alert(make);
	xmlHttp1.onreadystatechange = fillModelCombo;
	xmlHttp1.open("GET", "http://www.3suncorp.jp/stock//filter_model.php?make="+make, true);
	xmlHttp1.send(null);
}

function getXMLHttp()
{  
	var xmlHttp;
	try
	{    
		// Firefox, Opera 8.0+, Safari    
		return xmlHttp=new XMLHttpRequest();    
	}
	catch (e)
	{    
		// Internet Explorer    
		try
		{      
			return xmlHttp=new ActiveXObject("Msxml2.XMLHTTP");      
		}
		catch (e)
		{      
			try
			{        
			return xmlHttp=new ActiveXObject("Microsoft.XMLHTTP");        
			}
			catch (e)
			{        
				alert("Your browser does not support AJAX!");        
				return false;        
			}      
		}    
	}
	
}

function resetAdvSearch()
{
	document.advanceSearchForm.make.value = "";
	document.advanceSearchForm.maker.value = "";
	document.advanceSearchForm.price_from.value = "";
	document.advanceSearchForm.price_to.value = "";
	document.advanceSearchForm.mfg_from.value = "";
	document.advanceSearchForm.mfg_to.value = "";
	document.advanceSearchForm.keyword_search.value = "";
	return false;
}


function fillModelCombo()
{
	if(xmlHttp1.readyState==4)
	{
		var response;
		response = xmlHttp1.responseXML;
		var m_id = response.getElementsByTagName("maker");
		obj = document.getElementById("maker");
		obj.length = null;
		if(m_id.length>0)
		{
			for(i=0;i<m_id.length;i++)
			{
				if(m_id[i].firstChild)
				{
					obj.options[i]= new Option(m_id[i].firstChild.nodeValue,m_id[i].getAttribute("id"));
				}
			}
		}
	}
}

</script>

