function checkValidation_and_ajax(p_id){
	var offer_price=document.getElementById('contactFormOfferPrice_'+p_id).value;
	if(offer_price==""){
		alert('Please enter price');
      	//$("#contactFormOfferPrice_"+p_id).css('box-shadow', '0px 0px 7px red');
		document.getElementById('contactFormOfferPrice_'+p_id).focus();
		return false;
	}
  
  	var min_price=document.getElementById('min_price_'+p_id).value;
    var max_price=document.getElementById('max_price_'+p_id).value;
  	if(min_price!='' && max_price!=""){
    	
      	a=offer_price.replace(/\,/g,'');
      	final_price=parseFloat(a);
      	
      	org_min_price=min_price;
        org_max_price=max_price;
      
      	min_price1=min_price.replace(/\,/g,'');
      	min_price=parseFloat(min_price1);
      
        max_price1=max_price.replace(/\,/g,'');
      	max_price=parseFloat(max_price1);
      	
      	if(final_price > max_price || final_price < min_price)
        {
        	alert('Your offer must be in between $' +org_min_price+ ' and $' +org_max_price+ ' range.');
            document.getElementById('contactFormOfferPrice_'+p_id).focus();
            return false;
        }
      	
        
  	}
  
  	if(min_price!='' && max_price==""){
    	
      	a=offer_price.replace(/\,/g,'');
      	final_price=parseFloat(a);
      	
      	org_min_price=min_price;
        org_max_price=max_price;
      
      	min_price1=min_price.replace(/\,/g,'');
      	min_price=parseFloat(min_price1);
      
        max_price1=max_price.replace(/\,/g,'');
      	max_price=parseFloat(max_price1);
      	
      	if(final_price <= min_price)
        {
        	
          alert('Your offer should be above $' +org_min_price+ '.');
            document.getElementById('contactFormOfferPrice_'+p_id).focus();
            return false;
        }
      	
        
  	}
	
  	
  
	var name=document.getElementById('contactFormName_'+p_id).value;
	if(name==""){
		alert('Please enter name');
      	//$("#contactFormName_"+p_id).css('box-shadow', '0px 0px 7px red');
		document.getElementById('contactFormName_'+p_id).focus();
		return false;
	}
	
	var email=document.getElementById('contactFormEmail_'+p_id).value;
	if(email==""){
		alert('Please enter email');
		document.getElementById('contactFormEmail_'+p_id).focus();
      	//$("#contactFormEmail_"+p_id).css('box-shadow', '0px 0px 7px red');
		return false;
	}
	
	var atpos=email.indexOf("@");
	var dotpos=email.lastIndexOf(".");
	if(atpos<1 || dotpos<atpos+2 || dotpos+2>=email.length) {
		alert("Not a valid e-mail address");
		document.getElementById('contactFormEmail_'+p_id).focus();
      	//$("#contactFormEmail_"+p_id).css('box-shadow', '0px 0px 7px red');
		return false;
	}
	
	var arrive_by=document.getElementById('arrive_by_'+p_id).value;
  
  	if(arrive_by==""){
		alert('Please select How did you first hear of namepage.com?');
		document.getElementById('arrive_by_'+p_id).focus();
		return false;
	}
  
  	if (eval($("#question_"+p_id).val()) != $("#answer_"+p_id).val()){
            //$("#answer_"+p_id).css('box-shadow', '0px 0px 7px red');
      		$("#answer_"+p_id).val('');
      		$("#answer_"+p_id).focus();
      		alert('Please enter right ans');
            return false;
    }
  
  	$("#submit_btn_"+p_id).val('Sending');
	
	var domain_name=document.getElementById('contactFormDomainName_'+p_id).value;
	var phone=document.getElementById('contactFormPhone_'+p_id).value;
	var msg_body=document.getElementById('contactFormMessage_'+p_id).value;
  
    
	
	var action  = '/contact?';
	action += encodeURIComponent('form_type') +'='+ encodeURIComponent('contact');
	action += '&'+ encodeURIComponent('utf8') +'='+ encodeURIComponent('âœ“');
	action += '&'+ encodeURIComponent('contact[domain_name]') +'='+ encodeURIComponent(domain_name);
	action += '&'+ encodeURIComponent('contact[offer_price]') +'='+ encodeURIComponent(offer_price);
	action += '&'+ encodeURIComponent('contact[name]') +'='+ encodeURIComponent(name);
	action += '&'+ encodeURIComponent('contact[email]') +'='+ encodeURIComponent(email);
	
	if(phone!=""){
		action += '&'+ encodeURIComponent('contact[phone]') +'='+ encodeURIComponent(phone);	
	}
	
  	if(arrive_by!=""){
		action += '&'+ encodeURIComponent('contact[How did you first hear of namepage.com?]') +'='+ encodeURIComponent(arrive_by);
	}
  
	if(msg_body!=""){
		action += '&'+ encodeURIComponent('contact[body]') +'='+ encodeURIComponent(msg_body);
	}
  
  	/*alert(action);
  	return false;*/
  

	
	$.ajax({
