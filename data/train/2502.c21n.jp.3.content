(function($){

	$.fn.createAreamap = function(options){

		var defaults = {
			url            : {}, // required
			/* ====================
			
			# URLの連想配列
			
			'id（接頭辞なし）' : {
				kyoju   : '', // 賃貸居住
				jigyo   : '', // 賃貸事業
				tochi   : '', // 土地
				kodate  : '', // 戸建て
				mansion : '', // マンション
				zembu   : '', // 収益（全部）
				ichibu  : ''  // 収益（一部）
			}
			
			※何も入れなければ出現しない。
			
			==================== */
			idPrefix       : '', // li の ID に接頭辞がある場合
			childListClass : 'areamap-area-child-list', // 子リストに付与するクラス
			childItemClass : 'areamap-area-child-item', // 子リストアイテムに付与するクラス
			labels         : {
				kyoju      : '居住用',
				jigyo      : '事業用',
				tochi      : '土地',
				kodate     : '一戸建て',
				mansion    : 'マンション',
				zembu      : '全部',
				ichibu     : '一部'
			}
		};
		var conf = $.extend(defaults, options);

		return this.each(function(i){

			for(key in conf.url){
				// create a child list
				var $childList = $('<ul class="' + conf.childListClass + '"></ul>');
				for(childKey in conf.url[key]){
					// create a child item
					if(conf.url[key][childKey] && conf.url[key][childKey] != ''){
						var $childItem = $('<li class="' + conf.childItemClass + '-' + childKey + ' ' + conf.childItemClass + '"><a href="' + conf.url[key][childKey] + '">' + conf.labels[childKey] + '</a></li>');
						// append the child
						$childList.append($childItem).hide();
					}
				} // for
				// append a child list
				$('#' + conf.idPrefix + key)
				.append($childList)
				.hover(
					function(){
						$(this).css('z-index', 10).addClass('open').find('ul').show();
					},
					function(){
						$(this).css('z-index', 0).removeClass('open').find('ul').hide();
					}
				)
				.find('>a').click(function(){
					if(console) console.log('Please click a type of this.');
					return false;
				});
			} // for
			$(this).find('>ul').show();
	
		}); // return this.each

	} // plugin

})(jQuery);

