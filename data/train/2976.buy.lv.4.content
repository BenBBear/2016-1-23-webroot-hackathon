            // setting banners wrapper block max-width and max-height
            $etBannerBlockBuilder('div.grid-cell[data-zone-id]').each(function(){
                var zoneWidth = $etBannerBlockBuilder(this).width();
                var zoneHeight = $etBannerBlockBuilder(this).height();
                var bannerWrapperWidth = zoneWidth-bannerHorizontalMargin;
                var bannerVerticalMargin = parseInt($etBannerBlockBuilder('.banner_img_wrapper').css('margin-top'))+parseInt($etBannerBlockBuilder('.banner_img_wrapper').css('margin-bottom'));
                var titleHeight = $etBannerBlockBuilder('.banner_title',this).height();
                var titleVerticalMargin = parseInt($etBannerBlockBuilder('.banner_title').css('margin-top'))+parseInt($etBannerBlockBuilder('.banner_title').css('margin-bottom'));

                if(!titleHeight){
                    titleHeight = 0;
                    titleVerticalMargin = 0;
                }

                var bannerWrapperHeight = zoneHeight-bannerVerticalMargin-titleHeight-titleVerticalMargin;
                $etBannerBlockBuilder('div.banner_img_wrapper[data-zone-id]',this).css('max-width', bannerWrapperWidth);
                $etBannerBlockBuilder('div.banner_img_wrapper[data-zone-id]',this).css('max-height', bannerWrapperHeight);

                // setting banners description block width same like banners wrapper without margin
                var bannerDescriptionWidth = bannerWrapperWidth - bannerHorizontalMargin;
                $etBannerBlockBuilder('.banner_description',this).css('width', bannerDescriptionWidth);

                //insert into banners object Banner width and height for resize
                if(!isImagesCached && bannerHorizontalMargin){
                    if(bannersInfo.hasOwnProperty($etBannerBlockBuilder(this).attr('data-zone-id'))){
                        bannersInfo[$etBannerBlockBuilder(this).attr('data-zone-id')]["bannerWidth"] = bannerWrapperWidth;
                        bannersInfo[$etBannerBlockBuilder(this).attr('data-zone-id')]["bannerHeight"] = bannerWrapperHeight;
                    }
                }
            });
            // sending object with banners information for resize, getting new resized banner URL, Insert images in zones
            // on success.

            if(!isImagesCached && bannerHorizontalMargin){
                $etBannerBlockBuilder.ajax({
                    url: "https://www.elkor.lv/lat/etbannerblockbuilder/grids/resize/",
                    type: "POST",
                    data: {"bannersInfo" : bannersInfo},
                    success: function(data) {
                        bannersInfo = $etBannerBlockBuilder.parseJSON(data);
                        for(var zoneId in bannersInfo){
                            var imgHTML = '<img src="'+bannersInfo[zoneId]["bannerPath"]+'" />';
                            $etBannerBlockBuilder("div.banner_img_wrapper[data-zone-id = "+zoneId+"] .banner_link").append(imgHTML);
                        }
                    }
                });
            }

            // showing banners description on hover
            $etBannerBlockBuilder('div.grid-cell[data-zone-id]').hover(
                function () {
                    $etBannerBlockBuilder('.banner_description',this).slideDown('fast');
                },
                function () {
                    $etBannerBlockBuilder('.banner_description',this).slideUp('fast');
                }
            );

            // disable banner links with no href
            $etBannerBlockBuilder('.banner_link').click(function(){
                var linkHref = $etBannerBlockBuilder(this).attr('href');
                if(!linkHref){
                    return false;
                }
            });

        });
    </script>

		 
