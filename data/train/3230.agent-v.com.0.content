	<!-- イベント新着のテンプレート ここから -->
	<script id="template_event_new_entry" type="text/template">
		<% if (data !== false && data.error == 0 && data.events.length > 0) { %>
		<div class="event_info_body_inner" id="event_info_body_inner">
			<div class="event_list">
				<ul>
				<% for (var i = 0; i < data.events.length; i++) { %>
					<% var event = data.events[i]; %>
					<li>
						<a href="https://agent-v.sakura.ne.jp/production/secure/event/<%= event.id %>">
						<span class="event_new_meta">
							<span class="list_status_<%= event.status %>"><%= event.status_name %></span>
							<% if (event.eventcategory_name != "") { %>
							<span class="event_new_cate">【<%= event.eventcategory_name %>】</span>
							<% } %>
							<% if (event.is_anytime == 1) { %>
							<span class="event_new_day">常時開催</span>
							<% } else { %>
							<span class="event_new_day"><%= event.hi %>(<%= event.event_weekday %>) 開催</span>
							<% } %>
						</span>
						<span class="event_new_title"><%= event.title %></span>
						</a>
					</li>
				<% } %>
				</ul>
			<% } else { %>
				<div class="event_new_nolist">
					現在開催中のイベントはありません
				</div>
			<% } %>
			</div>
		</div>
	</script>
