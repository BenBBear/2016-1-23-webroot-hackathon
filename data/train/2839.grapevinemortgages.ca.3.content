<!-- AJAX validation --> 
<script type="text/javascript">
var formerror=0;

//validate name field
function name_check_sidebar()
{
var xx=document.forms["sidebar_form"]["name"].value;
	if (xx==null || xx=="")
	  {
	  document.getElementById('name_field').className="errorjs"
	  formerror=1;
	  }
	else 
	  {
		document.getElementById('name_field').className="success"  
	  }
}

//validate phone field
function phone_check_sidebar()
{
var xx=document.forms["sidebar_form"]["phone"].value;
	if (xx==null || xx=="")
	  {
	  document.getElementById('phone_field').className="errorjs"
	  formerror=1;
	  }
	else 
	  {
		document.getElementById('phone_field').className="success"  
	  }	
}

//validate email field
function email_check_sidebar()
{
var xx=document.forms["sidebar_form"]["email"].value;
var atpos=xx.indexOf("@");
var dotpos=xx.lastIndexOf(".")
	if (atpos<1 || dotpos<atpos+2 || dotpos+2>=xx.length)
		{
	  	document.getElementById('email_field').className="errorjs"
		formerror=1;
	  	}
	else 
	  {
		document.getElementById('email_field').className="success"  
	  }	
}
//validate inquiry field
function skilltest_check_sidebar()
{
var xx=document.forms["sidebar_form"]["skilltest"].value;
if(xx!="")
{
document.getElementById('skilltest').value="This field must be empty";
document.getElementById('skilltest').placeholder=""
document.getElementById('skilltest_field').className="errorjs"
formerror=1;
}
else
{
document.getElementById('skilltest_field').className="success"
document.getElementById('skilltest').placeholder="Thanks!"
document.getElementById('confirm_text').innerHTML="";
}
}

//validate on submit
function validateForm_sidebar()
{

name_check_sidebar(); 
phone_check_sidebar(); 
email_check_sidebar();
skilltest_check_sidebar();
 
	if (formerror==0)
		{
			MakeRequest_sidebar();	
		}
		
formerror=0;
return false;	
}

//send ajax request to server
function MakeRequest_sidebar() //this is a jquery function
{
document.forms["sidebar_form"]["isajax"].value="isajax";//change hidden input value	
var postvalues = $("form").serialize();//create list of input data to send to php for processing

$("#loading_sidebar").ajaxStart(function(){ //turn on the loading gif
  document.getElementById("loading_sidebar").style.visibility = "visible";
  document.getElementById("form_display_sidebar").style.visibility = "hidden";
}).ajaxStop(function(){ //turn off the loading gif
  document.getElementById("loading_sidebar").style.visibility = "hidden";
});

  $.ajax({ //jquery function
      url: "/includes/sidebar_form.php",
      type: "post",
      data: postvalues,
      success: function(){
          HandleResponse_sidebar();
      },
      error:function(){
          document.getElementById("form_display_sidebar").style.visibility = "hidden";
	  	document.getElementById("process_error_sidebar").style.visibility = "visible";
      }   
    });
return false; 
}
//receive ajax response from server
function HandleResponse_sidebar(response) //thank you message
	{
	  document.getElementById("form_display_sidebar").style.visibility = "hidden";
	  document.getElementById("form_display_success_sidebar").style.visibility = "visible";
	}
</script><hr />
