        function getUrlVars() {
            var vars = [], hash;
            var decodedUrl = decodeURIComponent(window.location.href);
            var hashes = decodedUrl.slice(decodedUrl.indexOf('?') + 1).split('&');
            for (var i = 0; i < hashes.length; i++) {
                hash = hashes[i].split('=');
                vars.push(hash[0]);
                vars[hash[0]] = hash[1];
            }
            return vars;
        }

        $("#videoBackground video").bind("ended", function () {
            var pageName = getUrlVars().page;
            var pathName = location.href;
