    function showSubCategoryList(productTypeId, url, shop_checkout) {

        if ($('#product_type_' + productTypeId).closest('li').has('ul').has('li').length) {
            $('.expand-all-sub-category').children('span').removeClass('second-label-menu-sub-minus');
            $('.expand-all-sub-category').children('span').addClass('second-label-menu-sub-plus');

            $('#product_type_' + productTypeId).addClass('change-backgroup');
            if ($('#subCategory' + productTypeId).css('display') == 'block') {

                $("#product_type_" + productTypeId).children('span').removeClass('second-label-menu-sub-minus');
                $("#product_type_" + productTypeId).children('span').addClass('second-label-menu-sub-plus');

                $('.all-list-category').hide('show');
                $('#subCategory' + productTypeId).hide('show');
            } else {
                if (shop_checkout) {
                    $('.all-list-category').hide('show');

                    $("#product_type_" + productTypeId).children('span').removeClass('second-label-menu-sub-plus');
                    $("#product_type_" + productTypeId).children('span').addClass('second-label-menu-sub-minus');
                    $('#subCategory' + productTypeId).show('show');
                } else {
                    window.location = url;
                }


            }
        }

    }


</script>


