	var hp	=	0;		// 合計の最高点
	var lp	=	0;		// 合計の最低点
	var a	=	0;		// 合計の差分


	function init(){
		f = document.forms[0];

		nm	=	"";

		for(i=0; i < f.elements.length; i++) {
			if (f.elements[i].type == "radio" && f.elements[i].name != nm){
				nm	=	f.elements[i].name;
				min	=	0;
				max	=	0;
				for(k=0; k < f.elements[nm].length; k++) {
					if (max < parseInt(f.elements[nm][k].value)){
						max	=	parseInt(f.elements[nm][k].value);
					}
					if (min > parseInt(f.elements[nm][k].value)){
						min	=	parseInt(f.elements[nm][k].value);
					}
				}
				hp	+=	max;
				lp	+=	min;
			}
		}

		a	=	hp - lp;
	}

	function culc(){
		f = document.forms[0];
		amt	=	0;
		nm	=	"";

		for(i=0; i < f.elements.length; i++) {
			if (f.elements[i].type == "radio" && f.elements[i].name != nm){
				nm	=	f.elements[i].name;
				flg	=	0;
				for(k=0; k < f.elements[nm].length; k++) {
					if (f.elements[nm][k].checked) {
						amt +=	eval(f.elements[nm][k].value);
						flg	=	1;
						break;
					}
				}
				if (!flg){
					alert("選択していない項目があります");
					return -999;
				}
			}
		}

		amt	=	Math.round(((amt - lp) / a ) * 100);
		return amt;
	}


	function setPoint(){
		pointAmount = culc();

		if (pointAmount > -999){
			document.forms[0].point.value	=	pointAmount;

			if (pointAmount>=74){
				str	=	"自覚症状はありませんか？間違いなく自律神経が機能していません。早急にケアを。";
			}
			else if (pointAmount>=49){
				str	=	"自律神経にかなり負担がかかり身体もツライはず。生活習慣を見直しましょう。";
			}
			else if (pointAmount>=34){
				str	=	"要注意です。身体を動かし、食事や睡眠に気をつけて生活しましょう。";
			}
			else if (pointAmount>=19){
				str	=	"まだ大丈夫ですがこれ以上キケン度が上がると身体の不調を感じるかも。";
			}
			else if (pointAmount>=4){
				str	=	"30代でこのレベルなら立派！生活を維持し、キケン度を上げないように頑張りましょう。";
			}
			else {
				str	=	"10代レベルの素晴らしい自律神経機能！このままの生活を維持しましょう。";
			}
			document.forms[0].msg.value	=	str;
		}
	}


