<script type="text/javascript">	
	$(document).ready(function() {
		$("#mainpageModal").modal('show');
		document.title = 'Saraç İnşaat - Anasayfa';
		
		$(".fancybox").fancybox({
			openEffect: 'elastic',
			closeEffect: 'elastic',
			nextEffect: 'elastic',
			prevEffect: 'elastic'
		});
		$("body").on("click", ".dropdownButton",function(){			
			var parent=$(this).parent();			
			if (parent.hasClass("open")){
				parent.removeClass("open");
			}
			else{
				parent.addClass("open");			
				if (parent.find(".dropdownMenu").hasClass("scrollable")){
					parent.find(".dropdownMenu").scrollTop(0);
					parent.find(".dropdownMenu").perfectScrollbar('update');			
				}
			}
		});
		
		$("body").on("click", ".dropdownMenu li", function(){
			var label=$(this).text();
			var value=$(this).attr("data-value");
			var color=$(this).html();
			var iscolor=$(this).attr('data-color');
			var parent=$(this).parent().parent();
			var inputID=parent.attr("data-input");
			$(inputID).val(value);
			
			if (iscolor){
				parent.find(".dropdownButton").find(".dropdownLabel").html(color);
			}
			else{
				var buttonText=parent.find(".dropdownButton").find(".dropdownLabel");
				buttonText.text(label);
			}
			parent.removeClass("open");
		});
		$(document).mouseup(function (e) {	
			var elem=$(".dropdownButton");
			if(e.target!=elem[0]&&!elem.has(e.target).length){ 				
				$(".dropdownButton").parent().removeClass("open");
			}			
		});


		$("#requestForm #inputName").keyup(function(){
			$("#inputName").removeClass("has-error");
		});
		$("#requestForm #inputPhone").keyup(function(){
			$("#inputPhone").removeClass("has-error");
		});
		$("#requestForm #inputGSM").keyup(function(){
			$("#inputGSM").removeClass("has-error");
		});
		$("#requestForm #inputEmail").keyup(function(){
			$("#inputEmail").removeClass("has-error");
		});
		$("#requestForm #inputMessage").keyup(function(){
			$("#inputMessage").removeClass("has-error");
		});
		$("#requestForm #inputCaptcha").keyup(function(){
			$("#inputCaptcha").removeClass("has-error");
		});
		$('#requestForm .btn-send').click(function(){
			$("#requestForm #inputName").removeClass("has-error");
			$("#requestForm #inputPhone").removeClass("has-error");
			$("#requestForm #inputGSM").removeClass("has-error");
			$("#requestForm #inputEmail").removeClass("has-error");
			$("#requestForm #inputMessage").removeClass("has-error");
			$("#requestForm #inputCaptcha").removeClass("has-error");
			var name=$("#requestForm #inputName").val();
			var phone=$("#requestForm #inputPhone").val();
			var gsm=$("#requestForm #inputGSM").val();
			var email=$("#requestForm #inputEmail").val();			
			var text=$("#requestForm #inputMessage").val();
			var captcha=$("#requestForm #inputCaptcha").val();			
			var button=$(this);
			var error=0;
			if (name==""){
				$("#requestForm #inputName").addClass("has-error");
				error++;
			}
			if (phone==""){
				$("#requestForm #inputPhone").addClass("has-error");
				error++;
			}
			if (gsm==""){
				$("#requestForm #inputGSM").addClass("has-error");
				error++;
			}
			if (email==""){
				$("#requestForm #inputEmail").addClass("has-error");
				error++;
			}
			if (text==""){
				$("#requestForm #inputMessage").addClass("has-error");
				error++;
			}
			if (captcha==""){
				$("#requestForm #inputCaptcha").addClass("has-error");
				error++;
			}
			if (!error){			
				button.addClass("animated");
				$.post("ajax/sendRequest.php", {name:name, phone:phone, email:email, gsm:gsm, message:text, captcha:captcha}, function(data){
					if(data=="0"){ var result="Mesajınız gönderilmiştir. Teşekkür ederiz."; var resClass="alert-success"; $('#requestForm').trigger("reset");}
					else if(data=="1"){ var result="Mesajınız gönderilmemiştir. Server hatası. "; var resClass="alert-warning";}
					else if(data=="2"){ var result="Güvenlik Kodu Hatalı. "; var resClass="alert-warning";}
					else if(data=="3"){ var result="Lütfen doğru bir E-mail adresi giriniz. "; var resClass="alert-warning";}
					$("#requestForm #resultField").addClass(resClass);
					$("#requestForm #resultField").text(result);
					$("#requestForm #resultField").fadeIn();
					button.removeClass("animated");
					setTimeout(function(){						
						$("#requestForm #resultField").hide();
						$("#requestForm #resultField").removeClass(resClass);
						$("#requestFormModal").modal('hide');
					}, 2500);
				});
			}
		});
	});
</script>

