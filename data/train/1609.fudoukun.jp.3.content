				<!-- フォームバリデーション（ jquery.validate ）の読み込みと設定 -->
				<script src="js/jquery.validate.js" type="text/javascript"></script>
				<script>
					$(function(){
						$.validator.addMethod('regex',
							function(value, element, param){
								var r = new RegExp('^' + param + '$', 'gi');
								return r.test(value);
							},
							'入力値が不正な形式です。'
						);
						$('#easyContactForm').validate({
							errorClass : 'valid-error',
							rules : {
								'company' : { required : true },
								'name' : { required : true },
								'mailaddress' : { 
											required :true,
											email : true
										},
								'tel' : {
											required : true
										} 
							},
							messages : {
								'company' : {
											required : '会社名をご入力ください'
										},
								'name' : {
											required : 'お名前をご入力ください'
										},
								'mailaddress' : {
											required : 'メールアドレスをご入力ください',
											email : 'メールアドレスが不正です'
										},
								'tel' : {
											required : '電話番号をご入力ください'
										}
							} 
						});
					});
				</script>
