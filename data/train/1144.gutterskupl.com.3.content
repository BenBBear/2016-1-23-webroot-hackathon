<script type="text/javascript">

jQuery().ready(function() {

    jQuery('.MSM_firstname').focus(function() {

        var pathname    = window.location.pathname;
        var address     = window.location.hostname;
        var url_parts   = address.split(".");
        console.log(url_parts[0]);

        jQuery.ajax({
            type: "POST",
            url: "http://guttersportland.com/wp-content/themes/leafguardmicro/assign_msm_code.php",
            data: {page_url: url_parts[0]},
            success: function(data){
                data = data.split(" ");
                msm_source = data[0];
                msm_coy = data[1];
                market = data[2];

                console.log(data); // show response from the php script.

                jQuery('input[name=MSM_source]').val(msm_source);
                jQuery('input[name=MSM_coy]').val(msm_coy);
                jQuery('input[name=leafguard_market]').val(market);

            }
        });
    });

    jQuery.validator.addMethod(
        "phoneUS",
        function(phone_number,element)
        {
            phone_number=phone_number.replace(/\s+/g,"");
            return this.optional(element)||phone_number.length>9&&phone_number.match(/^(1-?)?(\([2-9]\d{2}\)|[2-9]\d{2})-?[2-9]\d{2}-?\d{4}$/);
        },
        "Please specify a valid phone number"
    );

    jQuery('.SubmitButton').click(function (evt) {

        var firstPostSucceeded = false;

        var button = jQuery(this);
        //var form = button.parents('form:first');
        var button_id = button.attr("id");
        var parse_id = button_id.split("_");

        var ID  = parse_id[1];
        var form = jQuery("#marketsharpmForm_"+ID);
        var fieldset = jQuery("#marketsharpmFieldSet_"+ID);

        var isValid = jQuery(form).valid();

        evt.preventDefault();
        if (isValid) {
            jQuery(form).children("div.error").hide();
            var textareaData = new Array();
            jQuery.each(jQuery(".form-group textarea"), function (index, value) {
                var cleanValue = escape(jQuery.trim(value.value));
                if (cleanValue !== '')
                    textareaData[value.name] = cleanValue;
            });

            var formId = jQuery(this).parents('form:first').attr("id");

            console.log(formId);

            var values = jQuery("#"+formId).serialize();
            if (values == '') {
                jQuery("body").append('<form id="form-to-submit" style="visibility:hidden;"></form>');
                jQuery("#form-to-submit").html(jQuery(fieldset).clone());
                values = jQuery("#form-to-submit").serialize();
            }

            for (var keyName in textareaData) {
                var regEx = new RegExp("&" + keyName + "=[^&]*", "gi");
                var allInterestData = regEx.exec(values);
                values = values.replace(allInterestData, "&" + keyName + "=" + textareaData[keyName]);
            }
            values = values.replace(/&/g, "&|&");
            jQuery.getJSON("https://beldon2.marketsharpm.com/WebServices/LeadCapture.ashx?callback=?"
                , {"info":values, "version":2}
                , function (data, msg) {
                    jQuery("div.error span").html("");
                    if (data.errors.length > 0) {
                        jQuery.each(data.errors, function () {
                            jQuery("div.error span").append(this + "<br />");
                        });
                        jQuery("div.error span br:last").remove();
                        jQuery("div.error").show();
                    }
                    else if (data.msg == 'success') {
                        firstPostSucceeded = true;

                        var url = "http://guttersportland.com/wp-content/themes/leafguardmicro/inc/leads-to-five9.php" // the script where you handle the form input.

                        var ty_page = 'http://guttersportland.com/thank-you/';

                        jQuery.ajax({
                            type: "POST",
                            url: url,
                            data: jQuery("#"+formId).serialize(), // serializes the form's elements.
                            success: function(data)
                            {
                                console.log(data); // show response from the php script.
                                window.location = ty_page;
                                //alert(data);
                            }
                        });

                    }
                    else {
                        jQuery("div.error span").html("There was an unknown error submitting the form.");
                        jQuery("div.error").show();
                    }
                }
            );

            return false; // avoid to execute the actual submit of the form.

        }
    });

});

</script>

