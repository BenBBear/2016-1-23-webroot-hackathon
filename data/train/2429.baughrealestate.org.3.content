<script type="text/javascript"> 
//Morgage Calculator ---
$('#getClosingCorpData').click(function() {
    // Get Zip and Sales Price Data ------------
     var zipCode = $('#zipCode').val();
     var salesPrice = $('#salesPrice').val();    
                            
    //Verify if Fiels have values
      if ( !zipCode.length || !salesPrice.length || !isNumber(zipCode) || !isNumber(salesPrice) )
      { alert("Missing or invalid data, please verify Fields"); 
      } else { 
    //Cleare Fields ----------------------------
      $("#MORTAPR").val();
      $("#MORTMONTHLY").val();        
      $("#MORTCLOSING").val();                
    // Request URL        
      var ccURL = "http://www.z57.com/mx/mcc/ext/closingcorp/mccInterface.cfc?method=closingCorpWidget&zipCode="+zipCode+"&salesPrice="+salesPrice;
    // Hide Submit btn and show loading animation
      $('#getClosingDataDIV').hide('fast', function() {
      $('#loadingdata').show("fast");
                                                       });
   // Ajax Call
      $.ajax({
            url: ccURL,
       dataType: 'jsonp',
          jsonp: "callback",
  jsonpCallback: "jsonpcallback",
        success: OnSuccess,
          error: OnError
              }); // End Ajax call
    } // END if ( !zipCode.length || !salesPrice.length )                                                                                                                                                                                                                                   
}); // End getClosingCorpData click
function isNumber(n) {
    return !isNaN(parseFloat(n)) && isFinite(n);
}

function addCommas(nStr)
{
    nStr += '';
    x = nStr.split('.');
    x1 = x[0];
    x2 = x.length > 1 ? '.' + x[1] : '';
    var rgx = /(\d+)(\d{3})/;
      while (rgx.test(x1)) {
        x1 = x1.replace(rgx, '$1' + ',' + '$2');
                           }
      return x1 + x2;
}
function onError(request, status, error) {
          alert(request.responseText);
              }
              
function OnSuccess(data, status)
    {
      $('#loadingdata').hide('fast', function() {
      $('#getClosingDataDIV').show("fast");
                                                });
      $("#MORTAPR").val(data.APR);
      $("#MORTMONTHLY").val("$"+addCommas(data.MONTHLYPAYMENTS));        
      $("#MORTCLOSING").val("$"+addCommas(data.TOTALCLOSINGCOSTS));                
     }

function OnError(request, status, error)
     {
       alert('Please verify Zip Code and Purchase Price Information');
           $('#loadingdata').hide('fast', function() {
                $('#loadingdata').hide();
                $('#getClosingDataDIV').show();
                                                      });
     }
</script>
