	<script>
	$(function(){
		var imgHoverTarget = $("a img[mode='hover']");
		$(imgHoverTarget).on("mouseover",function(){
			$(this).fadeTo(0,0.6);
		});
		$(imgHoverTarget).on("mouseout",function(){
			$(this).fadeTo(0,1);
		});
		
		var main_img = $("#main_img .main_img_item");
		var img_no = 0;
		var img_length = $(main_img).length-1;
		var fade_speed = 1000;
		$.each(main_img, function(i){
			$(this).css({position:"relative",top:"-"+i*400+"px",left:"0px","z-index":1000}).hide();
		});
		$(main_img[img_no]).show();
		setInterval(img_switch, 5000);
		function img_switch(){
			$(main_img[img_no]).fadeTo(fade_speed, 0).css({"z-index":1000});
			if(img_no < img_length){
				img_no++;
			} else {
				img_no = 0;
			}
			$(main_img[img_no]).fadeTo(0, 0).fadeTo(fade_speed, 1).css({"z-index":1001});
		}


	});
	</script>
