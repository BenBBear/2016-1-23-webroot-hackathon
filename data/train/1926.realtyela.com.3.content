					        var contentPositionTop = ((windowHeight - outerHeight) / 2) + heightOffset;
					        if (contentPositionLeft < 0) {
					            contentPositionLeft = 0;
					        }
					        if (contentPositionTop < 0) {
					            contentPositionTop = 0;
					        }
						$('<div></div>').attr('id', 'contact-form').css({width:width + 'px', height:height + 'px', left:contentPositionLeft + 'px', top:contentPositionTop + 'px'}).append(response).appendTo(body);
