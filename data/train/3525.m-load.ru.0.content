<script>

var cur_index = 1;
function fadein_img(index)
{
	var next_index = index+1;
	if (next_index==4) next_index = 1;

	$('#img'+index+'s').hide();
	$('#img'+index+'sg').show();

	$('#img'+next_index+'s').show();
	$('#img'+next_index+'sg').hide();
}
function swap_images()
{
	fadein_img(cur_index);
	cur_index++;
	if (cur_index == 4) cur_index = 1;
}
setInterval('swap_images()', 5000);


var cur_index2 = 1;
function fadein_img2(index)
{
	var next_index = index+1;
	if (next_index==7) next_index = 1;

	$('#img'+index).animate({opacity:'0'},500);
	$('#img'+next_index).animate({opacity:'1.0'},500);
}
function swap_images2()
{
	fadein_img2(cur_index2);
	cur_index2++;
	if (cur_index2 == 7) cur_index2 = 1;
}
setInterval('swap_images2()', 5000);

</script>


