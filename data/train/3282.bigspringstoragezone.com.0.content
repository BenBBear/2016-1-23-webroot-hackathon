<script type="text/javascript">
$(document).ready(function() {		
	slideShow179(12000);
});
function slideShow179(speed) {
	$('ul.slideshow179').append('<li id="slideshow179-caption" class="caption"><div class="slideshow179-caption-container"><h3></h3><p></p></div></li>');
	$('ul.slideshow179 li').css({opacity: 0.0});
	$('ul.slideshow179 li:first').css({opacity: 1.0}).addClass('show');
$('#slideshow179-caption').slideToggle(300, function () { //additional code
	$('#slideshow179-caption h3').html($('ul.slideshow179 li.show').find('img').attr('title'));
	$('#slideshow179-caption p').html($('ul.slideshow179 li.show').find('img').attr('alt'));
	$('#slideshow179-caption').css({opacity: 0.80, bottom:0}); //extra code bottom:0
$('#slideshow179-caption').delay(3000).slideToggle(500); //additional code
});		
	var timer = setInterval('gallery()',speed);
	$('ul.slideshow179').hover(
		function () {
			clearInterval(timer);	
		}, 	
		function () {
			timer = setInterval('gallery()',speed);			
		}
	);
}
function gallery() {
	var current = ($('ul.slideshow179 li.show')?  $('ul.slideshow179 li.show') : $('#ul.slideshow179 li:first'));
	if(current.queue('fx').length === 0) {	
	var next = ((current.next().length) ? ((current.next().attr('id') == 'slideshow179-caption')? $('ul.slideshow179 li:first') :current.next()) : $('ul.slideshow179 li:first'));
	var title = next.find('img').attr('title');	
	var desc = next.find('img').attr('alt');	
	next.css({opacity: 0.0}).addClass('show').animate({opacity: 1.0}, 2000);
		$('#slideshow179-caption').slideToggle(300, function () { 
		$('#slideshow179-caption h3').html(title); 
		$('#slideshow179-caption p').html(desc);
		$('#slideshow179-caption').delay(3000).slideToggle(500);
	});		
	current.animate({opacity: 0.0}, 2000).removeClass('show');
	}
}
</script>

