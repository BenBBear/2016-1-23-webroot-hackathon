function validate_domain(field)
{
    var email_field = field.domain.value;
    var email_field_com = email_field.indexOf(".");
    if ((email_field_com) == -1 ){
	var email_field_com_added = email_field += (".com");
	field.domain.value = email_field_com_added;
	field.submit;

    } else {
	return true;
    }
}



