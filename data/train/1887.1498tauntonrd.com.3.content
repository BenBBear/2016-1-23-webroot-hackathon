            $( ".follow-spw" ).on('click', function(){

                $("#form-follow-home").trigger("reset");
                if(is_mr_listing == false){
                    // alert('call');
                    $('#fth_usernameerror').html('');
                    $('#fth_usernameerror').hide();

                }
            });


            function followhomesubmit(){

                if(document.getElementById("myname").value==''){
                    alert("Please enter first name.");
                    return false;
                }

                var emailRegEx = /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$/i;

                if (document.getElementById("myemail").value.search(emailRegEx) == -1) {
                    alert("Please enter a valid email address.");
                    return false;
                }else{
                    if(phonereq=='Y'){
                        if(document.getElementById("myphone").value==''){
                            alert("Please enter phone number.");
                            return false;
                        }
                    }
                }

                var values = $('#form-follow-home').serialize();



                if(is_mr_listing === true){
                    var btnvalue = 'Follow All Homes!';
                }else{
                    var btnvalue = btnlbl;
                }

                $('#fth_usernameerror').html('');
                $('#fth_usernameerror').hide();

                $("#spwfthformsubmit").val('Processing...');

                $.ajax({
                    type: "POST",
                    url: ""+website_url+"listings/"+lid+"/"+urladdress+extraurl,
                    data: values+"&ajax=true&fthform=1",
                    //	cache: false,
                    success: function(output){
                        // alert(output);
                        var data = $.parseJSON(output);

                        $("#spwfthformsubmit").val(btnvalue);

                        if(data['res'] == 0){
                            if(data.message == 'username-error'){
                                $('#fth_usernameerror').show();
                                $('#fth_usernameerror').html(data.msg);
                            }
                        }else{

                            response = data.msg;
                            if(response == "success"){
                                $('#fth_usernameerror').html('');
                                $('#fth_usernameerror').hide();

                                $("#fhmodal").modal('hide');
                                $('#fhmodal2').modal('show');
                                $("#form-follow-home").trigger("reset");
                            }
                        }

                    }

                });

                return false;

            }

