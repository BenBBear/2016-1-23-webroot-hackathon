        case "orderPaymentOption": return { ecomm_prodid :  getIds(r.orderView.items), ecomm_pagetype : 'purchase', ecomm_totalvalue : r.orderView.total};
        case "orderThankYou": return { ecomm_prodid :  getIds(r.orderView.items), ecomm_pagetype : 'purchase', ecomm_totalvalue : r.orderView.total};
        case "home": return {  ecomm_prodid: '', ecomm_pagetype: 'home', ecomm_totalvalue: '' };
        default:   return {  ecomm_prodid: '', ecomm_pagetype: 'other', ecomm_totalvalue: '' };
      }
  }
  else { 
    if (window.location.pathname == '' || window.location.pathname == '/' || window.location.pathname.startsWith('/?') || window.location.pathname.startsWith('/home.do')) {
      return {  ecomm_prodid: '', ecomm_pagetype: 'home', ecomm_totalvalue: '' };
    }
    else {
      return {  ecomm_prodid: '', ecomm_pagetype: 'other', ecomm_totalvalue: '' };
    }
  };  
  function getIds(list) { 
    ids = [];
    for(i = 0; i < list.length; i++) { ids.push( 'PRODUCT-' + list[i].productId); }
    return ids;
  }
})(BBApps.reporting);
    
