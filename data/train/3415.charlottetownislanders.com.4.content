<script>
    var games = false;

    function updateLiveScores () {
        clearTimeout(scheduleTimer);
        $.ajax({
            url: '/liveScoreUpdate/4',
            success: function (data) {
                setGames(data);
            },
            dataType: 'json'
        });
        scheduleTimer = setTimeout( updateLiveScores, 5000);
    }

    function setGames (data) {
        games = data;
        for(var i = 0; i < games.length; i++) {

            game_div = jQuery( '#sked_game_' + games[i].game_id );
            if(game_div.length) updateGame ( game_div, games[i] );

        }
    }

    function updateGame (game_div, game_data) {

        var state_change = false;

        // decide home and away team
        teams = sortHomeAway( game_data.team_games )
        // if game is live and was not live before, set live and flag state_change
        if ( game_data.state == "in-progress" ) {
            if (!jQuery( game_div ).hasClass( "In-Progress" ) ){
                setGameLive( game_div, game_data, teams );
                state_change = true;
                } else {
                //update period
                var mins = Math.floor(game_data.game_clock / 60);
                var secs = ("00" + (game_data.game_clock - (mins * 60))).slice(-2);

                jQuery('.period', game_div).html(game_data.current_period + " - " + mins + ":" + secs);

                // update scores
                state_change = updateScores( game_div , teams );
            }
        }

        // if game is final and was not final before, set final and flag state_change
        if ( game_data.state == "final" ) {
            if ( !jQuery( game_div ).hasClass( "Final") ) {
                setGameFinal( game_div, game_data, teams );
                state_change = true;
                } else {
                // just in case they are corrected by the league
                state_change = updateScores( game_div, teams );
            }
        }

        if (state_change)
        jQuery(game_div).animate( { opacity: 0.4 }, 200 ).animate( { opacity: 1 }, 400 );
    }

    /*
    * Home and away is stored inside the teams so we have to parse it to figure out who's home and away.
    */
    function sortHomeAway ( teams ) {
        home_away = { 'home': teams[1], 'away': teams[0] };
        if ( teams[0].home_team ) {
            home_away = { 'home': teams[0], 'away': teams[1] };
        }
        return home_away;
    }

    function setGameLive( game_div, game_data, teams ) {
        jQuery( '.score_tickets', game_div ).html('<span class="away_score">' + teams['away'].score + '</span> - <span class="home_score">' + teams['home'].score + '</span>'
                + '<span class="period">' + game_data.period + '</span>'
                );
        jQuery( game_div ).removeClass("Pre-Game Final").addClass("In-Progress");
        console.log(4);
    }

    function setGameFinal( game_div, game_data, teams ) {
        jQuery( '.score_tickets', game_div ).html('<span class="away_score">' + teams['away'].score + '</span> - <span class="home_score">' + teams['home'].score + '</span>'
                + '<span class="period">Final</span>'
                );
        jQuery( game_div ).removeClass("Pre-Game In-Progress").addClass("Final");
    }

    function updateScores( game_div, teams ) {
        var update = false;
        // update home score
        if ( parseInt( jQuery('.home_score', game_div).text(), 10 ) != teams['home'].score ){
            jQuery('.home_score', game_div).html(teams['home'].score);
            update = true;
        }
        // update away score
        if ( parseInt ( jQuery('.away_score', game_div).text(), 10 ) != teams['away'].score ){
            jQuery('.away_score', game_div).html(teams['away'].score);
            update = true;
        }
        return update;
    }

    var scheduleTimer;

    $(function () {
        scheduleTimer = setTimeout( updateLiveScores, 25000);
    });

</script>


<script type='text/javascript'>
    window.onload=function(){
        var cd_hrs = 0; var cd_min = 0; var cd_sec = 0;
        var cd_timer = null;
        jQuery(document).ready( function() {
                if(jQuery('#puckdrop')){
                //initialize values, grab them from PHP output
                cd_hrs = jQuery('#cd_hrs').html();
                cd_min = jQuery('#cd_min').html();
                cd_sec = jQuery('#cd_sec').html();
                cd_timer = setInterval(countdown, 1000);
                }
                });

        function countdown(){
            if(cd_sec-- == 0){
                cd_sec = 59;
                if(cd_min-- == 0){
                    cd_min = 59;
                    if(cd_hrs-- == 0){
                        clearInterval(cd_timer);
                        closeCountdown();
                        return false;
                    } jQuery('#cd_hrs').html( padNum(cd_hrs) );
                } jQuery('#cd_min').html( padNum(cd_min) );
            } jQuery('#cd_sec').html( padNum(cd_sec) );
            return false;
        }

        //leading zero if number is 9 or lower
        function padNum(num){ return (num > 9) ? num : 0 + num.toString(); }
        function closeCountdown(){
            //Hide the text, make div grow, and insert text, on click close notice,
            //or auto close after 60 seconds
            jQuery('#puckdrop').fadeOut(300);
        }
    };
</script>
