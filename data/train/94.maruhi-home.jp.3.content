$(function(){

	var execBeforeUnload = false; // 確認ダイアログを出すかどうかのフラグ

	// 変更されているときだけ確認ダイアログを出す
	$('#form1 *').change(function(){
		if($(this).val().length) execBeforeUnload = true;
	});

	function cancelBeforeUnload() {
	    execBeforeUnload = false;
	    setTimeout(function() {
	        execBeforeUnload = true;
	    }, 1000);
	}

	// IE だと href="javascript:void(0)" といったリンクを
	// クリックしても beforeunload が発生するのでキャンセルする
	$("a").bind('click', function(e) {
	    if (this.href.indexOf('javascript') == 0) {
	        cancelBeforeUnload();
	    }
	});

	// form の submit 時に beforeunload が発生するのでキャンセルする
	$('form').bind('submit', function(e) {
	    cancelBeforeUnload();
	});

	$(window).bind('beforeunload', function(e) {
	    if (execBeforeUnload) {
			send_data();
	        return "入力内容が破棄されます！移動してよろしいですか？";
	    }
	});

	function send_data(){
		$.ajax({
