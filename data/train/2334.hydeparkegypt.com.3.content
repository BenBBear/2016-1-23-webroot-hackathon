	function VHandleRunCommand()
	{
		if (http.readyState == 1)
		{
		document.getElementById('VDivCommand').innerHTML = '<img src=\'media/image/ajax/1.gif\' border=\'0\'>';
		}

		if (http.readyState == 4)
		{
		var response = http.responseText;
		
			if (response.match(/http/g))
			{
			window.location=response;
			}
			else
			{
			document.getElementById('VDivCommand').innerHTML = response;
			}
		}
	}

