<script type="text/javascript">
$(function(){

    //enterキーでsubmitする
    $("#sideloginForm").keypress(function(ev) {
        if ((ev.which && ev.which === 13) || (ev.keyCode && ev.keyCode === 13)) {
            submit_sidelogin();
        }
    });

    $("p.sidelogin_btn").click(function(){
        submit_sidelogin();
    });

    function submit_sidelogin(){
        $("div.login_inner").hide();
        $("p.login_loading").show();

        var formValue = $("#sideloginForm").serialize();

        $.ajax({
            type: "POST",
            url: "/member/login/sideloginexecute/",
            data: formValue,
            handleAs: "json",
            success: function(re){
                $("p.login_loading").hide();
                $("div.login_inner").show();
                var obj = jQuery.parseJSON(re);

                if(obj == null){
                    $("p.side_email").empty();
                    $("p.side_email").append('メールアドレス');
                    $("p.side_password").empty();
                    $("p.side_password").append('パスワード');
                    location.href = '/mypage';
                    $("p.sidelogin_btn").unbind();
                }

                if(obj.side_email){
                    $("p.side_email").empty();
                    $("p.side_email").append('メールアドレス');
                    $("p.side_email").append(obj.side_email);
                }else{
                    $("p.side_email").empty();
                    $("p.side_email").append('メールアドレス');
                }
                if(obj.side_password){
                    $("p.side_password").empty();
                    $("p.side_password").append('パスワード');
                    $("p.side_password").append(obj.side_password);
                }else{
                    $("p.side_password").empty();
                    $("p.side_password").append('パスワード');
                }
                if(obj.error){
                    $("font.error").remove();
                    $("div.loginform").prepend(obj.error);
                }
            },
            error: function(){
                alert('エラーです');
            }
        });
    }
});

</script>
