<script type="text/javascript">
$(function(){
    //パスワードの文末の文字表示
    $("input:password").dPassword();

    //enterキーでsubmitする
    $("#loginForm").keypress(function(ev) {
        if ((ev.which && ev.which === 13) || (ev.keyCode && ev.keyCode === 13)) {
            submit_login();
        }
    });

    $("p.login_btn").click(function(sumbmit){
        submit_login();
    });

    function submit_login(){
        $("div.inner").hide();
        $("p.loading").show();

        var formValue = $("#loginForm").serialize();
        $.ajax({
            type: "POST",
            url: "/member/login/toploginexecute/",
            data: formValue,
            handleAs: "json",
            success: function(re){
                $("p.loading").hide();
                $("div.inner").show();

                var obj = jQuery.parseJSON(re);

                if(obj == null){
                    $("p.email").empty();
                    $("p.email").append('メールアドレス');
                    $("p.pass").empty();
                    $("p.pass").append('パスワード');
                    location.href = '/mypage';
                    $("p.login_btn").unbind();
                }

                if(obj.email){
                    $("p.email").empty();
                    $("p.email").append('メールアドレス');
                    $("p.email").append(obj.email);
                }else{
                    $("p.email").empty();
                    $("p.email").append('メールアドレス');
                }
                if(obj.password){
                    $("p.pass").empty();
                    $("p.pass").append('パスワード');
                    $("p.pass").append(obj.password);
                }else{
                    $("p.pass").empty();
                    $("p.pass").append('パスワード');
                }
                if(obj.error){
                    $("font.error").remove();
                    $("div.form").prepend(obj.error);
                }
            },
            error: function(){
                alert('エラーです');
            }
        });
    }
});

