jQuery(function(){
	(function(){
		var $frame01 = jQuery('#main-visual-item-01'),
			$frame02 = jQuery('#main-visual-item-02'),
			$frame03 = jQuery('#main-visual-item-03'),
			$frame04 = jQuery('#main-visual-item-04');

		$frame01.hide();
		$frame02.hide();
		$frame03.hide();
		$frame04.hide();

		function frame01(){
			return $frame01.delay(200).fadeIn(500);
		}
		function frame02(){
			return $frame02.fadeIn(500);
		}
		function frame03(){
			return $frame03.delay(100).fadeIn(500).addClass('animated bounceInRight');
		}
		function frame04(){
			return $frame04.delay(100).fadeIn(500).addClass('animated flipInX');
		}

		var $deferred = jQuery.Deferred(function(deferred){
			deferred.then(frame01)
					.then(frame02)
					.then(frame03)
					.then(frame04);
		});

		$deferred.resolve();
	})()

	jQuery('#main-visual-item-04').hover(function(){
		jQuery(this).addClass('animated hinge');
	})


	jQuery('#nav-toggle-icon').click(function(){
		jQuery('#nav-toggle-body').slideToggle('fast');
	});
	jQuery('#mobile-nav-close').click(function(){
		jQuery('#nav-toggle-body').slideToggle('fast');
	});
	jQuery('#to-top-mobile').click(function(){
		jQuery('body, html').animate({scrollTop:0}, 'fast');
	});

	jQuery('#to-top').hide();	

	if(!navigator.userAgent.match(/(iPhone|iPad|Android)/)){
		jQuery(window).scroll(function () {
			if (jQuery(this).scrollTop() > 100) {
				jQuery('#to-top').fadeIn();
			} else {
				jQuery('#to-top').fadeOut();
			}
		});	
		jQuery('#top-link').click(function(){
			jQuery('#top-link').hide();
			jQuery('body, html').animate({scrollTop:0}, 3000);
			jQuery('#to-top')
				.stop()
				.animate({'bottom': 0},1500)
				.animate({'bottom': -100},1500)
				.animate({'bottom': 3000},1000,function(){
					jQuery(this)
						.hide()
						.animate({'bottom': -680},0);
				jQuery('#top-link').show();
			});
		});
	}
});
</script>


