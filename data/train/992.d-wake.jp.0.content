<script>
debug.setLevel(0);

(function(){
  if( $('img.js-lazy').length <= 0 ) return;
  $('img.js-lazy').lazyLoadXT({
    srcAttr: 'data-original'
  });
})();

var RELEASE_URL = 'https://daihatsu-port.jp/car/wake/',
    SHARE_TEXT  = 'ドデカクつかおう。WAKE【ダイハツ】';

function socialLink(targetURL, share_text){
  var $this = $(this),
      opts = 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,' +
        'height=500,width=600',
      twitter_url = 'https://twitter.com/share?',
      fb_url      = 'https://www.facebook.com/sharer/sharer.php?u=',
      url = '';
  switch(this.hash) {
    case '#twitter':
      url += twitter_url + '&text=';
      url += encodeURIComponent( $.trim( share_text ) );
      url += '&url=' + _.escape( targetURL );
      break;
    case '#facebook':
      url += fb_url + _.escape( targetURL );
      break;
  }
  $(this)
    .on('click', function(e){
      e.preventDefault();
      window.open(url,'SocialWind', opts);
    });
}

(function(){
  if( $('.js-sns').length <= 0 ) return;
  $('.js-sns')
    .each(function(i, el){
      socialLink.call( el, RELEASE_URL, SHARE_TEXT );
    });
})();

(function(){
  if( $('.js-sns-page').length <= 0 ) return;
  var myTitle = _.escape( $.trim( $('title').text().split('|')[0] ) );
  $('.js-sns-page')
    .each(function(i, el){
      socialLink.call( el, document.URL, myTitle + ' | ' + SHARE_TEXT );
    });
})();
</script>

