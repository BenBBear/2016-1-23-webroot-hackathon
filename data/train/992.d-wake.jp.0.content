<script>
function SpGmenu(){

  // 開閉フラグ
  this.is_show = false;

  this.$ghead = $('#js_ghead');
  this.$gbtn  = this.$ghead.find('.js-gbtn');
  this.$open  = this.$ghead.find('.gnav__btn__icon--open');
  this.$close = this.$ghead.find('.gnav__btn__icon--close');
  this.$gmenu = $('#js_gmenu');
  this.flgCls = 'gnav__btn__link--close';
  this.delay  = 300;

  var that = this;

  this.$gbtn.on('click', function(e){
    e.preventDefault();
    if (that.is_show) {
      that.close();
    }
    else {
      that.show();
    }
  });

}

SpGmenu.prototype = {

  set__show: function(bool){
    this.is_show = bool;
  },

  show: function(){
    var that = this,
        $menu  = that.$gmenu,
        $open  = that.$open,
        $gbtn  = that.$gbtn,
        $close = that.$close;
    $open.hide();
    $close.show();
    $gbtn.addClass(that.flgCls);
    $menu
      .velocity('stop')
      .velocity('transition.slideDownBigIn', that.delay);
    that.set__show(true);
  },

  close: function(is_reset){
    var that = this,
        $menu  = that.$gmenu,
        $open  = that.$open,
        $gbtn  = that.$gbtn,
        $close = that.$close;
    is_reset = is_reset ? is_reset : false;
    $open.show();
    // $menu.hide();
    $menu
      .velocity('stop')
      .velocity('transition.slideUpBigOut', that.delay);
    $close.hide();
    $gbtn.removeClass(that.flgCls);
    that.set__show(false);
  }

};

var spGmenu = new SpGmenu();
</script>

