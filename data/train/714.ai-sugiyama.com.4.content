            	$('#back-top').fadeIn();
        	} else {
            	$('#back-top').fadeOut();
        	}
    	});
    	$('#back-top a').click(function () {
        	$('body,html').animate({
            	scrollTop: 0
        	}, 500);
        	return false;
    	});
	});
});
</script>
