    <script type="text/javascript">
        
        $(document).ready(function(){
            var $container = $('#thumbs-container');
            var filterType = '*';
            $container.isotope({
                filter: filterType,
                animationOptions: {
                    duration: 750,
                    easing: 'linear',
                    queue: false,
                }
            });
            
            $container.attr('data-current', filterType);
            
            checkActive();
            
            function checkActive() {
                $('#nav1 a').each(function (){
                    
                    $(this).css({
                        color: '#595959'
                    });
                    
                    var filter = $(this).attr('project-filter');
                    
                    filter = '.' + filter;
                    if (filter == '.*') {
                        filter = '*';
                    }
                    
                    var currentFilter = $container.attr('data-current');
                    
                    if (filter == currentFilter) {
                        $(this).css({
                            color: '#333333'
                        });
                    }
                });
            }
                
            $('#nav1 a').click(function(){
                var selector1 = $(this).attr('project-filter');
                var text = $(this).text();
                if (text == "All") {
                    var selector = selector1;
                }
                else {
                    var selector = "." + selector1;
                }
                
                $container.attr('data-current', selector);
                    
                $container.isotope({ 
                    filter: selector,
                    animationOptions: {
                        duration: 750,
                        easing: 'linear',
                        queue: false, 
                    }
                });  
                checkActive();
                return false;
            });
        });
    </script> 
    
