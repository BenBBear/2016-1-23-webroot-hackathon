<script type="text/javascript">
    function clearfield(field, defaultvalue) { if (field.value == defaultvalue) { field.value = ""; } }
    function checkfield(field, defaultvalue) { if (field.value == "" || field.value == defaultvalue) { field.value = defaultvalue; } }
    function validateForm() {
        var txtZip = document.getElementById("Zip");
        var txtCity = document.getElementById("City");
        var ddlState = document.getElementById("ddlState");
        if (document.getElementById('scombo').style.display == "none") {
            if (txtZip.value.length == 0 || txtZip.value == "Zip Code") { alert("Please enter your zip code."); txtZip.value = "Zip Code"; txtCity.value = "City"; txtZip.focus(); return false; }
            if (txtZip.value.length != 0 && txtZip.value != "Zip Code") { reZip = new RegExp(/(^\d{5}$)|(^\d{5}-\d{4}$)/); if (!reZip.test(txtZip.value)) { txtZip.value = "Zip Code"; alert("Please enter a valid zip code."); txtZip.value = "Zip Code"; txtZip.focus(); return false; } }
        }
        else {
            if ((txtZip.value.length == 0 || txtZip.value == "Zip Code") && (txtCity.value.length == 0 || txtCity.value == "City")) { alert("Please enter your zip code OR city and state."); txtZip.value = ""; txtCity.value = "City"; txtZip.focus(); return false; }
            if (txtZip.value.length != 0 && txtZip.value != "Zip Code") { reZip = new RegExp(/(^\d{5}$)|(^\d{5}-\d{4}$)/); if (!reZip.test(txtZip.value)) { txtZip.value = "Zip Code"; alert("Please enter a valid zip code."); txtZip.value = "Zip Code"; txtZip.focus(); return false; } }
            if (txtCity.value.length != 0 && txtCity.value != "City") { if (ddlState.value == "State") { alert("Please select your state."); return false; } }
        }
