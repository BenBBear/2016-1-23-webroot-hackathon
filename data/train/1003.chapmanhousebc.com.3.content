        function initialize_marketing() {
            var address = document.getElementById('address1').value;
            var city = document.getElementById('City').value;
            var zip = document.getElementById('Zip').value;
            var PropDetail = document.getElementById('PropertyDetail_mfm').value;
            var thisLocation = address + ',' + city + ',' + zip;
            var myOptions = {
                    zoom: 15,
                    scrollwheel: false,
                    mapTypeId: google.maps.MapTypeId.ROADMAP,
                    disableDefaultUI: true
                }
            map = new google.maps.Map(document.getElementById("marketing-map"), myOptions);

            var resultsCanvas = document.getElementById('resultsCanvas');
            directionsDisplay = new google.maps.DirectionsRenderer();
            directionsDisplay.setMap(map);
            directionsDisplay.setPanel(document.getElementById('directions'));

            var latitude = "42.3792";
            var longitude = "-71.0370";

            if (latitude.length > 0 && longitude.length > 0) {
                latitude = 42.3792;
                longitude = -71.0370;

                var ll = new google.maps.LatLng(latitude, longitude);
                map.setCenter(ll);
                var marker = new google.maps.Marker({
                    position: ll,
                    map: map
                });
                
                google.maps.event.addListener(marker, "click", function () {
                    infowindow.close();
                    infowindow.setPosition(ll);
                    infowindow.maxWidth = 400;
                    infowindow.setContent(PropDetail);
                    infowindow.open(map, marker);

                    infoWindowOpen = true;
                    google.maps.event.addListener(infowindow, "closeclick", function () {
                        infoWindowOpen = false;
                    });
                });
            } else {
                geocoder = new google.maps.Geocoder();
                geocoder.geocode({ 'address': thisLocation }, function (results, status) {
                    if (status == google.maps.GeocoderStatus.OK) {
                        map.setCenter(results[0].geometry.location);
                        var marker = new google.maps.Marker({
                            map: map,
                            position: results[0].geometry.location
                        });
                        infowindow.setContent(PropDetail);
                        infowindow.open(map, marker);
                    } else {
                        alert(thisLocation + " not found");
                    }
                    google.maps.event.addListener(marker, "click", function () {
                        infowindow.close();
                        infowindow.setPosition(results[0].geometry.location);
                        infowindow.maxWidth = 400;
                        infowindow.setContent(PropDetail);
                        infowindow.open(map, marker);
                        infoWindowOpen = true;
                        google.maps.event.addListener(infowindow, "closeclick", function () {
                            infoWindowOpen = false;
                        });
                    });
                });
            }
            //if mapsanddirections is hidden, use this to recenter map on being shown.
            $("#mapTabLink").on("shown", function () {
                var currCenter = map.getCenter();
                google.maps.event.trigger(map, 'resize');
                map.setCenter(currCenter);
                infowindow.setContent(PropDetail);
                infowindow.open(map, marker);
            });

        }
        
        initialize_marketing();
    });
</script>


