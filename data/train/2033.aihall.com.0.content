	var curImg;
	var maxCnt = 5;	//表示数
	//init
	curImg = Math.floor( Math.random() * 5 ) + 1;
	$(".global_navi .bg_img img:nth-child(" + curImg + ")").show();

	//繰り返し処理
	setInterval(function(){ 
/* 完全なランダム処理の場合はこれを使う
		while (true){
			var tpRansu = Math.floor( Math.random() * 5 ) + 1;
			if(tpRansu != curImg){
				curImg = tpRansu
				break;
			}
		}
*/
		var newImg;
		if(curImg == maxCnt){
			newImg = 1;
		}else{
			newImg = curImg+1;
		}
		$(".global_navi .bg_img img:nth-child(" + curImg + ")").fadeOut(1000);
		$(".global_navi .bg_img img:nth-child(" + newImg + ")").fadeIn(1000);
		curImg = newImg;
	}, 5000);

</script>
