                //$(".newsletter #popup").hoverIntent(signupFormConfig);
            }

            function signupHoverOver() {
                var popup = $('.newsletter #popup');
                popup.fadeIn(1500);
            }

            function signupHoverOut() {
                doClosePopup();
            }

            function doDisplayPopup() {
                var isSent = $('#SentSignUp').val();
                Cufon.replace('.newsletter #popup .close', { fontFamily: 'Futura Md BT' });

                if (isSent == 'true') {
                    var popup = $('.newsletter #popup');
                    popup.fadeIn(1500);
                    window.setTimeout(function () { popup.css({ display: 'block' }) }, 1500);
                    return false;
                }
                else { return false; }
            }

            function doClosePopup() {
                var popup = $('.newsletter #popup')
                if (AjaxControlToolkit.ValidatorCalloutBehavior._currentCallout != null) {
                    AjaxControlToolkit.ValidatorCalloutBehavior._currentCallout.hide();
                }
                $('#SentSignUp').val('false');
                popup.fadeOut(1500);
                window.setTimeout(function () { popup.css({ display: 'none' }) }, 1500);
            }


            $(document).ready(function () {
               doNewsLetterSetup();
                
            });

