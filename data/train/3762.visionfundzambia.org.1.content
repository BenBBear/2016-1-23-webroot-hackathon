          <script type="text/javascript">
              // MFI Twitter params
              var mfi_timeout = 5000;
              var mfi_duration = 800;
              var mfi_fadeout = 1500;
              var mfi_account_name = 'VisionFund';
              var mfi_counter = 0;
                    
              $(document).ready(function () {
                        
                  nextMFITweet1();
                  doSelect(0);
              });

              function nextMFITweet1() {
                      
                  var msg = $('#upMFITweets p.msg');
                  //var p = $('#twitter #tweetMsg p.posted');
                  msg.css("text-indent", "0px");
                  if (typeof (arrMFITweet) !== 'undefined') {
                      if (arrMFITweet != null) {
                          var numtweets = arrMFITweet.length;
                          if (counter == numtweets) { mfi_counter = 0; }
                          var tweet = arrMFITweet[mfi_counter];
                          var id = arrMFIStatusId[mfi_counter];
                          var name = arrMFIUserScreenName[mfi_counter];
                          var posted = arrMFIUserCreated[mfi_counter];

                          msg.html(tweet).text();
                          //p.html('last updated ' + posted).text();
            
                          mfi_counter += 1;
                          showMFITweet1();
                      }
                  }
                  else {
                      var msg = $('#upMFITweets p.msg');
                      var msg_wait = $('#upMFITweets p.msg-wait')
                      //var p = $('#twitter #tweetMsg p.posted');
                      msg_wait.css("display", "none");
                      $("#upMFITweets").css({ 'display': 'block' });
                      msg.text('Twitter not available or maximum hit limit reached. I will try again in a while...');
                      //p.text('');
                  }
              }

              function showMFITweet1() {
                  var msg = $('#upMFITweets .container');
                  var msg_txt = $('#upMFITweets .container p.msg');
                  var msg_wait = $('#upMFITweets p.msg-wait');
                  var animSpeed = msg.width() - msg_txt.width();

                  //alert(msg.width() + '  -  ' + msg_txt.width());
                  if (animSpeed > 0) {animSpeed = 0;}

                  msg_wait.css("display", "none");
                  msg_txt.fadeIn(mfi_fadeout);

                  setTimeout(function () {
                      msg_txt.animate({ textIndent: animSpeed }, { duration: 6000, specialEasing: { textIndent: 'swing' },
                          complete: function () {
                              setTimeout(function () { nextMFITweet1(); }, mfi_timeout);
                              setTimeout(function () { msg_txt.fadeOut(mfi_fadeout); }, 2500);
                          }
                      });
