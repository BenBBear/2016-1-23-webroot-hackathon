var loggedin=0;

function receiveMessage(e){
	//var loginHtml='<a href="#" id="openLoginForm"><img src="http://www.bali-vacation-holiday.com/hi-menu-wp/wp-content/themes/hirochan-top-new/img/signup.png" alt="新規登録 ログイン" /></a>';
	//var loginFormHtml='<img src="http://www.bali-vacation-holiday.com/hi-menu-wp/wp-content/themes/hirochan-top-new/img/m_login.png" alt="ログイン" /><br><iframe width="100%" height="260" src="https://hirochan-group.com/member/auth/loginframe?microtime" scrolling="no" frameborder="0"></iframe><img src="http://www.bali-vacation-holiday.com/hi-menu-wp/wp-content/themes/hirochan-top-new/img/m_signup.png" alt="会員登録（無料）" /><br><a href="https://hirochan-group.com/member/auth/register" target="_blank" id="btn_regist"><img src="http://www.bali-vacation-holiday.com/hi-menu-wp/wp-content/themes/hirochan-top-new/img/button-register.png" alt="新規会員登録はこちら！" /></a>	';
	//var loggedinHtml='<div id="nameWrap">ようこそ <span></span></div><ul id="mymenu"><li><a href="https://hirochan-group.com/member/" id="icon_mypage" target="member" title="マイページ">マイページ</a></li><li><a href="http://hirochan-group.com/member/favorite" id="icon_favorite" target="member" title="お気に入りリスト">お気に入り</a></li><li><a href="http://www.hirochan-group.com/regular/contact.html" id="icon_otoiawase" target="member" title="お問い合わせ">お問い合わせ</a></li><li><a href="https://hirochan-group.com/member/auth/logout" id="icon_logout" title="ログアウト">ログアウト</a></li></ul>';

	if (e.origin === "http://hirochan-group.com" || e.origin === "http://www.hirochan-group.com" || e.origin === "http://file.hirochan-group.com"
		|| e.origin === "https://hirochan-group.com" || e.origin === "https://www.hirochan-group.com" || e.origin === "https://file.hirochan-group.com"){
		jQuery('.signup').css('background','none');
		jQuery('.signup').css('height','auto');

		if(typeof e.data === "object" ){
			var data=e.data;
			if(typeof data.loggedin !== 'undefined'){
				jQuery('.signup').css('background','none');
				if(data.loggedin==0){
					jQuery('.signup #login').show();
					jQuery('.signup #loggedin').hide();
					//jQuery('.signup').html(loginHtml);
					//jQuery('#formWrap').html(loginFormHtml);
				}else{
					loggedin=1;
					if($('#loginForm').hasClass('open')){$('#loginForm').removeClass('open');}

					jQuery('.signup #login').hide();
					jQuery('.signup #loggedin').show();
					//jQuery('.signup').html(loggedinHtml);
					jQuery('.signup #nameWrap span').text(data.name+' 様');
				}
			}
			if(typeof data.added !== 'undefined'){
				if($('#addFav'+data.id).size()>0){
					if(data.added==1){
						$('.loginBox2 .loginBoxBody').html('お気に入りに登録しました。');
					}else{
						$('.loginBox2 .loginBoxBody').html('<span style="color:red;">すでに登録されています。</span>');
					}
					$(".loginBox2").insertAfter('#addFav'+data.id);
					if($("#contents").size()>0){
						$('.loginBox2').css('top',30);
						$('.loginBox2').css('left','auto');
						$('.loginBox2').css('right',30);
					}
					$('.loginBox2').show();
					setTimeout(function(){
						$('.loginBox2').hide();
					},2000);
				}
			}
		}else{
			var val=e.data+'';
			if(!val.match(/[^0-9]+/)){
				//jQuery('#frame-one,#frameOne').css({'cssText': 'height: '+e.data+'px !important;'});
				//旧フレームID
				if(jQuery("#frame-one").size() > 0){
					jQuery('#frame-one').css({'cssText': 'height: '+val+'px !important;'});
				}

				//カーチャーターや地図で使用
				if(jQuery("#iframeTop").size() > 0){
					jQuery('#iframeTop').height(val);
				}
				//通常読み込み元ページ
				if(jQuery("#frameOne").size() > 0){
					jQuery('#frameOne').height(val);
				}
				//スペシャルページ（1カラム用）
				if(jQuery("#frameOneColumn").size() > 0){
					jQuery('#frameOneColumn').height(val);
				}

				//タブの切り替え
				vars=getUrlVars();
				if(vars['mode']=='review' && jQuery('ul.nav-tabs li.tab').size()>0){
					jQuery('ul.nav-tabs li.tab').eq(1).click();
				}

			//擬似アンカー処理
			}else if(result=val.match(/^(\d+)\|(\d+)$/)){
				var posFrame=0;
				//旧フレームID
				if(jQuery("#frame-one").size() > 0){
					jQuery('#frame-one').css({'cssText': 'height: '+result[2]+'px !important;'});
					posFrame=jQuery('#frame-one').offset().top;
				}
				//通常読み込み元ページ
				if(jQuery("#frameOne").size() > 0){
					jQuery('#frameOne').height(result[2]);
					posFrame=jQuery('#frameOne').offset().top;
				}

				//フレームの位置＋iframe内のアンカーの位置にスクロール
				window.scrollTo(0,parseInt(posFrame)+parseInt(result[1]));

			//prettyPhotoのID
			}else if(result=val.match(/^photo(\d+)$/)){
				if(jQuery('#'+val).size() > 0){
					jQuery('#'+val).click();
				}
			//上記以外のフレームIDを指定しているとき用
			}else if(result=val.match(/^(\w+)\|(\d+)$/)){
				jQuery('#'+result[1]).height(result[2]);
			}
		}
	}
}

