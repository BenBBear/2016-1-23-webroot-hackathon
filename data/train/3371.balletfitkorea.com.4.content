<script language="JavaScript">
function setCookie( name, value, expiredays ) { 
 var todayDate = new Date(); 
  todayDate.setDate( todayDate.getDate() + expiredays ); 
  document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";" 
 }

function closeWin() { 
 if ( document.getElementById("notice_popup").checked){ 
  setCookie( "pop", "done" , 1 ); 
 } 
document.all['pop'].style.visibility = "hidden";
}
</script>

<script language="Javascript">
cookiedata = document.cookie;    
if ( cookiedata.indexOf("pop=done") < 0 ){      
 document.all['pop'].style.visibility = "visible";
 } 
 else {
  document.all['pop'].style.visibility = "hidden";   
}
</script>



<script language="javascript">

	function getInternetExplorerVersion() {    
		var rv = 10; 
		if (navigator.appName == 'Microsoft Internet Explorer') {
			var ua = navigator.userAgent;
			var re = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
			if (re.exec(ua) != null) rv = parseFloat(RegExp.$1);
		}
		return rv;
	}
	if (getInternetExplorerVersion() < 10) {
	
		location.replace('fail.php');
	}
	
	window.onload = fnResize;
	window.onresize = fnResize;

	function fnResize() {

		var width1 = Math.round(document.body.clientWidth / 2.0);
		var width2 = Math.round(document.body.clientWidth / 4.0);

		if (document.body.clientWidth < 1080) {
			width1 = document.body.clientWidth;
			width2 = Math.round(document.body.clientWidth / 2.0);
		}
		if (width1 > width2 * 2) width1 -= 1;
		if (width1 < width2 * 2) width1 += 1;
		
		document.getElementById("post-1").style.height = width1 + 'px';
		document.getElementById("post-2").style.height = width2 + 'px';
		document.getElementById("post-3").style.height = width2 + 'px';
		document.getElementById("post-4").style.height = width2 + 'px';
		document.getElementById("post-5").style.height = width2 + 'px';
	}
	
$('#pop').show('fast');
	function fnClose() {
	
		$('#pop').hide();
	}
	
</script>
