		<script type="text/javascript">
				function confirmRegion(button_guid) {
					if( ! warnCookiesNotEnabled() ) {
						return(false);
					}
								
					if( region_prompt_response_done ) {
						//alert("region prompt done");
						return(true);
					} else {
						showRegionPromptOverlay();
						button_guid_to_reclick_after_selection = button_guid;
						button_href_to_goto_after_selection = $j("#" + button_guid).attr("href");
					}
										
					return(false);
				}
		
				function showRegionPromptOverlay() {
					//$j('#page_grey_overlay').show();
					$j('#region_prompt_background_overlay').show();
					$j('#region_prompt_overlay').show();
					//$j('#region_prompt_remember_overlay').show();
				}
		
				function hideRegionPromptOverlay() {
					//$j('#page_grey_overlay').hide();
					$j('#region_prompt_overlay').hide();
					$j('#region_prompt_background_overlay').hide();
					//$j('#region_prompt_remember_overlay').hide();
				}
		
				function regionSetComplete() {
					if( button_href_to_goto_after_selection != '' ) {
						window.location.href = button_href_to_goto_after_selection;
						button_href_to_goto_after_selection = '';
					}
				}
		
				function setRegionAjax(region_name, remember) {
					jQuery.ajax({type:'POST',dataType:'html',data:'region=' + region_name + '&remember=' + remember,complete:function(XMLHttpRequest, textStatus){regionSetComplete();},url:'/en/info/setRegionAjax'});	
					hideRegionPromptOverlay();
					$j("#region_prompt_remember_overlay").hide();
					region_prompt_response_done = true;
				}
				
				
		
				$j(document).ready(function() {
					//showRegionPromptOverlay();
				}); 
		</script>







