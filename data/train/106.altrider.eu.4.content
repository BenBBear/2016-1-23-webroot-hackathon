			$j('.typo').each(function() {
			        var wordArray = $j(this).text().split(" ");
			        wordArray[wordArray.length-2] += "&nbsp;" + wordArray[wordArray.length-1];
			        wordArray.pop();
			        $j(this).html(wordArray.join(" "));
			});
			
			// delay visibility of certain nodes
			$j('.delayDisplay').show();
	
			
		});
					
	</script>
	
	<script language="javascript">
		$j(document).ready(function() {
			if (isIE6()) {
				$j('#IE6Warning').html('For your protection, Altrider.com does not support your browser.  Please <a target="upgrade" style="color: white; font-weight: bold;" href="http://www.microsoft.com/nz/windows/internet-explorer/default.aspx">upgrade your browser</a>.');
				$j('#IE6Warning').show('fast');
			}
		});
	</script>
	
