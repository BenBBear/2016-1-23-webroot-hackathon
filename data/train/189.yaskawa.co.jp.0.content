<script type="text/javascript">
$(function() {
	// スライドショー
	var $panel = $('#panel_area');
	$panel.psycle({
		transition: 'fade',
		startIndex: 0,
		duration: 1200,
		delay: 10000,
		auto: true,
		repeat: 'loop',
		resizable: true
	});
	var p = $panel.data('psycle');

	p.marker(400, $.PsycleEvent.PANEL_CHANGE_START).appendTo('.marker');
	p.controller($('.pager'));

	$('.marker li').each(function() {
		var $this = $(this);
		var index = $this.index();
		var title = $panel.find('li').eq(index).attr('title');
		var $div = $('<div />');
		$div.text(title);
		title = $div.text().replace(/\\n/g, '<br />');
		$div.html(title);
		$this.append($div);
	});

});
</script>




