    // Büyük resmin urlsi kontrol edilerek küçük resmi bulunur ve active_thumb class'ı atanır 
    function Selector() {
        var bigImg = $('.product-zoom').attr("alt");

        $('a.gallery').each(function () {
            $(this).removeClass("active_thumb");
            if ($(this).attr("href") == bigImg) {
                $(this).addClass("active_thumb");
            }
        })
    }

    $(document).ready(function () {

