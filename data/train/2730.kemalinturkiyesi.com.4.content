        Selector();

        $('.product-zoom').addimagezoom({
            magnifiersize: [300, 300],
            magnifierpos: 'right',
            cursorshade: true,
            largeimage: bigImg
        });

        $('.zoomtracker, .cursorshade').click(function () {
            StartActive();
        })

        $('a.gallery').mouseenter(function () {
            var smallImg = $(this).children().attr("alt");
