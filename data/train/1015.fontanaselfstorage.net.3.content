                    thumbsTopMargin: 8,
                    thumbsRightMargin: 7,
                    customThumb: null,
                    customThumbSelected: null,
                    thumbsWidth: 12,
                    thumbsHeight: 12,
                    autoslide: true,
                    asTimer: 4000,
                    easeTime: 750,
                    typo: false,
                    galBorderWidth: 0,
                    thumbsBorderOpacity: 0,
                    buttonsTextColor: "#707070",
                    buttonsWidth: 20,
                    thumbsActiveBorderOpacity: 0.8,
                    thumbsActiveBorderColor: "black",
                    thumbsPercentReduction: 20,
                    leftButtonInner: '<img src="/core/resources/img/slideshow_l_arrow.png" />',
                    rightButtonInner: '<img src="/core/resources/img/slideshow_r_arrow.png" />',
                    buttonsWidth: 10,
                    shuffle: false,
                    transition: 'slide',
                    controlWidth : 10,
                    controlHeight : 23,
                    controlScroll : false                }).add(slideshow_magnify).fadeIn();
            })
            .attr("src", slideshow_images.eq(0).attr("src") + "?force=true") //Small hack to force IE browsers to fire the load event
            .filter(function() { //Grab the image if it is cached so we can manually fire the load event
                    return this.complete;
                })
            .load();
        }

        $('a.lightbox').lightBox({
            imageLoading: '/core/resources/images/lightbox-ico-loading.gif',
            imageBtnClose: '/core/resources/images/lightbox-btn-close.gif',
            imageBtnPrev: '/core/resources/images/lightbox-btn-prev.gif',
            imageBtnNext: '/core/resources/images/lightbox-btn-next.gif'
        });

        $(".prev-next .picture-slides-previous-image").click(function() {
            var wrap = $(this).closest(".home_slideshow_wrap"),
            currentThumb = wrap.find(".thumbSlider .current"),
                prevThumb = (currentThumb.prev().length > 0)? currentThumb.prev() : wrap.find("li:last");
            prevThumb.find("a").click();
        });

        $(".prev-next .picture-slides-next-image").click(function() {
            var wrap = $(this).closest(".home_slideshow_wrap"),
            currentThumb = wrap.find(".thumbSlider .current"),
                nextThumb = (currentThumb.next().length > 0)? currentThumb.next() : wrap.find("li:first");
            nextThumb.find("a").click();
        });
    });
</script>
	<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>
	<script type="text/javascript">
        var map;
        var all_markers = [];
		function initMap(data)
		{
			map = new google.maps.Map(document.getElementById("map_canvas2"), {
				zoom: 8,
                center: new google.maps.LatLng(data.center[0],data.center[1]),
                mapTypeId: google.maps.MapTypeId.ROADMAP,
                disableDefaultUI: true,
                zoomControl: true,
                zoomControlOptions: {
                    position: google.maps.ControlPosition.TOP_LEFT,
                    style: google.maps.ZoomControlStyle.SMALL
                }
				,scrollwheel:false			});


			// Custom map pin (file, size, origin, anchor)
			 var images = {};
			 			 images['pin_y_s.png'] = new google.maps.MarkerImage(
				"/core/resources/img/pin_y_s.png",
				new google.maps.Size(36, 37),
				new google.maps.Point(0, 0),
				new google.maps.Point(10, 34)
			);
			
			//Add the selection image
			
			var bounds = new google.maps.LatLngBounds();
			//Setup each marker
			for(k in data.marker) {
                var mark = data.marker[k];
                var marker = new google.maps.Marker({
                    position: new google.maps.LatLng(mark.latitude, mark.longitude),
                    map: map,
                    icon: images[mark.pin],
                    title:mark.title,
                    address1: mark.address1,
                    raiseOnDrag:false
                });
                all_markers.push(marker);

                var contentHtml =
                    '<div class="maptip">'
                        + '<h4>' + mark.title + '</h4>'
                        + '<div>' + mark.address1 + '</div>'
                        + '<div>' + mark.city + ', ' + mark.state + ' ' + mark.postal + '</div>'
                        + '<div class="maplink"><a href="' + mark.url + '">Click for More Information</a></div>'
                                    + '</div>';

                marker.infowindow = new google.maps.InfoWindow({
                    content: contentHtml,
                    maxWidth: 250                });

                var prevMarker = null;
                var prevIcon = null;
                google.maps.event.addListener(marker, 'click', function(event) {
                    if (prevMarker != null) {
                        prevMarker.infowindow.close(map, prevMarker);
                        prevMarker.setIcon(prevIcon);
                    }
                    
                    if (this.clickPin == undefined || this.clickPin == true) {
                        this.infowindow.open(map,this);
                    }
                    prevMarker = this;
                    prevIcon = this.getIcon();
                    
                    // Correct z-index of pins
                    $.each(all_markers,function(){
                        this.setZIndex(this.origZIndex);
                    });
                    this.setZIndex(9999);
                                    });

                mark.marker = marker;
                bounds.extend(mark.marker.position);
            }

						map.fitBounds(bounds);

			google.maps.event.addListenerOnce(map, "bounds_changed", function() {
				if (map.getZoom() > 14) {
					this.setZoom(14);
				}
			});
			
            // Record original z-index of each marker			
            $.each(all_markers,function(){
                this.origZIndex = this.getZIndex();
            });
		}
	</script>
