$(function(){
    var stid = "";
    //マウスオーバーで点滅（setIntervalで繰り返します）
    $("#idLogo").mouseover(function(){
        stid = setInterval(function(){
            $('#idLogo img').fadeOut(300,function(){$(this).fadeIn(300)});
        },600);
    });
    //マウスアウトで点滅をクリア（clearInterval）
    $("#idLogo").mouseout(function(){
        clearInterval(stid);
    });
});
</script>






