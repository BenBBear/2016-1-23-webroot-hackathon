                        <form action="#" class="quoted-form">
                            <div class="col">
                                <textarea placeholder="Hi <%=name%>.  I’m seeking cheaper car insurance.  Here are some details about my needs:"></textarea>
                            </div>
                            <div class="columns">
                                <div class="col name">
                                    <span class="icon-ico-object"></span>
                                    <input type="text" placeholder="My Name">
                                </div>
                                <div class="col mail">
                                    <span class="icon-ico-mail"></span>
                                    <input type="email" placeholder="Email">
                                </div>
                                <div class="col tel">
                                    <span class="icon-ico-mobile"></span>
                                    <input type="tel" placeholder="Phone">
                                </div>
                            </div>
                            <div class="btn-block">
