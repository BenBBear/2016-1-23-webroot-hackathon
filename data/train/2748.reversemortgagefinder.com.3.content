    <script>
        $('#zip').on('keyup', function () { 
            this.value = this.value.replace(/[^0-9\.]/g,'');
        });

        $('.select-form').on('submit', function(event){
            var zip_value = $('#zip').val();
            var zip_value_replaced = zip_value.replace(/[^0-9\.]/g,'');

            var zip = new RegExp('[0-9]{5}');

            if( zip.test(zip_value_replaced) )
            {
                return true;
            }

            alert('Zip codes must be a 5 digit number.');       

            return false;
        });

        $('select[name="category"]').on('change', function(){

            // The list of mortgage category ids.
            //
            var mortgage_category_ids = ['3','4','5','6','7','8', '16'];

            // Obain the id number of the selected category.
            //
            var category_id = $(this).val();

            // Obtain the category label.
            //
            var category_label = $('.jcf-select-text span').html();

            // Check if the category_id is in the list of mortgage_cateogry_ids.
            // 
            var is_in_category = $.inArray(category_id, mortgage_category_ids) !== -1;

            // $('.see').html( is_in_category ? 'See Rates and Lenders' : 'See Rates and Agents');
            
            $('.see').html( is_in_category ? "See Rates and Lenders" : 'See Rates and Agents');
            
            $('.see-sub').html( is_in_category ? 'Mortgage Specialists' : 'Insurance Specialists' );
        });
        
        $('select[name="category"]').change();
        
        $(window).on('load', function(){
            $('.auto-text-size').each(function(){
                var get_test_size = $(this).attr('test_size');
                $(this).css('font-size', (get_test_size > 30 ? 30 : get_test_size ) + 'px' );
            });
        });

        /**\/
        $('.tag').on('click', function(event)
        {
