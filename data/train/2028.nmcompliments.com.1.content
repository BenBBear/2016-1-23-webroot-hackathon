        else if (top.location.hostname.indexOf('strategicebs') >= 0 ) {
              window.location='http://www.nmfn.com/tn/aboutnet--pg_strg_ee_benf_intro';
        }
        else if (top.location.hostname.indexOf('internship.nmfn.com') >= 0 ) {
		      		  redirectToCareers('INTERN');
        }
        else if (top.location.hostname.indexOf('careers.nmfn.com') >= 0 ) {
				      redirectToCareers('FR');
        }
        else if (top.location.hostname.indexOf('balance.nmfn.com') >= 0 ) {
				      redirectToCareers('FR');
        }
        else if (top.location.hostname.indexOf('empowerment.nmfn.com') >= 0 ) {
				      redirectToCareers('FR');
        }
        else {
              window.location="http://www.nmfn.com"+location.pathname;
        }
     }



     function redirectToCareers(page){
		var url;
		if (page == 'FR'){
			url = "http://www.nmfn.com/tn/careers--fr0--fr?locSource=";
		}
		else{
			url = "http://www.nmfn.com/tn/careers--intern0--int?locSource=";
		}
 	    var locSource=document.referrer.split('/')[2];
 	    if (locSource==undefined){
 	    	locSource="";
     	}
 	    window.location = url + locSource;
     }

</script>
