<script type="text/javascript">
$(document).ready(function($){
    $('.megamenu').megaMenuCompleteSet({
        menu_speed_show : 300, // Time (in milliseconds) to show a drop down
        menu_speed_hide : 200, // Time (in milliseconds) to hide a drop down
        menu_speed_delay : 200, // Time (in milliseconds) before showing a drop down
        menu_effect : 'hover_fade', // Drop down effect, choose between 'hover_fade', 'hover_slide', etc.
        menu_click_outside : 1, // Clicks outside the drop down close it (1 = true, 0 = false)
        menu_show_onload : 0 // Drop down to show on page load (type the number of the drop down, 0 for none)
    });
});
</script>



<!--跳頁選單-->
<script type="text/javascript">
function MM_jumpMenu(targ,selObj,restore){ //v3.0
  eval(targ+".location='"+selObj.options[selObj.selectedIndex].value+"'");
  if (restore) selObj.selectedIndex=0;
}
</script>


<!--中央滑動廣告-->
<script type="text/javascript" src="js/jquery.wt-rotator.min.js"></script>

<link rel="stylesheet" type="text/css" href="wt-rotator.css"/>

<script type="text/javascript">
$(document).ready(	

			function() {

				$(".wt-rotator-container").wtRotator({

					width:1008,
					height:540,<!--滑動廣告高度-->

					thumb_width:24,<!--縮圖或數字按鈕寬度-->

            		thumb_height:24,<!--縮圖或數字按鈕高度-->

					button_width:24,<!--縮圖或數字按鈕寬度-->

					button_height:24,<!--縮圖或數字按鈕高度-->

					button_margin:5,<!--每個按鈕距離-->
					
					auto_start:true,

					delay:5000,

					play_once:false,

					transition:"h.slide",<!--滑動的轉場設定-->

					transition_speed:600,<!--滑動的轉場速度-->

					auto_center:true,

					easing:"",

					cpanel_position:"inside",

					cpanel_align:"BR",

					timer_align:"top",

					display_thumbs:false,<!--縮圖按鈕是否可見-->

					display_dbuttons:false,<!--方向按鈕是否可見-->

					display_playbutton:false,

					display_thumbimg:false,<!--縮圖是否用圖片-->

           			display_side_buttons:true,<!--左右邊的箭頭是否出現，如出現就不會有左右的縮圖箭頭-->

					display_numbers:true,<!--縮圖是否使用數字-->

					display_timer:true,<!--記數器長條是否出現-->

					mouseover_select:true,<!--滑鼠移到縮圖就直接換圖-->

					mouseover_pause:true,<!--滑鼠移到圖片就暫停-->

					cpanel_mouseover:false,<!--滑鼠移到圖片上才出現控制列-->

					text_mouseover:false,<!--滑鼠移到圖片上才出現文字-->

					text_effect:"fade",<!--文字轉場方式-->

					text_sync:true,<!--文字在轉場效果之後出現-->

					tooltip_type:"text",<!--縮圖按鈕上面的提示是用"image"圖片, "text"文字, "none"不出現-->

					shuffle:false,<!--隨機出現圖片-->

					block_size:75,

					vert_size:55,

					horz_size:50,

					block_delay:25,

					vstripe_delay:75,

					hstripe_delay:180			

				});

			}

		);
</script>
<!--END 中央滑動廣告-->





