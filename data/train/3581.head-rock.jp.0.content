	position: relative;
	overflow: hidden;
}
#slider_main ul {
	top: 0;
	left: 0;
	position: absolute;
	overflow: hidden;
}
#slider_main ul li {
	width: 565px;
	height: 272px;
	float: left;
	display: inline;
