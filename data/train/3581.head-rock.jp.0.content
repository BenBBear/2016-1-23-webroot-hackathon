}

#slider_thumb {
	margin: 5px auto;
	padding-top: 2px;
	width: 565px;
	height: 70px;
	text-align: center;
	background-color:#000;
}
#slider_thumb ul {
	display: inline;
}
#slider_thumb ul li {
	cursor: pointer;
	display: inline;
}
#slider_thumb ul li.active {
	filter:alpha(opacity=100)!important;
	-moz-opacity: 1!important;
	opacity: 1!important;
}

#slider_main ul:after {
	content: ".";
