<script type="text/javascript">
var map;

// Google Map 初期化
function initMap() {
	var latlng = new google.maps.LatLng(34.424266,135.563895);
	var mapOptions = {
		zoom: 16,
		center: latlng
	};
	map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);

	var marker = new google.maps.Marker({
		position: latlng,
		map: map,
		title: '加賀田小学校'
	});
	marker.setMap(map);
}

function getMapSize() {
	$('#map-canvas').css('width', parseFloat($(window).innerWidth())*0.8);
	$('#map-canvas').css('height', parseFloat($(window).innerHeight())*0.8);
}

$(document).ready(function() {
	getMapSize();
	//fancybox Version 2
	$('#map').fancybox({
		type: 'inline',
		beforeLoad: function() {
			if (typeof map === 'undefined') {
				initMap();
			}
			$('#map-canvas').css('display','block');
		},
		beforeClose: function(){
			$('#map-canvas').css('display','none');
		}
	});
});
</script>
