<script type="text/javascript">
var headerLocation = "top-retract";
var retractIsEnabled = (headerLocation == 'top-retract');

// Idempotent
function expandHeader()
{
    if ($('wrapper').hasClassName('expanded')) return true;
    updateHeader();
}

// Idempotent
function contractHeader(e)
{
    if (!$('wrapper').hasClassName('expanded')) return true;
    updateHeader();
}

function updateHeader()
{
    var positionHidden  = -1 * jQuery('#header-optional').height();
    var positionVisible = jQuery('#header').height();

    // Make sure it's in the right place to start
    if ($('wrapper').hasClassName('expanded'))
    {
        $('header-optional').setStyle({ top: positionVisible + 'px', visibility: 'visible' });
    } else {
        $('header-optional').setStyle({ top: positionHidden + 'px', visibility: 'visible' });
    }

    // Now toggle the class
    $('wrapper').toggleClassName('expanded');

    // Animate header info into place
    var animationTime = 400;
    jQuery('#header-optional').addClass('animating');
    if ($('wrapper').hasClassName('expanded'))
    {
        var targetColor     = '#3d3e41';
        var targetTextColor = '#ffffff';
        jQuery('.header-container').stop().animate(
            {
                backgroundColor: targetColor,
                color:           targetTextColor
            },
            {
                duration: animationTime
            }
        );
        jQuery('#header-optional').stop().animate(
            {
                top:             positionVisible,
                backgroundColor: targetColor,
                color:           targetTextColor
            },
            {
                duration: animationTime,
                complete: function(e) { jQuery('#header-optional').removeClass('animating'); }
            }
        );
    } else {
        var targetColor     = '#000000';
        var targetTextColor = '#fcfcfc';
        jQuery('.header-container').stop().animate(
            {
                backgroundColor: targetColor,
                color:           targetTextColor
            },
            {
                duration: animationTime
            }
        );
        jQuery('#header-optional').stop().animate(
            {
                top:             positionHidden,
                backgroundColor: targetColor,
                color:           targetTextColor
            },
            {
                duration: animationTime,
                complete: function(e) { jQuery('#header-optional').removeClass('animating'); }
            }
        );
    }
}

document.observe("dom:loaded", function() {
    $$('.toggle-header').each(function(element) {
        element.observe('mouseover', function(e) {
            if (element.hasClassName('do-not-show')) return true;
            expandHeader();
        });
    });
    $$('.header-wrapper').each(function(element) {
        element.observe('mouseout', function(e) {
            if (!$(e.target).hasClassName('header-wrapper')) return true;
            contractHeader(e);
        });
    });
    $$('.more-info-button').each(function(element) {
        element.observe('click', function(e) {
            if ($('header-optional').hasClassName('animating')) return true;
            var toggleHeaderClassName = 'toggle-header';
            var toggleHeaderElement = element;
            if (!element.hasClassName(toggleHeaderClassName))
            {
                toggleHeaderElement = element.up('.' + toggleHeaderClassName);
            }
            toggleHeaderElement.addClassName('do-not-show');
            toggleHeaderElement.observe('mouseout', function(f) {
                if (!$(f.target).hasClassName(toggleHeaderClassName)) return true;
                toggleHeaderElement.removeClassName('do-not-show');
            });
            contractHeader(e);
        });
    });
    $(document).observe('mousemove', function(e) {
        if (!$('wrapper').hasClassName('expanded')) return true;
        var element = $('header-optional');
        var header  = $('header');
        var elementBottom = Math.max(header.getHeight(), element.cumulativeOffset().top) + element.getHeight();
        if (e.clientY > elementBottom)
        {
            contractHeader(e);
        }
        return true;
    });
});


