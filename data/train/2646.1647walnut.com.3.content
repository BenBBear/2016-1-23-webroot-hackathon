PHOCOA.vtour.showPopupView = function(url, optionalLabel)
{
    try {
        swfobject.getObjectById('flashpano').pauseTour ();
    }
    catch (e) {
    }
    if (!optionalLabel) optionalLabel = '';
    if (PHOCOA.vtour.offlineMode)
    {
        window.open(url, 'popupView', 'width=600,height=400,toolbar=no,status=no,location=no,directories=no,menubar=no,scrollbars=yes');
    }
    else
    {
        var panel = PHOCOA.runtime.getObject('popupView').show(url);
        $(PHOCOA.runtime.getObject('popupView').header).update(optionalLabel);
    }
};

PHOCOA.vtour.ensureMinimumSize = function(minWidth, minHeight)
{
    minWidth = minWidth || 800;
    minHeight = minHeight || 600;
    if (PHOCOA.vtour.offlineMode || document.viewport.getWidth() <= minWidth || document.viewport.getHeight() <= minHeight)
    {
        PHOCOA.vtour.maximizeBrowserSize();
    }
};

PHOCOA.vtour.maximizeBrowserSize = function()
{
    try {
        // NOTE that safari won't allow JS browser resize if there are multiple tabs in the window.
        window.moveTo(1,1);
        window.resizeTo(screen.availWidth, screen.availHeight);
    } catch (e) {}  // offline tour in IE will throw an "access denied" error w/o this
};



