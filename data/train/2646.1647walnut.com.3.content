        this.widthEls.select(function(el) { return typeof el !== 'string'; }).collect(function(el) { widthSetback += el.getwidth(); });

        // window dims
        var windowDims = { width: this.windowWidth(), height: this.windowHeight() };

        // calculate player dimensions
        var playerWidth  = parseInt((1 - this.hSpacePct) * (windowDims.width  - widthSetback) - this.hSpacePx);
        var playerHeight = parseInt((1 - this.vSpacePct) * (windowDims.height - heightSetback) - this.vSpacePx);

        // style it up
        this.mediaEls.select(function(el) { return typeof el !== 'string'; }).map(function(el) {
                var styles = {};
                styles['height'] = playerHeight + 'px';
                styles['width'] = playerWidth + 'px';

                // Center the player within the hSpace and vSpace
                if (this.hSpacePct || this.hSpacePx)
                {
                    styles['left'] = parseInt((windowDims.width  - playerWidth )/2) + 'px';
                }
                if (this.vSpacePct || this.vSpacePx)
                {
                    styles['top'] = parseInt((windowDims.height - playerHeight)/2) + 'px';
                }

                // Apply styles
                el.setStyle(styles);
                if (this.callback)
                {
                    this.callback(el);
                }
            }.bind(this));
    },
    windowWidth: function()
    {
        // We believe this is subject to race conditions depending on whether
        // or not the parent element has been layed out. Seems more reliable
        // to patch the tour sizer to be able to subtract N pixels from the
        // window width.
        // var parent       = $('tourplayer').ancestors().first();
        // var width        = parent.getWidth();
        // var paddingLeft  = parseInt(parent.getStyle('padding-left').replace('px',''));
        // var paddingRight = parseInt(parent.getStyle('padding-right').replace('px',''));
        // return width - paddingLeft - paddingRight;

        var myWidth = 0;
        if (typeof(window.innerWidth) == 'number')
        {
            //Non-IE
            myWidth = window.innerWidth;
        }
        else if (document.documentElement && document.documentElement.clientWidth)
        {
            //IE 6+ in 'standards compliant mode'
            myWidth = document.documentElement.clientWidth;
        }
        else if (document.body && document.body.clientWidth)
        {
            //IE 4 compatible
            myWidth = document.body.clientWidth;
        }
        return myWidth;
    },
    windowHeight: function()
    {
        var myHeight = 0;
        if (typeof(window.innerHeight) == 'number')
        {
            //Non-IE
            myHeight = window.innerHeight;
        }
        else if (document.documentElement && document.documentElement.clientHeight)
        {
            //IE 6+ in 'standards compliant mode'
            myHeight = document.documentElement.clientHeight;
        }
        else if (document.body && document.body.clientHeight)
        {
            //IE 4 compatible
            myHeight = document.body.clientHeight;
        }
        return myHeight;
    }
});

PHOCOA.vtour.closePopupView = function() {
  PHOCOA.runtime.getObject('popupView').cancel();
