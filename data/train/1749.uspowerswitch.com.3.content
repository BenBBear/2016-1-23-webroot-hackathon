<script type="text/javascript">
function fncIsNumeric(n) {
  return !isNaN(parseFloat(n)) && isFinite(n);
  }
function onNext1()
{
	var GE;
	var ST;
	GE = document.getElementById("fGE").value;
	ST = document.getElementById("fST").value;
	if ((GE == "") || (ST == ""))
	{
		alert("Service type and State must be selected!");
		return false;
	}
	itemForm.submit();
	return true;
}

function onNext2()
{
	var RVAL;
	var UT;
	UT = document.getElementById("fUtility").value;
	if (UT == "")
	{
		alert("Utility must be selected!");
		return false;
	}
	var DT;
	DT = document.getElementById("fMonth").value;
	if (DT == "")
	{
		alert("Month must be selected!");
		return false;
	}
	var US;
	US = document.getElementById("fUsage").value;
	if (US == "")
	{
		alert("Usage must be filled!");
		return false;
	}
	var RVAL = fncIsNumeric(US);
	if (RVAL == false)
	{
		alert("Usage must be a number!");
		return false;
	}

	
	var PRO = document.getElementById("Provider").value;
	if (PRO == "Alternate Energy Supplier Price")
	{
		var AP;
		AP = document.getElementById("AltProvider").value;
		if ((AP == "") || (AP == null))
		{
			alert("Alternate Energy Supplier Price must be filled!");
			return false;
		}
		RVAL = fncIsNumeric(AP);
		if (RVAL == false)
		{
			alert("Alternate Energy Supplier Price must be a number!");
			return false;
		}
	}
		
	itemForm.submit();
	return true;
}

function onNext3(rid)
{
	document.getElementById("hID").value = rid;
//		alert(rid);
	itemForm.submit();
	return true;
}

function onNext4()
{
	var FN = document.getElementById("fFirstName").value;
	var LN = document.getElementById("fLastName").value;
	var ST = document.getElementById("fStreet").value;
	var CT = document.getElementById("fCity").value;
	var ZP = document.getElementById("fZip").value;
	var PH = document.getElementById("fPhone").value;
	var BT = document.getElementById("hBT").value;
	var ACCT = document.getElementById("fAccount").value;
	var PODID = document.getElementById("fPODID").value;
	var CUST = document.getElementById("fCustomerID").value;
	var CNK = document.getElementById("fCustomerNameKey").value;
	var STATE = document.getElementById("hState").value;

	if (FN == "")
	{
		alert("First Name MUST be entered!");
		document.getElementById("fFirstName").focus();
		return false;
	}
	if (LN == "")
	{
		alert("Last Name MUST be entered!");
		document.getElementById("fLastName").focus();
		return false;
	}
	if (ST == "")
	{
		alert("Stree Address MUST be entered!");
		document.getElementById("fStreet").focus();
		return false;
	}
	if (CT == "")
	{
		alert("City MUST be entered!");
		document.getElementById("fCity").focus();
		return false;
	}
	if (ZP == "")
	{
		alert("Zip Code MUST be entered!");
		document.getElementById("fZip").focus();
		return false;
	}
	if (PH == "")
	{
		alert("Phone MUST be entered!");
		document.getElementById("fPhone").focus();
		return false;
	}
	
	if (BT == "ACCT")
	{
		if (ACCT == "")
		{
			alert("Account MUST be entered!");
			document.getElementById("fAccount").focus();
			return false;
		}
	}

	if (BT == "CUST")
	{
		if (CUST == "")
		{
			alert("CustomerID MUST be entered!");
			document.getElementById("fCustomerID").focus();
			return false;
		}
	}

	if (BT == "PODID")
	{
		if (PODID == "")
		{
			alert("PODID MUST be entered!");
			document.getElementById("fPODID").focus();
			return false;
		}
	}

	if (STATE == "CT")
	{
		if (CNK == "")
		{
			alert("Customer Name Key MUST be entered!");
			document.getElementById("fCustomerNameKey").focus();
			return false;
		}
	}
	
	var RVAL = fncAcctValidate();
	if (RVAL == false)
		return false;

	RVAL = fncCustValidate();
	if (RVAL == false)
		return false;

	RVAL = fncPODIDValidate();
	if (RVAL == false)
		return false;

	RVAL = fncCNKValidate();
	if (RVAL == false)
		return false;

var CERT=document.getElementById('fAgree').checked;
if (CERT == false)
	{
		alert("You MUST agree to the terms by checking the box!");
		complete=false;
		document.getElementById('fAgree').focus();
		return complete;
	}
	document.getElementById("fAgree").value = 1;
	itemForm.submit();
	return true;
//		alert('Not ready yet!!');
}

function fncCleanPhone() {
	var PH = document.getElementById("fPhone").value;
	PH=PH.replace("-",""); 
	PH=PH.replace("(",""); 
	PH=PH.replace(")",""); 
	PH=PH.replace(" ",""); 
	document.getElementById("fPhone").value = PH;
}
function fncCertify() {
var CT=document.getElementById('fAgree').checked;
if (CT == false)
	document.getElementById('fAgree').value = 0;
else
	document.getElementById('fAgree').value = 1;

}

function fncAcctValidate() {
var BT = document.getElementById("hBT").value;
var ST = document.getElementById("hState").value;
	if ((BT == "ACCT") || (ST == "CT"))
	{
		var ACCT1 = document.getElementById("fAccount").value;
		var ACCT2 = document.getElementById("fAccount2").value;
		if (ACCT1 != ACCT2)
		{
			alert('Account compare does not match!');
			return false;
		}
	}
	return true;
}

function fncCustValidate() {
var BT = document.getElementById("hBT").value;
	if (BT == "CUST")
	{
		var ACCT1 = document.getElementById("fCustomerID").value;
		var ACCT2 = document.getElementById("fCustomerID2").value;
		if (ACCT1 != ACCT2)
		{
			alert('Customer # compare does not match!');
			return false;
		}
	}
	return true;
}

function fncPODIDValidate() {
var BT = document.getElementById("hBT").value;
	if (BT == "PODID")
	{
		var ACCT1 = document.getElementById("fPODID").value;
		var ACCT2 = document.getElementById("fPODID2").value;
		if (ACCT1 != ACCT2)
		{
			alert('PODID compare does not match!');
			return false;
		}
	}
	return true;
}

function fncCNKValidate() {
var ST = document.getElementById("hState").value;
	if (ST == "CT")
	{
		var ACCT1 = document.getElementById("fCustomerNameKey").value;
		var ACCT2 = document.getElementById("fCustomerNameKey2").value;
		if (ACCT1 != ACCT2)
		{
			alert('Customer Name Key compare does not match!');
			return false;
		}
	}
	return true;
}

function setAltP()
{
	var PRO = document.getElementById("Provider").value;
	if (PRO == "Utility Company Default Rate")
	{
		document.getElementById("ALT").style.visibility = "hidden";
		document.getElementById("AltProvider").value = null;
	}
	else
		document.getElementById("ALT").style.visibility = "visible";
}
