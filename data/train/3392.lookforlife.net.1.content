<script type="text/javascript">

$(document).ready(function () {

    jQuery('#main_form').hide();

    jQuery("#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_btnTopFormSubmit").bind(
        'click',
        function () {
            $('#main_form').show();

            ShowHidePartner();

            $('span#lblCoverPurpose').html(jQuery("#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_ddlCoverFor option:selected").text());
            UpdateLabelDob()
            $('span#lblSmoker').html(jQuery("#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_ddlSmoker option:selected").text());
        }
    );

    jQuery('#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_ddlDobDay').bind(
        'change',
        function () {
            UpdateLabelDob();
        });

    jQuery('#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_ddlDobMonth').bind(
    'change',
    function () {
        UpdateLabelDob();
        });

    jQuery('#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_ddlDobYear').bind(
    'change',
    function () {
        UpdateLabelDob();
    });

    jQuery('#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_ddlCoverFor').bind(
        'change',
        function () {
            ShowHidePartner();
        });

    jQuery('#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_ddlSmoker').bind(
        'change',
        function () {
            $('span#lblSmoker').html(jQuery("#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_ddlSmoker option:selected").text());
        });

});


function UpdateLabelDob() {

    $('span#lblDob').html(
            jQuery("#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_ddlDobDay option:selected").text()
            + "/"
            + jQuery("#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_ddlDobMonth option:selected").text()
            + "/"
            + jQuery("#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_ddlDobYear option:selected").text()
    );

}

function ShowHidePartner() {
    if (jQuery("#ContentPlaceHolderDefault_MainForm_LookForLifeForm_2_ddlCoverFor option:selected").text() == "You & your partner") {
        $('#partner_form').show();
    }
    else {
        $('#partner_form').hide();
    }
}

</script>




