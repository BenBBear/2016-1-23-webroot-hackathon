<script type="text/javascript">
//APIを読み込む
google.load("feeds", "1");
  
//初期関数
function initialize(){
  
//読み込むフィードを設定する
var feed = new google.feeds.Feed("http://blog.livedoor.jp/micol-micol/index.rdf");
  
//読み込む件数を設定する
feed.setNumEntries(5);
feed.load(dispfeed);
//フィードを読み込む
function dispfeed(result){
  
//エラーチェック
if(!result.error){
  
//表示するエレメントを設定する
var container = document.getElementById("feed_blog");
var htmlstr = "";
htmlstr += '<div id="feed-rapper">';
for (var i = 0; i < result.feed.entries.length; i++) {
 
var entry = result.feed.entries[i];
var eimg = "";
var imgCheck = entry.content.match(/(src="http:){1}[\S_-]+((\.jpg)|(\.JPG))/);
if(imgCheck){
         eimg += '<img ' + imgCheck[0] + '" width="225" >';
      }
 
htmlstr += '<div class="feed-Control">';
htmlstr +=  eimg + '<h4 class="title"><a href="' + entry.link + '">' + entry.title + '</a></h4>';
 
htmlstr += '<div class="entry-contents">' + entry.contentSnippet.substring(0,14)+' ...<a href="' + entry.link + '">続きを読む</a>' +'</div></div><br class="clear">';
 
}
htmlstr += '</div">';
 
container.innerHTML = htmlstr;
var elems = container.getElementsByTagName('a'); // containerに含まれるa要素
for (var j = 0, l = elems.length; j < l; j++) {
elems[j].target = '_blank'; // targetに'_blank'を設定
}
}
}
}
google.setOnLoadCallback(initialize);
</script>

