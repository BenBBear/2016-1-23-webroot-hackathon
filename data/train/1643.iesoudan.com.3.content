.local-form(@radius:5px; @w_addr:400px;@dl_dsp:table;@dt_dsp:table-cell;
			@left_w:180px;@dt_bdr:1px;){
	#fmtable {
		border:@form_bd 1px solid;
		div.div-tb{
			display:table;
			div.div-num{
				display:table-cell;
				height:100%;
				padding:10px;
				background-color:@org_color1;
				color:#fff;
				font-weight:bold;
				vertical-align:middle;
				border:#fff 2px solid;
			}
			div.div-right{
				display:table-cell;
				width:100%;
				padding:0;
			}
		}
		dl {
			margin:0;
			display:@dl_dsp;
			width:100%;
			border-bottom:@form_bd 1px solid;
			&:last-child {
				border-bottom:none;
			}
			dt {
				display:@dt_dsp;
				padding:15px;
				font-size:11pt;
				&:first-child {
					vertical-align:top;
					-moz-box-sizing: border-box;
					box-sizing: border-box;
					width:@left_w;
					border-right:@form_bd @dt_bdr solid;
					background-color:@form_left_bg;
					color:@form_left_fg;
					font-weight:bold;
					span#hissu {
						font-size:9pt;
						color:@form_hissu_fg;
						font-weight:normal;
						padding:1px 2px;
						margin-left:5px;
						vertical-align:14%;
						margin-top:2px;
						background:@form_hissu_bg;
						border-radius: @radius;
						-webkit-border-radius: @radius;
						-moz-border-radius: @radius;
					}
				}
				&:last-child {
					.sub {
						font-size:9pt;
					}
					.error,.error2,.error3 {
						display: none;
						position: absolute;
						top: -29px;
						z-index: 3;
						padding: 5px 0px;
						font-size:9pt;
						color:@form_error_fg;
						background:@form_error_bg;
						border-radius: @radius;
						-webkit-border-radius: @radius;
						-moz-border-radius: @radius;
						span {
							background-image:url(ico/form_error.gif);
							background-repeat:no-repeat;
							background-position:4px 5px;
							padding:5px 0px 5px 20px;
						}
					}
					.error:after,.error2:after,.error3:after {
						width: 100%;
						content: "";
						display: block;
						position: absolute;
						left: 0.5em;
						bottom: -5px;
						border-bottom:5px solid transparent;
						border-left:5px solid @form_error_bg;
					}
					#input {
						position:relative;
						display:block;
					}
				}
				input, textarea, select {
					-moz-box-sizing: border-box;
					box-sizing: border-box;
					border:@form_input_bd 1px solid;
					font-size:10pt; 
					padding:5px;
					border-radius: @radius;
					-webkit-border-radius: @radius;
					-moz-border-radius: @radius;
					background-color:@form_input_bg;
					&#iAddr,&#iAddrs,&#iMail {
						width:@w_addr;
					}
					&#iAddrp,&#iTel,&#iMail {
						ime-mode: disabled;
					}
				}
				span:nth-of-type(n+2) > input{
					margin-top:5px;
				}
			}
		}
		&.m-sp{
			input, textarea, select {
				padding:10px;
			}
		}
	}
}
