<script type="text/javascript">
var rss_num = 1;	// 表示件数
google.load("feeds", "1");
function initialize() {
	var feed = new google.feeds.Feed("http://nogeborderline.blog6.fc2.com/?xml");
	feed.setNumEntries(rss_num);
	feed.load(function(result)
	{
		if (!result.error)
		{
			var container = document.getElementById("td_rss");
			for (var i=0; i<result.feed.entries.length; i++)
			{
				var d = result.feed.entries[i];
				var dobj = new Date(d.publishedDate);
				var ds = dobj.getFullYear() + "." + (dobj.getMonth() + 1) + "." + dobj.getDate();
				container.innerHTML += '<a href="news.html">' + d.title + '　（' + ds + '）</a><br />';
			}
		}
	});
}
google.setOnLoadCallback(initialize);
</script>
