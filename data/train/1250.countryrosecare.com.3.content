        <script type="text/javascript">
        jQuery(document).ready(function($) {
        var images = [];
                images.push('http://countryrosecare.com/crgh/wp-content/uploads/2015/09/crgh_bg.jpg');
                var index = 0;
        var backstretchSettings = { fade: 750, duration:3500};
        var len = images.length;
        var totalDuration = (backstretchSettings.fade + backstretchSettings.duration);
        var timer = null;
        var rotate = function() {
            $('body').backstretch(images[0], backstretchSettings);
            timer = setInterval(function() {
                index++;
                if(index == len) {
                    index = 0;
                }
                $('body').backstretch(images[index], backstretchSettings);     
            }, totalDuration); 
        };
        rotate();
        });
        </script>
        
