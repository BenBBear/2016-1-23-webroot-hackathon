                var latlng = new google.maps.LatLng(this[1], this[2]);
        //var icon = 'http://www.google.com/mapfiles/marker' + String.fromCharCode(65 + this[3]) + '.png';
                var marker = new google.maps.Marker({
                    position: latlng,
                    map: mapObj,
                    title: this[0]
