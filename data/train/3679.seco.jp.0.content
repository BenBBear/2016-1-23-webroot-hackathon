                attachMessage(marker, content);
            });

            /* スタイル付き地図 */
var styleOptions = [{
  featureType: 'all',
  elementType: 'labels',
  stylers: [{ hue: '#0084d6' }, { saturation: '-70' }, { gamma: '1.5' }]
}, {
  featureType: 'all',
  elementType: 'geometry',
  stylers: [{ hue: '#0084d6' }, { saturation: '-70' }, { gamma: '0.5' }]
}];
            var styledMapOptions = { name: 'blue' }
            var lopanType = new google.maps.StyledMapType(styleOptions, styledMapOptions);
            mapObj.mapTypes.set('noText', lopanType);
            mapObj.setMapTypeId('noText');

        });
