<script type="text/javascript">
    /*クッキーに保存されていた場合*/
    function user_value(){
	    jQuery("#label_check2").hide();
	    var user_value = unescape(getCookie("harmore_name"));
		/*ユーザ名の表示とパスワード欄を非表示設定*/
		if((user_value != "")){
			jQuery("#edit-username").val(user_value);
			jQuery(".form-item-pass").hide();
			jQuery("#label_check1").hide();
			jQuery("#label_check2").show();
		}
    }
    window.onload = user_value;
    
    /*パスワード欄表示設定*/
    function show_password(){
    	check2 = document.getElementsByName('save2');
    	if(check2[0].checked){
    		jQuery("#label_check2").hide();
			jQuery(".form-item-pass").show();
			jQuery("#label_check1").show();
    	}
    }
    
    var createCookieMillis = function(name, value, milliseconds) {
        var expires;
        if (milliseconds) {
            var date = new Date();
            date.setTime(date.getTime() + milliseconds);
            expires = "; expires=" + date.toGMTString();
        }
        else {
            expires = "";
        }
        document.cookie = name + "=" + value + expires + "; path=/";
    }

    /*ログインボタン押下後*/
    function login_submit() {
        var username = jQuery("#edit-username").val();
        createCookieMillis("harmore_username", username, 12000000); //2 mins
        
        /*クッキーが保存させているか確認する*/
        var cookie = unescape(getCookie("harmore_password"));
	    /*保存チェックボックス確認*/
		var check = jQuery(':checkbox[name="save"]:checked').val();
	    /*保存チェックボックスにチェックが入っている場合*/
		if(check){
			/*パスワードと登録者の名前をクッキーに保存する*/
		    var password = jQuery("#edit-password").val();
		    setCookie("harmore_password",password ,1);
		    setCookie("harmore_name",username ,1);
		}
		/*クッキーが保存させている場合*/
		/*入力されたユーザ名と保存されているユーザ名の確認を行う*/
	    username2 = unescape(getCookie("harmore_name"));
		/*チェック済み2回目以降*/
	    if((cookie != "") && (username == username2)){
	    	jQuery("#edit-password").val(cookie);
		}else if(username != username2){
			setCookie("harmore_password",password ,0);
		    setCookie("harmore_name",username ,0);
	    }
        return true;
    }
    
    /*クッキーを取得する*/
    function getCookie(c_name){
        var st="";
        var ed="";
        if (document.cookie.length>0){
            st=document.cookie.indexOf(c_name + "=");
            if (st!=-1){ 
                st=st+c_name.length+1;
                ed=document.cookie.indexOf(";",st);
                if (ed==-1) ed=document.cookie.length;
                return unescape(document.cookie.substring(st,ed));
            } 
        }
        return "";
    }
    
    /*クッキーに保存する*/
    function setCookie(c_name,value,num){
        // 有効期限の日付
        var expires;
        date = new Date();
		if(num == 1){
			date.setTime(date.getTime() + 3600 * 24 * 365);
		}else{
			date.setTime(0);
		}
		expires = "; expires=" + date.toGMTString();
        // クッキーに保存する文字列を生成
        var s="";
        s+=c_name+"="+escape(value);
        s+=(expires==null)?"":"; expires="+expires + "; path=/";
        // クッキーに保存
        document.cookie=s;
    }
    
</script>

