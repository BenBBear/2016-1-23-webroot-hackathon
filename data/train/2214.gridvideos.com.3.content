	$('img.over').mouseover(function() {
		$(this).stop();
		$(this).fadeTo('slow', 0.1, function() { /* Animation complete. */ 	});
	});
	$('img.over').mouseout(function() {
		$(this).stop();				
		$(this).fadeTo('slow', 1.0, function() { /* Animation complete. */ 	});
