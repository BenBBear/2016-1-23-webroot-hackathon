<script type="text/javascript">

    /* <![CDATA[ */



    $(function() {
        var active_thumb = 1;

        $('#rotatorGal a img').hover(
                function(){
                    this.click();
                    $(this).fadeTo(0,1);
                    $('.active').removeClass('active');
                },
                function(){
                    $(this).fadeTo(0,0.8);
                }
        );

        $("#carousel").jCarouselLite({
            visible: 1,
            btnGo: [".p1", ".p2", ".p3", ".p4", ".p5", ".p6"],
            start:0,
            speed:600,
            circular : false
        });

        $('#next, #prev').click(function(){
            $(this).css('opacity','0.5');
            setTimeout(function(){$('#next, #prev').css('opacity','1')}, 100);
            rotate(this);
        });

        function rotate(obj){
            if($(obj).attr('id')=='prev'){
                active_thumb--;
            }else{
                active_thumb++;
            }
            if(active_thumb>=7){active_thumb=1}
            else if(active_thumb<=0){active_thumb=6}

            $('.active').removeClass('active');
            $('.p'+active_thumb+' img').addClass('active').click();
        }
        setInterval(rotate,3000);


        $(".slider").draggable({
            axis: "x",
            start: function(event) {
                var data = event.originalEvent.touches ? event.originalEvent.touches[0] : event;
                start = {
                    coords: [ data.pageX, data.pageY ]
                };
            },
            stop: function(event) {
                var data = event.originalEvent.touches ? event.originalEvent.touches[0] : event;
                stop = {
                    coords: [ data.pageX, data.pageY ]
                };
                start.coords[0]  > stop.coords[0] ? moveLeft() : moveRight();

                function moveLeft() {
                    $("#next").trigger("click");
                }

                function moveRight() {
                    $("#prev").trigger("click");
                }
                function dragDelta() {
                    return Math.abs(start.coords[0] - stop.coords[0]);
                }
            }
        });


    });


    /* ]]> */

</script>
