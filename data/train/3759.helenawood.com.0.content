		$j('#slide_video_'+key).show();
	}
	
	function hideSlideVideo(key)
	{
		var slider = $j('#slider1').bxSlider();
		slider.startShow();
		$j('#top_bg').fadeIn();
		$j('.bx-pager').fadeIn();
		$j('#slide_video_'+key).fadeOut('slow');
		$j('#bx_slide_'+key).fadeOut('slow');
		$j('#slide_video_'+key).css({top: '550px'});
	}
	
