							lng = mk[i].getElementsByTagName('lng')[0].firstChild.nodeValue;
						} else { lng = ""; }
						if (mk[i].getElementsByTagName('spot')[0].firstChild) {
							spot = mk[i].getElementsByTagName('spot')[0].firstChild.nodeValue;
						} else { spot = ""; }
						if (mk[i].getElementsByTagName('division')[0].firstChild) {
							division = mk[i].getElementsByTagName('division')[0].firstChild.nodeValue;
						} else { division = ""; }
						if (mk[i].getElementsByTagName('classification')[0].firstChild) {
							classification = mk[i].getElementsByTagName('classification')[0].firstChild.nodeValue;
						} else { classification = ""; }
						if (mk[i].getElementsByTagName('layouttype')[0].firstChild) {
							layouttype = mk[i].getElementsByTagName('layouttype')[0].firstChild.nodeValue;
						} else { layouttype = ""; }
						if (mk[i].getElementsByTagName('price')[0].firstChild) {
							price = mk[i].getElementsByTagName('price')[0].firstChild.nodeValue;
						} else { price = ""; }
						if (mk[i].getElementsByTagName('img')[0].firstChild) {
							img = mk[i].getElementsByTagName('img')[0].firstChild.nodeValue;
						} else { img = ""; }
						if (mk[i].getElementsByTagName('pageurl')[0].firstChild) {
							pageurl = mk[i].getElementsByTagName('pageurl')[0].firstChild.nodeValue;
						} else { pageurl = ""; }

						// 地点を追加して領域を拡大
						var mpoint = new google.maps.LatLng(lat, lng);
						gb.extend(mpoint);

						// マーカー生成
						var marker = createMarker(map, mpoint, spot, division,
													classification, layouttype,
													price, img, pageurl);
					}
					map.fitBounds(gb);	// ズームレベルを調整
				},
				onFailure		: function(req, json){
					alert('Failure');
				}
			}
		);
	}
}

//----- 物件マーカーの個別生成 ------//
function createMarker(map, mpoint, spot, division, classification, layouttype,
													price, img, pageurl) {
	// 画像とそのリンク
	var img_tag = ""
	if ((img != "") && (img != undefined)) {
		img_tag  = "<div style=\"width:150px; height:180px;\">"
		img_tag += "<img src=\"" + img + "\" width=\"150\" alt=\"\"" + " />";
		img_tag += "<" +  "/div><br" + " />";
	}

	// リンクの作成
	var link_anch = "";
	if ((pageurl != "") && (pageurl != undefined)) {
		link_anch = "<a href=\"" + pageurl +"\""
				  + " onclick=\"infoClick('"
				  + pageurl
				  + "'); return false;\">"
				  + "詳細<" + "/a>";
	}

	// マーカーのオプション設定
	var markerOpts;
	if (division < 4) {
		division = "売買物件";
		markerOpts = {
			position: mpoint,
			map: map,
			title: spot,
			icon: "http://hpbegin.jp/commonImg/mm_20_red.png"
		};
	}
	else {
		division = "賃貸物件";
		markerOpts = {
			position: mpoint,
			map: map,
			title: spot,
			icon: "http://hpbegin.jp/commonImg/mm_20_blue.png"
		};
	}

	// 種別を生成
	var classdetails = [
						"",
						"マンション",
						"アパート",
						"メゾネット",
						"戸建",
						"事業用",
						"土地その他",
						"駐車場"
						];
	// 間取タイプを生成
	var layouttypes = [
						"",
						"1R",
						"1K",
						"1DK",
						"1LDK",
						"2K",
						"2DK",
						"2LDK",
						"3K",
						"3DK",
						"3LDK",
						"4K",
						"4DK",
						"4LDK",
						"5K以上"
					];

	// 表示するHTMLを作成
	var html = "<div><strong>"+spot+"<" + "/strong><" + "/div>"
			 + "［" + division + "］"
			 + "［" + classdetails[classification] + "］";
	if (layouttypes[layouttype]) {
		html += "［" + layouttypes[layouttype] + "］";
	}
		html += "<br />"
			  + "<strong>" + price + "<" + "/strong>"
			  + "<br />"
			  + img_tag + "<br " + "/>"
			  + "<div style=\"text-align:right;\">"
			  + link_anch + "<" + "/div>";

	// マーカを作成
	var marker = new google.maps.Marker(markerOpts);

	// フキダシ（infowindow）のオプション設定
	var infowindow = new google.maps.InfoWindow({
		content: html,
		size: new google.maps.Size(50, 50)
	});

	// マーカーのclickイベントリスナ
	google.maps.event.addListener(marker, "click", function() {
		// 既に開いているフキダシがあったら閉じる
		if (currentInfoWindow) {
			currentInfoWindow.close();
		}
		// フキダシを開く
		infowindow.open(map, marker);
		currentInfoWindow = infowindow;
	});

}

// フキダシ内の詳細をクリックした際の挙動
function infoClick(url) {
	window.open(url, null);
}


