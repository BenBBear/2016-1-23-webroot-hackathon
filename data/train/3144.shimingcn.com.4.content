    <script type="text/javascript">
         $(function(){
            var arry=[
                {src:"fl_img01.jpg",alt:"一"},
                {src:"fl_img02.jpg",alt:"二"},
                {src:"fl_img03.jpg",alt:"三"},
                {src:"fl_img04.jpg",alt:"四"},
                {src:"fl_img05.jpg",alt:"四"}
            ];
            var canvas=$("#canvas");
            var buttons=$("<ul />").appendTo(canvas).addClass("buttons");
            var pictures=[];
            var timer=-1;
           
            for(i=0;i<arry.length;i++){
                pictures.push($("<img />").attr("src","images/"+arry[i].src).attr("alt",arry[i].alt).addClass("panel"));
                var button=$("<li />").appendTo(buttons).addClass("button").text(i+1).click(i,function(event){
                    var i=event.data;
                   
                    changePicture(canvas.data("curIndex"),i);
                });
                button.mouseover(i,function(event){
                    var i=event.data;
                    if(!$(this).hasClass("active")){
                    $(this).addClass("hover");
                    }
                
                });
                button.mouseout(i,function(event){
                    var i=event.data;
                    
                    $(this).removeClass("hover");
                    
                
                });
                  
                    
            }
            canvas.hover(function(){
                stop();
                },function(){
                    run();
                }
            );
            changePicture(-1,0);
            run();
            
                function run(){
                    timer= setInterval(function(){
                    var i=canvas.data("curIndex");
                    var j=(i+1)%arry.length;
                    changePicture(i,j);
                },3000);
            }
            
            function stop(){
                if(timer!=-1)clearInterval(timer);
            }
            
            function changePicture(i,j){
                if(i==j)return;
                canvas.data("curIndex",j);
                canvas.children(".buttons").children().eq(j).addClass("active").removeClass("hover");
                canvas.children(".panel").remove();
                if(i==-1){
                    pictures[j].appendTo(canvas);
                }
                else{
                    canvas.children(".buttons").children().eq(i).removeClass("active");
                    pictures[i].css({opacity:1}).appendTo(canvas);
                    pictures[j].css({opacity:0}).appendTo(canvas);
                    pictures[i].stop(true).animate({opacity:0},2000);
                    pictures[j].animate({opacity:1},1000);
                }
            
            }
            
            
        });
    
    </script>

    

     
