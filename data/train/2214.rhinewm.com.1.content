	<script type="text/javascript">
		function validate(){
			document.getElementById("msg").innerHTML="";
			var name = document.getElementById("name").value.trim();
			var password = document.getElementById("password").value.trim();
			if(name == null || name == ""){
				document.getElementById("msg").innerHTML="请输入手机号";
				return false;
			} else if(password == null || password == "") {
				document.getElementById("msg").innerHTML="请输入密码";
				return false;
			} else if(name.length > 20){
				document.getElementById("msg").innerHTML="用户名长度不正确";
				return false;
			} else if(password.length > 20){
				document.getElementById("msg").innerHTML="密码长度不正确";
				return false;
			}
			return true;
		}
		
		function jump(){
			location.href="http://www.rhinewm.com:80/booking.jsp";
			
		}
	</script>

