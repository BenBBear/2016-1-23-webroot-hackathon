  });
  $(function() {
    function adjust() {
      var footerHeight = $('#footer').outerHeight(true);
      $('#footer').peekaboo().data('pkb-offset', 0).on('appear', function(e) {
        $('.btn-contact').stop().animate({'bottom': footerHeight-57 + 'px'},600);
        });
      $('#footer').peekaboo().data('pkb-offset', footerHeight-1).on('disappear', function(e) {
        $('.btn-contact').stop().animate({'bottom': footerHeight*0},600);
        });
      };
    adjust();
    $(window).on('resize', function(){
       adjust();
    });
  });
</script>
