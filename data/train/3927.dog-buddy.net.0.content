		<!-- ふわふわアニメ -->
		<script type="text/javascript" language="javascript">
	$(function () {
    setTimeout('rect1()'); //アニメーションを実行
});
 
function rect1() {
    $('.rect0').animate({
        marginTop: '-=5px'
    }, 800).animate({
        marginTop: '+=5px'
    }, 800);
    setTimeout('rect1()', 2000); //アニメーションを繰り返す間隔
}

	</script>
	
		
			
				<!-- 透明アニメ		 -->	
			<script type="text/javascript" language="javascript">
	$(function () {
    setTimeout('rect()'); //アニメーションを実行
});
 
function rect() {
    $(".rect").animate({
        opacity: ".6"
    }, 2000).animate({
        opacity: "1"
    }, 2000);
    setTimeout("rect()", 5000); //アニメーションを繰り返す間隔
}
	</script>
	
	
	

	
	
