<script language="javascript">
	
	function countB(id)
	{
		var ticketamt=parseInt(document.getElementById("ticket_amount").value);
		var num_ticket=0;
		for(var i=0; i<=9; i++)
		{
			if(document.getElementById("B"+i+id).value!="")
			{
				num_ticket=num_ticket+(parseInt(document.getElementById("B"+i+id).value));
			}
		}
		var total_amt=ticketamt*num_ticket;
		
		
		document.getElementById("qtyB"+id).value=num_ticket;
		document.getElementById("amountB"+id).value=total_amt;
		
		var net_num_ticket=0;
		
					if(document.getElementById("qtyB"+1).value!="")
			{
				net_num_ticket=net_num_ticket+(parseInt(document.getElementById("qtyB"+1).value));
			}
					if(document.getElementById("qtyB"+2).value!="")
			{
				net_num_ticket=net_num_ticket+(parseInt(document.getElementById("qtyB"+2).value));
			}
					if(document.getElementById("qtyB"+3).value!="")
			{
				net_num_ticket=net_num_ticket+(parseInt(document.getElementById("qtyB"+3).value));
			}
					if(document.getElementById("qtyB"+4).value!="")
			{
				net_num_ticket=net_num_ticket+(parseInt(document.getElementById("qtyB"+4).value));
			}
					if(document.getElementById("qtyB"+5).value!="")
			{
				net_num_ticket=net_num_ticket+(parseInt(document.getElementById("qtyB"+5).value));
			}
				
		var net_amountB=net_num_ticket*ticketamt;
		
		document.getElementById("net_qtyB").value=net_num_ticket;
		document.getElementById("net_amountB").value=net_amountB;
	}
	</script>
