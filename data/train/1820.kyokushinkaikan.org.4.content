	<script type="text/javascript">
    $(function(){
      // message
        $("#introPost1").find("a").prependTo($("#msgPhoto1"));
        $("#introPost2").find("a").prependTo($("#msgPhoto2"));
        $("#introPost3").find("a").prependTo($("#msgPhoto3"));
      	// news1
      	for(i=0;i<$(".newsBox").length;i++){
      	      if($(".newsBox").eq(i).find(".excerpt").find("a").length){
      	      $(".newsBox").eq(i).find(".excerpt").find("a").prependTo($(".newsContent").eq(i)).wrap("<div class='photo'>");
      	      }else{
      	      $(".newsContent").eq(i).prepend("<div class='photo'></div>");
      	      }
      	}
				$('.newsContent .photo img').imagesLoaded(function(){
					var imgs = this;
					for(var i=0 ; i<imgs.length ; i++){
						var img = imgs[i];
						$(img).removeAttr("width");
						$(img).removeAttr("height");
						
						if($(img).width() < 213 && $(img).height() < 301){
							//小さな画像
							var pt = (301 - $(img).height())/2;
							$(img).parent().css("padding-top",pt+"px");
						}else{
							//まず縦を合わせる
							$(img).attr("height","301");
							//はみ出たものは修正
							if($(img).width()>213){
								$(img).removeAttr("height");
								$(img).attr("width","213");
								
								var pt = (301 - $(img).height())/2;
								$(img).parent().css("padding-top",pt+"px");
								}
						}
					}
				});
			
				// news1 library rollover
				$(document).ready(function(){
					$("#news1 .photo a").hover(function(){
						$(this).append($("<span></span>")).find("span:last-child").addClass("hoverImg");
						var hoverImgHeight = 301;
						$(".hoverImg").css("height",hoverImgHeight);
					},function () {
						$(this).find("span:last").remove();
					}
				);
			});
		});
</script>
