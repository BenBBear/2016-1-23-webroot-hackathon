define('widgets/views/frontpage_about_site',['require','exports','module','backbone','settings','underscore','jquery','widgets/views/informer','utils','hgn!widgets/templates/about_site','utils/sharer','utils/device','widgets/views/frontpage/share_buttons','widgets/views/frontpage/site_subscribe'],function (require, exports, module) {
    var BB = require('backbone'),
        Settings = require('settings'),
        _ = require('underscore'),
        $ = require('jquery'),
        Informer = require('widgets/views/informer'),
        Utils = require('utils'),
        template = require('hgn!widgets/templates/about_site'),
        sharer = require('utils/sharer'),
        device = require('utils/device'),
        ShareButtons = require('widgets/views/frontpage/share_buttons'),
        Subscription = require('widgets/views/frontpage/site_subscribe');

        return BB.View.extend({
            events:{
                'mouseenter': 'toggleButtons',
                'mouseleave': 'toggleButtons',
                'click .tw-intent' : 'openTwIntent',
                'click a.tw-share': 'share',
                'click a.fb-share': 'share',
                'click .start-edit': 'startEdit',
                'click .finish-edit': 'finishEdit',
                'click .btn-cancel': 'cancel',
                'keyup #about-site': 'aboutTextChanged',
                'click #about-site': 'aboutTextChanged',
                'click .fill-bio': 'startEdit'
            },
            initialize: function(options) {

                this.router = this.options.router;

                if (device.desktop || device.isiPad) {
                    this.setElement($('#post-list .rm-about-site'));
                }
                else {
                    this.setElement($('.mobile-bar .rm-about-site'));
                }
                this.$el.show();
                this.watchEvent(this.model, 'change:about_html', 'render', this);
            },
            render: function () {

                var that = this,
                    defaults = this.options.defaults,
                    json_model = _.defaults(this.model.toJSON(), _.extend( {about_html: ''}, defaults && defaults.toJSON())),
                    has_enough_width_for_full_header = device.desktop || device.isiPad,
                    show_about_module = this.showAboutModule(this.model) || this.options.show_about_module;

                if (!show_about_module) {
                    this.$el.hide();
                } else {
                    this.router.trigger('header:removeEmptyFollowBioClass');
                    this.$el.html(template(json_model, {
                        sitename: Settings.site.title,
                        is_full_mode: has_enough_width_for_full_header,
                        is_short_mode: !has_enough_width_for_full_header,
                        showBio: _.isBoolean(this.options.showBio) ? this.options.showBio : true,
                        share_page: _.isBoolean(this.options.sharePage) ? this.options.sharePage : true,
                        about_text: function () {
                            return this.about_html || '';
                        },
                        about_htmlize: function () {
                            return Utils.htmlize(this.about_html);
                        },
                        site_url: Utils.getReferrerURL(),
                        screen_name: function () {
                            return Settings.site.owner_data && Settings.site.owner_data.twitter_name || false;
                        },
                        display_feeds: function () {
                            return this.fb_feed || this.tw_feed || this.is_wsj() || this.is_time || this.insta_feed || this.googleplus_feed || this.googleplus_feed || this.flickr_feed || this.linkedin_feed || false;
                        },
                        is_bio: function() {
                            return _.isEmpty(this.about_html && this.about_html.replace(/\s+/g, ''));
                        },
                        is_wsj: function() {
                            return Settings.site.id === 226969 || Settings.site.id === 249545; // WSJFiscalCliff
                        },
                        tw_custom_feed_id: function() {
                            if (Settings.site.id === 226969) {
                                return 3108351; // WSJFiscalCliff
                            } else if (Settings.site.id === 314882) {
                                return 20851642; // @CurrenSy_Spitta a.k.a. Spitta
                            } else if (Settings.site.id === 313171) {
                                return 17581754; // @bobatl a.k.a. B.o.B
                            } else if (Settings.site.id === 326899) {
                                return 21475090; // @WakaFlockaBSM a.k.a Waka Flocka Flame
                            }
                            return false;
                        },
                        custom_insta_feed: function() {
                            if (Settings.site.id === 326899) {
                                return 'wakaflockabsm'; // WakaFlockaBSM a.k.a Waka Flocka Flame
                            }
                            return false;
                        },
                        is_user_bio: function () {
                            return this.about_html || Settings.user.can('edit_site') || false;
                        },
                        in_embed: Settings.inEmbed,
                        is_time: Settings.site.id === 223595, // POY2012 Time.com
                        is_ecko: Settings.site.id === 257948, // Ecko Unltd. rebelmouse.com/EckoUnltd
                        isEditable: Settings.user.can('edit_site')
                    })).show();
                }

                // var updatedTwitterBio = localStorage.getItem('closed-update-tw-bio');
                // if( Settings.user.can('edit_site') && Settings.site.owner_data && Settings.site.owner_data.twitter_name && !updatedTwitterBio) {
                //     var updateTwBioInst = new UpdateTwBioModel({id: Settings.site.id});
                //     this.$('.user-bio').append(new UpdateTwBioView({model: updateTwBioInst, className: 'update-tw-bio'}).render().el);
                // }

                Settings.inEmbed || (this.ShareButtonsView = this.makeInstance(ShareButtons, {router: this.router}).setElement(this.$('.share-page').html('')).render());
                Settings.site.settings.rebelalerts_disabled || this.makeInstance(Subscription, {subscribeTitle: this.options.subscribeTitle}).setElement(this.$('.subscribe-page').html('')).render();
                // if (Settings.inEmbed && this.showAboutModule(this.model) && !Settings.site.settings.rebelalerts_disabled) {
                //     this.$('.subscribe-page').html('') && this.makeInstance(Subscription, {}).setElement(this.$('.subscribe-page')).render();
                // }

                return this;
            },
            showAboutModule: function(model) {
                return Settings.inEmbed || Settings.user.can('edit_site') || model.get('about_html') || model.get('fb_feed') || model.get('tw_feed') || Settings.site.id === 226969 || Settings.site.id === 223595;
