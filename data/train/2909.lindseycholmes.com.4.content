                        height: 540
                    });
                    popup && evnt.preventDefault();
                }
            },
            share: function(evnt) {
                if (!evnt.metaKey) {
                    var link = $(evnt.currentTarget).attr('href');
                    var popup = sharer.openPopupWindow(link);
                    popup && evnt.preventDefault();
                }
            },
            toggleButtons: function (evnt) {
                !this.$('.about').hasClass('editable') && this.$('.user-prefs').toggle(evnt.type === 'mouseenter');
            },
            startEdit: function (evnt) {
                evnt && evnt.preventDefault();
                this.$('.user-prefs').css('display', 'none');
                this.$('.publish-story').css('display', 'block');
                this.$('.about').hide();
                this.$('.about').addClass('editable');
                this.$('.btn-save').addClass('disabled');
                this.$('.publish-story').show();
                this.$('.user-bio').removeClass('fill-bio');
                this.$('.update-tw-bio').css('display', 'none');
            },
            aboutTextChanged: function (evnt) {
                var about = this.$('#about-site').val();
                this.$('.btn-save').toggleClass('disabled', about === this.model.get('about_html'));
            },
            finishEdit: function () {
                var that = this,
                    about = this.$('#about-site').val(),
                    aboutText = about.length ? about : ' ';

                if (about === this.model.get('about_html')) {
                    return false;
                }
                this.$('.btn-save').addClass('disabled');
                this.$('.btn-cancel').addClass('disabled');
                this.$('.btn-save').text('Saving...');

                this.model.save({
                        'about_html': aboutText
                    }, {
                        error: function () {
                            (new Informer({
                                message: JSON.parse(arguments[1].responseText)['error'],
                                type: 'error',
                                headline: 'Error!',
                                controls: [{name: 'OK', callback: $.noop}]
                            })
                                ).render();
                            that.$('.btn-save').removeClass('disabled');
                            that.$('.btn-cancel').removeClass('disabled');
                            that.$('.btn-save').text('Save');
                        },
                        success: function () {
                            that.render(true);
                            that.$('.about').removeClass('editable');
                            that.$('.btn-save').removeClass('disabled');
                            that.$('.btn-cancel').removeClass('disabled');
                            that.$('.btn-save').text('Save');
                            that.$('.user-prefs').css('display', 'block');
                        }
                    }
                );
            },
            cancel: function () {
                this.render(true);
                this.$('.user-prefs').css('display', 'block');
            }
        });
    });

