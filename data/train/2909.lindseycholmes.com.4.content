    var BB = require('backbone'),
        Settings = require('settings'),
        _ = require('underscore'),
        SessionStorage = require('utils/session_storage'),
        consts = require('consts');

    return BB.Model.extend({
        settings: {}
        , currentTimeoutInSeconds: 0

        , defaults: {
            new_posts_count: 0
            , order_updated: true // order is updated and it is in actual state
        },

        initialize: function (options, settings) {
            settings || (settings = {});
            this.settings = _.extend(consts.newPosts, settings);
            this.currentTimeoutInSeconds = this.settings.timeoutInSeconds;
        },

        isCrossDomain: function() {
            return location.href.indexOf('beta.rebelmouse.com') === -1 && location.href.indexOf('.rebelmouselabs.com') === -1;
        },

        url: function() {
            if (this.isCrossDomain()) {
                return 'https://res.rbl.ms/res/frontpage/new_posts/';
            }
            return '/res/frontpage/new_posts/';
        },
        autoUpdateKey: 'auto_update_new_post_' + Settings.site.id,
        start: function() {
            var model = this;

            this.stop();

            this.timeoutId = setTimeout(function() {
                var sinceId = model.get('postCollection').getSincePostId() || 0,
                    subpageId = model.get('postCollection').getSubpageId(),
                    lastIds = _.first(model.get('postCollection').pluck('id'), 20).join(',');

                model.fetch({
                    data: {site_id: model.get('siteId'), since_id: sinceId, subpage_id: subpageId, last_ids: lastIds, crossDomain: model.isCrossDomain()},
                    complete: function() {
                        model.currentTimeoutInSeconds = model.get('new_posts_list').length ? model.settings.timeoutInSeconds :
                            Math.min(
                                model.currentTimeoutInSeconds + model.settings.changeTimeoutStepInSeconds
                                , model.settings.maxTimeoutInSeconds);

                        model.start();
                    }
                });
            }, this.getTimeout());
            return this;
        },
        stop: function() {
            this.timeoutId && clearTimeout(this.timeoutId);
            return this;
        },
        resetData: function (silent) {
            this.set({
                new_posts_count: this.defaults.new_posts_count,
                order_updated: this.defaults.order_updated
            }, {
                silent: _.isUndefined(silent) ? true : silent
            });
        },
        turnOnAutoUpdate: function() {
            SessionStorage.setItem(this.autoUpdateKey, true);
        },
        turnOffAutoUpdate: function() {
            SessionStorage.removeItem(this.autoUpdateKey);
        },
        isAutoUpdateTurnedOn: function() {
            return SessionStorage.getItem(this.autoUpdateKey);
        }
        , getTimeout: function () {
            return this.currentTimeoutInSeconds * 1000;
        }
    });
});

