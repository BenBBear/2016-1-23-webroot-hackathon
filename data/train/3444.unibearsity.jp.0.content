<script>
    $(function(){        // ready
        var targetURL = '/content/disney/jp/store/unibearsity/jcr:content/par/recommend_goods_auto.body.area_recommend-item.html' + location.search;
        $.ajax({
            url:targetURL,
            success: function(data){
                $('#recommend_goods_auto_scale').append(data);
                var dispData = $('#recommend_goods_auto_scale').find('.cmpItem3');
                if (dispData.size() == 0) {
                    // 表示対象がない場合、見出しを非表示にする
                    $('#recommend_goods_auto_h2').parent('div').parent('div').remove();

                    $('#recommend_goods_auto_scale').parent('div').remove();

                    goods_auto_keepGrid();
                } else {
                    // 高さ調整
                    goods_auto_preload('recommend_goods_auto');
                }
            }
        });
        // 高さ揃え
        function goods_auto_preload(targetID) {
            var img = [],
                img_arr = $('#' + targetID + '_cmp ul.slide img'), // ローディング対象の画像を指定
                img_arr_lngth = img_arr.size();
            img_arr.each(function(indx){
                img[indx] = new Image();
                img[indx].onload = function(){
                    img_arr_lngth = img_arr_lngth - 1;
                    if( 0 >= img_arr_lngth ){
                        // 読み込み完了後
                        
                        // 高さ調整
                        $('#' + targetID + '_cmp .slide').fixHeight({
                            target: '>li',
                            each:{
                                pc: 20,
                                tab: 20,
                                sp: 'wild'
                            }
                        });
                        goods_auto_keepGrid();
                    }
                };
                img[indx].onerror = function(){
                    img_arr_lngth = img_arr_lngth - 1;
                    if( 0 >= img_arr_lngth ){
                        // エラー時も同様の動き
                        
                        // 高さ調整
                        $('#' + targetID + '_cmp .slide').fixHeight({
                            target: '>li',
                            each:{
                                pc: 20,
                                tab: 20,
                                sp: 'wild'
                            }
                        });
                        goods_auto_keepGrid();
                    }
                };
                img[indx].src = $(this).attr('src');
            });
        }
        // 縮小率計算
        function goods_auto_keepGrid(){
            // コンポーネント縮小
            // --------------------------------------------------
            var UA = window.navigator.userAgent.toLowerCase();
            var APPVER = window.navigator.appVersion.toLowerCase();
            var FLG_IE8 = disney.fn.checkIE8();
            var FLG_TRANSITION = disney.fn.checkTransition();
            var W_DEVICE = disney.fn.widthDevice();
            var UA_DEVICE = disney.fn.uaDevice();
            var $BODY = $('body');
            var $STATUS = $('#myStatus');
            var $cmp = $('.setScale');
            var $cmpTab = $('.setScaleTab');
            var $cmpInner = $cmp.find('>div');
            var $cmpInnerTab = $cmpTab.find('>div');
            var wBase = 1200;
            var wBaseTab = 760;
            var wMargin = W_PC - wBase;
            var wMarginTab = W_TAB - wBaseTab;
            
            wBrowser = $(window).width() - wMargin;
            wBrowserTab = $(window).width() - wMarginTab;
            
            if(W_TAB < window.innerWidth){
                if(wBase <= wBrowser){
                    wBrowser = wBase;
                }
                zoomW = wBrowser / wBase;
                $cmpInner.css({
                    webkitTransform: 'scale(' + zoomW + ')',
                    transform: 'scale(' + zoomW + ')'
                }).attr('data-scale', zoomW);
                
                // scaleでの余白切り取り
                $cmp.each(function(e){
                    var cmpH = $(this).find('>div').innerHeight();
                    $(this).css({
                        height: cmpH * zoomW + 3
                    });
                });
                
            }else if(window.innerWidth <= W_SP){
                $cmpTab.css({
                    height: 'auto'
                });
                $cmpTab.css({
                    webkitTransform: 'scale(' + 1 + ')',
                    transform: 'scale(' + 1 + ')'
                }).attr('data-scale', 1);
            }else if(window.innerWidth <= W_TAB){
                $cmp.css({
                    height: 'auto'
                });
                $cmpInner.css({
                    webkitTransform: 'scale(' + 1 + ')',
                    transform: 'scale(' + 1 + ')'
                }).attr('data-scale', 1);
                
                // tab処理
                zoomW = wBrowserTab / wBase;
                $cmpInnerTab.css({
                    webkitTransform: 'scale(' + zoomW + ')',
                    transform: 'scale(' + zoomW + ')'
                }).attr('data-scale', zoomW);
                // scaleでの余白切り取り
                $cmpTab.each(function(e){
                    var cmpH = $(this).find('>div').innerHeight() + 20;
                    $(this).css({
                        height: cmpH * zoomW + 3
                    });
                });
            }
        }
    });
</script>
<script>
    $(function(){
        // グローバル変数（英語大文字）
        var UA = window.navigator.userAgent.toLowerCase();
        var APPVER = window.navigator.appVersion.toLowerCase();
        var FLG_IE8 = disney.fn.checkIE8();
        var FLG_TRANSITION = disney.fn.checkTransition();
        var W_DEVICE = disney.fn.widthDevice();
        var UA_DEVICE = disney.fn.uaDevice();
        
        var recommend_interval = 1000;

        var set_recommend = setInterval(function() {
            if($('#recommend_goods_auto_cmp').size() > 0) {
                clearInterval(set_recommend)

            // アイテムスライド3
            // --------------------------------------------------
                var $cmp = $('#recommend_goods_auto_cmp');
                $cmp.each(function(){
                    var $root = $(this);
                    var $slide = $root.find('>.slideWrap >ul.slide');
                    var $slideList = $slide.find('>li');
                    var $slideLink = $slideList.find('a');
                    var $nav = $root.find('>ul.navController1');
                    var $navList = $nav.find('>li');
                    var $navLink = $navList.find('>a');
                    var $showModal = $root.find('.showModal');
                    var speed = 500;
                    var numCurrent = 0;
                    var numMax = $slideList.size() - 1;
                    var group = 4;
                    
                    var pathStr = location.pathname;
                    var locPath = '/';
                    //URLにスラッシュがいくつ入ってても大丈夫なようにする
                    for(var i = 1;i<pathStr.length;i++){
                        if( !((pathStr.charAt(i) === '/') && (pathStr.charAt(i-1) === '/')) ){
                            locPath += pathStr.charAt(i);
                        }
                    }
                    //index.htmlが入っている場合は取り除く
                    if(locPath.match(/index\.html$/)){
                        locPath = locPath.replace('index.html', '');
                    }
                    //現在のURLのパスとhrefが一致しているliはタグごと削除する。
                    $slideLink.filter(function() {
                        return $(this).attr('href').match(locPath);
                    }).parent().remove();
                    //再キャッシュしておく
                    $slideList = $slide.find('>li');
                    $slideLink = $slideList.find('a');
                    
                    
                    // 初期表示
                    if(numMax === 0){
                        $root.addClass('bdrTopNone');
                    }
                    if(!$root.length || $root.hasClass('slideNone') || numMax+1 === 1){
                        $nav.hide();
                        return;
                    }
                    $slideLink.attr('tabindex', -1);
                    $slideList.eq(0).find('a').removeAttr('tabindex');
                    // デバイスによって表示数変更
                    if(W_DEVICE === 'tab'){
                        group = 2;
                    }else if(W_DEVICE === 'sp'){
                        group = 1;
                    }
                    if(numMax < group){
                        $nav.hide();
                    }
                    numMax = Math.floor(numMax / group);
                    setFocus($root);
                    setNav();

                    // 4つ以下の時は上線調整
                    if(numMax === 0){
                        $root.addClass('bdrTopNone');
                    }
                    
                    // ナビゲーションクリック
                    $navLink.on('click', function(){
                        // 連打防止
                        if($slide.is(':animated')){
                            return false;
                        }
                        // 両端はスライドキャンセル
                        if($(this).parent('li').hasClass('isInvalid')){
                            return false;
                        }
                        // 戻る or 進む
                        if($(this).parent('li').hasClass('next')){
                            moveSlide('next');
                        }else{
                            moveSlide('prev');
                        }
                        return false;
                    });
                    
                    //.showModalクリック
                    $showModal.on('click', function(e){
                        var numCurrentSlide = parseInt($(this).attr('data-modaleq'), 10);
                        idCurrent = $(this).attr('data-modalname');
                        if($('#' + idCurrent).find('.slide iframe').length > 0){
                            $('#' + idCurrent).find('.slide >li').eq(numCurrentSlide).addClass('isYoutubePlaying');
                        }
                        $('#' + idCurrent).find('.slide').css({
                            left : -(100 * numCurrentSlide) + '%'
                        }).attr('data-noweq', numCurrentSlide);
                        // ナビゲーション処理
                        var numMaxSlide = $('#' + idCurrent).find('.slide >li').size() - 1;
                        $('#' + idCurrent).find('.navController1 >li').removeClass('isInvalid');
                        if(numCurrentSlide === 0){
                            $('#' + idCurrent).find('.navController1').find('.prev').addClass('isInvalid');
                        }else if(numCurrentSlide === numMaxSlide){
                            $('#' + idCurrent).find('.navController1').find('.next').addClass('isInvalid');
                        }
                        return false;
                    });
                    
                    // フリック処理
                    var xStart;
                    var xEnd;
                    var direction;
                    $slideList.bind('touchstart', function() {
                        xStart = event.changedTouches[0].pageX;
                    });
                    $slideList.bind('touchend', function() {
                        xEnd = event.changedTouches[0].pageX;
                        direction = disney.fn.dirFlick(xStart, xEnd);
                        if(direction === 'next'){
                            moveSlide('next');
                        }else if(direction === 'prev'){
                            moveSlide('prev');
                        }
                    });
                    
                    // アニメーション処理
                    function moveSlide(direction){
                        if(direction === 'next'){
                            numCurrent++;
                            if(numMax < numCurrent){
                                numCurrent = 0;
                            }
                        }else{
                            numCurrent--;
                            if(numCurrent < 0){
                                numCurrent = numMax;
                            }
                        }
                        // フォーカス処理
                        $slideLink.attr('tabindex', -1);
                        $slideList.eq(numCurrent).find('a').removeAttr('tabindex');
                        // ブラウザ判定
                        if(!FLG_TRANSITION){
                            $.Animation($slide.get(0), {
                                left :  -(100 * numCurrent) + '%'
                            },{
                                duration: speed
                            });
                        }else{
                            // CSS3アニメーション
                            $slide.css({
                                left :  -(100 * numCurrent) + '%'
                            });
                        }
                        setFocus($root);
                        setNav();
                    }
                    // フォーカス調整
                    function setFocus($elm){
                        if(!$elm.hasClass('typeFew')){
                            $slideLink.attr('tabindex', -1);
                            for(var i = 0; i < group; i++){
                                f = numCurrent * group + i;
                                $slideList.eq(f).find('a').removeAttr('tabindex');
                            }
                        }
                    }
                    // ナビゲーション処理
                    function setNav(){
                        $navList.removeClass('isInvalid');
                        if(numCurrent === 0){
                            $nav.find('.prev').addClass('isInvalid');
                        }else if(numCurrent === numMax){
                            $nav.find('.next').addClass('isInvalid');
                        }
                    }
                });
            }
        }, recommend_interval);
    });
</script>

