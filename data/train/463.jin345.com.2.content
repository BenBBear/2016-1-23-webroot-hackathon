<script type="text/javascript">
	var single;
	var all;
	
	function getAllAnnouncement(url) {
		if (window.XMLHttpRequest) { // if Mozilla, Safari etc
			all = new XMLHttpRequest();
			if (all.overrideMimeType) {
				all.overrideMimeType("text/xml");
			}
		} else {
			if (window.ActiveXObject) { // if IE
				try {
					all = new ActiveXObject("Msxml2.XMLHTTP");
				}
				catch (e) {
					try {
						all = new ActiveXObject("Microsoft.XMLHTTP");
					}
					catch (e) {
					}
				}
			}
		}
		all.onreadystatechange = multiAnnouncement;
		all.open("POST", url,true);
		all.setRequestHeader("Content-Type","application/x-www-form-urlencoded");
		all.send();
	}
	
	function getFirstAnnouncement(url) {
		if (window.XMLHttpRequest) { // if Mozilla, Safari etc
			single = new XMLHttpRequest();
			if (single.overrideMimeType) {
				single.overrideMimeType("text/xml");
			}
		} else {
			if (window.ActiveXObject) { // if IE
				try {
					single = new ActiveXObject("Msxml2.XMLHTTP");
				}
				catch (e) {
					try {
						single = new ActiveXObject("Microsoft.XMLHTTP");
					}
					catch (e) {
					}
				}
			}
		}
		single.onreadystatechange = singleAnnouncement;
		single.open("POST", url,true);
		single.setRequestHeader("Content-Type","application/x-www-form-urlencoded");
		single.send();
	}
	
	function multiAnnouncement() {
		if (all.readyState == 4) {
			var data = JSON.parse(all.responseText);
			var html = "";
			var url = "/page/page-announcement.html;jsessionid=48AB2B058A5F3619562D1CD7D0E8EADE";
		    var i;
		    for(i = 0; i < data.length; i++) {
		    	html += '<font style="font-size: 13px;">' + ((i+1) + '. ') + '<a target="_blank" style="color: #fff;" href="' + url + '">' + data[i].content + '</a><br/><br/></font>';
		    }
			$("#allIndexAnnouncementMarquee").html(html);
		}
	}
	
	function singleAnnouncement() {
		if (single.readyState == 4) {
			var data = single.responseText;
			$("#latestAnnouncement").html(data);
		}
	}

