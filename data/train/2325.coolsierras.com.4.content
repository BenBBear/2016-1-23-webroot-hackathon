	<script type="text/javascript" src="http://www.easibuild.com/scripts/magnific/jquery.magnific-popup.js"></script>
	<script type="text/javascript" src="http://www.easibuild.com/scripts/flickrGal/jquery.flickrGal.js?v=1"></script>

	<script type="text/javascript" src="/scripts/jquery.awesomeCloud-0.2.min.js"></script>

	<script type="text/javascript" src="/scripts/waypoints.min.js"></script>

		<script type='text/javascript' src='http://cdn.easibuild.com/3.0/cycle/cycle.min.js'></script>



	<script  type="text/javascript">
		$('section.introduction').text($('section.introduction').text());
		//weather if class of weather exists grabn weather data, populate
		if ($(".weather").length > 0) {
			$.ajax("http://api.openweathermap.org/data/2.5/weather?q=cazorla,spain").done(function(data){
				console.log(data);

			});

		}


		$(document).ready(function() {


			$("a[href*='tel:']").on('click', function() {
				ga('send', 'event', 'tel', 'click', 'nav-buttons');
			});


			$(".orderText a").on('click', function() {
				ga('send', 'event', 'order', 'click', 'Home');
			});

					//CLICK TOPIC JS - hide all answers then slide down only the answers from the clicked topic id

			//Clicking on a list item slides up all other divs in list items then slides down the clicked one
			$('.accordion').on("click", function(){
			$('.accordion div').slideUp();
			$('div', this).slideDown();
			$('.accordion').removeClass('expanded');
			$(this).addClass('expanded');
			});




		$("div[data-loaded='notLoaded']").waypoint(function(direction){
			$thisSrc = $("figure",$(this)).attr("data-src");
			//console.log($thisSrc);
			$("figure",$(this)).attr("style",$thisSrc).fadeIn();

		},{offset: '90%',triggerOnce: true})
		/*
		$(".articleList .item").on({
			mouseenter: function () {
			//stuff to do on mouse enter
			$("figcaption",$(this)).fadeToggle();
		    },
		    mouseleave: function () {
		     //stuff to do on mouse leave
		     $("figcaption",$(this)).fadeToggle();
		    }
		});
		*/


		//Images in newstemplate need width and heights removed, alt placed below image
		$(".templateNews article p img").on("load",function(){
			var thisImg = $(this),
				thisDesc = $(this).attr("alt");
				thisImg.attr("width","").attr("height","");
			thisImg.wrap("<figure class='newsFeaturedPic'></figure>");
			thisImg.after("<figcaption>"+thisDesc+"</figcaption>")

		})

					//click on mobile menu item
		$(".mobileMenuIcon").on("click",function(){
			$("#topMenu ul").slideToggle();
			 this.classList.toggle( "active" );
		})

		//click on mobile submenu item
		$(".mobileSubMenuIcon").on("click",function(){
			$("#subMenu ul").slideToggle();
		})
			$("#tagCloud").awesomeCloud({
					"size" : {
						"grid" : 12,
						"normalize" : false,
						"factor" : 10
					},
					"color" : {
						"background" : "rgba(255,255,0)",
						"start" : "#f18b5c",
						"end" : "#10351c"
					},
					"options" : {
						"color" : "random-dark",
						"rotationRatio" : 0.4,
						"printMultiplier" : 2,
						"sort" : "highest"
					},
					"font" : "'Proxima N W01 Light', sans-serif",
					"shape" : "circle"
				});
			// !Flickr
			if ($(".galleryContain").length > 0) {

                    var apiKeyData = $(".galleryContain").attr("data-apiKey"),
                        userIdData = $(".galleryContain").attr("data-userId"),
                        startCollectionData = $(".galleryContain").attr("data-startCollection");



                    $(".galleryContain").showFlickrGallery({
                        apiKey : apiKeyData,
                        userId : userIdData,
                        startCollection : startCollectionData,
                        thumbSize : "q",
                        fullSize : "b",
                        catThumbSize : "q"
                    }).magnificPopup({delegate: 'a', type: 'image', gallery:{enabled:true}});
            }



            //Month / Year in News
            $(".month li").hide();
		$(".year li").hide();

		//MONTH CLICK FUNCTION
		if ($(".NewsStoryselected").length > "") {
			var thisParent = $(".NewsStoryselected").parent();
			$("li", thisParent).show();
				//MONTH CLICK FUNCTION
			$(".month").click(function(){
				$(".month li").hide(); //HIDE ALL LIST ITEMS
				$("li",this).show(); //NOW SHOW THE LIST ITEMS BELONGING TO THIS MONTH
			});
			$(".newsStoryselected").parent().show();
		}

		//YEAR CLICK FUNCTION
		var thisParent = $(".NewsStoryselected").parent();
		$("li", thisParent).show();
			//MONTH CLICK FUNCTION
		$(".year").click(function(){
			$(".year li").hide(); //HIDE ALL LIST ITEMS
			$("li",this).show(); //NOW SHOW THE LIST ITEMS BELONGING TO THIS MONTH
		});

					//No SVG Support, replace file extension with png
			if(!Modernizr.svg){
			  var images = document.getElementsByTagName("img");
			  for(var i = 0; i < images.length; i++){
			  	var thisImage = images[i];
			    var src = images[i].src;
			    var fileExtension = src.substring(src.lastIndexOf('.') + 1);
			    if (fileExtension == "svg") {
			    	src = src.replace(/\.[^/.]+$/, ".png");
				   thisImage.src = src;
			    }
			  }
			}

				//CYCLE PLUGIN
		//IF a Div with class '.fade' exists and has a length of more than 0 (has content) then run cycle plugin
		if ($(".fade").length > 0) {
			$(".fade").before('<div id="fadeNav">').cycle({timeout: 10000, pager:  '#fadeNav'});
			$("#fadeNav a").html("");
		}

		if ($(".simpleFade").length > 0) {
			$(".simpleFade").cycle({timeout: 1000});
		}



		//COMMMON FUNCTIONS PERHAPS THESE NEED MOVED INTO A STATIC JS FILE?:
		//LOGO CLICK TO HOME PAGE
		$("#logo").click(function(){window.location = "/default.asp"});

		//Click on box goes to features
		var $box = $(".bodyHome .box")
		if ($box.length > 0) {
			$box.click(function(){window.location = "/Features"});
		}

		//MONTH CLICK FUNCTION
		if ($(".NewsStoryselected").length > "") {
			var thisParent = $(".NewsStoryselected").parent();
			$("li", thisParent).show();
				//MONTH CLICK FUNCTION
			$(".month").click(function(){
				$(".month li").hide(); //HIDE ALL LIST ITEMS
				$("li",this).show(); //NOW SHOW THE LIST ITEMS BELONGING TO THIS MONTH
			});
			$(".newsStoryselected").parent().show();
		}

		//YEAR CLICK FUNCTION
		var thisParent = $(".NewsStoryselected").parent();
		$("li", thisParent).show();
			//MONTH CLICK FUNCTION
		$(".year").click(function(){
			$(".year li").hide(); //HIDE ALL LIST ITEMS
			$("li",this).show(); //NOW SHOW THE LIST ITEMS BELONGING TO THIS MONTH
		});

		//SELECT ONCHANGE FOR CATEGORIES
		if ($("#categorySelect").length > "") {
			$('#categorySelect').change(function() {
				var thisurl = $('#categorySelect :selected').attr('title');
				window.location = thisurl;
			});
		}
	});
	</script>
	<!-- END JQUERY -->


</body>
</html>