						</div>
					</div>
					<script type="text/template" id="template_event_new_entry">
					<% if (data !== false && data.error == 0 && data.events.length > 0) { %>
					<ul>
					<% for (var i = 0; i < data.events.length; i++) { %>
						<% var event = data.events[i]; %>
						<li><a href="https://hellohoken.sakura.ne.jp/production/secure/event/<%= event.id %>" target="_top" title="<%= event.title %>">
							<% if (event.status == "open") { %>
							<span><img src="/images/event/event_ok_min.png" alt="<%= event.status_name %>" class="list_status_ok" /></span>
							<% } else if (event.status == "closed") { %>
							<span><img src="/images/event/event_end_min.png" alt="<%= event.status_name %>" class="list_status_end" /></span>
							<% } else { %>
							<span><img src="/images/event/event_ng_min.png" alt="<%= event.status_name %>" class="list_status_ng" /></span>
							<% } %>
							<% if (event.is_anytime == "1") { %>
							<span class="event_data">常時開催</span>
							<% } else { %>
							<span class="event_data"><%= event.hi %>（<%= event.event_weekday %>）開催</span>
							<% } %>
							<span class="event_ttl"><%= event.title %></span></a></li>
					<% } %>
					</ul>
