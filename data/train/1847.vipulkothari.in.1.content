    <script>
        $(document).ready(function () {
            // Fix/Unfix Header
            var media = $('.header');
            var mediaTop = $(media).offset().top;

            $(document).scroll(function () {
                var scrollTop = $(document).scrollTop();

                //fix/unfix as necessary
                if (mediaTop < scrollTop) {
                    $(media).addClass('scroll');
                }
                else {
                    $(media).removeClass('scroll');
                }
            });

            //// Start Scroll Top Function ////
            $(window).bind('scroll', function () {
                if ($(this).scrollTop() > 200) {
                    $("#scrolltab").fadeIn('3000');
                }
                if ($(this).scrollTop() < 199) {
                    $("#scrolltab").fadeOut('3000');
                }
            });

            $('#scrolltab').live('click', function () {
                $("html, body").animate({ scrollTop: 0 }, 'slow');
            });
            //// End Scroll Top Function ////


        });
    </script>

