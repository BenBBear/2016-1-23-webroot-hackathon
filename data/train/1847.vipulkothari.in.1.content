    <!-- Clear Form Script -->
    <script type="text/javascript">
        function clearIt(field) {
            if (field.defaultValue == field.value) field.value = '';
            else if (field.value == '') field.value = field.defaultValue;
        }
        $(function () {
            $('input').keydown(function (e) {
                if (e.keyCode == 13) {
                    $(this).parents('form').submit();
                    return false;
                }
            });
        });
    </script>

