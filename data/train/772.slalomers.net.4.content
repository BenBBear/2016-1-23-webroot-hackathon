var curM = 0
var cbkColor = "#000080"
Ccount = new Array(7)
//[ ]内は親メニューのIDに対応、値は子メニューの数
Ccount[10] = 1
Ccount[20] = 7
Ccount[30] = 1
Ccount[40] = 1
Ccount[50] = 1
Ccount[60] = 8
Ccount[70] = 4
Mimage = new Array(7)
//[ ]内は親メニューのIDに対応、値は項目の状態　０：標準　１：強調　２：選択（子メニュー侮ｦ）
Mimage[10] = 0
Mimage[20] = 0
Mimage[30] = 0
