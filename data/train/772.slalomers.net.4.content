var cMenubk = new Image();
    cMenubk.src = "image/tm4.gif";

function setBGColor(layerName, color){
	if(document.layers) document.layers[layerName].bgColor=color
	if(document.all) document.all(layerName).style.backgroundColor=color
}

function showLayer(layerName){
	if(document.layers) document.layers[layerName].visibility = "show"
	if(document.all) document.all(layerName).style.visibility = "visible"
}

function hideLayer(layerName){
	if(document.layers) document.layers[layerName].visibility = "hide"
	if(document.all) document.all(layerName).style.visibility = "hidden"
}

function moveMmenu(i){
	if( i != curM ){
		for(j=i+1; j<=i +Ccount[i]; ++j){
			if(document.layers) setBGColor("cText" + j, null)
			if(document.all) setBGColor("cText" + j, "")
		}
		hideCmenu()
		if( Mimage[i] != 2 ){
			if(document.layers){
				document.layers["bkMenu"+i].document.images["mBack" +i].src=mMenubk.src
			}else{
				document.images("mBack" +i).src=mMenubk.src
			}
				Mimage[i] = 1
		}
	}
}

function outMmenu(i){
	if( Mimage[i] != 2 ){
		if(document.layers){
			document.layers["bkMenu"+i].document.images["mBack" +i].src=menubk.src
		}else{
			document.images("mBack" +i).src=menubk.src
		}
		Mimage[i] = 0
	}
}

function hideCmenu(){
	if(curM !=0 ){
		hideLayer("bkC" + curM)
		for(i=curM+1; i<=curM +Ccount[curM]; ++i){
			hideLayer("cText" + i)
		}
		if(document.layers){
			document.layers["bkMenu"+curM].document.images["mBack" +curM].src=menubk.src
		}else{
			document.images("mBack" +curM).src=menubk.src
		}
		Mimage[curM] = 0
		curM=0
	}
}

function clickMmenu(i){
	if(Ccount[i] != 0){
		if(document.layers){
			document.layers["bkMenu"+i].document.images["mBack" +i].src=clickbk.src
		}else{
			document.images("mBack" +i).src=clickbk.src
		}
		showLayer("bkC" + i)
		for(j=i+1; j<=i +Ccount[i]; ++j){
			showLayer("cText" + j)
		}
		curM=i
		Mimage[curM] = 2
	}
}

function moveCmenu(m,i){
	for(j=m+1; j<=m +Ccount[m]; ++j){
		if(document.layers) setBGColor("cText" + j, null)
		if(document.all) setBGColor("cText" + j, "")
	}
	setBGColor("cText" + i, cbkColor)
}

