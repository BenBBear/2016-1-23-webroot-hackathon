$(document).ready(function(){
	var errorcontainer = "<div class='errorplace'></div>"
	$("body").on("click","#ContactSendBtn",function(){
		var SendIfValid = true;
		var _ContactSendURL = $("#ContactSendURL").val()
 		var _name = $("#hfld_Name").val()
