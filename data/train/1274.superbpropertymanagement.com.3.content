<script type="text/javascript">
	$(function () {
		resizeContent(false);
		window.onresize = function () { $('#menuBox').addClass('Hided'); $('.divMenu').removeClass('divMenuHide'); resizeContent(false); }
		if ($('.divMenu:visible').length > 0) $('#menuBox').addClass('Hided');
		$('.divMenu').unbind('click').bind('click', function (event) {
			if (typeof (event.stopPropagation) == 'function') event.stopPropagation();
			if (typeof (event.preventDefault) == 'function') event.preventDefault();
			var mb = $('#menuBox'), mo = $('.divMenu'), cls = 'divMenuHide', hiden = 'Hided';
			function show() {
				mo.addClass(cls);
				mb.removeClass(hiden);
				resizeContent(true);
			}
			function hide() {
				mo.removeClass(cls);
				mb.addClass(hiden);
				resizeContent(false);
			}
			if (mo.hasClass(cls))
				hide();
			else
				show();
		});
	});
	</script> 
