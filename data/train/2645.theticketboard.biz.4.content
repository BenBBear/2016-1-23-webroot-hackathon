<script type="text/javascript">
$(document).ready(function() {
    // determine if the user is opting out on this page load
    if(GetQueryStringValue('tn_mcan') == 'true' && GetQueryStringValue('useroptno') == 'true') {
        mobileOptOut = 'true';
        TNR4CreateCookie('tnSegMobileOptOut', mobileOptOut);
    }

    if(continuingSession == null) {
        var referrer = ('').trim();
        var source = GetQueryStringValue('utm_source');
        if(source) {
            source = source.trim();
            var medium = GetQueryStringValue('utm_medium');
            if(medium)
            medium = medium.trim();
        } else {
            source = getGoogleCampaignSource();
            medium = getGoogleCampaignMedium();
        }
        
        if(GetQueryStringValue('ref') == 'cj') {
            source = 'Affiliate';
            medium = 'CJ';
        } else if(GetQueryStringValue('kbid')) {
            source = "Affiliate";
            medium = "KB";
        }
        
        marketingChannel = source && source != null ? (source + '-' + (medium && medium != null ? medium : 'Unknown')) : referrer != '' ? referrer : 'Unknown';
        TNR4CreateCookie('tnSegSessionMarketingChannel', marketingChannel);
        returningVisitor = TNR4ReadCookie('tnSegReturningVisitor');
        if(returningVisitor != null)
        TNR4CreateCookie('tnSegSessionReturningVisitor', 'true');
        TNR4CreateCookie('tnSegReturningVisitor', 'true', 365);

        TNR4CreateCookie('tnSegSession', 'true');
    } else {
        var cookieValue = TNR4ReadCookie('tnSegSessionMarketingChannel');
        marketingChannel = cookieValue != null ? cookieValue : marketingChannel;
    }
});
</script>

