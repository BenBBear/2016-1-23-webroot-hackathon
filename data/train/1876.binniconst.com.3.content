<!-- The JavaScript -->
<script type="text/javascript" src="http://binniconst.com/wp-content/themes/denizy/homenav/jquery.bgpos.js"></script>
<script type="text/javascript">
$(function() {
/* position of the <li> that is currently shown */
var current = 0;

var loaded  = 0;
for(var i = 1; i <4; ++i)
$('<img />').load(function(){
++loaded;
if(loaded == 3){
$('#bg1,#bg2,#bg3').mouseover(function(e){

	var $this = $(this);
	/* if we hover the current one, then don't do anything */
	if($this.parent().index() == current)
		return;

	/* item is bg1 or bg2 or bg3, depending where we are hovering */
	var item = e.target.id;

	/*
	this is the sub menu overlay. Let's hide the current one
	if we hover the first <li> or if we come from the last one,
	then the overlay should move left -> right,
	otherwise right->left
	 */
	if(item == 'bg1' || current == 2)
		$('#homenavmenu .sub'+parseInt(current+1)).stop().animate({backgroundPosition:"(-319px 0)"},400,function(){
			$(this).find('li').hide();
		});
	else
		$('#homenavmenu .sub'+parseInt(current+1)).stop().animate({backgroundPosition:"(319px 0)"},400,function(){
			$(this).find('li').hide();
		});

	if(item == 'bg1' || current == 2){
		/* if we hover the first <li> or if we come from the last one, then the images should move left -> right */
		$('#homenavmenu > li').animate({backgroundPosition:"(-800px 0)"},0).removeClass('bg1 bg2 bg3').addClass(item);
		move(1,item);
	}
	else{
		/* if we hover the first <li> or if we come from the last one, then the images should move right -> left */
		$('#homenavmenu > li').animate({backgroundPosition:"(800px 0)"},0).removeClass('bg1 bg2 bg3').addClass(item);
		move(0,item);
	}

	/*
	We want that if we go from the first one to the last one (without hovering the middle one),
	or from the last one to the first one, the middle menu's overlay should also slide, either
	from left to right or right to left.
	 */
	if(current == 2 && item == 'bg1'){
		$('#homenavmenu .sub'+parseInt(current)).stop().animate({backgroundPosition:"(-319px 0)"},400);
	}
	if(current == 0 && item == 'bg3'){
		$('#homenavmenu .sub'+parseInt(current+2)).stop().animate({backgroundPosition:"(319px 0)"},400);
	}


	/* change the current element */
	current = $this.parent().index();

	/* let's make the overlay of the current one appear */

	$('#homenavmenu .sub'+parseInt(current+1)).stop().animate({backgroundPosition:"(0 0)"},400,function(){
		$(this).find('li').fadeIn();
	});
});
}
    }).attr('src', 'http://binniconst.com/wp-content/themes/denizy/homenav/images/'+i+'.jpg');


/*
dir:1 - move left->right
dir:0 - move right->left
 */
function move(dir,item){
    if(dir){
        $('#bg1').parent().stop().animate({backgroundPosition:"(0 0)"},400);
        $('#bg2').parent().stop().animate({backgroundPosition:"(-319px 0)"},400);
        $('#bg3').parent().stop().animate({backgroundPosition:"(-638px 0)"},400,function(){
            $('#homenavmenuWrapper').removeClass('bg1 bg2 bg3').addClass(item);
        });
    }
    else{
        $('#bg1').parent().stop().animate({backgroundPosition:"(0 0)"},400,function(){
            $('#homenavmenuWrapper').removeClass('bg1 bg2 bg3').addClass(item);
        });
        $('#bg2').parent().stop().animate({backgroundPosition:"(-319px 0)"},400);
        $('#bg3').parent().stop().animate({backgroundPosition:"(-638px 0)"},400);
    }
}
});

