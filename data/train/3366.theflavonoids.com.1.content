	/**
	 * Validate the MakeAnOfferForm
	 *
	 * @return boolean
	 */
	function validateMakeAnOfferForm() {

		var submitForm = false;

		switch($('input[name=offer-type]:checked').val()){
			case 'offer': 
				submitForm = submitCalculatePreviewOffer($('#makeAnOfferForm'));
				break;
			case 'finance':
				submitForm = submitCalculatePreviewFinance($('#makeAnOfferForm'));
				break;
			case 'lease':
				submitForm = submitCalculatePreviewLease($('#makeAnOfferForm'));
				break;
		} 

		return submitForm;
	}	
	
	$(document).ready(function(){

		// Initial state of Forms
					$('#make-offer-container').show();
			$('#financing-options-container').hide();
			$('#lease-options-container').hide();
			
				$('input[name=offer-type]').live('change', function() {
			if ($(this).val() == 'finance') {
				$('#financing-options-container').show();
				$('#lease-options-container').hide();
				$('#make-offer-container').hide();
			} else if ($(this).val() == 'lease') {
				$('#lease-options-container').show();
				$('#financing-options-container').hide();
				$('#make-offer-container').hide();
			} else {
				$('#financing-options-container').hide();
				$('#lease-options-container').hide();
				$('#make-offer-container').show();
			}
		});		
	
		// if the financed payment option is selected by default (from pressing the
		// 'back' button onthe browser, then show the finance form
		if ($('input[name=offer-type]:checked').val() == 'finance') {
			$('#financing-options-container').show();
			$('#make-offer-container').hide();
			$('#lease-options-container').hide();
		}
		
		// if the leased payment option is selected by default (from pressing the
		// 'back' button onthe browser, then show the finance form
		if ($('input[name=offer-type]:checked').val() == 'lease') {
