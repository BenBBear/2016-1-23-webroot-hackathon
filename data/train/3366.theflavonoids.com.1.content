	 * Validate Offer Amount for regular Offer
	 *
	 * @param object form (Required) 
	 * @param string elementId (Optional) If ommitted will validate entire form
	 * @return boolean
	 */
	function submitCalculatePreviewOffer(form, elementId){
	
		var amount = parseFloat($(form).find('#offer_amount').val());
		var amountMin=5000.00;
		if(isNaN(amountMin)) amountMin=250;
		if(isNaN(amount) || amount < amountMin){
			$(form).find('.inline-error').show();
			$("#amountMin").html(amountMin);
			//$(form).find('#offer_amount').addClass('border-color-red');
		} else {
			$(form).find('.inline-error').hide();
			
			//$(form).find('#offer_amount').removeClass('border-color-red');
			return true;
		}
		
		return false;
	}	

		$(document).ready(function(){
		$('#makeAnOfferForm #offer_amount').blur(function() {
			submitCalculatePreviewOffer($(this).parents('form'));
		});
	});

</script>


			
