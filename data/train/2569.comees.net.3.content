		snowDiv.style.webkitAnimationName = 'fade, drop';
		divTag.style.webkitAnimationName = spinAnimationName;

		snowDiv.style.MozAnimationName = 'fade, drop';
		divTag.style.MozAnimationName = spinAnimationName;

		snowDiv.style.animationName = 'fade, drop';
		divTag.style.animationName = spinAnimationName;
		/* Figure out a random duration for the fade and drop animations */
		var fadeAndDropDuration = fDurationValue(fRandomFloat(5, 11));

		/* Figure out another random duration for the spin animation */
		var spinDuration = fDurationValue(fRandomFloat(4, 8));
		/* Set the -webkit-animation-duration property with these values */
		snowDiv.style.webkitAnimationDuration = fadeAndDropDuration + ', ' + fadeAndDropDuration;
		snowDiv.style.MozAnimationDuration = fadeAndDropDuration + ', ' + fadeAndDropDuration;
		snowDiv.style.animationDuration = fadeAndDropDuration + ', ' + fadeAndDropDuration;

		var snowDelay = fDurationValue(fRandomFloat(0, 5));
		snowDiv.style.webkitAnimationDelay = snowDelay + ', ' + snowDelay;
		divTag.style.webkitAnimationDuration = spinDuration;

		snowDiv.style.mozAnimationDelay = snowDelay + ', ' + snowDelay;
		divTag.style.MozAnimationDuration = spinDuration;

		snowDiv.style.animationDelay = snowDelay + ', ' + snowDelay;
		divTag.style.animationDuration = spinDuration;
		// add the <img> to the <div>
		snowDiv.appendChild(divTag);

		/* Return this img element so it can be added to the document */
		return snowDiv;
	}
	/* Calls the fInit function when the "Falling snow" page is full loaded */
	window.addEventListener('load', fInit, false);
</script></div>



