<script>
    var steps = [{"1":"Poor","2":"Fair","3":"Good","4":"Great","5":"Excellent"},{"1":"Poor","2":"Fair","3":"Good","4":"Great","5":"Excellent"},{"1":"Poor","2":"Fair","3":"Good","4":"Great","5":"Excellent"},{"1":"Poor","2":"Fair","3":"Good","4":"Great","5":"Excellent"}];
    function validateReview(){
        var errorFound = false;
        var errorsContainer = $('.review-errors');
        $('.review-errors').html('');
        $('.review-errors').hide();
        $('form.reviews-form .required').each(function(){
            if (!$(this).val()) {
                $('.review-errors').show();
                errorFound = true;
                errorsContainer.append($(this).data('error_content') + '<br>');
            }
        });
        return !errorFound;
    }
    $(function () {
        $('.write_a_review, .add-review').on('click', function(e){
            e.preventDefault();
            var review_modal = $('.review-modal');
            review_modal.dialog({
                modal:true,
                resizable: false,
                draggable: false,
                dialogClass: 'review-dialog',
                width: '400px'
            });
        });
        // open review immediately
        if (window.location.hash == "#write_review") {
            $('.write_a_review, .add-review').first().trigger('click');
        }
        $('.slider').slider({
            min: 1,
            max: 5,
            range: 'min',
            animate: true,
            value: 5,
            slide: function (event, ui) {
                var catId = $(this).data('id');
                var stepId = ui.value;
                $('#cat_'+catId).val(stepId);
                $('#step_'+catId).html(steps[catId][stepId]);
            }
        });
    });
    $('form.reviews-form').submit(function(e) {
        var reviewForm = $(this);
        e.preventDefault();
        $('#write-review').prop("disabled",true).val('Saving...');
        if (!validateReview()) {
            $('#write-review').prop("disabled",false).val('Submit');
            return false;
        }
        $.ajax({
            type:'POST',
            url: '/form/facility_reviews/81369/form?wwmodule=facility_reviews',
            data: $(this).serialize()+'&write_review=true',
            success: function(data) {
                if (data.success) {
                    reviewForm.html(data.message);
                } else {
                    $('.review-errors').html(data.message).show();
                }
            },
            error: function(data) {
                $('.review-errors').html(data.message).show();
            }
         });
    });
</script>
