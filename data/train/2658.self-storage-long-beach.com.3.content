<script type="text/javascript">
function selectFacilityTabA(tab) {
    //check if the tab has been google-translated
    if(tab.target && tab.target.nodeName && tab.target.nodeName == "FONT") {
        tab = $(tab.target).parent().parent();
    }

    if (tab.target) {
        tab = $(tab.target);
    } else {
        tab = $(tab);
    }
    
    //There are no tabs.
    if(!tab.length)
        return false;
    
    var hash = tab[0].hash;
    
    if (tab.hasClass("facility_active")) {
        $(hash).fadeIn();
        return false;
    }

    // hide previous active
    $("ul.tabs_a li a").removeClass("facility_active");
    $(".tab_a_content").hide();

    // show new active
    $("ul.tabs_a a[href='" + hash + "']").addClass("facility_active");
    $(hash).fadeIn();

    return false;
}

function selectFacilityTabB(tab) {
    //check if the tab has been google-translated
    if(tab.target && tab.target.nodeName && tab.target.nodeName == "FONT") {
        tab = $(tab.target).parent().parent();
    }
    if (tab.target) {
        tab = $(tab.target);
    } else {
        tab = $(tab);
    }
    
    //There are no tabs.
    if(!tab.length)
        return false;
    
    var hash = tab[0].hash;

    if (tab.hasClass("facility_active")) {
        $(hash).fadeIn();
        return false;
    }

    // hide previous active
    $("ul.tabs_b li a").removeClass("facility_active");
    $(".tab_b_content").hide();

    // show new active
    $("ul.tabs_b a[href='" + hash + "']").addClass("facility_active");
    $(hash).fadeIn();

    return false;
}

function sizeTabs(tab_group) {
    //First get the widths to aim for.
    var all_tabs = $('ul.' + tab_group).width();
    var currentWidth = 0;
    var tabs = $('ul.' + tab_group + ' li');

    for(var i = 0; i < tabs.length; i++) {
        currentWidth += tabs.eq(i).outerWidth();
    }

    //How much to add to each tab.
    var extraWidth = Math.floor((all_tabs - currentWidth) / tabs.length);

    //Extra due to rounding. This will go to the last tab since it will rarely
    //  be large enough to be noticed.
    var remainder = Math.floor((all_tabs - currentWidth)) % tabs.length;

    for(var i = 0; i < tabs.length; i++) {
        tabs.eq(i).css( {width: (extraWidth + tabs.eq(i).width()) + 'px' } );
    }

    //Make sure that the last tab extends all the way to the end. Sometimes this will cause it to break in Chrome, though.
            var lastTab = $('ul.' + tab_group + ' li.last');
        lastTab.css({ width: lastTab.width() + remainder + 'px' });
    }

function showFacilityModalFor(div) {
    if (div.charAt(0) != '#') {
        div = '#' + div;
    }
    $(div).dialog({
        modal: true,
        width: 565,
        height: 525,
        resizable: false,
        draggable: false,
        dialogClass: 'facility_form',
        closeText: 'Close'
    }).dialog('open');
    
    // Make sure the overlay behind it is full screen
    $('.ui-widget-overlay').height($(document).height());
}

$(document).ready(function(){	
    $(".tab_a_content").hide(); //Hide all content initially
    $(".tab_b_content").hide();
    var tab = ($("ul.tabs_a li a.facility_active").length > 0)? $("ul.tabs_a li a.facility_active") : $("ul.tabs_a li:first a");
    var oldtab = ($("ul.tabs_b li a.facility_active").length > 0)? $("ul.tabs_b li a.facility_active") : $("ul.tabs_b li:first a");

    selectFacilityTabA(tab); // select the first tab
    selectFacilityTabB(oldtab); // select the first tab

    //On Click Event
    $("ul.tabs_a li a").click(selectFacilityTabA);
    $("ul.tabs_b li a").click(selectFacilityTabB);

    sizeTabs('tabs_a');
    sizeTabs('tabs_b');

        LoadTooltips();
        
    if (window.location.hash) {
        $("ul.tabs a[href='" + window.location.hash + "']").click();
    }
});

$(window).ready(function() {
    // Iterate over slides - if one has a URL set, hide the mag glass
    $('.module_slideshow_facility li a').each(function() {
        if (!$(this).hasClass('lightbox')) {
            $('#slideshow_magnify').remove();
        }
    });
});
</script>

