<script type="text/javascript">
$(function(){
    var cntMax = 5;
    obj = $('#sliderTop').bxSlider({
        auto: true,
        autoHover: true,
        speed: 1000,
        pager: true,
        pagerCustom: '.bxThumbnail',
        onSliderLoad: function(){
            var cntSum = obj.getSlideCount();
            if ( cntSum > cntMax ) {
                for ( var idx=cntMax; idx<cntSum; idx++ ) {
                   document.getElementById('tn'+idx).style.display = 'none';
                }
            }
        },
        onSlideBefore: function( slideElement, oldIndex, newIndex ) {
            var cntSum = obj.getSlideCount();
            if ( cntSum > cntMax ) {
                var cntDiv = parseInt( newIndex / cntMax );
                for ( var idx = 0; idx < cntSum; idx++ ) {
                    document.getElementById('tn'+idx).style.display = ( idx >= cntMax * cntDiv && idx < cntMax * ( cntDiv + 1 ) ) ? 'inline' : 'none';
                }
            }
        }
    });
});
</script>
