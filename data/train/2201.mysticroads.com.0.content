        var google_conversion_value = 0;

        /*****************************************************************************
         * Placeholder behaviour
         ****************************************************************************/
        //set start value
        var value = 'mysticroads';
        $('.domeincheck_input').change(function () {
            //if the input value is not the start value
            if($(this).val() != value) {
                //add class to style the value
                $(this).addClass('input_changed');
            }
        });

        /*****************************************************************************
         * Youtube video
         ****************************************************************************/
        function showYoutubeVideo(holder, video_id, autoplay) {
            //build url string
            var video_src = "https://www.youtube.com/embed/" + video_id + "?rel=0&vq=hd1080";
            if (autoplay) {
                video_src += "&autoplay=1";
            }
            //hide image
            $('.start_img_video', holder).hide();
            //show video container and insert video url
            $('.video_frame', holder).show();
            $('.video_frame', holder).html('<iframe id="video_iframe" width="560" height="315" src="' + video_src + '" frameborder="0" allowfullscreen></iframe>');
        }

        //when clicked, show Youtube video instead of image
        $('.play_video').on('click', function(e) {
            e.preventDefault();
            var holder = $('.responsiveYoutubeVideo');
            showYoutubeVideo(holder,'uklAr147nQI', 1);
        });
    </script>
