<script type="text/javascript">
 (function($){
    var area_suggest_data;
    var free_word_suggest_data;
    $.ajax({
        type:"GET",
        url:"/direct/topics/suggest_json/",
        dataType:"json",
        success:function(data){
            area_suggest_data = data['area_suggest'];
            free_word_suggest_data = data['free_word_suggest'];
            attatch_js_events(area_suggest_data, free_word_suggest_data);
        }
    });

    if(typeof String.prototype.startsWith != 'function'){
        String.prototype.startsWith = function (str){
            return this.indexOf(str) == 0;
        };
    }
    
    function choiceSuggest(suggest_data){
        return function(request, response){
            if(request.term.length > 1){
                var result = suggest_data.reduce(function(result, item){
                    if(item.label.startsWith(request.term)){
                        result.push(item.value);
                    }
                    return result;
                }, []);
                response(result);
            }
        }
    }

    function findWord(word, resource) {
        if (word) {
            for (var i = 0; i < resource.length; i++) {
                var d = resource[i];
                if (d.value == word) {
                    return d;
                }
            }
            return false;
        } else {
            return true;
        }
    }

    function attatch_js_events(area_suggest_data, free_word_suggest_data){
        $(function(){
            $( "#area_station" ).autocomplete({
                source: choiceSuggest(area_suggest_data)
            });

            $("#area_station").on('blur', function() {
                if (findWord($(this).val(), area_suggest_data)) {
                    $('#suggest_err_msg').hide();
                } else {
                    $('#suggest_err_msg').show();
                }
            });
    
            $("#area_station").on('focus', function() {
                $('#suggest_err_msg').hide();
            });

            $( "#free_word" ).autocomplete({
                source: choiceSuggest(free_word_suggest_data)
            });
    
            $('#header_search_form').on('submit', function() {
                var area_station = $(this).find('#area_station').val();
                var no_area_station_flg = 1;
                if (findWord(area_station, area_suggest_data)) {
                    no_area_station_flg = 0;
                }
                $('#header_search_form input[name="no_area_station_flg"]').val(no_area_station_flg);
            });
            
            // こだわり検索
            $( ".search_area_station" ).autocomplete({
                source: choiceSuggest(area_suggest_data)
            });

            $(".search_area_station").on('blur', function() {
                if (findWord($(this).val(), area_suggest_data)) {
                    $('#kodawari_suggest_err_msg').hide();
                } else {
                    $('#kodawari_suggest_err_msg').show();
                }
            });
    
            $(".search_area_station").on('focus', function() {
                $('#kodawari_suggest_err_msg').hide();
            });

            $( ".search_topics_keyword" ).autocomplete({
                source: choiceSuggest(free_word_suggest_data)
            });
        });
    }
})(jQuery);
</script>




