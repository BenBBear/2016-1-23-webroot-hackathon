<script type="text/javascript">
(function($){
  $(function(){
    var areaHash = {"01" : "hokkaido",
                  "02-03-04-05-06-07" : "tohoku",
                  "15-16-17-18-19-20-21-22-23" : "chubu",
                  "08-09-10-11-12-13-14" : "kantou",
                  "24-25-26-27-28-29-30" : "kinki",
                  "31-32-33-34-35" : "chugoku",
                  "36-37-38-39" : "shikoku",
                  "40-41-42-43-44-45-46" : "kyushu",
                  "47" : "okinawa"};

    var CHECK_SELECTED_FLG = true; // エリア・都道府県以外が選択状態ならfalse
    
    // 最初のactionを保管
    var search_form = $('#search_param_form');
    var def_form_action = search_form.attr('action');
    var region_action_head = '/region/';
    var area_action_head = '/area/';
    
    // Enterでの送信を無効化
    $(search_form).find('input').keypress(function(e) {
        if ((e.which && e.which === 13) || (e.keyCode && e.keyCode === 13)) {
          return false;
        } else {
          return true;
        }
    });

    $(document).on('click', '#form_submit_btn', function(){
        var form_action = '';
        var area_select = $('[name="search_ext_col_15[]"]').val();
        var tdfk_select = $('#tdfk_select').val();

        // フォーム内の選択状態をチェック
        CHECK_SELECTED_FLG = check_input_selected();

        // trueの場合はactionの書き換え
        if (CHECK_SELECTED_FLG) {
            if(tdfk_select){
                form_action = area_action_head + tdfk_select + '/';
            } else if(area_select){
                form_action = region_action_head + areaHash[area_select] + '/';
            } else {
                form_action = def_form_action;
            }
            // エリア・都道府県へ
            location.href = form_action;
        } else {
            search_form.submit();
        }
    });
    
    function check_input_selected() {
        // 初期化
        CHECK_SELECTED_FLG = true;
        
        // チェックボックスのチェック
        $(search_form).find('input[type=checkbox]:checked').each(function() {
          CHECK_SELECTED_FLG = false;
        });
        
        // selectのチェック
        $(search_form).find('.check_select').each(function() {
          if ($(this).val()) {
            CHECK_SELECTED_FLG = false;
          }
        });
        
        // フリーワードのチェック
        var check_freeword = $(search_form).find('[name=topics_keyword]').val();
        if(check_freeword) {
          CHECK_SELECTED_FLG = false;
        }
        
        return CHECK_SELECTED_FLG;
    }
      
    function filter_tdfk(area){
        var tdfk_ids = area.split("-");
        var tdfk_val = $("#tdfk_select").val();
        $("#tdfk_select option").each(function(){
            var $this = $(this);
            var text = $this.text();
            var value = $this.val();
            
            var $esb_item = $('#tdfk_select_div .esb-item:contains("'+text+'")');
            if(value === '' || $.inArray(value, tdfk_ids) >= 0){
                $this.removeAttr("disabled");
                $esb_item.show();
            } else {
                $this.attr("disabled", "disabled");
                $esb_item.hide();
                if( value == tdfk_val ){
                    $("#tdfk_select_div .esb-displayer").text("");
                    $("#tdfk_select").val("");
                }
            }
        });
    }

    function toggle_checked($this){
        if($this.is(":checked")){
            $this.parent().addClass("select");
        } else {
            $this.parent().removeClass("select");
        }
    }
    
    function reset(){
        $("input[type=checkbox]:checked").each(function(){
            $(this).removeAttr("checked");
            toggle_checked($(this));
        });

        $("select.easy-select-box").easySelectBox("select", "", true);

        $("input[type=text]").val("");
        
        $('#search_reset_dialog').dialog({
        modal: true,
        buttons: {
            "OK": function(){
                $(this).dialog('close');
                }
            }
        });
    }

    function toggleRentalContents(){
        if($("#0501011").is(":checked")){
            $(".type_goods_rental input").removeAttr("disabled");
            $(".type_goods_rental input").parent().removeClass("disabled");
        } else {
            $(".type_goods_rental input").parent().addClass("disabled");
            $(".type_goods_rental input").parent().removeClass("select");
            $(".type_goods_rental input").removeAttr("checked");
            $(".type_goods_rental input").attr("disabled", "disabled");
        }
    }

    $("#search_change").on("click", function() {
        $(this).css("display","none");
        $("#search_form").slideToggle();
    });
    
    $(".close").on("click", function() {
        $("#search_change").slideToggle();
        $("#search_form").slideToggle();
        
        $('html, body').animate({
            scrollTop: $("#head_box").offset().top,
            scrollLeft:0
        }, 400);
    });
    
    $("#search_param_form input[type=checkbox]").change(function(){ toggle_checked($(this)); });
    $("#search_param_form input[type=checkbox]").each(function(){ toggle_checked($(this)); });
    
    $("#reset_button").on('click', function(){
        reset();
    });
    
    $('select.easy-select-box').easySelectBox({
        onClick: function(data){
            if($(this).parent().attr("id") == "area_select_div"){
                var sel = $('#tdfk_select');
                $('#tdfk_select_div .esb-displayer').text(sel[0][0].innerText);
                filter_tdfk(data.value);
            }
            $(this).parent().children("select").change();
        }
    });
    
        
    $("#0501011").change(toggleRentalContents);
    toggleRentalContents();
  });
})(jQuery);
</script>
