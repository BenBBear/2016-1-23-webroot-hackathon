	<script type="text/javascript">
		jQuery(document).ready(function($){
			$('#morelinks ul:first-child').db_dropdown({
				slideDownSpeed : 200,
				orientation: 'vertical',
				firstLevelOffset: 0,
				firstLevelPadding: 0,
				secondLevelPadding: 0,
				secondLevelOffset: 0
			}); 
					
			if ($('#divNavBlock').next('.search_options').length) {
				$('body').addClass('search_options_organizer');
			}
			
			var keep_inputs = '#cityzip, #searchResultsQueryInput';
			$(keep_inputs).baynet('inputs_keep_init_val');
			$('#access').baynet('menus_add_submenu_classes');
	
			$(window).load(function() {
				$('.flexslider').flexslider({
					animation: "slide",
					controlsContainer: ".flex-container",
					controlNav : false
				});
			});
			
			// Footer Offices
			///////////////////////////////////////////////////
			var offices = $('.b-office');
			var offices_onMouseEnter = function(){
				var $this = $(this);
				var $officeInfo =$this.find('.b-office-info');
				
				$this.unbind('mouseenter', offices_onMouseEnter);
				$officeInfo.stop(true, true).fadeIn(200);
				$this.bind('mouseleave', offices_onMouseLeave);
				
			} 
			var offices_onMouseLeave = function(){
				var $this = $(this);
				var $officeInfo =$this.find('.b-office-info');
				
				$this.unbind('mouseleave', offices_onMouseLeave);
				$officeInfo.stop(true, true).fadeOut(200);
				$this.bind('mouseenter', offices_onMouseEnter);
			} 
			
			offices.bind({mouseenter : offices_onMouseEnter});
			
			
			// sidebar moving links
			///////////////////////////////////////////////////
			var moreLinks = $('#morelinks span');
			var links_onMouseEnter = function(){
				var $this = $(this);
				$this.unbind('mouseenter', links_onMouseEnter);
				
				$this.stop(true, true).animate({left:"+=" + 30}, 300);
				$this.bind('mouseleave', links_onMouseLeave);
			}
			var links_onMouseLeave = function() {
				var $this = $(this);
				$this.unbind('mouseleave', links_onMouseLeave);
				
				$this.stop(true, true).animate({left:0 },300);
				$this.bind('mouseenter', links_onMouseEnter);
			}
			
			moreLinks.bind('mouseenter', links_onMouseEnter);
		});
	</script>
	
	
