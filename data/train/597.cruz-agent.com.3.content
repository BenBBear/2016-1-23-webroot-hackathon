	<h2 class="flex-title">Quick Search</h2>
	
	<script type="text/javascript">
jQuery(document).ready(function($){
	var selected = false;
	
	$('#cityzip').focus(function(){
		if (selected == false && $(this).attr('value') == 'City / Zip Code / MLS#') {
			$(this).attr('value', '');
			selected = true;
		}
	}).blur(function(){
		if ($(this).attr('value') == '') {
			$(this).attr('value', 'City / Zip Code / MLS#');
			selected = false;
		}
	});
	
	$('#qs_submit').click(doSearch);
	$('#cityzip').keydown(function(e) {
  		if(e.keyCode == '13') {
			doSearch();
		}
	});

	function doSearch() {
		var url = "";
		var properties = "";
		var searchString = $.trim($('#cityzip').val());
		var options = {
			minprice : $('#minprice').val() || "",
			maxprice : $('#maxprice').val() || "",
			minbed   : $('#minbed').val()   || "",
			minbath  : $('#minbath').val()  || "",
			sqft     : $('#sqft').val()     || ""
		};
		var propertyTypes = {
			residential : $('#resClass')  || "",
			condo       : $('#conClass')  || "",
			land        : $('#landClass') || ""
		
		}
		
		if(searchString.length > 0 && searchString != $.trim('City / Zip Code / MLS#')) {
			url += "&searchstr=" + escape(searchString);
		} else {
			alert("Enter a city/zipcode/area/address/mlsnumber");
			$('#cityzip').focus();
			return;
		}	
		
		$.each(options, function(option, value){
			if(value.length > 0)url += "&" + option + "=" + value;
		});
				
		$.each(propertyTypes, function(property, value){
			if(value != null && value.is(':checked')) {
				properties += "," + value.val();
			}
		});
		
		if (properties == "") {
			alert('Select a property type');
			return;
		} else {
			// add to url and remove the comma at the begining of string
			url += "&clsn=" + properties.substring(1);
		}

		window.location = "/Search/QuickSearchAnalysis.aspx?pp=-2" + url;	
	}
});
</script>

