<!-- JS for Access Code-->
<script src="/Scripts/jquery.placeholder.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#code').blur(function () {
            this.value = this.value.toUpperCase();
        });
        $("#submitButton").click(function () {
            if ($('#code').val() == "") {
                $("span.ui-dialog-title").html("Error");
                $("#logoutDialogBox p").html("Please enter an Access Code. ");
                $("#logoutDialogBox").dialog(dialogOptions).dialog("open");
                return false;
            }
            else if ($('#code').val().length < 6) {
                $("span.ui-dialog-title").html("Error");
                $("#logoutDialogBox p").html("Access Code is not valid.");
                $("#logoutDialogBox").dialog(dialogOptions).dialog("open");
                return false;
                    }
                    else {
                        $("#AccessCodeForm").submit();
                    }
        });
    });
</script>
