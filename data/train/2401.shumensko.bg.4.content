    var loadedSoFar = 0;
    var line = null;
    var single_init = false;
    var pictures = null;
    var img = new Image();

    function preloadResources(page, direction, diferred) {

        resetLoader(direction);
        pictures = new Array();
        if (pictures !== undefined && resources[page].length > 0) {
            for( var i = 0; i < resources[page].length; i++ ) {
                if((flag_small_image_size && resources[page][ i ].indexOf("_s")!=-1) || (!flag_small_image_size && resources[page][ i ].indexOf("_s")==-1)){
                    pictures.push(resources[page][ i ]);
                }
            }
            for( var i = 0; i < pictures.length; i++ ) {
                img = null;
                img = new Image();
                img.src = pictures[ i ];
                img.onload = function( ) {
                    updateProgress(diferred);
                }
            }
        } else {
            diferred.resolve();
            $(".loader.gray-bg").fadeOut(500);
            $("#loader").fadeOut(500);
        }
    };


     function updateProgress(diferred) {
        loadedSoFar++;
        var prc_complete = parseInt((100*loadedSoFar)/ pictures.length)/100;
        line.animate(prc_complete, function() {
            //if (diferred !== null) {
            if(loadedSoFar==pictures.length){
                diferred.resolve();
                $(".loader.gray-bg").fadeOut(500);
                $("#loader").fadeOut(500);
            }
        });

    }

    function resetLoader(direction) {
        if (!single_init) {
            var element = document.getElementById('loader');
            line =  new ProgressBar.Line(element, {
                        color: '#FCB03C',
                        strokeWidth: 1,
                        trailWidth: 1
                    });
            single_init = true;
        }
        if (direction == "up") {
            $("#loader").css( "top", 0 );
        } else {
            $("#loader").css( "bottom", 0 );            
        }
        line.set(0);
        $('.loader.gray-bg, #loader').fadeIn();
        loadedSoFar = 0;
    }

</script>


<script type="text/javascript">

    var HOME_URL="http://www.shumensko.bg/";
