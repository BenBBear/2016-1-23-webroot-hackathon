    function fixDateYear(year){
        var today = new Date();
        var return_value = String(today.getFullYear()).substring(2,4);
        if(year.length==1){
            return_value="0"+year;
        }else if(year.length==2){
            return_value=year;
        }else if(year.length==3){
            return_value=year.substring(1,3);
        }else if(year.length==4/* && */){
            if(year<Number("19"+return_value)){
                return_value=year.substring(2,4);
            }else{
                return_value=year;
            }
        }
        if(String(return_value).length<4){
            if(Number(return_value)<=Number(String(today.getFullYear()).substring(2,4))){
                return_value = "20"+return_value;
            }else{
                return_value = "19"+return_value;
            }
        }
        return return_value;
    }

    function restrictNumber(el, swap, evt, max_val){
        if(el.id.indexOf("year")!=-1 && evt.type=="blur"){
            el.value = fixDateYear(el.value);
        }

        var err_flag=false;
        var charCode = (evt.which) ? evt.which : evt.keyCode;
        if(charCode==36||charCode==33||charCode==34||charCode==35||charCode==37||charCode==38||charCode==39||charCode==40||charCode==8||charCode==46)return true;


        var str_arr=el.value.split(" ");
        var str="";
        for(var j=0; j<str_arr.length; j++){
            for(var i=0; i<str_arr[j].length; i++){
                if(!isNaN(str_arr[j].charAt(i))){
                    str+=str_arr[j].charAt(i);
                }else{
                    err_flag=true;
                }
            }
        }
        if(err_flag==true){
            jAlert("Текстът, който въвеждаш, трябва да съдържа само цифри!", "");
        }
        if(el.value!=str)
            el.value=str;
        if(el.value=="")
            el.value=swap;
        if(evt.type=="blur" && max_val!=undefined && el.value.length<String(max_val).length && el.value.length>0){
            el.value="0"+el.value;
        }
        if(max_val!=undefined && max_val<el.value){
            el.value=max_val;
        }
    }

    function setCookie(name, value, days) {
        var expires;
        if (days) {
            var date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
        } else {
            var date = new Date();
            //10 years
            date.setTime(date.getTime() + (10 * 365 * 24 * 60 * 60 * 1000));
        }
        expires = "; expires=" + date.toGMTString();
        document.cookie = name + "=" + value + expires + "; path=/";
    }

