        });
    };

    function resizeYoutubePlayer(){
        //ако плейъра се е заредил
        if(player1940!=undefined && player1940.mute!=undefined && $("iframe#video").length>0 && $("iframe#video").width()!=undefined){
            $("iframe#video").height($("iframe#video").width()*3/4);
            if(video1940_interval!=undefined){
