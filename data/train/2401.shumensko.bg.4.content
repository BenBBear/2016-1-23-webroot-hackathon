    function isElementInViewport (el) {
        if (typeof jQuery === "function" && el instanceof jQuery) {
            el = el[0];
        }
        var rect = el.getBoundingClientRect();
        return (
            rect.top >= -el.offsetHeight &&
            rect.bottom <= $(window).height()+el.offsetHeight
        );
    }

    function fixImageSize(){
        if($(window).width()>=600 && $(window).height()>=400){
            flag_small_image_size=false;
        }else{
            flag_small_image_size=true;
        }
        $("img").each(function(){
            $(this).attr("src", fixImageSizeInString($(this).attr("src")));
        })
    }


    function fixImageSizeInString(str){
        if(!flag_small_image_size && str.indexOf('_s')!=-1){
            str = str.replace(/_s(.jpg|.png)/g, "$1");
            str = str.replace(/bar_bg_s(.mp4|.ogv|.webm)/g, "bar_bg$1");
        }else if(flag_small_image_size && str.indexOf('_s')==-1 && str.indexOf("maps.")==-1){
            str = str.replace(/(.jpg|.png)/g, "_s$1");
            str = str.replace(/bar_bg(.mp4|.ogv|.webm)/g, "bar_bg_s$1");
        }
        return str;
    }

