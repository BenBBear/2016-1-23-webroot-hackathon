	<script type="text/javascript">
		$(document).ready(function(){
			$('.sendAjax').click(function(){
				var $obj = $(this);
				if(typeof($obj.attr("href")) == 'undefined') // <FORM>
				{
					var $form = $obj.parents("form");
					if($form.length < 1)
					{						
						$form = $("#form_"+$obj.attr("id"));
					}
					$request = $form.serializeArray();
					$request[$request.length] = {name:"action", value:"ADD2BASKET"};

					var config = {
						url: $form.attr("action"),
						dataType: 'html',
						type: 'GET',
						data: $request,
						beforeSend: function(){processButton($obj, "button_view_cart_active");},
						success: function(data, textStatus){reloadHTML(data, "topBasket");processButtonEnd($obj, "button_view_cart_active");},
					};

					if($form.find("select").val() != 0)
					{
						$obj.removeClass("null");
						sendRequest(config);
						$('#some_information__icon').css("background-position", "0 top");
						$('#some_information_text').html('Вы успешно добавили товар в корзину');
						$('#information_message').css('background-color', '#ecf5d9').slideDown("fast").delay(5000).fadeOut("slow");
					}
					else
					{
						$obj.addClass("null");
						temporyLight($form, "select.form__select", "select_error");
						$('#some_information__icon').css("background-position", "-26px top");
						$('#some_information_text').html('Выбирите ваш размер');
						$('#information_message').css('background-color', '#fde1e1').slideDown("fast").delay(5000).fadeOut("slow");
					}
				}
				else // <A>
				{
					var config = {
						url: $obj.attr("href"),
						dataType: 'html',
						type: 'GET',
						data: '',
						beforeSend: function(){processButton($obj, "button_view_light_active");},
						success: function(data, textStatus){
							reloadHTML(data, "topBasket");
							reloadHTML(data, "topFavorites");
							processButtonEnd($obj, "button_view_light_active");

							if($obj.hasClass("addToFavor") == true)
							{
								$('#some_information__icon').css("background-position", "0 top");
								$('#some_information_text').html('Добавлено в избранное');
								$('#information_message').css('background-color', '#ecf5d9').slideDown("fast").delay(5000).fadeOut("slow");
							}
						}
					};

					sendRequest(config);
				}

				
				return false;
			});
		});
	</script>
	
