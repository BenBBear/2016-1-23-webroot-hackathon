	<script>
	$(function() {


	  // 変数
	  var navHeight = $(".nav-top-bar").outerHeight() + $(".nav-bar-wrap").outerHeight();
	  var flg = true;

	  // 画像のプリロード
	  function preloadFunc() {
	    for(var i = 0; i< arguments.length; i++){
	      $("<img>").attr("src", arguments[i]);
	    }
	  }
	  preloadFunc("http://www.newgin.co.jp/wordpress/wp-content/themes/newgin/img/nav_entertainment_on.png","http://www.newgin.co.jp/wordpress/wp-content/themes/newgin/img/nav_guide_on.png","http://www.newgin.co.jp/wordpress/wp-content/themes/newgin/img/nav_machine_on.png","http://www.newgin.co.jp/wordpress/wp-content/themes/newgin/img/nav_slide_pachinko_on.png", "http://www.newgin.co.jp/wordpress/wp-content/themes/newgin/img/nav_slide_slot_on.png","http://www.newgin.co.jp/wordpress/wp-content/themes/newgin/img/nav_store_on.png");


	  // 初期化
	  (function init() {

	    $(".slide-helper").css("height", navHeight);
	    $(".slide-machine").css("marginTop", -($(".slide-machine").outerHeight() - navHeight));
	    $(".slide-store").css("marginTop", -($(".slide-store").outerHeight() - navHeight));
	    // ２番目移行のスライドコンテンツを畳む
	    $(".nav-slide").children().not(":first").css({
	      "width": "60px",
	      "cursor": "pointer"
	    });

	  })();

	  // ナビゲーションがクリックされた時の処理
	  $(".nav a").on("click", function() {

	    var $_obj = $(this);
	    var targetId = $_obj.attr("id");

	    if (flg) {
	      flg = false;
	      if ($_obj.hasClass("current")) {
	        changeSlide(targetId, 500, false);
	      } else {
	        changeSlide(targetId, 500, true);
	      }
	      changeNavBg(targetId);
	    };


	  });

	  $('#store_search').on("click", function(e) {

	  		var $_obj = $('#store');
		    var targetId = $_obj.attr("id");
			changeSlide(targetId, 500, true);
			changeNavBg(targetId);

			$('body, html').animate({ scrollTop: 0 }, 500);

			e.preventDefault();
	  });

	  // ナビゲーションを変更する
	  function changeNavBg(target) {

	    var $_targetObj = $("#" + target);
	    var notTarget = $(".nav-slide").not("#" + target).attr("id");
	    // クリックしたナビゲーションの背景画像を変更
	    if ($_targetObj.hasClass("current")) {
	      $_targetObj.removeClass("current")
	        .css({
	          "backgroundImage": "url(http://www.newgin.co.jp/wordpress/wp-content/themes/newgin/img/nav_" + target + ".png)",
	          "backgroundRepeat": "no-repeat"
	      });
	    } else {
	      $_targetObj.addClass("current")
	        .css({
	          "backgroundImage": "url(http://www.newgin.co.jp/wordpress/wp-content/themes/newgin/img/nav_" + target + "_on.png)",
	          "backgroundRepeat": "no-repeat"
	      });
	      $("#" + notTarget).removeClass("current")
	        .css({
	          "backgroundImage": "url(http://www.newgin.co.jp/wordpress/wp-content/themes/newgin/img/nav_" + notTarget + ".png)",
	          "backgroundRepeat": "no-repeat"
	      });
	    }

	  }

	  // スライドを変更する
	  function changeSlide(target, speed, flag) {

	    // スライドを開閉するならtrue,閉じるだけならfalse
	    var isOpenFlg = flag;
	    var $_obj = $(this);
	    // スライドさせる対象
	    var $_targetObj = $(".slide-" + target);
	    var targetHeight = String($_targetObj.css("height")).replace("px", "");
	    targetHeight = Number(targetHeight);

	    // スライドを開閉する場合
	    if (isOpenFlg == true) {
	      // ターゲットを開く
	      $_targetObj.css("z-index", 1)
	        .not(":animated").animate({
	          "marginTop": navHeight
	        }, speed);
	      // 非ターゲットを閉じる
	      $(".slide-wrap > div").not(".slide-" + target).css("z-index", 0).not(":animated").animate({
	          "marginTop": -($(".slide-wrap > div").not(".slide-" + target).outerHeight() - navHeight)
	        }, speed);
	      // ヘルパーの高さをターゲットの高さに
	      $(".slide-helper").not("animated").animate({
	        "height": targetHeight + navHeight
	      }, speed, function() {
	        flg = true;
	      });
	    // スライドを閉じる場合
	    } else if (isOpenFlg == false) {
	      // ターゲットを閉じる
	      $_targetObj.css("z-index", 0)
	        .not(":animated").animate({
	          "marginTop": -($_targetObj.outerHeight() - navHeight)
	      }, speed);
	      // ヘルパーの高さをナビゲーションの高さに
	      $(".slide-helper").not(":animated").animate({
	        "height": navHeight
	      }, speed, function() {
	        flg = true;
	      });
	    }

	  };

	  // スライドがクリックされた時の処理
	  $(".nav-slide div h2").on("click", function() {

	    // 退避
	    var $_obj = $(this);

	    if (!$_obj.parent("div").hasClass("current")) {

	      $_obj.css({
	        "backgroundImage": "url(http://www.newgin.co.jp/wordpress/wp-content/themes/newgin/img/nav_slide_" + $_obj.attr("class") + "_on.png)"
	      }).parent().siblings().children("h2").css({
	        "backgroundImage": "url(http://www.newgin.co.jp/wordpress/wp-content/themes/newgin/img/nav_slide_" + $_obj.parent().siblings().children("h2").attr("class") + ".png)"
	      });

	      $_obj.parent("div").animate({
	        "width": "917px"
	      });

	      $(".nav-slide").find(".current").animate({
	        "width": "60px"
	      }).css("cursor","pointer")
	      .removeClass("current");
	      $(this).parent("div").addClass("current").css('cursor','default');

	    } else {
	      return false;
	    }

	  });

	  //snsボタン
	  $('.sns_button').click(function(){
	  	var width=550;
		var height=450;
	  	var lefmegLiTpos=Number((window.screen.width-width)/2);
		var topPos=Number((window.screen.height-height)/2);
