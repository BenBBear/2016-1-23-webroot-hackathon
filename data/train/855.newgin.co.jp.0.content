	  	return false;
	  });

	  	//megamenu画像にホバーすると拡大する処理
		(function(){
			$('.global-nav .MegPtYBxsR')
				.hover(
					function () {
						//拡大前画像のボックスに指定されてるzoombox-idをもとに拡大後画像ボックスIDを指定
						$_targetBox = $('.MegZoomBox' + $(this).attr('zoombox-id'));
						$_targetBox.fadeIn('normal');
					},
					function () {
						$('.MegZoomBox')
							.hover(
								function () {
									$_targetBox.show();	
								},
								function () {
									$('.MegZoomBox').hide();
								}
							);
						$('.MegZoomBox').hide();
					}
				);
		})();

		$('.MegZoomBox').click(
			function(){
				$('.MegZoomBox').hide();
			}
		);

	});
	</script>


