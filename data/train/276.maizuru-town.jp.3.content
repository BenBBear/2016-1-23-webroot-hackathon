<script type="text/javascript">

  var currentInfoWindow = null;   //最後に開いた情報ウィンドウを記憶

  function initialize() {
    var latlng = new google.maps.LatLng(35.460981, 135.381955);
    var myOptions = {
      zoom: 13,
      center: latlng,
      mapTypeId: google.maps.MapTypeId.ROADMAP,	  
	};
	
    var map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);
	
	
	//オリジナルマーカー（アイコン）読み込み
    var makerImg = 'http://maizuru-town.jp/images/icon_house2_kumonna.png';
	
	//オリジナルマーカーの表示
	var markerA = new google.maps.Marker({
      position: new google.maps.LatLng(35.434413, 135.332586),
      map: map,
	  icon: "http://maizuru-town.jp/images/icon_house2_kumonna.png" ,
      title:"公文名"
	});
	
	var markerB = new google.maps.Marker({
      position: new google.maps.LatLng(35.482423, 135.434870),
	  icon: "http://maizuru-town.jp/images/icon_house_denen.png",
      map: map,
	  
      title:"田園町"
	});
	

	
	var contentStringA =
    '<h1><b>公文名（くもんな）</b></h1>'+'<br>'+
    '<img src="http://maizuru-town.jp/images/logo240_2.png" alt="舞鶴タウン" width="200" />'+
'<br>'+	'<p>公文名（くもんな）の物件をお探しなら「舞鶴タウンナビ」</p>'+'<br>'+'<a href="http://maizuru-town.jp/">→舞鶴タウンナビ総合案内はこちら</a>';
	
	var contentStringB =
    '<h1><b>公文名（くもんな）</b></h1>'+'<br>'+'<img src="http://maizuru-town.jp/images/logo240_2.png" alt="舞鶴タウン" width="200" />'+'<br>'+'<br>'+'<img src="http://maizuru-town.jp/images/side_denen.jpg" alt="メロウタウン" width="200" />'+'<br>'+'<br>'+	'<p>メロウタウン田園町の分譲住宅情報はこちら</p>'+'<br>'+'<a href="http://maizuru-town.jp/estate/denen.html">→ メロウタウン田園町分譲情報はこちら</a>'+'<br>'+'<a href="http://maizuru-town.jp/">→舞鶴タウンナビ総合案内はこちら</a>';

	var infowindowA = new google.maps.InfoWindow({
      content: contentStringA,
      position: latlng,
	  size: new google.maps.Size(350, 100)
	});
	
	var infowindowB = new google.maps.InfoWindow({
      content: contentStringB,
	  size: new google.maps.Size(350, 100)
	});

	google.maps.event.addListener(markerA, 'click', function() {
      //先に開いた情報ウィンドウがあれば、closeする
      if (currentInfoWindow) {
      　currentInfoWindow.close();
      }
      //情報ウィンドウを開く
      infowindowA.open(map,markerA);
      currentInfoWindow = infowindowA;
	});
	google.maps.event.addListener(markerB, 'click', function() {
      //先に開いた情報ウィンドウがあれば、closeする
      if (currentInfoWindow) {
      　currentInfoWindow.close();
      }
      //情報ウィンドウを開く
      infowindowB.open(map,markerB);
      currentInfoWindow = infowindowB;
	});
	
	
    
  }
</script>


