<script>

gate_modal_w = [];
gate_modal_h = [];
gate_modal_type = [];
modal_index = 1;

$(function () {
	//特定の引数がある場合は別ウインドウ
	$('a').each(function() {
		var url = this.href;
		var org_url = url.split('?');
		var url2 = url.split('type=');
		var ele1 = String(url2[1]).indexOf('_blank');
		var ele2 = String(url2[1]).indexOf('catalog');
		var ele3 = String(url2[1]).indexOf('modal');
		if (ele1==0){this.target="_blank";this.href=org_url[0];}
		else if (ele2==0) {
			$(this).removeAttr("href");
			$(this).bind("click", function() {popupCatalogWindow(org_url[0]); return false;});
		}
		else if (ele3==0) {
			var vars = getUrlVars(url);
			var fix = "fix_off"; if (vars['size']) fix = vars['size'];
			var bg = "none"; if (vars['bg']) bg = vars['bg'];
			var btn = "none"; if (vars['btn']) btn = vars['btn'];
			var m_index = modal_index;

			gate_modal_type[modal_index] = fix;

			if (vars['w']) {
				gate_modal_w[modal_index] = vars['w'];
			}

			if (vars['h']) {
				gate_modal_h[modal_index] = vars['h'];
			}

			$(this).unbind('click');
			$(this).removeAttr("href");
			$(this).removeClass("externalLink");
			$(this).removeAttr("target");
			$(this).attr("href", "#");
			$(this).bind("click", function() {gate_modal(m_index,fix,org_url[0],bg,btn); return false;});
			modal_index++;
		}
	});

	function getUrlVars(url)
	{
	    var vars = [], hash;
	    var hashes = url.slice(url.indexOf('?') + 1).split('&');
	    for(var i = 0; i < hashes.length; i++) {
	        hash = hashes[i].split('=');
	        vars.push(hash[0]);
	        vars[hash[0]] = hash[1];
	    }
	    return vars;
	}

	//if (appVersion.indexOf("msie 8.") != -1) {
	//	$(".header_service_btns").css("width", "");
	//}
});
</script>



