<script>
	//If page is email-link-handler, do not show any Janrain buttons.
	//Otherwise, show them.
	if(window.location.href.indexOf("email-link-handler") == -1) {
		if(IS_USER_LOGGED_IN) {
			displayDropdownWithoutJQuery();
		} else {
			hideDropdownWithoutJQuery();
		}
	}

    function displayDropdownWithoutJQuery() {
        var welcomeText = decodeURIComponent(document.getElementById('manage-welcome').value) || "",
            editProfileText = decodeURIComponent(document.getElementById('manage-editprofile').value) || "",
            editProfileLink = decodeURIComponent(document.getElementById('manage-editlink').value),
            signoutText = decodeURIComponent(document.getElementById('manage-logout').value) || "",
            welcomeUsername = "",
            totalMessage,
            maxLength = (getWindowWidth() <= 1024) ? 21 : 94;

        //adding text dynamically 
        document.getElementById('welcomeText').innerHTML = welcomeText;
        document.getElementById('captureProfileLink').innerHTML = editProfileText;
        document.getElementById('captureProfileLink').setAttribute('href', editProfileLink);
        document.getElementById('captureSignOutLink').innerHTML = signoutText;
        document.getElementById('welcomeUsername').innerHTML = displayName;

        //calculating length of the welcome message and adding elipsis(...)
        //if the length is more than 100.
        //welcomeText = $('#welcomeText').text(),
        welcomeUsername = displayName;
        totalMessage = welcomeText+welcomeUsername;
        if(welcomeText.length > maxLength){
            welcomeText = welcomeText.substring(0, maxLength-3)+"...";
            document.getElementById('welcomeText').innerHTML = welcomeText;
            document.getElementById('welcomeUsername').innerHTML = "";
        }else{
            if(totalMessage.length > maxLength){
                welcomeUsername = totalMessage.substring(welcomeText.length, maxLength-3)+"...";
                document.getElementById('welcomeUsername').innerHTML = welcomeUsername;
            } 
        }

        //Hide "Sign In" and "Register" links
        if(document.getElementById("captureSignInLink").style.display != 'none')
            document.getElementById("captureSignInLink").style.display  = 'none';
        if(document.getElementById("captureRegisterLink").style.display != 'none')
            document.getElementById("captureRegisterLink").style.display  = 'none';
        
        //Show the new user dropdown
        if(document.getElementById("captureUserOptionsDropdownMenu").style.display != 'block')
            document.getElementById("captureUserOptionsDropdownMenu").style.display = 'block';
    }
    
    function hideDropdownWithoutJQuery() {
        var signInVal = decodeURIComponent(document.getElementById('manage-signin').value) || "Sign In"
            registerVal = decodeURIComponent(document.getElementById('manage-register').value) || "Register";

        document.getElementById('welcomeUsername').innerHTML = "";

        document.getElementById('captureSignInLinkHREF').innerHTML = signInVal;
        document.getElementById('captureRegisterLinkHREF').innerHTML = registerVal;

        //Show "Sign In" and "Register" links
        if(document.getElementById("captureSignInLink").style.display != 'block')
            document.getElementById("captureSignInLink").style.display  = 'block';
        if(document.getElementById("captureRegisterLink").style.display != 'block')
            document.getElementById("captureRegisterLink").style.display  = 'block';
        
        //Hide the user dropdown
        if(document.getElementById("captureUserOptionsDropdownMenu").style.display != 'none')
            document.getElementById("captureUserOptionsDropdownMenu").style.display = 'none';
    }
	
	function getWindowWidth() {
		var winW = 0;
		if (document.body && document.body.offsetWidth) {
		 winW = document.body.offsetWidth;
		}
		if (document.compatMode=='CSS1Compat' &&
			document.documentElement &&
			document.documentElement.offsetWidth ) {
		 winW = document.documentElement.offsetWidth;
		}
		if (window.innerWidth && window.innerHeight) {
		 winW = window.innerWidth;
		}
		return winW;
	}
</script>
                            
                        
                    
						
					 
 























