<script id="walletVoucher-template" type="text/x-handlebars-template">
{{#if showVoucherOnPage}}
        <article class="vouchertileWrapper {{arabicOffer}} col-xs-8 col-sm-4 col-md-2 col-lg-2"  data-rel={{filterTag}} data-sourceGeos={{sourceGeos}}>            
                <div class="vouchertile" >
                        <div class="voucherImage">
							<a href="{{detailUrl}}">
                            <div data-url="{{detailUrl}}" class="voucherImgcontainer imgcontainer mcloader"> 
							{{#createRendition tileImage tileImageAltText }}{{/createRendition}}
							</div>							
							{{#if productStatus.value}}
			                        <div class="imageovertype">
			                            <span class="grayBgStyle">{{productStatus.value}}</span>
			                        </div>
			                 {{/if}}
							 </a>
                        </div>
                        <div class="vouchertilecontent">
                            <div class="offerCity commonTileStyles"><span class="city-name">{{{returnAsHtml true geoTitle}}}</span> {{{returnAsHtml true categories}}} </div>
                            <a href="{{detailUrl}}"><h2>{{{returnAsHtml false headline}}}</h2></a>
                            <p style="height: auto;"> {{{returnAsHtml false subtitle}}}  </p>
                            <div class="voucher-info-txt">{{trans.offer}}: <span class="offer-txt">{{cost}}</span></div>
                            <div class="voucher-info-txt">{{trans.expires}}: {{offerBookingEndDate}} </div>
							{{#if cepVoucherShow }}
							<div class="voucher-info-txt redeem-btn" id="{{i18nofferID}}Redeem"><a class="redeemButton" href="{{pinValidationUrl}}"> {{trans.redeem}} </a></div>
							{{/if}}
							<div class="desktop-print-email">
								<div class="voucher-info-txt print-btn" id="{{i18nofferID}}PrintDesk">							
								<input type="hidden" id="staticVoucherValues{{i18nofferID}}" data-offerid="{{cepvoucherofferID}}" data-offerlocale="{{cepvoucherlocal}}"/>
								<form id="{{formID}}_print"  method="POST" action="{{pdfDownloadURL}}"><input type="hidden" name="voucherDtls"/><input type="hidden" name="voucherOfferID" class="voucherOfferID" value="{{i18nofferID}}"/><button type="button" class="printButton" data-dynamicvouchertype="{{cepvoucher_type}}" data-currentLocaleOfferID="{{cepvoucherofferID}}" data-currentOfferLocale="{{cepvoucherlocal}}"> {{trans.print}} </button> </form></div>							
								<div class="voucher-info-txt vocher-last-element emailself-desk"> 
									<input type="hidden" name="voucherOfferID" class="voucherOfferID" value="{{i18nofferID}}"/> <div id="{{i18nofferID}}EmailLink" class="emailLink" data-dynamicvouchertype="{{cepvoucher_type}}" data-currentLocaleOfferID="{{cepvoucherofferID}}" data-currentOfferLocale="{{cepvoucherlocal}}" emailtoslefflag="true">{{trans.emailToSelf}}</div> 
									<div id="{{i18nofferID}}Success" class="email-sent-msg success"><div class="msg-info-txt">{{trans.emailSuccessMsg}}</div><span class="close-txt">X</span> </div>  
									<div id="{{i18nofferID}}Fail" class="email-sent-msg fail"><div class="msg-info-txt">{{trans.emailFailMsg}}</div><span class="close-txt">X</span> </div>
								</div>
							</div>
							<div class="mobile-print-email">
								<div class="voucher-info-txt print-btn-mobile" id="{{i18nofferID}}Print"><form id="{{formID}}_mob_print"  method="POST" action="{{pdfDownloadURL}}"><input type="hidden" name="voucherDtls"/> <input type="hidden" name="voucherOfferID" class="voucherOfferID" value="{{i18nofferID}}"/><a href="javascript:void(0)" class="printButton" data-dynamicvouchertype="{{cepvoucher_type}}" data-currentLocaleOfferID="{{cepvoucherofferID}}" data-currentOfferLocale="{{cepvoucherlocal}}"> {{trans.print}} </a> </form><span>|</span></div>
								<div class="voucher-info-txt emailself-mob"> 
									<input type="hidden" name="voucherOfferID" class="voucherOfferID" value="{{i18nofferID}}"/> 
									<div id="{{i18nofferID}}MobEmailLink" class="emailLink" data-dynamicvouchertype="{{cepvoucher_type}}" data-currentLocaleOfferID="{{cepvoucherofferID}}" data-currentOfferLocale="{{cepvoucherlocal}}"emailtoslefflag="true">{{trans.email}}</div>
								</div>
								<div id="{{i18nofferID}}MobSuccess" class="email-sent-msg success">{{trans.emailSuccessMsg}}<span class="close-txt">X</span> </div>  
								<div id="{{i18nofferID}}MobFail" class="email-sent-msg fail">{{trans.emailFailMsg}}<span class="close-txt">X</span> </div>
                            <div>
                        </div>
    			</div>    		
    	</article>
	{{/if}}
</script>

	





