<script>
	function getJanrainSessionCookie(name) {
	  var value = "; " + document.cookie;
	  var parts = value.split("; " + name + "=");
	  if (parts.length == 2) return parts.pop().split(";").shift();
	}
		
	//If page is email-link-handler, do not show any Janrain buttons.
	//Otherwise, show them.
	if(window.location.href.indexOf("email-link-handler") == -1) {
		//Determine if user is logged in via cookie stored by CQ_Analytics.ProfileDataMgr.
		var IS_USER_LOGGED_IN = false;
		var displayName = "";

		var userCookie = getJanrainSessionCookie("SessionPersistence");
		if(userCookie != null && userCookie != "") {
			//Decode characters (ex. convert %3D to = sign)
			//PCEV-12615 Hotfix-Social Registration Junk characters for chinese
			userCookie = decodeURIComponent(decodeURIComponent(userCookie));
			
			//Remove profiledata string
			var profileDataString = "PROFILEDATA:=";
			var profileDataIndex = userCookie.indexOf(profileDataString);
			if(profileDataIndex != -1) {
				var userAttributes = userCookie.substring(profileDataString.length);
				
				//Find user's name
				var userAttributeArray = userAttributes.split(",");
				var userNameString = "displayName";
				if(userAttributeArray.length > 1) {
					for(var count = 0; count < userAttributeArray.length; count++) {
						if(userAttributeArray[count].indexOf(userNameString) != -1) {
							var keyValuePair = userAttributeArray[count].split("=");
							if(keyValuePair.length > 1) {
								IS_USER_LOGGED_IN = true;
								displayName = keyValuePair[1];
							}
						}
					}
				}
			}
		}
		
		//By default, Janrain does not use session cookies/storage.
		//That means when the user closes the browser, Janrain is
		//still logged in. However, the CQ ProfileDataMgr uses session
		//cookies. Therefore, when its cookie is deleted, we should
		//also log out Janrain via clearing local storage.
		if(!IS_USER_LOGGED_IN) {
			localStorage.removeItem("janrainCaptureToken");
			localStorage.removeItem("janrainCaptureToken_Expires");
		}
	}
</script>
