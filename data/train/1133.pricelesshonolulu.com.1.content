<script>
var pageRelType = "city";
var siteNavPage = "city";

function createTmpCookie(name, value, days) {
	if (parseInt(days, 10)) {
		var date = new Date();
		date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
		var expires = "; expires=" + date.toGMTString();
	} else {
		expires = "";
	}
	
	if ("https:" == document.location.protocol) {
		document.cookie = name + "=" + escape(value) + expires + "; path=/" + "; secure";
	} else {
		document.cookie = name + "=" + escape(value) + expires + "; path=/";
	}

}
    function getTmpCookie(name) {
	var nameEQ = name + "=";
	var cookieArray = document.cookie.split(';');
	for (var i = 0; i < cookieArray.length; i++) {
		var cookieObj = cookieArray[i];
		while (cookieObj.charAt(0) == ' ') {
			cookieObj = cookieObj.substring(1, cookieObj.length);
		}
		if (cookieObj.indexOf(nameEQ) === 0) {
			return decodeURIComponent(cookieObj.substring(nameEQ.length, cookieObj.length));
		}
	}
	return null;
}

createTmpCookie("impPageType",pageRelType,"");
function getImpressionPageTypeValue(name){
	var impressionTypeValue = "";
	switch(name){
			case "aboutus":
				impressionTypeValue = "Aboutus Page";
			break;
			case "sitemap":
				impressionTypeValue = "Site Map Page";
			break;
			case "termsofuse":
				impressionTypeValue = "Terms of use Page";
			break;
			case "FAQ":
				impressionTypeValue = "FAQ Page";
			break;
			case "contactus":
				impressionTypeValue = "Contactus Page";
			break;
			case "category":
				impressionTypeValue = "Category Home Page";
			break;
			case "awaycategory":
				impressionTypeValue = "Category Away Page";
			break;
			case "city":
				impressionTypeValue = "City Home Page";
			break;
			case "awaycity":
				impressionTypeValue = "City Away Page";
			break;
			case "nationallanding":
				impressionTypeValue = "National Home Page";
			break;
			case "awaynationallanding":
				impressionTypeValue = "National Away Page";
			break;
			case "regionallanding":
				impressionTypeValue = "Regional Home Page";
			break;
			case "awayregionallanding":
				impressionTypeValue = "Regional Away Page";
			break;
			case "searchresult":
				impressionTypeValue = "Search Result Page";
			break;
			case "error":
				impressionTypeValue = "Error Page";
			break;	
			case "offerdetail":
				impressionTypeValue = "Offer Details Page";
			break;		
			default:
				impressionTypeValue = "Direct";
			break;
		}
	return impressionTypeValue;
}

var getsiteNavCookieVal  = getTmpCookie('pageImpTypeVal');
var pageImpressionTypeValue = getImpressionPageTypeValue(getsiteNavCookieVal);
if(siteNavPage != getsiteNavCookieVal){
	createTmpCookie("pageImpTypeVal",siteNavPage,"");
}
</script>
