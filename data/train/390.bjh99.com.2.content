function login(myid){
	var token = $("#"+myid+" #csrfhash").val();
	var un = $.trim($("#"+myid+" #username").val());
	var pwd = $.trim($("#"+myid+" #password").val());
	var captcha = $("#"+myid+" #code").val();

	var pc = $("#"+myid+" #pc").attr("checked") ? 1 : 0;
	if (token == '') return false;
 
	if(un == "" || un == "输入ba开头账号" ){
		show_tip('用户名不能为空');
		return false;
	}
	if(pwd.length == 0){
		show_tip('密码不能为空');
		return false;
	}
	if(typeof(captcha) != "undefined" && captcha.length == 0){
		show_tip('验证码不能为空');
		return false;
	}

	$.post("/member/login",
		  {csrfhash: token, user: un, pwd: pwd, code: captcha},
		  function(data){
			var obj = eval("(" + data + ")");
			if(obj.error == 0){
			
				window.location.href = 'http://' + window.location.host+"/?1" + '.' + Math.floor(Math.random()*(1024-9172)+9172);
				return true;
			}else if(obj.error == -1){
				show_tip('用户名不存在');
			    
				$("#"+myid+" #username")[0].focus();
			}else if(obj.error == -2){
			    show_tip('您输入的帐号或者密码有误，请重新输入');
			
				$("#"+myid+" #password")[0].focus();
			}else if(obj.error == -3){
				show_tip('IP被封15分钟');
			 
				$("#"+myid+" #username")[0].focus();
			}else if(obj.error == -4){
				show_tip('您的账户已经被停用，不允许登录');
			
			}else if(obj.error == -10001){
				show_tip('验证码错误，请重新输入');
			   
				$("#"+myid+" #code").val('');
			    $("#"+myid+" #code").focus();
				getsec();
			}
		
			return false;
			
	});
}

</script>
