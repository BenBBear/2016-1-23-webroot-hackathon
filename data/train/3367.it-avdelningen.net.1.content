<script>

    function send_mail() {

        if ($('#name').val() == "") {

            alert("Please enter name");

            return false;

        }

        if ($('#email').val() == "") {

            alert("Please enter Email address");

            return false;

        }

        if ($('#project').val() == "") {

            alert("Please enter Project title");

            return false;

        }

        if ($('#message').val() == "") {

            alert("Please enter Message");

            return false;

        }

        var name = $('#name').val();

        var email = $('#email').val();

        var project = $('#project').val();

        var message = $('#message').val();

        jQuery.ajax({

            type: "POST",

            url: 'response.php?name=' + name + '&email=' + email + '&project=' + project + '&message=' + message,

            success: function (data) {

                alert("Your message send successfully");

                $('#name').val('');

                $('#email').val('');

                $('#project').val('');

                $('#message').val('');

            }

        });

    }
function change_language(lan)
{
	if(lan == 'se')
	{
		window.location ='http://it-avdelningen.net/se';
	}
}
</script>