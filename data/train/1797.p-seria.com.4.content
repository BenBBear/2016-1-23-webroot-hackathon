                if ($(window).width() > 600) {
                    var marginL = $('.main-img').css('margin-left');
                    var marginR = $('.main-img').css('margin-right');
                    $('.leftside a').mouseenter(function () {
                        $('.main-img').stop(true, true).animate({marginLeft: '100'}, 300);
