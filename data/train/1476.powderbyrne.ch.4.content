<script charset="utf-8">
  	var hpslider_num_slides = 7;
  	var hpslider_current_slide = 1;

	  $(document).ready(function() {
			$(".hpslider_back_arrow").click(function() {
				$("#hpslider-dots-" + hpslider_current_slide).removeClass('flex-active');
				if ($(".hpslider_slides").scrollLeft() < 50) {
					$(".hpslider_slides").scrollTo("6510px", {axis:'x'});
				}
				$(".hpslider_slides").scrollTo("-=933px", {axis:'x', duration:600});
				hpslider_current_slide--;
				if (hpslider_current_slide < 1) {
					hpslider_current_slide = hpslider_num_slides;
				}
				$("#hpslider-dots-" + hpslider_current_slide).addClass('flex-active');
			});
			$(".hpslider_forward_arrow").click(function() {
				$("#hpslider-dots-" + hpslider_current_slide).removeClass('flex-active');
				if ($(".hpslider_slides").scrollLeft() > 6481) {
					$(".hpslider_slides").scrollTo("0px",{axis:'x'});
				}
				$(".hpslider_slides").scrollTo("+=933px",{axis:'x', duration:600});
				hpslider_current_slide++;
				if (hpslider_current_slide > hpslider_num_slides) {
					hpslider_current_slide = 1;
				}
				$("#hpslider-dots-" + hpslider_current_slide).addClass('flex-active');
			});
	  });

		function gotoHPSlide(slide) {
			if (slide > 0 && slide <= hpslider_num_slides) {
				$("#hpslider-dots-" + hpslider_current_slide).removeClass('flex-active');
				$(".hpslider_slides").scrollTo(((slide-1)*930) + "px", {axis:'x', duration:600});
				hpslider_current_slide = slide;
				$("#hpslider-dots-" + hpslider_current_slide).addClass('flex-active');
				return false;
			}
		}
