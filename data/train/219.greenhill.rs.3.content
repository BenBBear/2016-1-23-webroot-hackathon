	<script type="text/javascript">
	function initialize() {
		if (GBrowserIsCompatible()) {
			//var map = new GMap2(document.getElementById("map_canvas"));
			//map.setCenter(new GLatLng(44.817284,20.476083), 17);
			//map.setUIToDefault();
			
							
			
			
			
			var baseIcon = new GIcon("http://www.greenhill.rs/cms/template/markerGreenHill.png");
			baseIcon.shadow = "shadow.png";
			baseIcon.iconSize = new GSize(90, 55);
			baseIcon.shadowSize = new GSize(9, 37);
			baseIcon.iconAnchor = new GPoint(9, 2);
			baseIcon.infoWindowAnchor = new GPoint(9, 2);
			// Creates a marker whose info window displays the letter corresponding
			// to the given index.
			//Create marker lokacija
			function createMarker(point, index) {
				// Create a lettered icon for this point using our icon class
				var letter = String.fromCharCode("A".charCodeAt(0) + index);
				var letteredIcon = new GIcon(baseIcon);
				letteredIcon.image = "http://www.greenhill.rs/cms/template/markerGreenHill.png";
				// Set up our GMarkerOptions object
				markerOptions = { icon:letteredIcon };
				var marker = new GMarker(point, markerOptions);
				GEvent.addListener(marker, "click", function() {
					marker.openInfoWindowHtml("<b>Green Hill Dedinje</b><br /><br />");
				});
				return marker;
			}
			//Create marker contact
			function createMarkerBHG(point, index) {
				// Create a lettered icon for this point using our icon class
				var letter = String.fromCharCode("A".charCodeAt(0) + index);
				var letteredIcon = new GIcon(baseIcon);
				letteredIcon.image = "http://www.greenhill.rs/cms/template/markerBHG.png";
				// Set up our GMarkerOptions object
				markerOptions = { icon:letteredIcon };
				var marker = new GMarker(point, markerOptions);
				GEvent.addListener(marker, "click", function() {
					marker.openInfoWindowHtml("<b>BPF, \"Belgrade Pioneer Funkd\"</b><br /><br />Đorđa Stanojevića 9b/33,<br />11070 Novi Beograd, Serbia<br />T:  +381 (0) 11 630 20 59 <br />F:  +381 (0) 11 630 20 37  <br />M: +381 (0) 69 311 51 91<br />E:  marina.cabarkapa@bpf.rs");
				});
				return marker;
			}
			//kontakt mapa
			//var latlng = new GLatLng(44.817284,20.476083);
			//map.addOverlay(createMarker(latlng, 17));
			
						
		}
		else {
			alert("Žao nam je, ali Google Maps API nije kompatibilan sa vašim browser-om");
		}
	}
