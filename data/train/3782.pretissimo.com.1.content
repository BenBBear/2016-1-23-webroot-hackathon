<script type="text/javascript">


$(document).ready(function() {
	$("#valider").click(function(){
		$('#fm_log').submit();
	});
});

$(function(){
	$("button").button();
});



    
    function valide(e) {
        if (!e)
            e = window.event;

        if (e.type == "keypress")
            if (e.keyCode == 13) // par sur même ligne sinon pas valide W3C
                document.getElementById('fm_log').submit();

        return true;
    }
    document.onkeypress = valide;
</script>
