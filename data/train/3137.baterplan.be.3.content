			var FRAME_PER_SEC = 20;
			function moz_fade_in(imageId)
			{
				var img = document.getElementById(imageId);
				var incr = 1/((img.transition_time/3)*FRAME_PER_SEC);
				var newOp = parseFloat(img.style.MozOpacity)+parseFloat(incr);
				newOp = (newOp > 1) ? 1 : newOp;
				img.style.MozOpacity = newOp;
				if (newOp < 1) setTimeout('moz_fade_in("'+imageId+'")',1000/FRAME_PER_SEC);
			}
var images = new Array();
function run_diapo(diapoId) {
obj = document.getElementById(diapoId);obj.src = images[diapoId][obj.index].src;
obj.alt = images[diapoId][obj.index].alt;
obj.title = images[diapoId][obj.index].alt;
if (document.all){
obj.style.filter="blendTrans(duration=obj.transition_time)";
obj.filters.blendTrans.Apply();
obj.filters.blendTrans.Play();
}
else {
obj.style.MozOpacity = 0;moz_fade_in(diapoId);}
obj.index+=1;
if (obj.index > (obj.length_diapo-1)) obj.index=0;
obj.timeout = setTimeout('run_diapo("'+diapoId+'")', obj.transition_pause * 1000);
}
</script>


