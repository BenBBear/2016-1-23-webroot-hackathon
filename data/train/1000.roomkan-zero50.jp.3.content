<script type="text/javascript">
$(function(){
$(document).click(function() { $('.panel').hide();});
            $("#tN").keypress(function(ev) {
        if ((ev.which && ev.which === 13) || (ev.keyCode && ev.keyCode === 13)) {
            var classVal = $(".search_class").val();
            var tnVal = $("#tN").val();
            var adVal = $(".search_address").val();
            var stnVal = $(".search_station").val();
            if((empty(classVal)) || (empty(tnVal) && empty(adVal) && empty(stnVal))){
                var validateStr = '';
                if(empty(classVal)){
                    validateStr += ("種別を選択してください\n\n");
                }
                if(empty(tnVal) && empty(adVal) && empty(stnVal)){
                    validateStr += ("沿線エリアを選択、もしくは物件名を入力してください");
                }
                alert(validateStr);
                return false;
            }
        } else {
            return true;
        }
    });

    var auto_count_flg = false;
    var click_flg = true;
    $("#tN").bind('keyup',function(){
        var tnVal = $("#tN").val();
        $("#tN_2").val(tnVal);
        $("#tN_3").val(tnVal);
        var adVal = $(".search_address").val();
        var stnVal = $(".search_station").val();
        if((empty(tnVal) == false) || (empty(adVal) == false || empty(stnVal) == false)){
            getBknCnt();
        }else{
            $(".free_search_cnt").text('-');
        }
    });

        function getBknCnt(){
            if(empty($(".search_class").val()) == false && click_flg == true){
                click_flg = false;
                auto_count_flg = false;
                var cnt;//カウント数
                formValue = $("form.free_search").serialize();
                $.ajax({
                    type: "POST",
                    url: "/search/ajax/bkncnt/",
                    data: formValue,
                    handleAs: "json",
                    success: function(re){
                        $(".free_search_cnt").text(re);
                        click_flg = true;
                    },
                    error: function(){
//                    alert('エラーです');
                    }
                });
            }else if(empty($(".search_class").val()) == false){
                auto_count_flg = true;
                auto_bknCnt();
            }
        }
                function auto_bknCnt(){
            setTimeout(function(){
                if(auto_count_flg == true){
                    auto_count_flg = false;
                    click_flg = true;
                    getBknCnt();
                }
            },1000);
        }

