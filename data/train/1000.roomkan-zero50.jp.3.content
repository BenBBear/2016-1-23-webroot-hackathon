            $(".free_search_cnt").text('-');
        }
    });

        function getBknCnt(){
            if(empty($(".search_class").val()) == false && click_flg == true){
                click_flg = false;
                auto_count_flg = false;
                var cnt;//カウント数
                formValue = $("form.free_search_2").serialize();
                $.ajax({
                    type: "POST",
                    url: "/search/ajax/bkncnt/",
                    data: formValue,
                    handleAs: "json",
                    success: function(re){
                        $(".free_search_cnt").text(re);
                        click_flg = true;
                    },
                    error: function(){
//                    alert('エラーです');
                    }
                });
            }else if(empty($(".search_class").val()) == false){
                auto_count_flg = true;
                auto_bknCnt();
            }
        }
                function auto_bknCnt(){
            setTimeout(function(){
                if(auto_count_flg == true){
                    auto_count_flg = false;
                    click_flg = true;
                    getBknCnt();
                }
            },1000);
        }

