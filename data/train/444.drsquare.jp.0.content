          <div class="span4 well">
            <form action="https://pulse.drsquare.jp/login" method="post" class="form-vertical">
              <fieldset>
                <legend>
                  <img style="margin-left: 100px" src="/images/mind_sonar_login_title.png" />
                </legend>
                <div class="control-group">
                  <label for="email" style="padding-top: 20px">Email</label>
                  <div class="control">
                    <input type="text" name="email" class="span4" placeholder="pulse@example.com" id="email" />
                  </div>
                </div>
                <div class="control-group">
                  <label for="password">Password</label>
                  <div class="control">
                    <input type="password" name="password" class="span4" placeholder="password" id="password" />
                  </div>
                </div>
                <a href="/user/password/remind">パスワードを忘れた方はこちら</a>
                <div class="form-actions" style="text-align: center">
                  <input type="submit" class="btn btn-primary btn-large" style="width: 150px; " value="ログイン" />
                </div>
              </fieldset>
            </form>
          </div><!-- /.span -->
          <div class="span1">　</div>
        </div><!-- /.row -->
