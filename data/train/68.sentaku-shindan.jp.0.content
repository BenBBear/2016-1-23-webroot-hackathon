<!-- 1020追加 -->
<script>
(function() {
    var advertiser_id = 647;
    var price = 0;
    var convtype = 0;
    var dat = '';
    var dest_url = '//sentaku-shindan.jp/enquete/';

    var MTBADVS = window.MTBADVS = window.MTBADVS || {};
    MTBADVS.mtb_signal = function(e) {
        var img = new Image();
        window.setTimeout(transitNextPage, 500);

        var queries = [
            'advertiser_id' + '=' + encodeURIComponent(advertiser_id),
            'price' + '=' + encodeURIComponent(price),
            'convtype' + '=' + encodeURIComponent(convtype),
            'conv_dat' + '=' + encodeURIComponent(dat),
            'url' + '=' + encodeURIComponent(location.href),
            'cachebuster' + '=' + Math.floor(Math.random() * 10e12),
            'is_click_cv=1'
        ];
        img.src = '//conv.mtburn.com/conv?' + queries.join('&');
    }

    function transitNextPage() {
        document.location.href = dest_url;
    }
})();
</script>

