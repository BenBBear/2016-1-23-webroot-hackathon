    <script>
        $(document).foundation();

        if($('.slider').length) {

            $('.slider').each(function() {

                var _slider = $(this);

                $('.slide', _slider).first().addClass('active');

                var sliderLoop;

                function startSlider() {
                    sliderLoop = setInterval(changeSlide, 5000);
                }

                function changeSlide() {
                    var _self = $('.slide.active', _slider).addClass('active');
                    $('.slide.active', _slider).next().addClass('active');
                    _self.removeClass('active');

                    if($('.slide.active', _slider).length < 1) {
                        $('.slide', _slider).first().addClass('active');
                    }
                }

                startSlider();
            });
        }
    </script>



