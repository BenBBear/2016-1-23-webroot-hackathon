    <script type="text/javascript">
        var currentConvType = $('input.ratetype[type=radio]:checked').attr('rel');
        var lastChangeInput=null;
        $('input.ratetype[type=radio]').change(function (e) {
            currentConvType = $(this).attr('rel');
            if (lastChangeInput == null){ lastChangeInput = $('input#sellamount');}
            var iso1 = $(lastChangeInput).parent().find('select').val();
            var targetInput =$(lastChangeInput).parents('.rates').find('input[type=text][id!='+$(lastChangeInput).attr('id')+']');
            var iso2 = $(targetInput).parent().find('select').val();

            convertRate($(lastChangeInput).val(),iso1,iso2,targetInput);
        });

        var valRates = {
            
                    "EUR": 
                    {"conv": { "sell": 2.7450, "buy": 2.5980 },"kas": { "sell": 2.7340, "buy": 2.6140 }},
            
                    "GEL": 
                    { "kas": { "sell": 1, "buy": 1 }, "conv": { "sell": 1, "buy": 1 } },
            
