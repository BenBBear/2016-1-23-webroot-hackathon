            $("select#selliso").change(function (e) {
           
                if ($(this).val()!= "GEL") {
                    $("select#buyiso").val('GEL');
                    $("select#buyiso").selectBoxIt().data("selectBox-selectBoxIt").refresh();
                }
                convertRate($('input#sellamount').val(),$("select#selliso").val(),$("select#buyiso").val(),$('input#buyamount'));
            
            });

            $("select#buyiso").change(function (e) {
           
                if ($(this).val() != "GEL") {
                    $("select#selliso").val('GEL');
                    $("select#selliso").selectBoxIt().data("selectBox-selectBoxIt").refresh();
                }
                convertRate($('input#buyamount').val(),$("select#buyiso").val(),$("select#selliso").val(),$('input#sellamount'));
            
            });

            $('input#sellamount').keyup(function (e) { 
                convertRate($(this).val(), $("select#selliso").val(),$("select#buyiso").val(),$('input#buyamount'));
                lastChangeInput = this;
            });
            $('input#buyamount').keyup(function (e) { 
                convertRate($(this).val(),$("select#buyiso").val(),$("select#selliso").val(), $('input#sellamount'));
                lastChangeInput = this;
            });
        
            $('input#sellamount, input#buyamount').keypress(function(e){
                if (e.charCode != 46 && e.charCode > 31 && (e.charCode < 48 || e.charCode > 57)) {e.preventDefault();return;}
                if ($(this).val().indexOf('.')>=0 &&  e.charCode==46){e.preventDefault(); return;}
            });
        });

        function convertRate(amount, fromIso, toIso, targetTb) {
            if (amount ==""){targetTb.val('');return;}
            var fi = valRates[fromIso][currentConvType];
            var ti = valRates[toIso][currentConvType];
            var resultValue ;

            var useBuyValue = $('#buyiso').val().toUpperCase() == "GEL";

            if(fromIso=="GEL"){
                resultValue = amount/(useBuyValue?ti.buy:ti.sell);
            }
            else{
                resultValue=amount*(useBuyValue?fi.buy:fi.sell);
            }
            if (isNaN(resultValue)){targetTb.val('');}
            else{
                var fullValue = resultValue.toFixed(4);
                // fullValue = fullValue.substr(0, fullValue.length-2);
                targetTb.val(fullValue);
            }
        }
        //convertRate($('input#buyamount').val(),$("select#buyiso").val(),$("select#selliso").val(), $('input#sellamount'));
        convertRate($('input#sellamount').val(),$("select#selliso").val(),$("select#buyiso").val(), $('input#buyamount'));
    </script>


    


</div>


                
