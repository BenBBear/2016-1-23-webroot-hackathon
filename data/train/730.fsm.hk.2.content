function addUrlQueryString(key,value) {
    var vars = [], hash;
    var newQueryString = "";
    var parameterString = "";
    var alreadyHasKeyParameter = false;
    
    if(window.location.href.indexOf('?')!=-1){
		newQueryString = window.location.href.substring(0,window.location.href.indexOf('?'));

		var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
	    
		for(var i = 0; i < hashes.length; i++)
		{
	        hash = hashes[i].split('=');
			var prefix="";
			
	        if(i==0){
	        	prefix = "?";
	        }else{
	        	prefix = "&";
	        }
	        
	        if(hash[0]!="" && typeof(hash[0])!="undefined" && hash[1]!="" && typeof(hash[1])!="undefined"){
		        if(hash[0]==key){
		        	alreadyHasKeyParameter = true;
		        	parameterString = parameterString + prefix + hash[0] + "=" + value;
		        }else{
		        	parameterString = parameterString + prefix + hash[0] + "=" + hash[1];
		        }
	        }
	    }
    }else{
    	newQueryString = window.location.href;
    }
    
    newQueryString = newQueryString + parameterString;
    
    if(!alreadyHasKeyParameter){
		if(parameterString==""){
			newQueryString = newQueryString + "?";
    	}else{
    		newQueryString = newQueryString + "&";
    	}
		
		newQueryString = newQueryString + key + "=" + value;
    }
    
    location.href=newQueryString;
}

function getCookie(){
	var c_name = "FSM_HK";
	var c_value = document.cookie;
	var c_start = c_value.indexOf(" " + c_name + "=");
	
	if (c_start == -1) {
		c_start = c_value.indexOf(c_name + "=");
	}
	if (c_start == -1) {
		c_value = null;
	} else {
		c_start = c_value.indexOf("=", c_start) + 1;
		var c_end = c_value.indexOf(";", c_start);
		
		if (c_end == -1) {
			c_end = c_value.length;
		}
		c_value = unescape(c_value.substring(c_start,c_end));
	}
	
	if (c_value!=null && c_value!="" && c_value=="true"){
        have_accepted_tnc = true;
		return true;
	}
	return false;
}


function setCookie(){
	createCookieForFsmHk("FSM_HK","true","/");
}

function learnMore(type){
	if(getCookie()){
		redirectTo(type);		
	}else{
		if(type=="FUND"){
			toggle();
		}
		else if(type=="BOND"){
			toggle_bonds();
		}
		else if(type=="DPMS"){
			toggle_dpms();
		}

	}
}

function redirectTo(type){
	setCookie();
	if(type=="FUND"){
		window.location = "/hk/main/home/index.svdo";
	}
	else if(type=="BOND"){
		window.location = "/hk/main/directBond/index.svdo";			 
	}
	else if(type=="DPMS"){
		window.location = "/hk/main/dpms/home/home.tpl";			 
	}
}




