            <script>
function fnc_ck(str){
	if(document.all){
		fobj=document.all.tags("input");
	}else{
		fobj=document.getElementsByTagName("input");
	}
	if ((document.getElementById('rosen0').checked && str=='station0') ||(document.getElementById('rosen1').checked && str=='station1')){
		re = new RegExp(str, "i");
		for(i=0;i<fobj.length;i++){
			if (fobj[i].className.match(re)){
				fobj[i].checked=true;
			}
		}
	}
	if ((!document.getElementById('rosen0').checked && str=='station0') ||(!document.getElementById('rosen1').checked && str=='station1')){
		re = new RegExp(str, "i");
		for(i=0;i<fobj.length;i++){
			if (fobj[i].className.match(re)){
				fobj[i].checked=false;
			}
		}
	}
}
</script>
