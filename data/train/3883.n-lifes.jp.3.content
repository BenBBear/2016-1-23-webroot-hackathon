<script type="text/javascript">
function formatString(str){
str = str.replace(/((ftp|http|https?):\/\/([-\w\.]+)+(:\d+)?(\/([\w/_\.]*(\?\S+)?)?)?)/gm,'<a href="$1" target="_blank">$1</a>');
return str;
}
function relativeTime(pastTime){
var dd = new Date(pastTime);
var yy = dd.getYear();
var mm = dd.getMonth() + 1;
var dd = dd.getDate();
if (yy < 2000) { yy += 1900; }
if (mm < 10) { mm = "0" + mm; }
if (dd < 10) { dd = "0" + dd; }
return yy + "." + mm + "." + dd;
}
