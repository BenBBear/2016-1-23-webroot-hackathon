function PG_show(linkId){
	var de = document.documentElement;
	var w = self.innerWidth || (de&&de.clientWidth) || document.body.clientWidth;

	var sc_top = document.body.scrollTop || document.documentElement.scrollTop;
	//マウスをポインターに設定
	$('#' + linkId).css('cursor','pointer');

	var clickElementy;
	
	if(sc_top + $('#osusume_'+linkId).height() - 20 < getAbsoluteTop(linkId)){
		//上に入る
		clickElementy = getAbsoluteTop(linkId) - $('#osusume_'+linkId).height() + 20; //set y position
		$('#osusume_'+linkId+'_top').removeClass("top_b");
		$('#osusume_'+linkId+'_bottom').removeClass("bottom_b");
		$('#osusume_'+linkId+'_top').addClass("top");
		$('#osusume_'+linkId+'_bottom').addClass("bottom");
	}else{
		//下に入れる
		clickElementy = getAbsoluteTop(linkId) + 110; //set y position
		$('#osusume_'+linkId+'_top').removeClass("top");
		$('#osusume_'+linkId+'_bottom').removeClass("bottom");
		$('#osusume_'+linkId+'_top').addClass("top_b");
		$('#osusume_'+linkId+'_bottom').addClass("bottom_b");
	}
	var arrowOffset = getElementWidth(linkId) - 300;
	var clickElementx = getAbsoluteLeft(linkId) + arrowOffset; //set x position
	
	$('#osusume_'+linkId).css({left: clickElementx+"px", top: clickElementy+"px"});
	$('#osusume_'+linkId).show();
}

function CountUp(userid){
	//ポップアップブロッカー対応
	if(document.images){
		(new Image()).src="http://"+cgi_host+"/homepage/counter.php?user_id="+userid;
	}
	return true;
}
// -->
