			<!-- /ko -->
			<td class="align-right condensed" data-bind="attr: { 'colspan' : tipo() == 'ENV' ? 3 : 1 }">
				<span data-bind="text:$root.obtenerSimboloMonedaPorId(idMoneda())"></span>
				<span data-bind="text:accounting.formatMoney(total(), '')"></span>
			</td>
		</tr>
	</script>
	
	
