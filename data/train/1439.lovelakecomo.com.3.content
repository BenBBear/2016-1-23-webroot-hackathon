<script>

	function disableForm()
	{
	    $("#name").prop('disabled', true);
	    $("#email").prop('disabled', true);
	    $("#message").prop('disabled', true);
	    $("#indexContactForm").prop('disabled', true);
	}
	
	function enableForm()
	{
	    $("#name").prop('disabled', false);
	    $("#email").prop('disabled', false);
	    $("#message").prop('disabled', false);
	    $("#indexContactForm").prop('disabled', false);
	}
	
	$("#indexContactForm").click(function() {
	    
	    if( !$('#name').val() ) {
	        $("#form-note").show();
	    }
	    if( !$('#email').val() ) {
	        $("#form-note").show();
	    }
	    if( !$('#message').val() ) {
	        $("#form-note").show();
	    }
	    else {
	        $("#form-note").hide ();
	        sendEnquiry();
	    }
	    
	});
	
	
	function sendEnquiry()
	{
	    
	    disableForm();
	    
	    var name = jQuery("#name").val();
	    var email = jQuery("#email").val();
	    var message = jQuery("#message").val();
	    
	    var url = "ajax_string_sendEnquiry.html";
	    
	    var query = "";
	    query += "&name=" + name;
	    query += "&email=" + email;
	    query += "&message=" + message;
	    
	    ajaxAsyncTextCall(url, query, sendEnquiry_callback);
	    
	}
	
	function sendEnquiry_callback(ajaxResponse, textStatus, XMLHttpRequest)
	{
	    enableForm();
	    
	    if(ajaxResponse != null && ajaxResponse.indexOf("error") == 0)
	    {
	       $("#form-note").addClass('alert-error').show().html('<div class=\"alert alert-error\"><h6>' + "Failed to send enquiry: " + ajaxResponse.substring(ajaxResponse.indexOf("_") + 1) + '</h6></div>');
	       return;
	    }
	    
	    $("#form-note-success").show();
	}

</script>
        
        
        
        
        
        
