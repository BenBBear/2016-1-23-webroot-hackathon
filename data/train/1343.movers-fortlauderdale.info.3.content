							case 4: dateOfMoveValid = false;document.getElementById("dateOfMoveError").innerHTML = "<span style='color:#f00;'>*In April, June, September and November the month has only 30 days</span>"; break
							case 5: dateOfMoveValid = false;document.getElementById("dateOfMoveError").innerHTML = "<span style='color:#f00;'>*February has only 28 days</span>"; break
							case 6: dateOfMoveValid = false;document.getElementById("dateOfMoveError").innerHTML = "<span style='color:#f00;'>*It's leap year, February has 29 days</span>"; break					
							default: 
