					function checkDate(y, m, d) { 

					    if (typeof(y)=='undefined' ||
					        typeof(m)=='undefined' ||
					        typeof(d)=='undefined') return 1;

					    d = Math.abs(d) || 0, m = Math.abs(m) || 0, y = Math.abs(y) || 0;

					    return arguments.length != 3 ? 1 :
						  d < 1 || d > 31 ? 2 :
						  m < 1 || m > 12 ? 3 :
						  /4|6|9|11/.test(m) && d == 31 ? 4 :
						  m == 2 && (d > ((y = !(y % 4) && (y % 1e2) ||
						  !(y % 4e2)) ? 29 : 28)) ? 5 + !!y : 0;

