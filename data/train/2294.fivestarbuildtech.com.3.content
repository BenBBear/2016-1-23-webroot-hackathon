	    else {
	        if ($('.navbar-inverse').hasClass('navbar-fixed-top')){
	            $('.navbar-inverse').addClass('navbar-static-top');
	            $('.navbar-inverse').removeClass('navbar-fixed-top');
	        }
	    }
	
	});
  </script>
  


