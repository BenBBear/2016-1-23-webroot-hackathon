  }
  window.status = statmsg;
  _hsspos = (-_hsspos > msg.length) ? 80 : _hsspos - 1;  
  fname = "_HpbScrollStatus('" + msg + "')";
  _hsstmrID = window.setTimeout(fname, delay);
}
