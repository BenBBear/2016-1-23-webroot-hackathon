        var uft = setInterval(trans,3000);
        var stsarrt= ["eventscnt","reportscnt","resultscnt","boatscnt","boattypescnt","sailorscnt"];
        var current = "events";
        var sttcnt=0;
        function trans() {
            if(!vis())
                return;

            $('#'+stsarrt[sttcnt]).fadeOut("slow", function(){
                if(sttcnt == stsarrt.length-1){
                    sttcnt = -1;
                }
                current = stsarrt[sttcnt+1];
                $('#'+stsarrt[sttcnt+1]).fadeIn("slow");
                sttcnt+=1;
            });
        }

        function handle( elem, img, state )
        {
            if ((typeof(elem.onerror) === 'function' && state === 'fail') 
                || (elem.width === 0)
              )
            {
                elem.src = img;
            }
        }
        function hideWarning(ctrl) {
            $("#genNotify").hide();
        }
        $(function () {
           
            if($.cookie("unauth") != undefined)
            {
                $('#warningMessage').text($.cookie("unauth"));
                $('#genNotify').show();
            }
        });
