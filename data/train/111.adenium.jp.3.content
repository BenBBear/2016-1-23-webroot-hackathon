<script>
$(function(){
	window._topSlider = $('#topSlider').bxSlider({
		slideWidth: 960,
		slideMargin: 0,
		//startSlide: 1,
		controls: false,
		auto: true,
		minSlides: 3,
		maxSlides: 3,
		moveSlides: 1,
		speed: 1000,
		pause: 5000,
		onSliderLoad: function(){
			$('#topSliderWrapper').css('visibility', 'visible');
			var objs = getObj("topSlider").getElementsByTagName("LI");
			for(var i=0; i<objs.length; i++){
				if(objs[i]._animation) objs[i]._animation.clearAnimation();
			}
			this.walkCurrent();
			addEvent(document, "contextmenu", bind(this.stopCurrent));
		},
		onSlideBefore: function(slideElement, oldIndex, newIndex){
			var objs	= getObj("topSlider").getElementsByTagName("LI");
			for(var i=0; i<objs.length; i++){
				if(objs[i]._moved){
					var x = objs[i].getElementsByTagName("IMG")[0].style.marginLeft.replace("px","");
					if(objs[i]._animation) objs[i]._animation.clearAnimation();
					objs[i]._animation  = new easyAnimation(objs[i].getElementsByTagName("IMG")[0], "marginLeft", x, 0, 0.5, 2).start();
					objs[i]._moved		= false;
				}
			}
		},
		onSlideAfter: function(slideElement, oldIndex, newIndex){
			this.walkCurrent();
		},
		walkCurrent: function(){
			var obj		= bxSliderSearchCurrentObj();
			if(obj._animation) obj._animation.clearAnimation();
			obj._animation 	= new easyAnimation(obj.getElementsByTagName("IMG")[0], "marginLeft", 0, -100, 5.0, 1).start();
			obj._moved		= true;
		},
		stopCurrent: function(){
			var obj		= bxSliderSearchCurrentObj();
			obj._animation.clearAnimation();
			window._topSlider.goToNextSlide();
		}
	});
	$('#bxSliderBtnL').click(function(){
		window._topSlider.stopAuto();
		window._topSlider.goToPrevSlide();
		window._topSlider.startAuto();
	});
	$('#bxSliderBtnR').click(function(){
		window._topSlider.stopAuto();
		window._topSlider.goToNextSlide();
		window._topSlider.startAuto();
	});
});
function bxSliderSearchCurrentObj(){
	var objs	= getObj("topSlider").getElementsByTagName("LI");
	var obj		= objs[0];
	var w		= getBrowserWidth();
	for(var i=0; i<objs.length; i++) objs[i]._position = cumulativeOffset(objs[i]);
	for(var i=0; i<objs.length; i++){
		if(objs[i]._position[0] <= w/2 && obj._position[0] <= objs[i]._position[0]){
			obj = objs[i];
		}
	}
	return obj;
}
</script>
<script>
$(document).ready(function() {
    $('.linklibox').click(function() {
		window.open($(this).find('a').attr('href'));
		return false;
    });
	window._orientation = window.orientation;
});
$(window).resize(function(){
	if(msie && msie <= 8
	|| ua.toUpperCase().match(/ANDROID/) && window._orientation == window.orientation) return false;
	window._orientation = window.orientation;
	if(window._reloadTimer) clearTimeout(window._reloadTimer);
	window._reloadTimer = setTimeout(reloadSlider, 500);
});
function reloadSlider(){
	window._topSlider.reloadSlider();
}
function checkSize(){
	if(getObj("sizeCheckerSS").offsetTop) return 0;
	else if(getObj("sizeCheckerS").offsetTop) return 1;
	else if(getObj("sizeCheckerM").offsetTop) return 2;
	else return 3;
}

function checkboxClick(btn){
	var obj = btn.getElementsByTagName("SPAN")[0];
	setClass(obj, getClass(obj).match("off")?"on":"off");
	
	var obj2 = btn.getElementsByTagName("input")[0];
	obj2.checked = getClass(obj).match("off")? false : true;
	
	CheckLump();
}


function linkBoxHoverInit(){
	if(!checkSize()){
		setTimeout(linkBoxHoverInit, 1000);
		return;
	}
	var objs = getElementsByClassName2(document, "linkBox", "DIV");
	for(var i=0; i<objs.length; i++){
		var obj = objs[i].getElementsByTagName("IMG")[0];
		if(obj){
			var w1	= obj.offsetWidth;
			var w2	= Math.floor(w1*1.25);
			var h1	= obj.offsetHeight;
			var h2	= Math.floor(h1*1.25);
			var x	= Math.round(-(w2-w1)/1.8);
			var y	= Math.round(-(h2-h1)/1.8);
			obj._animation1 = new easyAnimation(obj, "width", w1, w2, 0.5, 1, "", "", boxAnimationGroup);
			obj._animation2 = new easyAnimation(obj, "height", h1, h2, 0.5, 1);
			obj._animation3 = new easyAnimation(obj, "left", 0, x, 0.5, 1);
			obj._animation4 = new easyAnimation(obj, "top", 0, y, 0.5, 1);
			obj._animation5 = new easyAnimation(obj, "opacity", 1, 0.6, 0.5, 1);
			addEvent(objs[i], "mouseover", bind(linkBoxHoverMouseOver, [obj]));
			addEvent(objs[i], "mouseout", bind(linkBoxHoverMouseOut, [obj]));
		}
	}
}

function boxAnimationGroup(n, handler){
	handler._objs[0]._animation2.animation(n);
	handler._objs[0]._animation3.animation(n);
	handler._objs[0]._animation4.animation(n);
	handler._objs[0]._animation5.animation(n);
}

function linkBoxHoverMouseOver(obj){
	if(checkSize()){
		obj._animation1.start();
	}
}

function linkBoxHoverMouseOut(obj){
	obj._animation1.rewind();
}

addEvent(window, "load", linkBoxHoverInit);

function scrollHandler(){
	var obj		= getObj("pageTopBtn");
	var isShow	= (getScrollTop() > 0);
	if(!isShow && obj._animation._i <= 0){
		obj._animation.start();
	} else if(isShow && obj._animation._i >= 1){
		obj._animation.rewind();
	}
}
addEvent(window, "load", scrollHandlerInit);

function scrollHandlerInit(){
	getObj("pageTopBtn")._animation = new easyAnimation(getObj("pageTopBtn"), "opacity", 1, 0, 0.25, 1);
	addEvent(window, "scroll", scrollHandler);
	scrollHandler();
}

function tabBtnGroupInit(){
	var objs = document.getElementsByTagName("IMG");
	window._tabBtnGroups = new Array();
	for(var i=0; i<objs.length; i++){
		if(hasAttribute(objs[i], "rel") && objs[i].getAttribute("rel").match(/tabBtnGroup\[([^,\s]*)[\s,]+([^,\s]*)[\s,]+([^,\s]*)\]/)){
			objs[i]._gid	= RegExp.$1;
			objs[i]._tar	= getObj(RegExp.$2);
			objs[i]._class	= RegExp.$3;
			window._tabBtnGroups.push(objs[i]);
			addEvent(objs[i], "mousedown", bind(tabBtnGroupClick, [objs[i]]));
		}
	}
}

function tabBtnGroupClick(obj){
	var objs = window._tabBtnGroups;
	for(var i=0; i<objs.length; i++){
		if(obj == objs[i]){
			if(objs[i]._tar && !objs[i]._tar._stat){
				switchObj(objs[i]._tar);
			}
			$(objs[i]).removeEasyOver();
			objs[i].src = objs[i].src.replace(/_?o?v?\.gif/, "_ov.gif");
		} else{
			if(objs[i]._tar && objs[i]._tar._stat){
				switchObj(objs[i]._tar);
			}
			objs[i].src = objs[i].src.replace(/_?o?v?\.gif/, ".gif");
			$(objs[i]).easyOver();
		}
	}
}


function tabBtnClose(){
	var objs = window._tabBtnGroups;
	for(var i=0; i<objs.length; i++){
		if(objs[i]._tar && objs[i]._tar._stat){
			switchObj(objs[i]._tar);
		}
	}
}

function searchCaseInit(){
	window._articles = getElementsByClassName2(getObj("contentsArea"), "linkBox", "DIV");
	for(var i=0; i<window._articles.length; i++){
		if(hasAttribute(window._articles[i], "rel")){
			searchCaseSetArg(window._articles[i]);
		}
	}
}

function searchCaseSetArg(obj){
	obj._features = new Array();
	var arg = obj.getAttribute("rel").split(";");
	for(var i=0; i<arg.length; i++){
		var words = arg[i].split("=");
		if(words[0] == "sort"){
			obj._area = words[1];
		} else if(words[0]){
			obj._features[words[0]] = 1;
		}
	}
}

function searchCase(type){
	masonryRun();

	var ignoreBlocks = getElementsByClassName2(document, "searchHide", "SECTION");
	if(type == "all"){
		for(var i=0; i<window._articles.length; i++){
			var obj		= window._articles[i];
			var tar		= parentsSearchTagName(obj, "SECTION");
			if(tar._isHide)	$("#contentsArea").masonry('reveal', [$("#contentsArea").masonry('getItem', tar)]);
			tar._isHide = false;
		}
		for(var i=0; i<ignoreBlocks.length; i++){
			if(ignoreBlocks[i]._isHide){
				$("#contentsArea").masonry('reveal', [$("#contentsArea").masonry('getItem', ignoreBlocks[i])]);
				ignoreBlocks[i]._isHide = false;
			}
		}
	} else{
		var areas		= searchCaseGetThings("searchArea");
		var features	= searchCaseGetThings("searchFeature");
		for(var i=0; i<window._articles.length; i++){
			var obj		= window._articles[i];
			var tar		= parentsSearchTagName(obj, "SECTION");
			var isHit	= false;
			if(type == "sort"){
				isHit = (areas[obj._area]);
			} else{
				for(var i2 in features){
					if(obj._features[features[i2]]){
						isHit = true;
						break;
					}
				}
			}
			if(isHit && tar._isHide)	$("#contentsArea").masonry('reveal', [$("#contentsArea").masonry('getItem', tar)]);
			if(!isHit && !tar._isHide)	$("#contentsArea").masonry('hide', [$("#contentsArea").masonry('getItem', tar)]);
			tar._isHide = !isHit;
		}
		tabBtnClose();
		for(var i=0; i<ignoreBlocks.length; i++){
			if(!ignoreBlocks[i]._isHide){
				$("#contentsArea").masonry('hide', [$("#contentsArea").masonry('getItem', ignoreBlocks[i])]);
				ignoreBlocks[i]._isHide = true;
			}
		}
	}
	setTimeout('ie9init()', 500);
}

function searchCaseGetThings(id){
	var objs	= getObj(id).getElementsByTagName("SPAN");
	var ret		= (id == "searchArea")?new Array():new Object();
	for(var i=0; i<objs.length; i++){
		if(getClass(objs[i]).match("on")){
			var rel = objs[i].parentNode.getAttribute("rel");
			if(rel.match(/sort=([^;]+)/)){
				ret[RegExp.$1] = 1;
			} else if(rel.match(/feature=([^;]+)/)){
				ret[RegExp.$1] = RegExp.$1;
			}
		}
	}
	return ret;
}

addEvent(window, "load", searchCaseInit);
addEvent(window, "load", tabBtnGroupInit);

</script>
