      window.GLOBAL = {};
    }

    GLOBAL.subscribe = function(name, f) {
      if(!obj[name]) obj[name] = [];

      obj[name].push(f);
    };

    GLOBAL.publish = function(name, data) {
      if(!obj[name] || obj[name].length == 0) return;

      for(var i = 0; i < obj[name].length; i++) {
        obj[name][i](data);
      }
