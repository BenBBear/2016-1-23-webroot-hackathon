$(function(){
    var page = 0;
    var max = 4;

    $('#shop_information_prev').click(function(){
        if ( page <= 0 ) {
            page = 0;
            return false;
        }
        
        page = page - 1;
        
        $('.shop_information').css('display', 'none');
        $('#shop_information_'+page).css('display', 'block');
        
        return false;
    });

    $('#shop_information_next').click(function(){
        if ( page >= max ) {
            page = max;
            return false;
        }
        
        page = page + 1;
        
        $('.shop_information').css('display', 'none');
        $('#shop_information_'+page).css('display', 'block');
        
        return false;
    });
});
//]]>
</script>
