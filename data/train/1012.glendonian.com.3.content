<?xml version="1.0"?>
<!-- http://www.webstarts.com/crossdomain.xml -->
<!DOCTYPE cross-domain-policy SYSTEM "http://www.macromedia.com/xml/dtds/cross-domain-policy.dtd">
<cross-domain-policy>
	<allow-access-from domain="webstarts.com" secure="false" />
	<allow-access-from domain="*.webstarts.com" secure="false" />

	<allow-http-request-headers-from domain="webstarts.com" secure="false" />
	<allow-http-request-headers-from domain="*.webstarts.com" secure="false" />
</cross-domain-policy>