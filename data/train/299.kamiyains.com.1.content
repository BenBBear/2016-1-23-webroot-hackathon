<script>// <![CDATA[
function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays*24*60*60*1000));
        var expires = "expires="+d.toUTCString();
        document.cookie = cname + "=" + cvalue + "; " + expires;
    }
    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1);
            if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
        }
        return "";
    }
    //setup cookie on submit click
    $( "form" ).submit(function() {
        var informakersFormsType = {6495:"text",6499:"text",6497:"text",6498:"textarea"};
        for (var key in informakersFormsType){
            if (informakersFormsType.hasOwnProperty(key)) {
                var individualFormId = "#FormSubmission" + key;
                //check the input field type
                if(informakersFormsType[key] == "text") {
                    setCookie(key, encodeURIComponent($(individualFormId).val()), 1);
                } else if(informakersFormsType[key] == "textarea") {                 
                    setCookie(key, encodeURIComponent($(individualFormId).val()), 1);
                } else if(informakersFormsType[key] == "select") {
                   setCookie(key, $(individualFormId).val(), 1);
                } else if(informakersFormsType[key] == "checkbox") {                    
                    setCookie(key, $(individualFormId).is(':checked'), 1);
                } else if(informakersFormsType[key] == "radio") {                    
                    var radioName = 'data[FormSubmission][' + key + ']'; 
                    setCookie(key, $('input:radio[name="' + radioName + '"]:checked').val());
                } else if(informakersFormsType[key] == "date") {
                    setCookie(key, $(individualFormId).val(), 1);
                } else if(informakersFormsType[key] == "email") {
                    setCookie(key, $(individualFormId).val(), 1);
                } else if(informakersFormsType[key] == "file") {
                    //file can't be put into session. PHP and JS limitation + security issue
                }                    
            }
        }
        //console.log(document.cookie);
    });
    //script for keeping data when back button from confirm page is pressed
    var backLocation = document.referrer;
    var isConfirmForm = backLocation.split('/');
    if(isConfirmForm[isConfirmForm.length-1] == "confirmForm") {
        //script for back button to maintain value data
        var informakersFormsType = {6495:"text",6499:"text",6497:"text",6498:"textarea"};        
        window.onload = function () { 
            for (var key in informakersFormsType){
                if (informakersFormsType.hasOwnProperty(key)) {                    
                    var individualFormId = "#FormSubmission" + key;
                    //check the input field type
                    if(informakersFormsType[key] == "text") {
                        $(individualFormId).val(decodeURIComponent(getCookie(key)));
                    } else if(informakersFormsType[key] == "textarea") {                        
                        $(individualFormId).val(decodeURIComponent(getCookie(key)));
                    } else if(informakersFormsType[key] == "select") {
                       $(individualFormId).val(getCookie(key));
                    } else if(informakersFormsType[key] == "checkbox") {
                        if(getCookie(key) == "true") {
                            $(individualFormId).attr('checked', true);
                        } else {
                            $(individualFormId).attr('checked', false);
                        }
                    } else if(informakersFormsType[key] == "radio") {                    
                        var radioName = 'data[FormSubmission][' + key + ']'; 
                        var radioValue = getCookie(key);
                        $('input[name="' + radioName + '"][value="' + radioValue + '"]').attr('checked', true);
                    } else if(informakersFormsType[key] == "date") {
                        $(individualFormId).val(getCookie(key));
                    } else if(informakersFormsType[key] == "email") {
                        $(individualFormId).val(getCookie(key));
                    } else if(informakersFormsType[key] == "file") {
                        //file can't be put into session. PHP and JS limitation + security issue
                    }                   
                }
            }
        }
    }
// ]]></script>
