(function(){
    if(jQuery.ui !== undefined && jQuery.ui.dialog !== undefined)
    {
        var UIDialogInit = jQuery.ui.dialog.prototype._init;
        jQuery.ui.dialog.prototype._init = function()
        {
            var self = this;
            UIDialogInit.apply(this, arguments);

            //save position on drag stop for sticky scrolling
            this.uiDialog.bind('dragstop', function(event, ui)
            {
                if (self.options.sticky === true)
                {
                    self.options.position = [(Math.floor(ui.position.left) - jQuery(window).scrollLeft()),
                                             (Math.floor(ui.position.top) - jQuery(window).scrollTop())];
                }
            });

            //we only want to do this once
            if (jQuery.ui.dialog.dialogWindowScrollHandled === undefined)
            {
                jQuery.ui.dialog.dialogWindowScrollHandled = true;
                jQuery(window).scroll(function(e)
                {
                    jQuery('.ui-dialog-content').each(function()
                    {   //check if it's in use and that it is sticky
                        if (jQuery(this).dialog('isOpen') && jQuery(this).dialog('option', 'sticky'))
                        {
                            jQuery(this).dialog('option', 'position', jQuery(this).dialog('option','position'));
                        }
                    });
                });
            }
        };

      //  jQuery.ui.dialog.defaults.sticky = false;
    }
})();
 

