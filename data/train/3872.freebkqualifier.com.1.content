                      var aback = data.split("~~");
                      var dtext = aback[0];
                      var ltext = aback[1];
                      dialog.html(dtext);
                      dialog.dialog('option','title',ltext);
            },
            cache : false,
            async : false
            });
            dialog.dialog('open');
            return false;
}

