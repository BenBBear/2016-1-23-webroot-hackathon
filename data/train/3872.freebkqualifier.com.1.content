<script type="text/javascript">

        var  lp_to = '';
        var LEADPOPDATA = {};
        var LEADPOP = function($) {

            return {

                postsummarize : function (leadpop_lead_id,step,appendto) {

                    $.ajax( {
                            type : "POST",
                            url : '/getInfoFromStep.php',
                            data : "leadpop_lead_id="+ leadpop_lead_id + "&step=" + step,
                            success : function(data) {
                            $('#' + appendto).html();
                            $('#' + appendto).html(data);
                    },
                    cache : false,
                    async : false
                    });

                },

                summarize : function (option,makeyear,makemake,makemodel) {

                    if(option == 'fill') {
                        var s = "<p class='summaryDirectionsText'>Your vehicle is: ";
                        var year = $('#' + makeyear).val();
                        var make = $('#' + makemake).val();
                        var model = $('#' + makemodel  + ' :selected').text();
                        s = s + year + ' ' + make + ' ' + model + "</p>";
                        $('#rightside').html('');
                        $('#rightside').html(s);
                    }
                    else if(option == 'empty') {
                        $('#rightside').html('');
                    }
                },
                emptySelect : function (theSelect) {
                    $('#' + theSelect).empty();
                }, // 'lp_wrap_id_three','lp_replace_two','lp_replace_one','lp_replace_three');

                setMakeClickHandler : function (wrapperdiv,appendto,makeyearid) {

                  $('#' + wrapperdiv).find("ul").find("li").each(function() {

  		       $(this).unbind().bind('click', function() {

                        var makeyear =  $('#' + makeyearid).val();

                        var makemake =  $(this).children('span').text();

                           LEADPOP.getSelectListModelsFromVals(appendto,makeyear,makemake);

                       });

                   });

                },

                getSelectListModelsFromVals: function(appendto,makeyear,makemake) {

                        if(makeyear == "" || makemake == "") {

                            return false;

                        }

                        else {

                            $.ajax( {

                                    type : "POST",

                                    url : '/getSelectListModels.php',

                                    data : "makemake="+ makemake+"&makemodel=",

                                    success : function(data) {

                                    $('#' + appendto).empty();

                                    $('#' + appendto).append(data);

                                    $('#' + appendto).custSelectBox({selectwidth : 138,wrappername : 'lp_select_wrap_two',wrapperHtmlDiv : 'lp_hidden_wrapper_two',scrollminitems: 13,scrollheight: 230, bottomToggleDiv: 'lp_toggle_input2'}).scrollNextPop();

                            },

                            cache : false,

                            async : false

                            });

                        }

                },

                getSelectListModels: function(appendto,makemodel,makeyearid,makemakeid) {

                        var makeyear =  $('#' + makeyearid).val();

                        var makemake =  $('#' + makemakeid).val();

                        if(makeyear == "" || makemake == "") {

                            return false;

                        }

                        else {

                            $.ajax( {

                                    type : "POST",

                                    url : '/getSelectListModels.php',

                                    data : "makemake="+ makemake+"&makemodel="+makemodel,

                                    success : function(data) {

                                    $('#' + appendto).empty();

                                    $('#' + appendto).append(data);

                            },

                            cache : false,

                            async : false

                            });

                        }

                },

                getSelectList: function(tbl,val,col,appendto,order) {

                        $.ajax( {

                                type : "POST",

                                url : '/getSelectList.php',

                                data : "tbl="+ tbl + "&val=" + val + "&col=" + col + "&order=" +  order,

                                success : function(data) {

                                $('#' + appendto).append(data);

                        },

                        cache : false,

                        async : false

                        });

                },

                saveContactData  : function() {
                       var qa = "";
                       var tObj = {};
                       var tRel;
                       var trs = $('#replacediv').find('td');
                       trs.each(function()
                       {
                               if($(this).attr('rel').indexOf("@@y~") >-1) {
                                   tRel =  $(this).attr('rel');
