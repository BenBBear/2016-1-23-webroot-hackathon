			if (this.id == 'main_visual_left') {
				e = $('#main_visual_switch li').first();
			} else {
				e = $('#main_visual_switch li').last();
			}
		}
		e.length && e.find('a').trigger('click');
		return false;
	});

	$('#main_visual_switch li').last().find('a').trigger('click');

	$('a.dekabannerCon div').on('click', function () {
		var jq = $(this).parents('a');
		window.open(jq.attr('href'));
		return false;
	});
});
