			$('#main_visual_switch .act').removeClass('act');
			var tbx = e.addClass('act').attr('id').replace('topMainBtnItem', '');

			GSTOPPER = true;

			$('#main_visual91').animate({ 'margin-left': parseInt(tbx) * 716 * -1 }, 'fast', function () {
				GSTOPPER = null;
				GTIMER = setTimeout(function () {
					var e = $('#main_visual_switch .act').parent().prev().find('a');
					if (!e.length) {
						e = $('#main_visual_switch a').last();
					}
					e.trigger('click');
				}, 12000);
			});
		}

		return false;
	});

	$('#main_visual_left, #main_visual_right').on('click', function () {
		var e = $('#main_visual_switch .act').parent()[this.id == 'main_visual_left' ? 'next' : 'prev']();
