<script type="text/javascript">
    function check_form(){
        var frm = document.forms['form1'];
        var intentAccount = frm.elements['intentAccount'].value;
        var account = frm.elements['province'].value;
        var phone = frm.elements['phone'].value;
        var valicode = frm.elements['valicode'].value;
        //add by xueyinhao 20141030
        /*
         $(function(){
         $(".user-name").each(function(i){
         console.log($(this).attr("borrowId"));
         });
         });*/
        var repTest =  /^(1[3-4][0-9]{9})|(1[7-8][0-9]{9})|(15[0-35-9][0-9]{8})$/;
        if (intentAccount==""){
            alert("借款金额不能为空");
            return false;
        }else if (intentAccount<20 || intentAccount>1000)
        {
            alert("借款金额在20万元-1000万元之间");
            return false;
        }
        else if(account==0)
        {
            alert("所在城市不能为空");
            return false;
        }else if(phone !="" && !repTest.test(phone))
        {
            alert("请输入正确的手机号码");
            return false;
        }
        else if(phone=="")
        {
            alert("手机不能为空");
            return false;
        }
        else if(valicode=="")
        {
            alert("验证码不能为空");
            return false;
        }
        else{
            frm.submit();
            return true;
        }
    }
    function fnKeep(){
        var oAside=$("#homeAside"),     //侧栏元素
                iHeight=$("#productList").height()-oAside.height(), //高度之差
                iTop = oAside.offset().top,     //侧栏距离顶部距离
                iLeft,            //主体区左侧距离
                iScrollTop;			//滚动条距离
        if(iHeight>0){
            $(window).bind("scroll load resize", function() {
                iScrollTop=$(window).scrollTop();
                iLeft=$("#productList").offset().left;
                if (iScrollTop >= iTop&&iScrollTop <= iTop+iHeight) {
                    oAside.removeClass("m_t_10").css({
                        "position": "fixed",
                        "top": 0,
                        "right":iLeft
                    });
                }else{
                    if(iScrollTop>iTop){
                        oAside.addClass("m_t_10").css({
                            "position": "absolute",
                            "top": iHeight,
                            "right":0
                        });
                    }else{
                        oAside.addClass("m_t_10").css({
                            "position": "static"
                        });
                    }
                }
            }).load();
        }
    }
    fnKeep();
    $(".sucbox").slideFn({isTop:true,slideTimer:"3000"});
    jQuery(function(){
        //jQuery("[rel='tooltip']").tooltip();
    });
    function showRemainTime(){
        var endtimes=$(".endtime");
        endtimes.each(function(){
            RemainTime($(this));
        });
    }
    showRemainTime();
    setInterval("showRemainTime()",1000);

    function RemainTime(t){

        var iDay,iHour,iMinute,iSecond,account;

        var sDay="",sTime="";

        var at="data-time"

        var iTime=t.attr(at);

        if (iTime >= 0){

            iDay = parseInt(iTime/24/3600);

            iHour = parseInt((iTime/3600)%24);

            iMinute = parseInt((iTime/60)%60);

            iSecond = parseInt(iTime%60);

            if (iDay > 0){

                sDay = iDay + "天";

            }

            sTime =sDay + iHour + "时" + iMinute + "分" + iSecond + "秒";

            if(iTime==0){

                sTime="<span style='color:green'>时间到了！</span>";

            }

            t.attr(at,iTime-1);

        }else{

            sTime="<span style='color:red'>此标已过期！</span>";

        }

        t.html(sTime);

    }

</script>

