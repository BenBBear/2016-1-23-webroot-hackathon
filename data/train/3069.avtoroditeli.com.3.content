			$.cookie("bik", null);
			$.cookie("rs", null);
			$.cookie("cs", null);
			$.cookie("email", null);
			$.cookie("phone", null);
			$.cookie("comment", null);
			$.cookie("type", null);
			$.cookie("addresso2", null);
			$.cookie("raion", null);
			$.cookie("addresso3", null);
			$('#totalPrice').text('0');
			$('#totalGoods').text('0');
			$('.hPb').hide();
			$('.hPe').show();
			$(this).hide();
			$('#checkOut').hide();
