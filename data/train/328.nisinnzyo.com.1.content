function is_sp () {
    $useragents = array(
      'iPad',
      'iPhone',
      'iPod',
      'Android',
      'BlackBerry',
      'IEMobile',
    );
    $pattern = '/'.implode('|', $useragents).'/i';
    return preg_match($pattern, $_SERVER['HTTP_USER_AGENT']);
  }
function is_mobile () {
    $useragents = array(
      'DoCoMo',
      'UP.Browser',
      'SoftBank',
      'Vodafone',
      'J-PHONE',
      'WILLCOM',
    );
    $pattern = '/'.implode('|', $useragents).'/i';
    return preg_match($pattern, $_SERVER['HTTP_USER_AGENT']);
  }
