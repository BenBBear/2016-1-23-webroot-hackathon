                            info.prepend("<img class='avatarImage' src='"+imgSrc+"' />");
                        }
                        var row_pos = trigger.position().top;
                        var cur_pos = trigger.offset().top;
                        var block_top_pos = $('.jScrollPaneContainer').offset().top;
                        var block_bottom_pos = block_top_pos + block_height;
                        if ((block_bottom_pos - cur_pos) < ((block_bottom_pos - block_top_pos) / 2)) {
                            top_position = row_pos - distance;
                        } else {
                            top_position = row_pos + distance;
                        }
                        var scroll_position = $('.jScrollPaneDrag').position().top;
                        info.css({
                            display: 'block',
                            top: top_position + 'px'
                        })
                    }).mouseout(function () {
