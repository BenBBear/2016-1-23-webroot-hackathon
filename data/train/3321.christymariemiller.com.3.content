}

function toPassword(oInput) {

    var newEl = document.createElement('input');
    newEl.setAttribute('type', 'password');
    newEl.setAttribute('name', 'security/password');
    newEl.setAttribute('id', 'security_password');
    newEl.className = oInput.className;
    newEl.style.width = oInput.style.width;
    oInput.parentNode.replaceChild(newEl,oInput);
    toPassword.el = newEl;
    setTimeout('toPassword.el.focus()',100);

    return true;
}

