var RealPingControl = RealPingControl || (function ($) {
    $(function () {
        var $realPingContainer = $(".realping_container");

        function isDaylightSavingTime() {
            var rightNow = new Date(),
                date1 = new Date(rightNow.getFullYear(), 0, 1, 0, 0, 0, 0),
                date2 = new Date(rightNow.getFullYear(), 6, 1, 0, 0, 0, 0),
                temp = date1.toGMTString(),
                date3 = new Date(temp.substring(0, temp.lastIndexOf(" ") - 1)),
                temp2 = date2.toGMTString(),
                date4 = new Date(temp2.substring(0, temp2.lastIndexOf(" ") - 1)),
                hoursDiffStdTime = (date1 - date3) / (1000 * 60 * 60),
                hoursDiffDaylightTime = (date2 - date4) / (1000 * 60 * 60);
            return hoursDiffDaylightTime !== hoursDiffStdTime;
        }

        function getRealPingTimezone() {
            var now = new Date(),
                RealPingTimezone = {
                    EASTERN: 0,
                    CENTRAL: 1,
                    MOUNTAIN: 2,
                    PACIFIC: 3,
                    HAWAII: 5,
                    ALASKA: 4
                },
                dstOffsetMap = {
                    "4": RealPingTimezone.EASTERN,
                    "5": RealPingTimezone.CENTRAL,
                    "6": RealPingTimezone.MOUNTAIN,
                    "7": RealPingTimezone.PACIFIC,
                    "10": RealPingTimezone.HAWAII,
                    "8": RealPingTimezone.ALASKA
                },
                stdOffsetMap = {
                    "5": RealPingTimezone.EASTERN,
                    "6": RealPingTimezone.CENTRAL,
                    "7": RealPingTimezone.MOUNTAIN,
                    "8": RealPingTimezone.PACIFIC,
                    "10": RealPingTimezone.HAWAII,
                    "9": RealPingTimezone.ALASKA
                },
                offsetMap = isDaylightSavingTime() ? dstOffsetMap : stdOffsetMap;
            return offsetMap[(now.getTimezoneOffset() / 60).toString()];
        }
        
