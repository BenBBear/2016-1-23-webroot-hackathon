	<script type="text/javascript">
		$(document).ready(function ()  {
			/*************************
			 * メインイメージスライダー
			 ************************/
			$('.bxslider').bxSlider({
				mode: 'fade', // horizontal, vartivcal, fadeの3パターン
				captions: false, // 画像の下部に説明文を表示することができる 画像のtitleタグのテキストを表示
				auto: true, // 自動遷移を有効にする
				pager: false, // ページャを表示する
				controls: false, // コントロールを表示する
				speed: 2000, // 一回のスライドにかかる時間を指定する（ミリ秒）
				pause: 10000, // 自動遷移の間隔（停止時間）を指定する（ミリ秒）
				touchEnabled: false, // スワイプできるようにするか否か（スマートフォン）
				autoControls: false // 自動遷移のコントロールを表示する autoプロパティがtrueの場合にのみ有効になる
			}).show();
			
			var items = $('.estate-check').find('input');
			$("#box-allcheck_send input").attr("disabled", "").css({
				opacity: "0.75",
				cursor: "default"
			});
			/*************************
			* 全件一括チェックと送信ボタン表示判定
			*************************/
			$('#allCheck,.box-allCheck label').click(function(){ //全選択・全解除をクリックしたとき
				if($(this).is(':checked')) { //全選択・全解除がcheckedだったら
					$(items).prop('checked', true); //アイテムを全部checkedにする
					$("#box-allcheck_send input").removeAttr("disabled").css({
						opacity: "1.0",
						cursor: "pointer"
					}); //送信ボタンを表示
				} else { //全選択・全解除がcheckedじゃなかったら
					$(items).prop('checked', false); //アイテムを全部checkedはずす
					$("#box-allcheck_send input").attr("disabled", "").css({
						opacity: "0.75",
						cursor: "default"
					}); //送信ボタンを非表示
				}
			});
			/*************************
			 * 物件チェック個別の送信ボタン表示判定
			 *************************/
				items.click(function(){ // 物件チェックボックスをクリックして
					if (items.is(':checked')) { // チェック済みの物件がある場合
						$("#box-allcheck_send input").removeAttr("disabled").css({
							opacity: "1.0",
							cursor: "pointer"
						}); //送信ボタンを表示
					} else {
						$("#box-allcheck_send input").removeAttr("disabled").css({
							opacity: "0.75",
							cursor: "default"
						}); //送信ボタンを表示
					}
				});
		});
	</script>
	
