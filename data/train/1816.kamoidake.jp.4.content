hpb.ContentLoader.prototype={
	loadSiteMenu:function(url){
		if(window.ActiveXObject) {
			// IE
			try {
				this.request = new ActiveXObject ("Msxml2.XMLHTTP");
			} catch(e) {
				this.request = new ActiveXObject ("Microsoft.XMLHTTP");
