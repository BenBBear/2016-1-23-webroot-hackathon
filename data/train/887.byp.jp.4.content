<script language="JavaScript"><!--
var msg = "北海道ツーリング2012　お盆休み編終了！　妻は走り足りなかった模様！　(^^;";
var delay = 120;
var pos = 120;
function scroll_status() {
	setTimeout("scroll_status()", delay);
	var out = "";
	if (pos >= 0) {
		for (c = 0; c < pos; c++) {
			out += " ";
		}
		out += msg;
	}
	else {
		out = msg.substring(-pos, msg.length);
	}
	pos = (-pos > msg.length) ? 100 : pos - 1;
	window.status = out;
}
scroll_status();
//--></FONT></script></font></font>
