<script>
$(document).ready(function(){
	// $('#header').css('top','-40px');

	var isDeviceNonMobile = $.restive.isNonMobile();
	var controller = new ScrollMagic.Controller();
	if ( isDeviceNonMobile == true ) {
		var controllerNonMobile = new ScrollMagic.Controller();
	};

	// change header
	var sceneHeader1 = new ScrollMagic.Scene({triggerElement:'#templates', triggerHook:'onLeave', offset:-4})
		.setClassToggle('#header','header-fixed')
		.addTo(controller);
	var sceneHeader2 = new ScrollMagic.Scene({triggerElement:'#templates-wedding', triggerHook:'onLeave', offset:-4})
		.setClassToggle('#header','header-wedding')
		.addTo(controller);
	var sceneHeader3 = new ScrollMagic.Scene({triggerElement:'#scene', triggerHook:'onLeave', offset:-4})
		.setClassToggle('#header','header-sepalated')
		.addTo(controller);
	// pin slide
	var scenePin1 = new ScrollMagic.Scene({triggerElement:'#templates', duration:600, triggerHook:'onLeave'})
		.setPin('#templates')
		.addTo(controllerNonMobile);
	var scenePin2 = new ScrollMagic.Scene({triggerElement:'#templates-wedding', duration:600, triggerHook:'onLeave'})
		.setPin('#templates-wedding')
		.addTo(controllerNonMobile);

	// animate scroll
	controller.scrollTo(function (newpos) {
		TweenMax.to(window, 0.5, {scrollTo: {y: newpos}});
	});

	// bind scroll to anchor links
	if ( isDeviceNonMobile == true ) {
		$(document).on("click", "a[href^='#']", function (e) {
			var id = $(this).attr("href");
			if ($(id).length > 0) {
				e.preventDefault();
				controller.scrollTo(id);
				if (window.history && window.history.pushState) {
					history.pushState("", document.title, id);
				}
			}
		});
	};
});
</script>

