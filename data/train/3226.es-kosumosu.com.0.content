<p>
  <script>
(function(){
  var o = document.getElementById("hutia");
  var s = o.innerHTML;
  var p = document.createElement("span");
  var n = document.createElement("a");
  p.innerHTML = s.substring(0,100);
  n.innerHTML = s.length > 100 ? "...続きを読む" : "";
  n.href = "###";
  n.onclick = function(){
    if (n.innerHTML == "...続きを読む"){
      n.innerHTML = "戻る";
      p.innerHTML = s;
    }else{
      n.innerHTML = "...続きを読む";
      p.innerHTML = s.substring(0,100);
    }
  }
  o.innerHTML = "";
  o.appendChild(p);
  o.appendChild(n);
})();
  </script>
