click: {type: 'internationalReset', source: 'click'}
});
EDDIE.init('body', {
// sendInternational: Sends Ajax for results
sendInternational: function(e) {
var _dataPacket = {
};
//console.log('sendInternational', e, serviceBaseURL + 'sales_tools/rep_finder/international/' + e.value); 
jQuery.ajax(
serviceBaseURL + 'sales_tools/rep_finder/international/' + e.value,
{
data: _dataPacket,
type: 'GET',
dataType: 'jsonp',
accepts: 'text/json',
success: function(json) {
var html = '', s='';
//console.log('Get International Success!',json);
html = html + '<p>';
html = html + json.rep_name + '<br />';
html = html + json.rep_title + '<br />';
html = html + json.rep_email + '</p>';
$('body').trigger({
type: 'internationalRep',
html: html
});
$('body').trigger({
type: 'rfv4_action',
id: 'international',
status: 'result'
});
if(typeof(Storage) !== 'undefined') {
localStorage.setItem('rfv4_int_country', json.country);
localStorage.setItem('rfv4_int_result_html', html);
}
// ANALYTICS
//Rep Finder Selected Section (eVar 45)
//Int Rep Finder Country (eVar 46)
//Rep Finder Result Viewed (Event 34)
s=s_gi('pearsonschool');
s.linkTrackVars='eVar45,eVar46,events';
s.linkTrackEvents='event34';
s.eVar45='international';
s.eVar46=json.country;
s.tl(this, 'o', 'Rep Finder');
// ANALYTICS END
},
error: function(trans, atext) {
console.log('Get International FAIL!',trans, atext);
}
}
);
},
// internationalRep: Displays results
internationalRep: function(e) {
//console.log('internationalRep:', e);
jQuery('#rf_internationalForm').hide();
jQuery('#rf_internationalRep').html(e.html)
jQuery('#rf_internationalRepPanel').show();
},
// internationalReset: Reset everything
internationalReset: function(e) {
jQuery('#rf_internationalForm').show();
jQuery('#rf_internationalRepPanel').hide();
jQuery('#rf_internationalRep').html('');
}
});
