    <script>
		$(function() {
			var lv = {};
			//
						//
			$.backstretch("data/img/bg/sfeerhut_achtergrond__1_of_1_.png", {speed: 0});
			$("#overlay").animate({opacity:0},500,function(){
				$("#overlay").css({display:'none'});
			});
			$(".bgtxt").css({opacity:0});
			//
						//
			$.superbox();
			//
						//
			//
			//
			$(window).resize(function(){
				lv.curposx = ($(window).width() - $('#cmainhold').outerWidth())/2;
				if(lv.curposx < 350){lv.curposx = 350;}
				lv.curposy = ($(window).height() - $('#cmainhold').outerHeight())/2;
				if(lv.curposy < 150){lv.curposy = 150;}
				$('#cmainhold').css({top: lv.curposy, left: lv.curposx});
				$('#nav-left').css({top: lv.curposy, left: lv.curposx-(56+38)});
				$('#nav-right').css({top: lv.curposy, left: lv.curposx+(485+56)});
				//
				lv.curposx = ($(window).width() - $('#top-logo').outerWidth())/2;
				if(lv.curposx < 460){lv.curposx = 460;}
				$('#top-logo').css({left: lv.curposx});
				//
				lv.curposy = ($(window).height() - $('#navhold').outerHeight())/2;
				if(lv.curposy < 150){lv.curposy = 150;}
				$('#navhold').css({top: lv.curposy});
				//
				lv.ftrpos = $(window).height()-110;
				if(lv.ftrpos < 720){lv.ftrpos = 720;}
				$('#footer').css({top: lv.ftrpos});
			});
			$(window).resize();
			//
			$('#top-logo').css({visibility: "visible"});
			$('#navhold').css({visibility: "visible"});
			$('#nav-left').css({visibility: "visible"});
			$('#nav-right').css({visibility: "visible"});
			$('#footer').css({visibility: "visible"});
			$('#cmainhold').css({visibility: "visible"}); //init
			//
						//
			//
			//
			//
		});
	</script>
