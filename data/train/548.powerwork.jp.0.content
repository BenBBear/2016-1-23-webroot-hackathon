<script type="text/javascript">
var header_job = "";
var header_job_child = "";
var header_state = "";
var header_city = "";
var action = "";
var syokiflg = "true";
var df1 = $.Deferred();
var df2 = $.Deferred();
var df3 = $.Deferred();
var city_url = "http://powerwork.jp/?act=axcity";
var job_url = "http://powerwork.jp/?act=axjobchild";
var getcount_url = "http://powerwork.jp/?act=axsearchcount";

if(action === "" || action === "top"){
$(function() {
	catname1 ="header_job";
	areaname ="header_state";
	document.headerSearch.elements[catname1].selectedIndex = 0;
	catchange2(0,0,0);
	document.headerSearch.elements[areaname].selectedIndex = 0;
	setAreaItem2(0,0,0);
	
});

$.when(df1, df2).done(function(){
	 searchCondChg2();
	 syokiflg = "false";
});
}else{
$(function() {
	if(header_job != ""){
		catname1 ="header_job";
		valLen = document.headerSearch.elements[catname1].length;
		for (valSelCnt = 0; valSelCnt < valLen; valSelCnt++){
			valCheck = document.headerSearch.elements[catname1][valSelCnt].value;
			if(valCheck == header_job){
				document.headerSearch.elements[catname1].selectedIndex = valSelCnt;
				catchange2(0,header_job,0);
				header_job="";
				break;
			}
		}
	}

	if(header_state != ""){
		areaname ="header_state";
		valLen = document.headerSearch.elements[areaname].length;
		for (valSelCnt = 0; valSelCnt < valLen; valSelCnt++){
			valCheck = document.headerSearch.elements[areaname][valSelCnt].value;
			if(valCheck == header_state){
				document.headerSearch.elements[areaname].selectedIndex = valSelCnt;
				setAreaItem2(0,header_state,0);
				header_state="";
				break;
			}
		}
	}

});

$.when(df1, df2).done(function(){
	 searchCondChg2();
	 syokiflg = "false";
});
}
function catchange2(type,code,cld){
	$.ajax({
		url : job_url,
		type : "get",
		data : ({
			"code":code
		}),
		dataType: 'json',
		success: function(request) {
			var cldname = "header_job_child";
			if(0 < cld){
				wkcnt = cld + 1;
				cldname = cldname + wkcnt;
			}
			var optionCount = document.headerSearch.elements[cldname].options.length; //市区郡のOPTION数取得
			for ( i=0 ; i <= optionCount ; i++ ){
				document.headerSearch.elements[cldname].options[0]=null
			} //市区郡削除
			document.headerSearch.elements[cldname].options[0] = new Option("選択してください",0);
			if(request != null){
				for( i=0; i<request.length; i++){
					ii = i + 1 //OPTIONは2番目から表示
					var ct_name = request[i].name;
					var ct_cd = request[i].id;
					document.headerSearch.elements[cldname].options[ii] = new Option(ct_name,ct_cd);
				}
				
				if(header_job_child != ""){
        			valLen = document.headerSearch.elements[cldname].length;
        		      for (valSelCnt = 0; valSelCnt < valLen; valSelCnt++){
        		          valCheck = document.headerSearch.elements[cldname][valSelCnt].value;
        		          if(valCheck == header_job_child){
        		              document.headerSearch.elements[cldname].selectedIndex = valSelCnt;
        		              header_job_child="";
        		              break;
        		          }
        		      }
        		}
			}
			if(syokiflg == "true"){
				df1.resolve();
			}else{
				df3.resolve();
				df3.done(function(){
					 searchCondChg2();
				});
			}
		}
	});
}

function setAreaItem2(type,code,cld){
	$.ajax({
		url : city_url,
		type : "get",
		data : ({
			"area":code
		}),
		dataType: 'json',
		success: function(request) {
			var cldname = "header_city";
			if(0 < cld){
				wkcnt = cld + 1;
				cldname = cldname + wkcnt;
			}
			var optionCount = document.headerSearch.elements[cldname].options.length; //市区郡のOPTION数取得
			for ( i=0 ; i <= optionCount ; i++ ){
				document.headerSearch.elements[cldname].options[0]=null
			} //市区郡削除
			if(request != null){
				document.headerSearch.elements[cldname].options[0] = new Option("選択してください",0);
				for( i=0; i<request.length; i++){
					ii = i + 1 //OPTIONは2番目から表示
					var ct_name = request[i].name;
					var ct_cd = request[i].id;
					document.headerSearch.elements[cldname].options[ii] = new Option(ct_name,ct_cd);
				}
				if(header_city != ""){
        			valLen = document.headerSearch.elements[cldname].length;
        		      for (valSelCnt = 0; valSelCnt < valLen; valSelCnt++){
        		          valCheck = document.headerSearch.elements[cldname][valSelCnt].value;
        		          if(valCheck == header_city){
        		              document.headerSearch.elements[cldname].selectedIndex = valSelCnt;
        		              header_city="";
        		              break;
        		          }
        		      }
        		}
			}
			if(syokiflg == "true"){
				df2.resolve();
			}else{
				df3.resolve();
				df3.done(function(){
					 searchCondChg2();
				});
			}
		}
	});
}

function searchCondChg2(){
	var f=document.headerSearch;

	header_state = document.headerSearch.header_state.options[document.headerSearch.header_state.selectedIndex].value;
	header_city = document.headerSearch.header_city.options[document.headerSearch.header_city.selectedIndex].value;
	header_job = document.headerSearch.header_job.options[document.headerSearch.header_job.selectedIndex].value;
	header_job_child = document.headerSearch.header_job_child.options[document.headerSearch.header_job_child.selectedIndex].value;
	
	if(header_city != "0"){
		header_state = 0;
	}
	if(header_job_child != "0"){
		header_job = 0;
	}
	

	cont = '&state=' + header_state + '&city=' + header_city + '&job=' + header_job + '&job_child=' + header_job_child;

	$.ajax({
		url : getcount_url + cont,
		type : "get",
		success: function(request) {
			if(request != null){
				var re = $.parseJSON(request);
				jQuery("#header_mirai").html('<strong style="margin-bottom: 0px;color:#CC0000;font-size:15px;font-weight:bold;font-style:normal;">'+ re[0].cnt +'</strong>');			}
		}
	});
}

function HeaderSearchSubmit(){
	header_state = document.headerSearch.header_state.options[document.headerSearch.header_state.selectedIndex].value;
	header_city = document.headerSearch.header_city.options[document.headerSearch.header_city.selectedIndex].value;
	header_job = document.headerSearch.header_job.options[document.headerSearch.header_job.selectedIndex].value;
	header_job_child = document.headerSearch.header_job_child.options[document.headerSearch.header_job_child.selectedIndex].value;
	
	
	if(header_city != "0"){
		$('#header_state_hidden').val(0);
	}else{
		$('#header_state_hidden').val(header_state);
	}
	$('#header_city_hidden').val(header_city);
	
	if(header_job_child != "0"){
		$('#header_job_hidden').val(0);
	}else{
		$('#header_job_hidden').val(header_job);
	}
	$('#header_job_child_hidden').val(header_job_child);
	
	state = document.headerSearch.header_state_hidden.value;
	city = document.headerSearch.header_city_hidden.value;
	job = document.headerSearch.header_job_hidden.value;
	job_child = document.headerSearch.header_job_child_hidden.value;
	

	document.headerSearch.submit();
}



</script>

