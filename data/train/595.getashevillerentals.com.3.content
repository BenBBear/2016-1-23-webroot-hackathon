<script>
function setCookieShowed () {
    if (getCookie("externalUrl")) {
        return false;
    }
    var d = new Date();
    d.setTime(d.getTime() + 365 * 24 * 60 * 60 * 1000);
    time = d.toUTCString();
    setCookie(
        "externalUrl", true,
        time, "/"
    );

    return true;
}
var formId = FormValidator.configure(
        [{"elementId":"emailelement","type":"TEXT","validators":
            [{"validator":"NotEmpty","params":[],"validStop":true,"messages":"Please enter your email address."},
             {"validator":"EmailAddress","params":[],"validStop":false,"messages":"Please enter your email address."}
            ]},
             {"elementId":"keywordselement","type":"TEXT","validators":
                 [{"validator":"NotEmpty","params":[],"validStop":true,"messages":"Please specify keywords."},
            ]},
             {"elementId":"locationelement","type":"TEXT","validators":
                 [{"validator":"NotEmpty","params":[],"validStop":true,"messages":"Please specify location."}
             ]}]
        );

$('#submitButton').click(function(e){
    if (FormValidator.check(formId)) {
        e.target.disabled = true;
        $.ajax({
            url: '/locationcheck',
            data: {q: $('#locationelement').val()},
            method: 'GET',
            dataType: "json",
            success: function(r) {
                if (r.success) {
                    checkNotification();
                } else {
                    $('#locationelement').after('<ul class="errors"><li>'+eval(r.msg)+'</li></ul>');
                }
                e.target.disabled = false;
            }
        });
        e.preventDefault();
    } else {
        e.preventDefault();
    }
});

function checkNotification() {
    var data = {
        'email': $('#emailelement').val(),
        'keywords': $('#keywordselement').val(),
        'location': $('#locationelement').val()
    };
    var config = {
        url: '/check-notification-alert',
        success: function(r) {
            if (r.status != 'ok') {
                el = $("#exitFormError");
                if (el.length) {
                    el.html("You are already subscribed to this search alert");
                    $('#ux-window').setHeight(modal.getHeight + 10 + 'px');
                }
            } else {
                createAlert();
            }
        },
        data: data,
        type: "POST",
        dataType: "json"
    };
    $.ajax(config);
}

function createAlert() {
     var data = {
         'email': $('#emailelement').val(),
         'keywords': $('#keywordselement').val(),
         'location': $('#locationelement').val()
     };
     var config = {
         url: "/doneMega-notification-alert",
         success: function (res, o)
         {
             if (res.status != 'ok') {
                 var uri = '/notification-request.html';
                 GetIt.postToURL(uri, data);
             }
         },
         data: data,
         type: "POST",
         dataType: "json"
     };
     $.ajax(config);
     $('#popupHeader').html('Your Search Alert has been Created!');
     $('#exitHref').remove();
     $('#ux-window-outerContainer').css('height', 'auto');
     $('#popupBody').html('<p style="margin-bottom: 10px;">To active your email alert notification,'
         + 'please check your email and click the activation link.</p>'
         + '<p>Remember, you can unsubscribe at anytime.</p><br>'
         + '<div class="button"><center><table><tr><td><img src="/images/left-button.png" width=12 height=41 alt=""/>'
         + '</td><td class="buttonText"><b><a id = "exitHref" href = "#" class="simplemodal-close">Take me back to my search</a><b></td><td>'
         +  '<img src="/images/right-button.png" width=12 height=41 alt=""/></td></tr></table></center></div>');

     $("#exitHref").click(function() {
         $.modal.close();
     });
}

function setCookie(name, value, expires, path, domain, secure) {
    document.cookie = name + "=" + escape(value) +
    ((expires) ? "; expires=" + expires : "") +
    ((path) ? "; path=" + path : "") +
    ((domain) ? "; domain=" + domain : "") +
    ((secure) ? "; secure" : "");
};

function getCookie(name) {
    var matches = document.cookie.match(new RegExp(
      "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
    ));
    return matches ? decodeURIComponent(matches[1]) : false;
}
