<script>
function UserEmail(e) {
    var v = e.val();
    if (v.length == 0) {
        return {
            valid: false,
            code: "Please enter a valid email address."
        };
    }

    if ( v.indexOf('@') < 1 ||
            v.indexOf('@') != v.lastIndexOf('@') ||
            v.indexOf('.', v.indexOf('@')) == -1 ||
            v.lastIndexOf('.') == v.length-1 ||
            v.charAt(v.indexOf('@') + 1) == '.'
        ) {
        return {
            valid: false,
            code: "Please enter a valid email address."
        };
    }
    return {
        valid: true
    }
    }
</script>
