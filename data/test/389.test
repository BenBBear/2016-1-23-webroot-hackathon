    function GetMarketTopsData(highestLowestType,elementId) {
        $.post(
            $resource + "HighestLowestType=" + highestLowestType + "&ServiceType=GetMarketDataHighestLowest",
            function ($x) {
                ArrayMarketTops = $x;
                if(HighestLowestCount < 8)
                    ShowMarketTops( elementId);
                HighestLowestCount ++;
            }
        );
    }

    function GetMarketSummaryData() 
    {
        $.post(
            $resource + "&ServiceType=GetMarketSummaryData",
            function ($x) {

                $x.NavigateToDetails = function() {
                    window.location = "/default.aspx?tabid=" + TabId + "&ctl=details&isin=" + this.LowestChangeValue;
                };

                $x.TseIndex       = AddComma($x.TseIndex);
                $x.TradedVolum    = AddComma($x.TradedVolume);
                $x.TradedValue    = AddComma($x.TradedValue);
                $x.MostTradedVolumeClosingPrice   = AddComma($x.MostTradedVolumeClosingPrice);
                $x.MostTradedValueClosingPrice    = AddComma($x.MostTradedValueClosingPrice);
                $x.MostTradedValue    = AddComma($x.MostTradedValue);
                $x.MostTradedVolume   = AddComma($x.MostTradedVolume);

                ko.cleanNode(document.getElementById("MSummary"));
                ko.applyBindings($x , document.getElementById("MSummary"));
                first = false;
            }
        );
    }

    function ShowMarketTops(elementId) {
        
        for (i = 0; i < ArrayMarketTops.length; i++) {
                    
            ArrayMarketTops[i].VariationStatus      = ArrayMarketTops[i].PriceVariation < 0 ? "negative" : "positive";
            ArrayMarketTops[i].TotalTradeValue      = AddComma(ArrayMarketTops[i].TotalTradeValue);
            ArrayMarketTops[i].TotalTradeQuantity   = AddComma(ArrayMarketTops[i].TotalTradeQuantity);
            ArrayMarketTops[i].LastTradePrice       = AddComma(ArrayMarketTops[i].LastTradePrice);
            ArrayMarketTops[i].ClosingPrice         = AddComma(ArrayMarketTops[i].ClosingPrice );
