            ArrayMarketTops[i].BestSellLimitQuantity= AddComma(ArrayMarketTops[i].BestSellLimitQuantity);
            ArrayMarketTops[i].BestBuyPrice         = AddComma(ArrayMarketTops[i].BestBuyPrice );
            ArrayMarketTops[i].BestBuyLimitQuantity = AddComma(ArrayMarketTops[i].BestBuyLimitQuantity );
            ArrayMarketTops[i].TradeCount           = AddComma(ArrayMarketTops[i].TradeCount );
            ArrayMarketTops[i].Queued               = AddComma(ArrayMarketTops[i].Queued);
