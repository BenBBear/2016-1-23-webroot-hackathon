                $resource, { SelectedCategory: SelectedCategory, ModuleIDSettings: ModuleIDSettings },
                function($response) {
                    if ($response) {
                        $target.html($response);
                        $(".NewsTicker").modernTicker(OPTIONS);
                    }
                }); 
        }

    });

