        var NewsColor           = '#fff' ;
        var NewsColorHover      = '#fff' ;
        var Width               = '100%' ;
        var BorderRadius        = '1' ;

        ApplyCss();

        ShowNews();

        var OPTIONS;

        function ApplyCss()
        {
            $(".mt-label , .mt-controls").css("background-color" , StaticBGColor);
            $(".mt-label , .mt-controls").css("color" , StaticColor);
