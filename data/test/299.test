			}
			else if(window.innerWidth < 1158)
			{
				sl("menu-6", baseffset + 65, 36);
			}
			else if(window.innerWidth < 1375)
			{
				sl("menu-6", baseffset + 25, 36);
			}
			else if(window.innerWidth < 1690)
			{
				sl("menu-6", baseffset + 65, 36);
			}
			else
			{
				sl("menu-6", baseffset + 45, 36);
			}
			*/
			var space = ( sizePlate.width / 2 ) - ( sizeLogo.width / 2); 
			
			if(window.innerWidth > 1800)
			{
				var addOffset = 70;
			}
			else
			{
				var addOffset = 20; // ( ( space / 2 ) - ( sizeMenu.width / 2 ) ) - 7;
			}
			
			sl("menu-6", baseffset + addOffset, 36);
			
			/*
			if(window.innerWidth < 1186)
			{
				sl("menu-6", baseffset + 50, 36);  // ( sizePlate.width / 15 )
			}
			else 
			{ 
				sl("menu-6", baseffset + 40, 36);  // 74 ( sizePlate.width / 24 )
			}
