		// get HTML object by its ID 
		function go(element)
		{
			return document.getElementById(element);
		}

		// add an event handler to HTML object
		function addEvent(element, type, handler)
		{
			if(typeof element == "string")
			{
				var element = go(element);
			}
			
			if(element.addEventListener)
			{
				element.addEventListener(type, handler, false);
			} 
			else 
			{
				element.attachEvent("on"+type, handler);
			}
		}

		// get sizes of HTML element
		function gs(element)
		{
			if(typeof element == "string")
			{
				var element = go(element);
			}
			
			var width = element.offsetWidth;
			var height = element.offsetHeight;
			return {"width": width, "height": height, "w": width, "h": height};
		}

		// get offset(position)  of HTML object
		function gp(element)
		{
			if(typeof element == "string")
			{
				var element = go(element);
			}
			
			var left = 0;
			var top = 0;
					
			do
			{
				left += element.offsetLeft;
				top += element.offsetTop;
				element = element.offsetParent;
			}
			while(element.offsetParent)
			
			if(navigator.userAgent.indexOf("Mac") != -1 && typeof document.body.leftMargin != "undefined")
			{
				left += document.body.leftMargin;
				top += document.body.topMargin;
			}
			
			left += element.offsetLeft;
			top += element.offsetTop;
			
			return {x: left, y: top};
		}

		// get all offsets of object (x, x2, y, y2)
		function gps(element)
		{
			if(typeof element == "string")
			{
				var element = go(element);
			}
			
			var position = gp(element);
			var sizes = gs(element);
			
			var x = parseInt(position.x, 10);
			var x2 = parseInt(position.x, 10) + parseInt(sizes.width, 10);
			var y = parseInt(position.y, 10);
			var y2 = parseInt(position.y, 10) + parseInt(sizes.height, 10);
			
			return {x: x, x2: x2, y: y, y2: y2};
		}
		
		// remove HTML object from DOM
		function remEl(element)
		{
			if(typeof element == "string")
			{
				var element = go(element);
			}
			
			if(element) 
			{
				element.parentNode.removeChild(element);
			}
		}
		
		// set location to HTML object, move to the place of X and Y coords on the screen
		function sl(element, x, y)
		{
			if(typeof element == "string")
			{
				var element = go(element);
			}
			
			element.style.left = x+"px";
			element.style.top = y+"px";
		}

		function makeResizeStuff()
		{
			var posPlate = gps("nav_bar_container");
			var sizePlate = gs("nav_bar_container");
			
			var sizeLogo = gs("nav_bar_logo");
			
			go("nav_bar_logo").style.position = "absolute";
			go("nav_bar_logo").style.zIndex = 1000000;
			
			sl("nav_bar_logo", ( sizePlate.width / 2 ) - ( sizeLogo.width / 2), 0);
			
			go("menu-6").style.position = "absolute";
			go("menu-6").style.zIndex = 100000000;
			
			
			var posLogo = gps("nav_bar_logo");
			
			var sizeMenu = gs("menu-6");
			
			
			var baseffset = ( sizePlate.width / 2 ) + ( sizeLogo.width / 2);
			
