<script type="text/JavaScript"><!--

function check_login(){

	var id = $('#inp_id').val();
	var pass = $('#inp_pass').val();
	var errmsg = '';

	if(id == ''){
		$('#inp_id').focus();
		errmsg = '「ID」を入力してください。';
	} else if (pass == ""){
		$('#inp_pass').focus();
		errmsg = '「パスワード」を入力してください。';
	} else if (id.length < 6){
		$('#inp_id').focus();
		errmsg = '「ID」の形式に誤りがあります。';
	}

	if(errmsg != ''){
		alert(errmsg);
		return false;
	}
	return true;
}


//--></script>

